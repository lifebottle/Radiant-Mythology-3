<Entries>
  <Entry>
    <PointerOffsetDec>874</PointerOffsetDec>
    <PointerOffsetHex>0x36a</PointerOffsetHex>
    <JapaneseText>じゃあな、ジーニアス！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>897</PointerOffsetDec>
    <PointerOffsetHex>0x381</PointerOffsetHex>
    <JapaneseText>うん、カイウス。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>915</PointerOffsetDec>
    <PointerOffsetHex>0x393</PointerOffsetHex>
    <JapaneseText>また明日！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>926</PointerOffsetDec>
    <PointerOffsetHex>0x39e</PointerOffsetHex>
    <JapaneseText>あ、姉さん……と、リタ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>949</PointerOffsetDec>
    <PointerOffsetHex>0x3b5</PointerOffsetHex>
    <JapaneseText>ちょっと何よ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>965</PointerOffsetDec>
    <PointerOffsetHex>0x3c5</PointerOffsetHex>
    <JapaneseText>そのオマケみたいな呼び方</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>990</PointerOffsetDec>
    <PointerOffsetHex>0x3de</PointerOffsetHex>
    <JapaneseText>随分楽しそうに話していたわね。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1022</PointerOffsetDec>
    <PointerOffsetHex>0x3fe</PointerOffsetHex>
    <JapaneseText>明日もまた、会う約束を？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1047</PointerOffsetDec>
    <PointerOffsetHex>0x417</PointerOffsetHex>
    <JapaneseText>うん！　</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1057</PointerOffsetDec>
    <PointerOffsetHex>0x421</PointerOffsetHex>
    <JapaneseText>一緒に遊ぶんだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1072</PointerOffsetDec>
    <PointerOffsetHex>0x430</PointerOffsetHex>
    <JapaneseText>そう。　</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1082</PointerOffsetDec>
    <PointerOffsetHex>0x43a</PointerOffsetHex>
    <JapaneseText>いい事だわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1093</PointerOffsetDec>
    <PointerOffsetHex>0x445</PointerOffsetHex>
    <JapaneseText>ほら、カイウスってリカンツでしょ？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1129</PointerOffsetDec>
    <PointerOffsetHex>0x469</PointerOffsetHex>
    <JapaneseText>獣に変身出来るから、今までと違う</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1163</PointerOffsetDec>
    <PointerOffsetHex>0x48b</PointerOffsetHex>
    <JapaneseText>色んな遊びが出来るんじゃないかなって</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1200</PointerOffsetDec>
    <PointerOffsetHex>0x4b0</PointerOffsetHex>
    <JapaneseText>知ってた？　獣になるとね、周りの色とか</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1240</PointerOffsetDec>
    <PointerOffsetHex>0x4d8</PointerOffsetHex>
    <JapaneseText>匂いとか、全然普段と違うんだって！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1275</PointerOffsetDec>
    <PointerOffsetHex>0x4fb</PointerOffsetHex>
    <JapaneseText>すごいなぁ……。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1293</PointerOffsetDec>
    <PointerOffsetHex>0x50d</PointerOffsetHex>
    <JapaneseText>一体どんな感じなんだろ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1316</PointerOffsetDec>
    <PointerOffsetHex>0x524</PointerOffsetHex>
    <JapaneseText>ジーニアス……気持ちはわかるけれど</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1352</PointerOffsetDec>
    <PointerOffsetHex>0x548</PointerOffsetHex>
    <JapaneseText>あまり大きな声で「リカンツ」という</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1388</PointerOffsetDec>
    <PointerOffsetHex>0x56c</PointerOffsetHex>
    <JapaneseText>種族名を口にしない方がいいわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1417</PointerOffsetDec>
    <PointerOffsetHex>0x589</PointerOffsetHex>
    <JapaneseText>どうしてさ？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1430</PointerOffsetDec>
    <PointerOffsetHex>0x596</PointerOffsetHex>
    <JapaneseText>元々は蔑称だったのよ、リカンツって。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1468</PointerOffsetDec>
    <PointerOffsetHex>0x5bc</PointerOffsetHex>
    <JapaneseText>すっごい大昔の話だけど</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1491</PointerOffsetDec>
    <PointerOffsetHex>0x5d3</PointerOffsetHex>
    <JapaneseText>えっ！？　そうだったの？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1516</PointerOffsetDec>
    <PointerOffsetHex>0x5ec</PointerOffsetHex>
    <JapaneseText>もちろん、現在は他意なく</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1542</PointerOffsetDec>
    <PointerOffsetHex>0x606</PointerOffsetHex>
    <JapaneseText>種族名として使われてはいるけれど</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1576</PointerOffsetDec>
    <PointerOffsetHex>0x628</PointerOffsetHex>
    <JapaneseText>元々の由来を思うと……複雑なところね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1613</PointerOffsetDec>
    <PointerOffsetHex>0x64d</PointerOffsetHex>
    <JapaneseText>そう？　</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1623</PointerOffsetDec>
    <PointerOffsetHex>0x657</PointerOffsetHex>
    <JapaneseText>別にいいんじゃない？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1645</PointerOffsetDec>
    <PointerOffsetHex>0x66d</PointerOffsetHex>
    <JapaneseText>当のカイウスは気にしてないみたいだし</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1682</PointerOffsetDec>
    <PointerOffsetHex>0x692</PointerOffsetHex>
    <JapaneseText>気にし過ぎる方が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1700</PointerOffsetDec>
    <PointerOffsetHex>0x6a4</PointerOffsetHex>
    <JapaneseText>かえって良くないでしょ、こういうの</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1735</PointerOffsetDec>
    <PointerOffsetHex>0x6c7</PointerOffsetHex>
    <JapaneseText>それはそうだけれど……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1758</PointerOffsetDec>
    <PointerOffsetHex>0x6de</PointerOffsetHex>
    <JapaneseText>ま、根本は「何が差別か？」って</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1790</PointerOffsetDec>
    <PointerOffsetHex>0x6fe</PointerOffsetHex>
    <JapaneseText>とこなんでしょうけど。　あたしにとっては</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1832</PointerOffsetDec>
    <PointerOffsetHex>0x728</PointerOffsetHex>
    <JapaneseText>気にする方も気にしろって方も同罪ね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1867</PointerOffsetDec>
    <PointerOffsetHex>0x74b</PointerOffsetHex>
    <JapaneseText>……リタは手厳しいわね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1890</PointerOffsetDec>
    <PointerOffsetHex>0x762</PointerOffsetHex>
    <JapaneseText>率直な意見を述べたまでよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
</Entries>