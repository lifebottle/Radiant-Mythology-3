<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>70</PointerOffsetDec>
    <PointerOffsetHex>0x46</PointerOffsetHex>
    <JapaneseText>襞</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>4</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1028</PointerOffsetDec>
    <PointerOffsetHex>0x404</PointerOffsetHex>
    <JapaneseText>あんた達、聞いたかい？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1052</PointerOffsetDec>
    <PointerOffsetHex>0x41c</PointerOffsetHex>
    <JapaneseText>世界中でやってた戦争が、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1078</PointerOffsetDec>
    <PointerOffsetHex>0x436</PointerOffsetHex>
    <JapaneseText>次々と終戦になってるそうじゃないか</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1113</PointerOffsetDec>
    <PointerOffsetHex>0x459</PointerOffsetHex>
    <JapaneseText>でも、その一方で</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1131</PointerOffsetDec>
    <PointerOffsetHex>0x46b</PointerOffsetHex>
    <JapaneseText>ジルディアのキバからの侵食が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1161</PointerOffsetDec>
    <PointerOffsetHex>0x489</PointerOffsetHex>
    <JapaneseText>進んでいるんでしょ？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1182</PointerOffsetDec>
    <PointerOffsetHex>0x49e</PointerOffsetHex>
    <JapaneseText>戦争なんてしてる場合じゃないって、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1218</PointerOffsetDec>
    <PointerOffsetHex>0x4c2</PointerOffsetHex>
    <JapaneseText>やっと気付いたんだよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1239</PointerOffsetDec>
    <PointerOffsetHex>0x4d7</PointerOffsetHex>
    <JapaneseText>皮肉よね。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1251</PointerOffsetDec>
    <PointerOffsetHex>0x4e3</PointerOffsetHex>
    <JapaneseText>国を越えた危機が訪れない限り、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1283</PointerOffsetDec>
    <PointerOffsetHex>0x503</PointerOffsetHex>
    <JapaneseText>人は争いを止められないのかしら？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1316</PointerOffsetDec>
    <PointerOffsetHex>0x524</PointerOffsetHex>
    <JapaneseText>そういう見方もありますわね。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1346</PointerOffsetDec>
    <PointerOffsetHex>0x542</PointerOffsetHex>
    <JapaneseText>ですが、他にも理由があるかも</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1376</PointerOffsetDec>
    <PointerOffsetHex>0x560</PointerOffsetHex>
    <JapaneseText>しれませんわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1389</PointerOffsetDec>
    <PointerOffsetHex>0x56d</PointerOffsetHex>
    <JapaneseText>そうですわね…例えば、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1413</PointerOffsetDec>
    <PointerOffsetHex>0x585</PointerOffsetHex>
    <JapaneseText>人々が争う事に疲れ果ててしまった、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1449</PointerOffsetDec>
    <PointerOffsetHex>0x5a9</PointerOffsetHex>
    <JapaneseText>とか…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1456</PointerOffsetDec>
    <PointerOffsetHex>0x5b0</PointerOffsetHex>
    <JapaneseText>ナタリアも、そろそろライマに</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1486</PointerOffsetDec>
    <PointerOffsetHex>0x5ce</PointerOffsetHex>
    <JapaneseText>帰れるんじゃないの？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1507</PointerOffsetDec>
    <PointerOffsetHex>0x5e3</PointerOffsetHex>
    <JapaneseText>いいえ、その前に王族として、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1537</PointerOffsetDec>
    <PointerOffsetHex>0x601</PointerOffsetHex>
    <JapaneseText>国民を守らなくてはなりませんわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1568</PointerOffsetDec>
    <PointerOffsetHex>0x620</PointerOffsetHex>
    <JapaneseText>その為にも、最後までこのアドリビトムで</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1608</PointerOffsetDec>
    <PointerOffsetHex>0x648</PointerOffsetHex>
    <JapaneseText>お手伝いをさせて頂きたいのです</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1639</PointerOffsetDec>
    <PointerOffsetHex>0x667</PointerOffsetHex>
    <JapaneseText>それに…世界を守った後、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1665</PointerOffsetDec>
    <PointerOffsetHex>0x681</PointerOffsetHex>
    <JapaneseText>私には成すべき計画がありますの</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1696</PointerOffsetDec>
    <PointerOffsetHex>0x6a0</PointerOffsetHex>
    <JapaneseText>計画？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1703</PointerOffsetDec>
    <PointerOffsetHex>0x6a7</PointerOffsetHex>
    <JapaneseText>ええ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1708</PointerOffsetDec>
    <PointerOffsetHex>0x6ac</PointerOffsetHex>
    <JapaneseText>主権者による一方的な支配、統治ではなく、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1750</PointerOffsetDec>
    <PointerOffsetHex>0x6d6</PointerOffsetHex>
    <JapaneseText>全ての人々に権利を与え、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1776</PointerOffsetDec>
    <PointerOffsetHex>0x6f0</PointerOffsetHex>
    <JapaneseText>共に歩む世界を作る計画ですわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1805</PointerOffsetDec>
    <PointerOffsetHex>0x70d</PointerOffsetHex>
    <JapaneseText>今まで人々は、それぞれの自己意識は</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1841</PointerOffsetDec>
    <PointerOffsetHex>0x731</PointerOffsetHex>
    <JapaneseText>もちろんの事、国を愛する心を持っていて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1882</PointerOffsetDec>
    <PointerOffsetHex>0x75a</PointerOffsetHex>
    <JapaneseText>お互い簡単に譲れるものでは</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1910</PointerOffsetDec>
    <PointerOffsetHex>0x776</PointerOffsetHex>
    <JapaneseText>ありませんでしたわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1929</PointerOffsetDec>
    <PointerOffsetHex>0x789</PointerOffsetHex>
    <JapaneseText>ですが今、この危機を迎える事によって、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1969</PointerOffsetDec>
    <PointerOffsetHex>0x7b1</PointerOffsetHex>
    <JapaneseText>人々は国や種族を越えて、意識を結び付ける</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2011</PointerOffsetDec>
    <PointerOffsetHex>0x7db</PointerOffsetHex>
    <JapaneseText>事に気が付き始めています</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2036</PointerOffsetDec>
    <PointerOffsetHex>0x7f4</PointerOffsetHex>
    <JapaneseText>私は、この意識があれば、国を越えて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2074</PointerOffsetDec>
    <PointerOffsetHex>0x81a</PointerOffsetHex>
    <JapaneseText>世界を統合していけると思うのです</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2107</PointerOffsetDec>
    <PointerOffsetHex>0x83b</PointerOffsetHex>
    <JapaneseText>きっと、世界を変える事が出来ますわ！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2144</PointerOffsetDec>
    <PointerOffsetHex>0x860</PointerOffsetHex>
    <JapaneseText>でも、競争とか、主張とか…。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2174</PointerOffsetDec>
    <PointerOffsetHex>0x87e</PointerOffsetHex>
    <JapaneseText>人間ってそういうものじゃない？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2205</PointerOffsetDec>
    <PointerOffsetHex>0x89d</PointerOffsetHex>
    <JapaneseText>簡単には行かないと思うけど…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2234</PointerOffsetDec>
    <PointerOffsetHex>0x8ba</PointerOffsetHex>
    <JapaneseText>競争も、広い視野で見れば、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2262</PointerOffsetDec>
    <PointerOffsetHex>0x8d6</PointerOffsetHex>
    <JapaneseText>あり方が変わってくるはずですわ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2296</PointerOffsetDec>
    <PointerOffsetHex>0x8f8</PointerOffsetHex>
    <JapaneseText>オルタ・ビレッジの様に</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2319</PointerOffsetDec>
    <PointerOffsetHex>0x90f</PointerOffsetHex>
    <JapaneseText>意識が変われば、全く別のものに</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2351</PointerOffsetDec>
    <PointerOffsetHex>0x92f</PointerOffsetHex>
    <JapaneseText>なっていくのでしょう</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2372</PointerOffsetDec>
    <PointerOffsetHex>0x944</PointerOffsetHex>
    <JapaneseText>そうすれば、世界中の対立を、みんなが越えて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2418</PointerOffsetDec>
    <PointerOffsetHex>0x972</PointerOffsetHex>
    <JapaneseText>人々が結び付いていく様な方向に</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>123</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2450</PointerOffsetDec>
    <PointerOffsetHex>0x992</PointerOffsetHex>
    <JapaneseText>いくだろうと考えていますわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>124</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2477</PointerOffsetDec>
    <PointerOffsetHex>0x9ad</PointerOffsetHex>
    <JapaneseText>そうすれば、資源も分かち合える様になり、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>125</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2519</PointerOffsetDec>
    <PointerOffsetHex>0x9d7</PointerOffsetHex>
    <JapaneseText>新しい倫理を打ち立てる事も出来るでしょう</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>126</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2560</PointerOffsetDec>
    <PointerOffsetHex>0xa00</PointerOffsetHex>
    <JapaneseText>全てのものを全ての人が分かち合う</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>127</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2594</PointerOffsetDec>
    <PointerOffsetHex>0xa22</PointerOffsetHex>
    <JapaneseText>文明は必ず生み出せますわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>128</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2619</PointerOffsetDec>
    <PointerOffsetHex>0xa3b</PointerOffsetHex>
    <JapaneseText>政治的に統一されて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>129</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2641</PointerOffsetDec>
    <PointerOffsetHex>0xa51</PointerOffsetHex>
    <JapaneseText>一つの国になるというんじゃ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>130</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2669</PointerOffsetDec>
    <PointerOffsetHex>0xa6d</PointerOffsetHex>
    <JapaneseText>ないんだね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>131</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2680</PointerOffsetDec>
    <PointerOffsetHex>0xa78</PointerOffsetHex>
    <JapaneseText>はい。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>132</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2688</PointerOffsetDec>
    <PointerOffsetHex>0xa80</PointerOffsetHex>
    <JapaneseText>それぞれの多様性を認め、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>133</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2714</PointerOffsetDec>
    <PointerOffsetHex>0xa9a</PointerOffsetHex>
    <JapaneseText>文化的に結び付いていく、という事です</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>134</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2751</PointerOffsetDec>
    <PointerOffsetHex>0xabf</PointerOffsetHex>
    <JapaneseText>いいね。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>135</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2761</PointerOffsetDec>
    <PointerOffsetHex>0xac9</PointerOffsetHex>
    <JapaneseText>そういう世界になれば</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>136</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2782</PointerOffsetDec>
    <PointerOffsetHex>0xade</PointerOffsetHex>
    <JapaneseText>ううん、そうしなくっちゃいけないんだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>137</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2819</PointerOffsetDec>
    <PointerOffsetHex>0xb03</PointerOffsetHex>
    <JapaneseText>はい。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>138</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2827</PointerOffsetDec>
    <PointerOffsetHex>0xb0b</PointerOffsetHex>
    <JapaneseText>だから私は、帰れませんの♪</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>139</Id>
    <Status />
  </Entry>
</Entries>