<Entries>
  <Entry>
    <PointerOffsetDec>1032</PointerOffsetDec>
    <PointerOffsetHex>0x408</PointerOffsetHex>
    <JapaneseText>ジーニアスくん？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1050</PointerOffsetDec>
    <PointerOffsetHex>0x41a</PointerOffsetHex>
    <JapaneseText>……どうしたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1067</PointerOffsetDec>
    <PointerOffsetHex>0x42b</PointerOffsetHex>
    <JapaneseText>……あ……クレア……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1088</PointerOffsetDec>
    <PointerOffsetHex>0x440</PointerOffsetHex>
    <JapaneseText>……ちょっと本を読んでただけなんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1123</PointerOffsetDec>
    <PointerOffsetHex>0x463</PointerOffsetHex>
    <JapaneseText>ぐすっ……ごめんね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1145</PointerOffsetDec>
    <PointerOffsetHex>0x479</PointerOffsetHex>
    <JapaneseText>恥ずかしいところ見せちゃった……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1178</PointerOffsetDec>
    <PointerOffsetHex>0x49a</PointerOffsetHex>
    <JapaneseText>悲しいお話だったのね……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1206</PointerOffsetDec>
    <PointerOffsetHex>0x4b6</PointerOffsetHex>
    <JapaneseText>ほら、涙を拭いて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1223</PointerOffsetDec>
    <PointerOffsetHex>0x4c7</PointerOffsetHex>
    <JapaneseText>どんなお話だったか、聞いてもいい？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1270</PointerOffsetDec>
    <PointerOffsetHex>0x4f6</PointerOffsetHex>
    <JapaneseText>ハーフエルフと、人間の話だよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1299</PointerOffsetDec>
    <PointerOffsetHex>0x513</PointerOffsetHex>
    <JapaneseText>お互いがお互いを忌み嫌って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1327</PointerOffsetDec>
    <PointerOffsetHex>0x52f</PointerOffsetHex>
    <JapaneseText>憎み合って……ヒトだけじゃなくて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1361</PointerOffsetDec>
    <PointerOffsetHex>0x551</PointerOffsetHex>
    <JapaneseText>最後は、世界まで傷付けてしまう話</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1403</PointerOffsetDec>
    <PointerOffsetHex>0x57b</PointerOffsetHex>
    <JapaneseText>頭ではわかってるんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1427</PointerOffsetDec>
    <PointerOffsetHex>0x593</PointerOffsetHex>
    <JapaneseText>このままじゃいけないって。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1455</PointerOffsetDec>
    <PointerOffsetHex>0x5af</PointerOffsetHex>
    <JapaneseText>その教訓の為に書かれた本なんだって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1490</PointerOffsetDec>
    <PointerOffsetHex>0x5d2</PointerOffsetHex>
    <JapaneseText>でもボク、どうしても心から</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1518</PointerOffsetDec>
    <PointerOffsetHex>0x5ee</PointerOffsetHex>
    <JapaneseText>人間を好きになれないなって……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1552</PointerOffsetDec>
    <PointerOffsetHex>0x610</PointerOffsetHex>
    <JapaneseText>そういう気持ちが、なかなか消せないんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1591</PointerOffsetDec>
    <PointerOffsetHex>0x637</PointerOffsetHex>
    <JapaneseText>嫌な奴だよね……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1608</PointerOffsetDec>
    <PointerOffsetHex>0x648</PointerOffsetHex>
    <JapaneseText>……あなたもたくさん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1630</PointerOffsetDec>
    <PointerOffsetHex>0x65e</PointerOffsetHex>
    <JapaneseText>辛い事があって、傷付いて来たのね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1672</PointerOffsetDec>
    <PointerOffsetHex>0x688</PointerOffsetHex>
    <JapaneseText>ジーニアスくん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1690</PointerOffsetDec>
    <PointerOffsetHex>0x69a</PointerOffsetHex>
    <JapaneseText>ハーフエルフにも、ヒトにも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1718</PointerOffsetDec>
    <PointerOffsetHex>0x6b6</PointerOffsetHex>
    <JapaneseText>同じ様に心があるわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1737</PointerOffsetDec>
    <PointerOffsetHex>0x6c9</PointerOffsetHex>
    <JapaneseText>傷付いたら、誰だって痛いと感じる。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1773</PointerOffsetDec>
    <PointerOffsetHex>0x6ed</PointerOffsetHex>
    <JapaneseText>なぜだと思う？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1788</PointerOffsetDec>
    <PointerOffsetHex>0x6fc</PointerOffsetHex>
    <JapaneseText>……同じ、ヒトだからっていうの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1821</PointerOffsetDec>
    <PointerOffsetHex>0x71d</PointerOffsetHex>
    <JapaneseText>半分正解で、半分はずれかしら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1850</PointerOffsetDec>
    <PointerOffsetHex>0x73a</PointerOffsetHex>
    <JapaneseText>答えはね……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1866</PointerOffsetDec>
    <PointerOffsetHex>0x74a</PointerOffsetHex>
    <JapaneseText>心には、種族なんてないからよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1895</PointerOffsetDec>
    <PointerOffsetHex>0x767</PointerOffsetHex>
    <JapaneseText>心には……種族がない……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1920</PointerOffsetDec>
    <PointerOffsetHex>0x780</PointerOffsetHex>
    <JapaneseText>誰かを愛しいと思ったり、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1946</PointerOffsetDec>
    <PointerOffsetHex>0x79a</PointerOffsetHex>
    <JapaneseText>食べ物を美味しいと感じたり……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1977</PointerOffsetDec>
    <PointerOffsetHex>0x7b9</PointerOffsetHex>
    <JapaneseText>そういう時、自分がどんな種族であるかを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2017</PointerOffsetDec>
    <PointerOffsetHex>0x7e1</PointerOffsetHex>
    <JapaneseText>あなたは意識しているかしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2046</PointerOffsetDec>
    <PointerOffsetHex>0x7fe</PointerOffsetHex>
    <JapaneseText>……そんな風に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2062</PointerOffsetDec>
    <PointerOffsetHex>0x80e</PointerOffsetHex>
    <JapaneseText>思ってみた事もなかったよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2087</PointerOffsetDec>
    <PointerOffsetHex>0x827</PointerOffsetHex>
    <JapaneseText>そうか……そうなんだね……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2114</PointerOffsetDec>
    <PointerOffsetHex>0x842</PointerOffsetHex>
    <JapaneseText>今度、ジーニアスくんの為に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2142</PointerOffsetDec>
    <PointerOffsetHex>0x85e</PointerOffsetHex>
    <JapaneseText>美味しいピーチパイを焼くわ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2172</PointerOffsetDec>
    <PointerOffsetHex>0x87c</PointerOffsetHex>
    <JapaneseText>だから、良かったら食べに来て</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2201</PointerOffsetDec>
    <PointerOffsetHex>0x899</PointerOffsetHex>
    <JapaneseText>ロイドさんやコレットさんや……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2233</PointerOffsetDec>
    <PointerOffsetHex>0x8b9</PointerOffsetHex>
    <JapaneseText>あなたの大切なお友達と一緒に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2262</PointerOffsetDec>
    <PointerOffsetHex>0x8d6</PointerOffsetHex>
    <JapaneseText>……うん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2271</PointerOffsetDec>
    <PointerOffsetHex>0x8df</PointerOffsetHex>
    <JapaneseText>みんなと……絶対、食べに行くよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
