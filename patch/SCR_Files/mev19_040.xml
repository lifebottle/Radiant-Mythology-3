<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>66</PointerOffsetDec>
    <PointerOffsetHex>0x42</PointerOffsetHex>
    <JapaneseText>鴨</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>4</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1128</PointerOffsetDec>
    <PointerOffsetHex>0x468</PointerOffsetHex>
    <JapaneseText>カノンノ、返事をしろ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1149</PointerOffsetDec>
    <PointerOffsetHex>0x47d</PointerOffsetHex>
    <JapaneseText>戻らなきゃ…！！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1166</PointerOffsetDec>
    <PointerOffsetHex>0x48e</PointerOffsetHex>
    <JapaneseText>身体に戻らなきゃ、戻らなきゃ…！！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1201</PointerOffsetDec>
    <PointerOffsetHex>0x4b1</PointerOffsetHex>
    <JapaneseText>…ぁっ…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1210</PointerOffsetDec>
    <PointerOffsetHex>0x4ba</PointerOffsetHex>
    <JapaneseText>カノンノ…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1221</PointerOffsetDec>
    <PointerOffsetHex>0x4c5</PointerOffsetHex>
    <JapaneseText>…元に、戻れた…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1238</PointerOffsetDec>
    <PointerOffsetHex>0x4d6</PointerOffsetHex>
    <JapaneseText>現身から意識が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1254</PointerOffsetDec>
    <PointerOffsetHex>0x4e6</PointerOffsetHex>
    <JapaneseText>分離しかけているんだな</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1277</PointerOffsetDec>
    <PointerOffsetHex>0x4fd</PointerOffsetHex>
    <JapaneseText>頭より気を取り込んで、腹に溜め、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1311</PointerOffsetDec>
    <PointerOffsetHex>0x51f</PointerOffsetHex>
    <JapaneseText>そのまま足より流せ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1330</PointerOffsetDec>
    <PointerOffsetHex>0x532</PointerOffsetHex>
    <JapaneseText>心身と自然を一つにする、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1356</PointerOffsetDec>
    <PointerOffsetHex>0x54c</PointerOffsetHex>
    <JapaneseText>武術の基本だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1369</PointerOffsetDec>
    <PointerOffsetHex>0x559</PointerOffsetHex>
    <JapaneseText>はい…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1376</PointerOffsetDec>
    <PointerOffsetHex>0x560</PointerOffsetHex>
    <JapaneseText>………頭から…気を取り込んで…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1408</PointerOffsetDec>
    <PointerOffsetHex>0x580</PointerOffsetHex>
    <JapaneseText>お腹に……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1419</PointerOffsetDec>
    <PointerOffsetHex>0x58b</PointerOffsetHex>
    <JapaneseText>あ…、不思議…。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1437</PointerOffsetDec>
    <PointerOffsetHex>0x59d</PointerOffsetHex>
    <JapaneseText>ちゃんと身体に心が収まっていく</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1469</PointerOffsetDec>
    <PointerOffsetHex>0x5bd</PointerOffsetHex>
    <JapaneseText>感じがする！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1482</PointerOffsetDec>
    <PointerOffsetHex>0x5ca</PointerOffsetHex>
    <JapaneseText>フ…、お前は繋がりを知っているのだな。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1522</PointerOffsetDec>
    <PointerOffsetHex>0x5f2</PointerOffsetHex>
    <JapaneseText>知らない者はいくらやっても</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1550</PointerOffsetDec>
    <PointerOffsetHex>0x60e</PointerOffsetHex>
    <JapaneseText>出来ないのだが</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1565</PointerOffsetDec>
    <PointerOffsetHex>0x61d</PointerOffsetHex>
    <JapaneseText>繋がり…？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1576</PointerOffsetDec>
    <PointerOffsetHex>0x628</PointerOffsetHex>
    <JapaneseText>世界との繋がりだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1593</PointerOffsetDec>
    <PointerOffsetHex>0x639</PointerOffsetHex>
    <JapaneseText>ヒトは地に足をつけて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1617</PointerOffsetDec>
    <PointerOffsetHex>0x651</PointerOffsetHex>
    <JapaneseText>世界と共に生きるものだ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1643</PointerOffsetDec>
    <PointerOffsetHex>0x66b</PointerOffsetHex>
    <JapaneseText>だが、ほとんどの者はそれを忘れている</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1680</PointerOffsetDec>
    <PointerOffsetHex>0x690</PointerOffsetHex>
    <JapaneseText>目に見える部分ばかりに気を取られてな。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1720</PointerOffsetDec>
    <PointerOffsetHex>0x6b8</PointerOffsetHex>
    <JapaneseText>そうして、自らを自らの意志で</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1750</PointerOffsetDec>
    <PointerOffsetHex>0x6d6</PointerOffsetHex>
    <JapaneseText>世界から切り離して生きている</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1779</PointerOffsetDec>
    <PointerOffsetHex>0x6f3</PointerOffsetHex>
    <JapaneseText>ヒトで世界は溢れているが、皆孤独なのだ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1821</PointerOffsetDec>
    <PointerOffsetHex>0x71d</PointerOffsetHex>
    <JapaneseText>繋がりを忘れてしまっている</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1848</PointerOffsetDec>
    <PointerOffsetHex>0x738</PointerOffsetHex>
    <JapaneseText>お互いを全く意識せずに</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1872</PointerOffsetDec>
    <PointerOffsetHex>0x750</PointerOffsetHex>
    <JapaneseText>それぞれの人生を生きていると</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1902</PointerOffsetDec>
    <PointerOffsetHex>0x76e</PointerOffsetHex>
    <JapaneseText>思い込んでいる</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1917</PointerOffsetDec>
    <PointerOffsetHex>0x77d</PointerOffsetHex>
    <JapaneseText>そこから、心の飢餓が始まる。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1947</PointerOffsetDec>
    <PointerOffsetHex>0x79b</PointerOffsetHex>
    <JapaneseText>争いや略奪…他にも様々な事が起こりうる</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1986</PointerOffsetDec>
    <PointerOffsetHex>0x7c2</PointerOffsetHex>
    <JapaneseText>ヒトや様々な生き物、そして世界。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2020</PointerOffsetDec>
    <PointerOffsetHex>0x7e4</PointerOffsetHex>
    <JapaneseText>これらが等しく「繋がり」を知る事が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2056</PointerOffsetDec>
    <PointerOffsetHex>0x808</PointerOffsetHex>
    <JapaneseText>出来れば…、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2069</PointerOffsetDec>
    <PointerOffsetHex>0x815</PointerOffsetHex>
    <JapaneseText>自他を慈しみ、権利を奪わず、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2099</PointerOffsetDec>
    <PointerOffsetHex>0x833</PointerOffsetHex>
    <JapaneseText>差を作る気持ちは起こらない</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2126</PointerOffsetDec>
    <PointerOffsetHex>0x84e</PointerOffsetHex>
    <JapaneseText>うん、そうだね。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2144</PointerOffsetDec>
    <PointerOffsetHex>0x860</PointerOffsetHex>
    <JapaneseText>風来草やユルングの樹はそれを教えてくれた</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2185</PointerOffsetDec>
    <PointerOffsetHex>0x889</PointerOffsetHex>
    <JapaneseText>ヒトって、どうして</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2205</PointerOffsetDec>
    <PointerOffsetHex>0x89d</PointerOffsetHex>
    <JapaneseText>そうなっていったんだろう…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2232</PointerOffsetDec>
    <PointerOffsetHex>0x8b8</PointerOffsetHex>
    <JapaneseText>覚えておくのだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>123</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2247</PointerOffsetDec>
    <PointerOffsetHex>0x8c7</PointerOffsetHex>
    <JapaneseText>どういう事態においても、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>124</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2273</PointerOffsetDec>
    <PointerOffsetHex>0x8e1</PointerOffsetHex>
    <JapaneseText>原因を知るという事は、再び危機が訪れた時、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>125</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2317</PointerOffsetDec>
    <PointerOffsetHex>0x90d</PointerOffsetHex>
    <JapaneseText>それを回避する為の手立てにはなる</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>126</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2350</PointerOffsetDec>
    <PointerOffsetHex>0x92e</PointerOffsetHex>
    <JapaneseText>だが、大事なのは原因を追うばかりになる</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>127</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2390</PointerOffsetDec>
    <PointerOffsetHex>0x956</PointerOffsetHex>
    <JapaneseText>過去に留まらぬ事</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>128</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2407</PointerOffsetDec>
    <PointerOffsetHex>0x967</PointerOffsetHex>
    <JapaneseText>今は、変化を起こす行動を選択する事だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>129</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2444</PointerOffsetDec>
    <PointerOffsetHex>0x98c</PointerOffsetHex>
    <JapaneseText>心を「今」から離してはならない。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>130</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2478</PointerOffsetDec>
    <PointerOffsetHex>0x9ae</PointerOffsetHex>
    <JapaneseText>今からお前達がする様に、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>131</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2504</PointerOffsetDec>
    <PointerOffsetHex>0x9c8</PointerOffsetHex>
    <JapaneseText>過去を知りにいくならなおさらだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>132</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2535</PointerOffsetDec>
    <PointerOffsetHex>0x9e7</PointerOffsetHex>
    <JapaneseText>言葉を心に留める</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>133</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2552</PointerOffsetDec>
    <PointerOffsetHex>0x9f8</PointerOffsetHex>
    <JapaneseText>はい。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>134</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2560</PointerOffsetDec>
    <PointerOffsetHex>0xa00</PointerOffsetHex>
    <JapaneseText>わかりました</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>135</Id>
    <Status />
  </Entry>
</Entries>