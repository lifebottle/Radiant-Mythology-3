<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>672</PointerOffsetDec>
    <PointerOffsetHex>0x2a0</PointerOffsetHex>
    <JapaneseText>お前は、いつ王位を継ぐつもりなんだ？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>709</PointerOffsetDec>
    <PointerOffsetHex>0x2c5</PointerOffsetHex>
    <JapaneseText>いつまでも、今の生活を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>733</PointerOffsetDec>
    <PointerOffsetHex>0x2dd</PointerOffsetHex>
    <JapaneseText>続けるわけじゃないんだろう</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>760</PointerOffsetDec>
    <PointerOffsetHex>0x2f8</PointerOffsetHex>
    <JapaneseText>そうだな……。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>776</PointerOffsetDec>
    <PointerOffsetHex>0x308</PointerOffsetHex>
    <JapaneseText>大臣達からも、口うるさく</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>802</PointerOffsetDec>
    <PointerOffsetHex>0x322</PointerOffsetHex>
    <JapaneseText>言われてはいるのだが……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>827</PointerOffsetDec>
    <PointerOffsetHex>0x33b</PointerOffsetHex>
    <JapaneseText>……まだ、気にしているのか。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>857</PointerOffsetDec>
    <PointerOffsetHex>0x359</PointerOffsetHex>
    <JapaneseText>父君の事を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>868</PointerOffsetDec>
    <PointerOffsetHex>0x364</PointerOffsetHex>
    <JapaneseText>……知っての通り、父は</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>892</PointerOffsetDec>
    <PointerOffsetHex>0x37c</PointerOffsetHex>
    <JapaneseText>名実共に「賢王」と名高い人物だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>923</PointerOffsetDec>
    <PointerOffsetHex>0x39b</PointerOffsetHex>
    <JapaneseText>果たして今の自分が、その後を継ぐに</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>959</PointerOffsetDec>
    <PointerOffsetHex>0x3bf</PointerOffsetHex>
    <JapaneseText>ふさわしいのか……日々、考えている</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>994</PointerOffsetDec>
    <PointerOffsetHex>0x3e2</PointerOffsetHex>
    <JapaneseText>だから甘いと言うんだ、お前は</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1023</PointerOffsetDec>
    <PointerOffsetHex>0x3ff</PointerOffsetHex>
    <JapaneseText>生まれついての名君などいるはずもない。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1063</PointerOffsetDec>
    <PointerOffsetHex>0x427</PointerOffsetHex>
    <JapaneseText>王位に就き、周囲の助力や援助の中で</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1099</PointerOffsetDec>
    <PointerOffsetHex>0x44b</PointerOffsetHex>
    <JapaneseText>名君へと成長していくものだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1126</PointerOffsetDec>
    <PointerOffsetHex>0x466</PointerOffsetHex>
    <JapaneseText>違うのか</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1135</PointerOffsetDec>
    <PointerOffsetHex>0x46f</PointerOffsetHex>
    <JapaneseText>アッシュ君は、そう思うのだね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1164</PointerOffsetDec>
    <PointerOffsetHex>0x48c</PointerOffsetHex>
    <JapaneseText>ああ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1172</PointerOffsetDec>
    <PointerOffsetHex>0x494</PointerOffsetHex>
    <JapaneseText>民の生活や国の安寧に心を砕くお前には、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1212</PointerOffsetDec>
    <PointerOffsetHex>0x4bc</PointerOffsetHex>
    <JapaneseText>誰より王の資質があるとも思っている</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1247</PointerOffsetDec>
    <PointerOffsetHex>0x4df</PointerOffsetHex>
    <JapaneseText>それは、光栄だ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1265</PointerOffsetDec>
    <PointerOffsetHex>0x4f1</PointerOffsetHex>
    <JapaneseText>だが……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1274</PointerOffsetDec>
    <PointerOffsetHex>0x4fa</PointerOffsetHex>
    <JapaneseText>今少し、考える時間が欲しいのだよ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1310</PointerOffsetDec>
    <PointerOffsetHex>0x51e</PointerOffsetHex>
    <JapaneseText>真実を見極め……本当に民が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1338</PointerOffsetDec>
    <PointerOffsetHex>0x53a</PointerOffsetHex>
    <JapaneseText>必要としている事、己のなすべき事、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1373</PointerOffsetDec>
    <PointerOffsetHex>0x55d</PointerOffsetHex>
    <JapaneseText>その全てに、誠実に向き合う時間が……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1410</PointerOffsetDec>
    <PointerOffsetHex>0x582</PointerOffsetHex>
    <JapaneseText>……そうか。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1424</PointerOffsetDec>
    <PointerOffsetHex>0x590</PointerOffsetHex>
    <JapaneseText>お前がそう思うなら、仕方がない</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1455</PointerOffsetDec>
    <PointerOffsetHex>0x5af</PointerOffsetHex>
    <JapaneseText>その代わり、思いや信念は最後まで貫けよ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1497</PointerOffsetDec>
    <PointerOffsetHex>0x5d9</PointerOffsetHex>
    <JapaneseText>半端な決意はいらん、誰にとっても迷惑だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1536</PointerOffsetDec>
    <PointerOffsetHex>0x600</PointerOffsetHex>
    <JapaneseText>励ましてくれているんだね。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1564</PointerOffsetDec>
    <PointerOffsetHex>0x61c</PointerOffsetHex>
    <JapaneseText>すまない……アッシュ君</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1587</PointerOffsetDec>
    <PointerOffsetHex>0x633</PointerOffsetHex>
    <JapaneseText>別に……励ましてなどいない</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1614</PointerOffsetDec>
    <PointerOffsetHex>0x64e</PointerOffsetHex>
    <JapaneseText>フン……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
</Entries>