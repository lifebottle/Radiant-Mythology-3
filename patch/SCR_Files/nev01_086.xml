    <EnglishText></EnglishText>
    <Notes />
    <Id>156</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>4311</PointerOffsetDec>
    <PointerOffsetHex>0x10d7</PointerOffsetHex>
