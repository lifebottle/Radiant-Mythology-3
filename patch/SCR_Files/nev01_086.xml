    <EnglishText></EnglishText>
    <Notes />
    <Id>187</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>5267</PointerOffsetDec>
    <PointerOffsetHex>0x1493</PointerOffsetHex>
