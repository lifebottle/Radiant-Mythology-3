    <EnglishText></EnglishText>
    <Notes />
    <Id>164</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>4539</PointerOffsetDec>
    <PointerOffsetHex>0x11bb</PointerOffsetHex>
    <JapaneseText>変な話でも聞いたの？</JapaneseText>
    <EnglishText></EnglishText>
