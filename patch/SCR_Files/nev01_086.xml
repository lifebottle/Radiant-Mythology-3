<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>1602</PointerOffsetDec>
    <PointerOffsetHex>0x642</PointerOffsetHex>
    <JapaneseText>まずは、１階にいる皆との挨拶を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1634</PointerOffsetDec>
    <PointerOffsetHex>0x662</PointerOffsetHex>
    <JapaneseText>終わらせなくっちゃね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1655</PointerOffsetDec>
    <PointerOffsetHex>0x677</PointerOffsetHex>
    <JapaneseText>ここのメンバーは、いい人達ばかりだから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1697</PointerOffsetDec>
    <PointerOffsetHex>0x6a1</PointerOffsetHex>
    <JapaneseText>すぐに皆と打ち解けると思うよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1726</PointerOffsetDec>
    <PointerOffsetHex>0x6be</PointerOffsetHex>
    <JapaneseText>しばらくの間は、あなたについて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1758</PointerOffsetDec>
    <PointerOffsetHex>0x6de</PointerOffsetHex>
    <JapaneseText>仕事を教える事になるよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1784</PointerOffsetDec>
    <PointerOffsetHex>0x6f8</PointerOffsetHex>
    <JapaneseText>頑張ってね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1797</PointerOffsetDec>
    <PointerOffsetHex>0x705</PointerOffsetHex>
    <JapaneseText>これから一緒に働けるんだね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1827</PointerOffsetDec>
    <PointerOffsetHex>0x723</PointerOffsetHex>
    <JapaneseText>えへへ、何だか嬉しいなぁ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1855</PointerOffsetDec>
    <PointerOffsetHex>0x73f</PointerOffsetHex>
    <JapaneseText>これからも、よろしくね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1878</PointerOffsetDec>
    <PointerOffsetHex>0x756</PointerOffsetHex>
    <JapaneseText>ディセンダーの予言？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1900</PointerOffsetDec>
    <PointerOffsetHex>0x76c</PointerOffsetHex>
    <JapaneseText>私は、素敵な話だと思うよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1928</PointerOffsetDec>
    <PointerOffsetHex>0x788</PointerOffsetHex>
    <JapaneseText>あなたも信じる？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1945</PointerOffsetDec>
    <PointerOffsetHex>0x799</PointerOffsetHex>
    <JapaneseText>食堂へ行こうよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1963</PointerOffsetDec>
    <PointerOffsetHex>0x7ab</PointerOffsetHex>
    <JapaneseText>ロックスのシナモンロールは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1991</PointerOffsetDec>
    <PointerOffsetHex>0x7c7</PointerOffsetHex>
    <JapaneseText>とっても美味しいんだから！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2018</PointerOffsetDec>
    <PointerOffsetHex>0x7e2</PointerOffsetHex>
    <JapaneseText>ロックスのシナモンロール、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2046</PointerOffsetDec>
    <PointerOffsetHex>0x7fe</PointerOffsetHex>
    <JapaneseText>美味しかったでしょ？　みんな、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2078</PointerOffsetDec>
    <PointerOffsetHex>0x81e</PointerOffsetHex>
    <JapaneseText>毎日ロックスのおやつを楽しみにしてるの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2117</PointerOffsetDec>
    <PointerOffsetHex>0x845</PointerOffsetHex>
    <JapaneseText>私が描いた風景の中に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2141</PointerOffsetDec>
    <PointerOffsetHex>0x85d</PointerOffsetHex>
    <JapaneseText>いつか立つ事が出来るかな…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2168</PointerOffsetDec>
    <PointerOffsetHex>0x878</PointerOffsetHex>
    <JapaneseText>お姫様なんて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2184</PointerOffsetDec>
    <PointerOffsetHex>0x888</PointerOffsetHex>
    <JapaneseText>遠い遠い世界の人だって思ってたよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2220</PointerOffsetDec>
    <PointerOffsetHex>0x8ac</PointerOffsetHex>
    <JapaneseText>エステルさん…素敵だなぁ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2249</PointerOffsetDec>
    <PointerOffsetHex>0x8c9</PointerOffsetHex>
    <JapaneseText>星晶は、マナが物質化したものとはわかって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2291</PointerOffsetDec>
    <PointerOffsetHex>0x8f3</PointerOffsetHex>
    <JapaneseText>いるけど、どうやって生成されているのかは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2333</PointerOffsetDec>
    <PointerOffsetHex>0x91d</PointerOffsetHex>
    <JapaneseText>色々仮説があるだけで、解明はされていないの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2376</PointerOffsetDec>
    <PointerOffsetHex>0x948</PointerOffsetHex>
    <JapaneseText>キールとメルディは、元々アドリビトムの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2416</PointerOffsetDec>
    <PointerOffsetHex>0x970</PointerOffsetHex>
    <JapaneseText>メンバーだったんだよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2440</PointerOffsetDec>
    <PointerOffsetHex>0x988</PointerOffsetHex>
    <JapaneseText>大学に通っている間は抜けていたんだけどね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2481</PointerOffsetDec>
    <PointerOffsetHex>0x9b1</PointerOffsetHex>
    <JapaneseText>ふふ、どうしたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2501</PointerOffsetDec>
    <PointerOffsetHex>0x9c5</PointerOffsetHex>
    <JapaneseText>あなたもこの場所が好き？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2526</PointerOffsetDec>
    <PointerOffsetHex>0x9de</PointerOffsetHex>
    <JapaneseText>モラード村からの依頼、かぁ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2556</PointerOffsetDec>
    <PointerOffsetHex>0x9fc</PointerOffsetHex>
    <JapaneseText>あそこはとても小さな村で、医者も設備も</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2596</PointerOffsetDec>
    <PointerOffsetHex>0xa24</PointerOffsetHex>
    <JapaneseText>不足して困っているって話だったな…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2631</PointerOffsetDec>
    <PointerOffsetHex>0xa47</PointerOffsetHex>
    <JapaneseText>そろそろ、スケッチブックが切れそうだなぁ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2675</PointerOffsetDec>
    <PointerOffsetHex>0xa73</PointerOffsetHex>
    <JapaneseText>また新しいのを買わなくちゃ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2702</PointerOffsetDec>
    <PointerOffsetHex>0xa8e</PointerOffsetHex>
    <JapaneseText>記憶はいつか、きっと戻るよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2732</PointerOffsetDec>
    <PointerOffsetHex>0xaac</PointerOffsetHex>
    <JapaneseText>だって、名前を覚えてるんだもの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2766</PointerOffsetDec>
    <PointerOffsetHex>0xace</PointerOffsetHex>
    <JapaneseText>そのうち、何かの拍子で思い出す事があるよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2807</PointerOffsetDec>
    <PointerOffsetHex>0xaf7</PointerOffsetHex>
    <JapaneseText>ミブナの里は、忍びの里…かぁ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2839</PointerOffsetDec>
    <PointerOffsetHex>0xb17</PointerOffsetHex>
    <JapaneseText>そういえば、小さい頃、眠る前によく</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2875</PointerOffsetDec>
    <PointerOffsetHex>0xb3b</PointerOffsetHex>
    <JapaneseText>ロックスが忍びの話をしてくれたな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2908</PointerOffsetDec>
    <PointerOffsetHex>0xb5c</PointerOffsetHex>
    <JapaneseText>ドクメントって、みんなにあるものなんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2950</PointerOffsetDec>
    <PointerOffsetHex>0xb86</PointerOffsetHex>
    <JapaneseText>すごいなぁ、私達の身体に、まだまだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2986</PointerOffsetDec>
    <PointerOffsetHex>0xbaa</PointerOffsetHex>
    <JapaneseText>あんな知らない事があったなんて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3017</PointerOffsetDec>
    <PointerOffsetHex>0xbc9</PointerOffsetHex>
    <JapaneseText>何だか急に仲間が増えたね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3045</PointerOffsetDec>
    <PointerOffsetHex>0xbe5</PointerOffsetHex>
    <JapaneseText>でも、みんないい人ばかり。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3073</PointerOffsetDec>
    <PointerOffsetHex>0xc01</PointerOffsetHex>
    <JapaneseText>名前を覚えるのがちょっと大変だけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3108</PointerOffsetDec>
    <PointerOffsetHex>0xc24</PointerOffsetHex>
    <JapaneseText>私、ディセンダーの予言は信じてる。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3144</PointerOffsetDec>
    <PointerOffsetHex>0xc48</PointerOffsetHex>
    <JapaneseText>でも、暁の従者が連れ去った存在は…、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3182</PointerOffsetDec>
    <PointerOffsetHex>0xc6e</PointerOffsetHex>
    <JapaneseText>本当にディセンダーなのかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3211</PointerOffsetDec>
    <PointerOffsetHex>0xc8b</PointerOffsetHex>
    <JapaneseText>また王族の人が来るなんて…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>123</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3241</PointerOffsetDec>
    <PointerOffsetHex>0xca9</PointerOffsetHex>
    <JapaneseText>アンジュさんは落ち着いて応対してるけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>124</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3283</PointerOffsetDec>
    <PointerOffsetHex>0xcd3</PointerOffsetHex>
    <JapaneseText>私だったら緊張しちゃうかも…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>125</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3312</PointerOffsetDec>
    <PointerOffsetHex>0xcf0</PointerOffsetHex>
    <JapaneseText>暁の従者に賛同する人達は、まだまだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>126</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3348</PointerOffsetDec>
    <PointerOffsetHex>0xd14</PointerOffsetHex>
    <JapaneseText>各地にいるみたいだね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>127</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3372</PointerOffsetDec>
    <PointerOffsetHex>0xd2c</PointerOffsetHex>
    <JapaneseText>この間、街で大々的な演説会をやっていたよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>128</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3413</PointerOffsetDec>
    <PointerOffsetHex>0xd55</PointerOffsetHex>
    <JapaneseText>やっぱり、あの存在は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>129</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3435</PointerOffsetDec>
    <PointerOffsetHex>0xd6b</PointerOffsetHex>
    <JapaneseText>ディセンダーじゃなかったんだね…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>130</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3468</PointerOffsetDec>
    <PointerOffsetHex>0xd8c</PointerOffsetHex>
    <JapaneseText>食堂へ急いで。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>131</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3484</PointerOffsetDec>
    <PointerOffsetHex>0xd9c</PointerOffsetHex>
    <JapaneseText>チョコケーキが待ってるよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>132</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3511</PointerOffsetDec>
    <PointerOffsetHex>0xdb7</PointerOffsetHex>
    <JapaneseText>あなたの力になれたらいいのにな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>133</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3545</PointerOffsetDec>
    <PointerOffsetHex>0xdd9</PointerOffsetHex>
    <JapaneseText>私、何も出来ていなくて、悔しい…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>134</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3578</PointerOffsetDec>
    <PointerOffsetHex>0xdfa</PointerOffsetHex>
    <JapaneseText>精霊に会うってだけでもすごいのに、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>135</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3614</PointerOffsetDec>
    <PointerOffsetHex>0xe1e</PointerOffsetHex>
    <JapaneseText>世界の始まりを聞くだなんて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>136</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3644</PointerOffsetDec>
    <PointerOffsetHex>0xe3c</PointerOffsetHex>
    <JapaneseText>まるでおとぎ話みたいだね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>137</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3671</PointerOffsetDec>
    <PointerOffsetHex>0xe57</PointerOffsetHex>
    <JapaneseText>やっぱり、あなたがディセンダーだったんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>138</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3715</PointerOffsetDec>
    <PointerOffsetHex>0xe83</PointerOffsetHex>
    <JapaneseText>あの時…、ルバーブ連山で出会ったあの時、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>139</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3757</PointerOffsetDec>
    <PointerOffsetHex>0xead</PointerOffsetHex>
    <JapaneseText>あなたは生まれたんだね…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>140</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3782</PointerOffsetDec>
    <PointerOffsetHex>0xec6</PointerOffsetHex>
    <JapaneseText>絵を描いていたら、ものすごい音がして…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>141</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3824</PointerOffsetDec>
    <PointerOffsetHex>0xef0</PointerOffsetHex>
    <JapaneseText>窓の外に人が倒れてたんだよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>142</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3854</PointerOffsetDec>
    <PointerOffsetHex>0xf0e</PointerOffsetHex>
    <JapaneseText>無事だったみたいだけど、びっくりしたなぁ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>143</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3897</PointerOffsetDec>
    <PointerOffsetHex>0xf39</PointerOffsetHex>
    <JapaneseText>ヘーゼル村は…無くなっちゃうんだね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>144</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3935</PointerOffsetDec>
    <PointerOffsetHex>0xf5f</PointerOffsetHex>
    <JapaneseText>帝国に星晶を奪われなければ…、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>145</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3967</PointerOffsetDec>
    <PointerOffsetHex>0xf7f</PointerOffsetHex>
    <JapaneseText>みんな穏やかに暮らせていたのに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>146</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>3998</PointerOffsetDec>
    <PointerOffsetHex>0xf9e</PointerOffsetHex>
    <JapaneseText>オルタ・ビレッジもたくさんの人が集まって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>147</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>4042</PointerOffsetDec>
    <PointerOffsetHex>0xfca</PointerOffsetHex>
    <JapaneseText>本格始動だってアンジュさんが言っていたよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>148</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>4083</PointerOffsetDec>
    <PointerOffsetHex>0xff3</PointerOffsetHex>
    <JapaneseText>研…、究室に行かなくちゃ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>149</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>4109</PointerOffsetDec>
    <PointerOffsetHex>0x100d</PointerOffsetHex>
    <JapaneseText>いけないんだよね…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>150</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>4131</PointerOffsetDec>
    <PointerOffsetHex>0x1023</PointerOffsetHex>
    <JapaneseText>○○、一緒に行ってくれるかな…？</JapaneseText>
