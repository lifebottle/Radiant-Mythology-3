    <EnglishText></EnglishText>
    <Notes />
    <Id>179</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>5028</PointerOffsetDec>
    <PointerOffsetHex>0x13a4</PointerOffsetHex>
    <JapaneseText>しれないけれど…、会いたいの</JapaneseText>
