<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>1150</PointerOffsetDec>
    <PointerOffsetHex>0x47e</PointerOffsetHex>
    <JapaneseText>なあなあ、すずの住んでる里、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1180</PointerOffsetDec>
    <PointerOffsetHex>0x49c</PointerOffsetHex>
    <JapaneseText>どんな怖い話あるか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1201</PointerOffsetDec>
    <PointerOffsetHex>0x4b1</PointerOffsetHex>
    <JapaneseText>怖い話……怪談の事でしょうか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1232</PointerOffsetDec>
    <PointerOffsetHex>0x4d0</PointerOffsetHex>
    <JapaneseText>でしたら、番台皿屋敷の</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1256</PointerOffsetDec>
    <PointerOffsetHex>0x4e8</PointerOffsetHex>
    <JapaneseText>お花さんの話などが有名ですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1285</PointerOffsetDec>
    <PointerOffsetHex>0x505</PointerOffsetHex>
    <JapaneseText>オハナサン……？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1302</PointerOffsetDec>
    <PointerOffsetHex>0x516</PointerOffsetHex>
    <JapaneseText>はい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1310</PointerOffsetDec>
    <PointerOffsetHex>0x51e</PointerOffsetHex>
    <JapaneseText>お花さんは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1322</PointerOffsetDec>
    <PointerOffsetHex>0x52a</PointerOffsetHex>
    <JapaneseText>番台の大切にしていた皿を……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1351</PointerOffsetDec>
    <PointerOffsetHex>0x547</PointerOffsetHex>
    <JapaneseText>そんなものは迷信さ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1370</PointerOffsetDec>
    <PointerOffsetHex>0x55a</PointerOffsetHex>
    <JapaneseText>大体、いくら生前の未練が強いとはいえ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1408</PointerOffsetDec>
    <PointerOffsetHex>0x580</PointerOffsetHex>
    <JapaneseText>死してなお現世に霊体が現出するなど考え難い</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1451</PointerOffsetDec>
    <PointerOffsetHex>0x5ab</PointerOffsetHex>
    <JapaneseText>ましてや皿を数えるなんて、非科学的だ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1491</PointerOffsetDec>
    <PointerOffsetHex>0x5d3</PointerOffsetHex>
    <JapaneseText>本当に霊体ならば、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1511</PointerOffsetDec>
    <PointerOffsetHex>0x5e7</PointerOffsetHex>
    <JapaneseText>どうやって物体と接触出来る？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1540</PointerOffsetDec>
    <PointerOffsetHex>0x604</PointerOffsetHex>
    <JapaneseText>そもそもの設定からして矛盾している</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1575</PointerOffsetDec>
    <PointerOffsetHex>0x627</PointerOffsetHex>
    <JapaneseText>そうでしょうか……？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1596</PointerOffsetDec>
    <PointerOffsetHex>0x63c</PointerOffsetHex>
    <JapaneseText>世界には、科学で解明出来ない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1626</PointerOffsetDec>
    <PointerOffsetHex>0x65a</PointerOffsetHex>
    <JapaneseText>不思議な事象もあると聞きます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1655</PointerOffsetDec>
    <PointerOffsetHex>0x677</PointerOffsetHex>
    <JapaneseText>現実に、そういった出来事をまとめた</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1691</PointerOffsetDec>
    <PointerOffsetHex>0x69b</PointerOffsetHex>
    <JapaneseText>文献や書物も編纂されていますし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1722</PointerOffsetDec>
    <PointerOffsetHex>0x6ba</PointerOffsetHex>
    <JapaneseText>子どもだましのお伽話だろ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1749</PointerOffsetDec>
    <PointerOffsetHex>0x6d5</PointerOffsetHex>
    <JapaneseText>信頼に値するだけの価値があるか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1781</PointerOffsetDec>
    <PointerOffsetHex>0x6f5</PointerOffsetHex>
    <JapaneseText>怪しいものだな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1796</PointerOffsetDec>
    <PointerOffsetHex>0x704</PointerOffsetHex>
    <JapaneseText>そうとは言い切れなくてよ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1824</PointerOffsetDec>
    <PointerOffsetHex>0x720</PointerOffsetHex>
    <JapaneseText>キール</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1831</PointerOffsetDec>
    <PointerOffsetHex>0x727</PointerOffsetHex>
    <JapaneseText>その昔、神話の時代に使われたという</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1867</PointerOffsetDec>
    <PointerOffsetHex>0x74b</PointerOffsetHex>
    <JapaneseText>伝説の箱舟を、見事現代で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1893</PointerOffsetDec>
    <PointerOffsetHex>0x765</PointerOffsetHex>
    <JapaneseText>発掘してみせたという成功例もあるわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1928</PointerOffsetDec>
    <PointerOffsetHex>0x788</PointerOffsetHex>
    <JapaneseText>現在「お伽話」と称される幾つかの事物は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1968</PointerOffsetDec>
    <PointerOffsetHex>0x7b0</PointerOffsetHex>
    <JapaneseText>あくまでその後の人間の価値観に照らして</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2008</PointerOffsetDec>
    <PointerOffsetHex>0x7d8</PointerOffsetHex>
    <JapaneseText>後付けされた呼称に過ぎない……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2039</PointerOffsetDec>
    <PointerOffsetHex>0x7f7</PointerOffsetHex>
    <JapaneseText>そうも考えられるのではなくて？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2070</PointerOffsetDec>
    <PointerOffsetHex>0x816</PointerOffsetHex>
    <JapaneseText>そ、それは……そうかもしれませんが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2106</PointerOffsetDec>
    <PointerOffsetHex>0x83a</PointerOffsetHex>
    <JapaneseText>でも、番台皿屋敷の件に関しては</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2137</PointerOffsetDec>
    <PointerOffsetHex>0x859</PointerOffsetHex>
    <JapaneseText>何だか、話をする雰囲気では</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2165</PointerOffsetDec>
    <PointerOffsetHex>0x875</PointerOffsetHex>
    <JapaneseText>なくなってしまいましたね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2190</PointerOffsetDec>
    <PointerOffsetHex>0x88e</PointerOffsetHex>
    <JapaneseText>バイバ！　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2202</PointerOffsetDec>
    <PointerOffsetHex>0x89a</PointerOffsetHex>
    <JapaneseText>オハナサン……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2217</PointerOffsetDec>
    <PointerOffsetHex>0x8a9</PointerOffsetHex>
    <JapaneseText>どんな話か気になるよー</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>123</Id>
    <Status />
  </Entry>
</Entries>