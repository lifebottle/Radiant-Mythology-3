<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>282</PointerOffsetDec>
    <PointerOffsetHex>0x11a</PointerOffsetHex>
    <JapaneseText>手伝ってくれるのかい？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>20</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>306</PointerOffsetDec>
    <PointerOffsetHex>0x132</PointerOffsetHex>
    <JapaneseText>すまないね……、恩に着るよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>21</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>335</PointerOffsetDec>
    <PointerOffsetHex>0x14f</PointerOffsetHex>
    <JapaneseText>あの技術を使っている間は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>22</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>363</PointerOffsetDec>
    <PointerOffsetHex>0x16b</PointerOffsetHex>
    <JapaneseText>どうしても、あたしは無防備になっちまうんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>23</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>409</PointerOffsetDec>
    <PointerOffsetHex>0x199</PointerOffsetHex>
    <JapaneseText>あんたが援護してくれると、本当に助かるよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>24</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>457</PointerOffsetDec>
    <PointerOffsetHex>0x1c9</PointerOffsetHex>
    <JapaneseText>ああ、技術っていうのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>26</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>481</PointerOffsetDec>
    <PointerOffsetHex>0x1e1</PointerOffsetHex>
    <JapaneseText>ミブナの里の光気丹術……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>27</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>509</PointerOffsetDec>
    <PointerOffsetHex>0x1fd</PointerOffsetHex>
    <JapaneseText>ソウルアルケミーの事さ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>28</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>532</PointerOffsetDec>
    <PointerOffsetHex>0x214</PointerOffsetHex>
    <JapaneseText>あたしがそれに成功すれば、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>560</PointerOffsetDec>
    <PointerOffsetHex>0x230</PointerOffsetHex>
    <JapaneseText>もう一度、昔契約していた精霊に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>30</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>592</PointerOffsetDec>
    <PointerOffsetHex>0x250</PointerOffsetHex>
    <JapaneseText>会えるんじゃないかって……思ってね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>31</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>627</PointerOffsetDec>
    <PointerOffsetHex>0x273</PointerOffsetHex>
    <JapaneseText>あたしの初めての友達で、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>32</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>653</PointerOffsetDec>
    <PointerOffsetHex>0x28d</PointerOffsetHex>
    <JapaneseText>大切な相棒だったんだけど。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>33</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>681</PointerOffsetDec>
    <PointerOffsetHex>0x2a9</PointerOffsetHex>
    <JapaneseText>今はもう、この世にいないんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>34</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>710</PointerOffsetDec>
    <PointerOffsetHex>0x2c6</PointerOffsetHex>
    <JapaneseText>だけど……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>35</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>724</PointerOffsetDec>
    <PointerOffsetHex>0x2d4</PointerOffsetHex>
    <JapaneseText>どうしても、もう一度あの子に会って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>760</PointerOffsetDec>
    <PointerOffsetHex>0x2f8</PointerOffsetHex>
    <JapaneseText>伝えなきゃいけない事があるんだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>37</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>802</PointerOffsetDec>
    <PointerOffsetHex>0x322</PointerOffsetHex>
    <JapaneseText>……少しだけ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>39</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>818</PointerOffsetDec>
    <PointerOffsetHex>0x332</PointerOffsetHex>
    <JapaneseText>あたしのわがままに付き合っとくれ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>854</PointerOffsetDec>
    <PointerOffsetHex>0x356</PointerOffsetHex>
    <JapaneseText>頼むよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
</Entries>