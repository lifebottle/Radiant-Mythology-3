<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>512</PointerOffsetDec>
    <PointerOffsetHex>0x200</PointerOffsetHex>
    <JapaneseText>しかし、外の世界ってのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>538</PointerOffsetDec>
    <PointerOffsetHex>0x21a</PointerOffsetHex>
    <JapaneseText>こうも複雑で、広くて……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>566</PointerOffsetDec>
    <PointerOffsetHex>0x236</PointerOffsetHex>
    <JapaneseText>色んな人間が暮らしているものなんだねえ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>605</PointerOffsetDec>
    <PointerOffsetHex>0x25d</PointerOffsetHex>
    <JapaneseText>はい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>613</PointerOffsetDec>
    <PointerOffsetHex>0x265</PointerOffsetHex>
    <JapaneseText>ここで暮らす様になってからしばらくは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>651</PointerOffsetDec>
    <PointerOffsetHex>0x28b</PointerOffsetHex>
    <JapaneseText>毎日が小さな発見の連続でした</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>680</PointerOffsetDec>
    <PointerOffsetHex>0x2a8</PointerOffsetHex>
    <JapaneseText>そうだね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>692</PointerOffsetDec>
    <PointerOffsetHex>0x2b4</PointerOffsetHex>
    <JapaneseText>あたし達の里は、それだけ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>718</PointerOffsetDec>
    <PointerOffsetHex>0x2ce</PointerOffsetHex>
    <JapaneseText>徹底して外との接触を避けてきた</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>749</PointerOffsetDec>
    <PointerOffsetHex>0x2ed</PointerOffsetHex>
    <JapaneseText>そういう意味じゃ、あたし達が漠然と</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>785</PointerOffsetDec>
    <PointerOffsetHex>0x311</PointerOffsetHex>
    <JapaneseText>外だと思ってた世界も</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>807</PointerOffsetDec>
    <PointerOffsetHex>0x327</PointerOffsetHex>
    <JapaneseText>本当の意味で外なんかじゃなかったんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>844</PointerOffsetDec>
    <PointerOffsetHex>0x34c</PointerOffsetHex>
    <JapaneseText>それを今回、つくづく思い知らされたよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>881</PointerOffsetDec>
    <PointerOffsetHex>0x371</PointerOffsetHex>
    <JapaneseText>忍びは、掟に従って生きるもの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>913</PointerOffsetDec>
    <PointerOffsetHex>0x391</PointerOffsetHex>
    <JapaneseText>非情でなくては務まらない……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>942</PointerOffsetDec>
    <PointerOffsetHex>0x3ae</PointerOffsetHex>
    <JapaneseText>これまではそれが当たり前だったのに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>978</PointerOffsetDec>
    <PointerOffsetHex>0x3d2</PointerOffsetHex>
    <JapaneseText>外はこんなにも豊かで、変化に満ちて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1014</PointerOffsetDec>
    <PointerOffsetHex>0x3f6</PointerOffsetHex>
    <JapaneseText>みんなが泣いて、笑って暮らしている……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1053</PointerOffsetDec>
    <PointerOffsetHex>0x41d</PointerOffsetHex>
    <JapaneseText>それがここの「当たり前」なんですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1088</PointerOffsetDec>
    <PointerOffsetHex>0x440</PointerOffsetHex>
    <JapaneseText>そうだねぇ。ルールや規則はあっても、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1126</PointerOffsetDec>
    <PointerOffsetHex>0x466</PointerOffsetHex>
    <JapaneseText>あたし達の掟程厳しいわけじゃないし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1162</PointerOffsetDec>
    <PointerOffsetHex>0x48a</PointerOffsetHex>
    <JapaneseText>ましてや、命に関わる掟もない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1191</PointerOffsetDec>
    <PointerOffsetHex>0x4a7</PointerOffsetHex>
    <JapaneseText>今まで普通だと思ってた事が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1219</PointerOffsetDec>
    <PointerOffsetHex>0x4c3</PointerOffsetHex>
    <JapaneseText>普通じゃなくなるってのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1245</PointerOffsetDec>
    <PointerOffsetHex>0x4dd</PointerOffsetHex>
    <JapaneseText>どうにも妙な感覚だね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1266</PointerOffsetDec>
    <PointerOffsetHex>0x4f2</PointerOffsetHex>
    <JapaneseText>だからこそ、知る事が出来たものも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1300</PointerOffsetDec>
    <PointerOffsetHex>0x514</PointerOffsetHex>
    <JapaneseText>たくさんあります</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1317</PointerOffsetDec>
    <PointerOffsetHex>0x525</PointerOffsetHex>
    <JapaneseText>皆さんと出会えて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1335</PointerOffsetDec>
    <PointerOffsetHex>0x537</PointerOffsetHex>
    <JapaneseText>旅をしたからこそわかった事が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1364</PointerOffsetDec>
    <PointerOffsetHex>0x554</PointerOffsetHex>
    <JapaneseText>ああ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1372</PointerOffsetDec>
    <PointerOffsetHex>0x55c</PointerOffsetHex>
    <JapaneseText>あたしもそう思うよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1391</PointerOffsetDec>
    <PointerOffsetHex>0x56f</PointerOffsetHex>
    <JapaneseText>それって、多分……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1411</PointerOffsetDec>
    <PointerOffsetHex>0x583</PointerOffsetHex>
    <JapaneseText>なかなか体験出来る事じゃないし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1443</PointerOffsetDec>
    <PointerOffsetHex>0x5a3</PointerOffsetHex>
    <JapaneseText>とっても恵まれてるって事……なんだよね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1482</PointerOffsetDec>
    <PointerOffsetHex>0x5ca</PointerOffsetHex>
    <JapaneseText>はい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1487</PointerOffsetDec>
    <PointerOffsetHex>0x5cf</PointerOffsetHex>
    <JapaneseText>お互いに、新しい世界が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1511</PointerOffsetDec>
    <PointerOffsetHex>0x5e7</PointerOffsetHex>
    <JapaneseText>広がったという事なのだと思います。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1547</PointerOffsetDec>
    <PointerOffsetHex>0x60b</PointerOffsetHex>
    <JapaneseText>きっと……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
</Entries>