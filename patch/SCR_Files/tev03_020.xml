<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>202</PointerOffsetDec>
    <PointerOffsetHex>0xca</PointerOffsetHex>
    <JapaneseText>ピッポの防具屋にようこそだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>15</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>236</PointerOffsetDec>
    <PointerOffsetHex>0xec</PointerOffsetHex>
    <JapaneseText>ここでは選りすぐりの防具を仕入れて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>16</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>272</PointerOffsetDec>
    <PointerOffsetHex>0x110</PointerOffsetHex>
    <JapaneseText>取り扱っているんだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>17</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>297</PointerOffsetDec>
    <PointerOffsetHex>0x129</PointerOffsetHex>
    <JapaneseText>ここで買った装備は、メニューの「装備」で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>18</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>339</PointerOffsetDec>
    <PointerOffsetHex>0x153</PointerOffsetHex>
    <JapaneseText>装備出来るキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>19</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>354</PointerOffsetDec>
    <PointerOffsetHex>0x162</PointerOffsetHex>
    <JapaneseText>武器や防具によって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>20</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>376</PointerOffsetDec>
    <PointerOffsetHex>0x178</PointerOffsetHex>
    <JapaneseText>装備出来る部位が異なるキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>21</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>403</PointerOffsetDec>
    <PointerOffsetHex>0x193</PointerOffsetHex>
    <JapaneseText>そうそう、大事な事を言い忘れたキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>22</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>441</PointerOffsetDec>
    <PointerOffsetHex>0x1b9</PointerOffsetHex>
    <JapaneseText>武器や防具には、装備する為に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>23</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>471</PointerOffsetDec>
    <PointerOffsetHex>0x1d7</PointerOffsetHex>
    <JapaneseText>一定のレベルが要求されるんだキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>24</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>504</PointerOffsetDec>
    <PointerOffsetHex>0x1f8</PointerOffsetHex>
    <JapaneseText>要求されているレベルに達してないと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>25</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>540</PointerOffsetDec>
    <PointerOffsetHex>0x21c</PointerOffsetHex>
    <JapaneseText>装備出来ないんだキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>26</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>564</PointerOffsetDec>
    <PointerOffsetHex>0x234</PointerOffsetHex>
    <JapaneseText>忘れないで欲しいキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>27</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>585</PointerOffsetDec>
    <PointerOffsetHex>0x249</PointerOffsetHex>
    <JapaneseText>これからも品数を増やしていくキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>28</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>621</PointerOffsetDec>
    <PointerOffsetHex>0x26d</PointerOffsetHex>
    <JapaneseText>試着も出来るから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>641</PointerOffsetDec>
    <PointerOffsetHex>0x281</PointerOffsetHex>
    <JapaneseText>ぜひ気軽に立ち寄って欲しいキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>30</Id>
    <Status />
  </Entry>
</Entries>