<Entries>
  <Entry>
    <PointerOffsetDec>642</PointerOffsetDec>
    <PointerOffsetHex>0x282</PointerOffsetHex>
    <JapaneseText>これから、世界は</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>660</PointerOffsetDec>
    <PointerOffsetHex>0x294</PointerOffsetHex>
    <JapaneseText>どうなっていくのかしら……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>687</PointerOffsetDec>
    <PointerOffsetHex>0x2af</PointerOffsetHex>
    <JapaneseText>わからないわ……ただ、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>710</PointerOffsetDec>
    <PointerOffsetHex>0x2c6</PointerOffsetHex>
    <JapaneseText>このままラザリスの浸食が進めば</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>742</PointerOffsetDec>
    <PointerOffsetHex>0x2e6</PointerOffsetHex>
    <JapaneseText>そう遠くない未来に、取り返しのつかない</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>782</PointerOffsetDec>
    <PointerOffsetHex>0x30e</PointerOffsetHex>
    <JapaneseText>事態になる……それだけは確かね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>813</PointerOffsetDec>
    <PointerOffsetHex>0x32d</PointerOffsetHex>
    <JapaneseText>……不安か？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>826</PointerOffsetDec>
    <PointerOffsetHex>0x33a</PointerOffsetHex>
    <JapaneseText>え、ええ……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>839</PointerOffsetDec>
    <PointerOffsetHex>0x347</PointerOffsetHex>
    <JapaneseText>この先の事を考えると……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>865</PointerOffsetDec>
    <PointerOffsetHex>0x361</PointerOffsetHex>
    <JapaneseText>どうしても……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>880</PointerOffsetDec>
    <PointerOffsetHex>0x370</PointerOffsetHex>
    <JapaneseText>ならばまず、その負の感情を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>908</PointerOffsetDec>
    <PointerOffsetHex>0x38c</PointerOffsetHex>
    <JapaneseText>捨て去る事だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>921</PointerOffsetDec>
    <PointerOffsetHex>0x399</PointerOffsetHex>
    <JapaneseText>捨てる……？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>935</PointerOffsetDec>
    <PointerOffsetHex>0x3a7</PointerOffsetHex>
    <JapaneseText>不安になるなという事？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>958</PointerOffsetDec>
    <PointerOffsetHex>0x3be</PointerOffsetHex>
    <JapaneseText>未来が不確かだからといって</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>986</PointerOffsetDec>
    <PointerOffsetHex>0x3da</PointerOffsetHex>
    <JapaneseText>恐れを抱いて何になる？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1010</PointerOffsetDec>
    <PointerOffsetHex>0x3f2</PointerOffsetHex>
    <JapaneseText>それは「まだ起きていない」のだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1041</PointerOffsetDec>
    <PointerOffsetHex>0x411</PointerOffsetHex>
    <JapaneseText>必要なのは、今から先へと踏み出す一歩、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1081</PointerOffsetDec>
    <PointerOffsetHex>0x439</PointerOffsetHex>
    <JapaneseText>その為の一握の勇気だろう</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1106</PointerOffsetDec>
    <PointerOffsetHex>0x452</PointerOffsetHex>
    <JapaneseText>確かに……わたし達の不安は</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1134</PointerOffsetDec>
    <PointerOffsetHex>0x46e</PointerOffsetHex>
    <JapaneseText>望まない未来を思い描く事で</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1162</PointerOffsetDec>
    <PointerOffsetHex>0x48a</PointerOffsetHex>
    <JapaneseText>生まれているわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1177</PointerOffsetDec>
    <PointerOffsetHex>0x499</PointerOffsetHex>
    <JapaneseText>その不安が恐れとなり、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1201</PointerOffsetDec>
    <PointerOffsetHex>0x4b1</PointerOffsetHex>
    <JapaneseText>一歩を踏み出す勇気を奪う……。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1233</PointerOffsetDec>
    <PointerOffsetHex>0x4d1</PointerOffsetHex>
    <JapaneseText>結果、望む未来からは遠ざかる……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1266</PointerOffsetDec>
    <PointerOffsetHex>0x4f2</PointerOffsetHex>
    <JapaneseText>……わたし達の不安が、逆に</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1294</PointerOffsetDec>
    <PointerOffsetHex>0x50e</PointerOffsetHex>
    <JapaneseText>望まない未来を引き寄せてしまっているのね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1335</PointerOffsetDec>
    <PointerOffsetHex>0x537</PointerOffsetHex>
    <JapaneseText>そういう事だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1348</PointerOffsetDec>
    <PointerOffsetHex>0x544</PointerOffsetHex>
    <JapaneseText>慎重を期す事と臆病である事は違う。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1384</PointerOffsetDec>
    <PointerOffsetHex>0x568</PointerOffsetHex>
    <JapaneseText>それを肝に銘じておく事だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
</Entries>