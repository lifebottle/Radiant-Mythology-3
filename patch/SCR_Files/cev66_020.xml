<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>236</PointerOffsetDec>
    <PointerOffsetHex>0xec</PointerOffsetHex>
    <JapaneseText>あなた、随分勉強熱心なのね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>19</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>263</PointerOffsetDec>
    <PointerOffsetHex>0x107</PointerOffsetHex>
    <JapaneseText>当然だ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>20</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>273</PointerOffsetDec>
    <PointerOffsetHex>0x111</PointerOffsetHex>
    <JapaneseText>知識は有しているだけでは</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>21</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>299</PointerOffsetDec>
    <PointerOffsetHex>0x12b</PointerOffsetHex>
    <JapaneseText>ただの情報に過ぎないが</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>22</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>322</PointerOffsetDec>
    <PointerOffsetHex>0x142</PointerOffsetHex>
    <JapaneseText>利用の仕方によっては、多くを生み出し</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>23</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>360</PointerOffsetDec>
    <PointerOffsetHex>0x168</PointerOffsetHex>
    <JapaneseText>多くを助ける術にも成り得る</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>24</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>387</PointerOffsetDec>
    <PointerOffsetHex>0x183</PointerOffsetHex>
    <JapaneseText>だからぼくは、ぼくの知らない事を知りたい。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>25</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>431</PointerOffsetDec>
    <PointerOffsetHex>0x1af</PointerOffsetHex>
    <JapaneseText>そして、得た知識から、この世界の未来を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>26</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>471</PointerOffsetDec>
    <PointerOffsetHex>0x1d7</PointerOffsetHex>
    <JapaneseText>切り開く手段を絶対に見つけてみせる</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>27</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>506</PointerOffsetDec>
    <PointerOffsetHex>0x1fa</PointerOffsetHex>
    <JapaneseText>「オルタ・ビレッジ」を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>28</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>530</PointerOffsetDec>
    <PointerOffsetHex>0x212</PointerOffsetHex>
    <JapaneseText>ただの理想で終わらせない為にも</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>562</PointerOffsetDec>
    <PointerOffsetHex>0x232</PointerOffsetHex>
    <JapaneseText>それは必要な事なんだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>30</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>583</PointerOffsetDec>
    <PointerOffsetHex>0x247</PointerOffsetHex>
    <JapaneseText>志が高いのね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>31</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>596</PointerOffsetDec>
    <PointerOffsetHex>0x254</PointerOffsetHex>
    <JapaneseText>あなたの知らない世界の情報なんて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>32</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>632</PointerOffsetDec>
    <PointerOffsetHex>0x278</PointerOffsetHex>
    <JapaneseText>それこそ限りがないと思うけど</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>33</Id>
    <Status />
  </Entry>
</Entries>