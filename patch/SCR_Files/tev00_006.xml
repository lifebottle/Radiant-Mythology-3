    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>976</PointerOffsetDec>
    <PointerOffsetHex>0x3d0</PointerOffsetHex>
    <JapaneseText>０になるまで出来るんだキュ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1003</PointerOffsetDec>
    <PointerOffsetHex>0x3eb</PointerOffsetHex>
