<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>404</PointerOffsetDec>
    <PointerOffsetHex>0x194</PointerOffsetHex>
    <JapaneseText>少しいいか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>28</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>417</PointerOffsetDec>
    <PointerOffsetHex>0x1a1</PointerOffsetHex>
    <JapaneseText>折り入って頼みたい事が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>441</PointerOffsetDec>
    <PointerOffsetHex>0x1b9</PointerOffsetHex>
    <JapaneseText>あるのだが……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>30</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>463</PointerOffsetDec>
    <PointerOffsetHex>0x1cf</PointerOffsetHex>
    <JapaneseText>実は、村に残してきた</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>32</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>485</PointerOffsetDec>
    <PointerOffsetHex>0x1e5</PointerOffsetHex>
    <JapaneseText>家族の事が気になっていてな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>33</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>521</PointerOffsetDec>
    <PointerOffsetHex>0x209</PointerOffsetHex>
    <JapaneseText>いや、お前が想像している様な</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>35</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>551</PointerOffsetDec>
    <PointerOffsetHex>0x227</PointerOffsetHex>
    <JapaneseText>意味での「家族」ではない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>576</PointerOffsetDec>
    <PointerOffsetHex>0x240</PointerOffsetHex>
    <JapaneseText>身寄りのない幼い子どもを一人、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>37</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>608</PointerOffsetDec>
    <PointerOffsetHex>0x260</PointerOffsetHex>
    <JapaneseText>俺が引き取って面倒を見ていたのだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>38</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>650</PointerOffsetDec>
    <PointerOffsetHex>0x28a</PointerOffsetHex>
    <JapaneseText>ギルドの仕事はまだ早いだろうと、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>684</PointerOffsetDec>
    <PointerOffsetHex>0x2ac</PointerOffsetHex>
    <JapaneseText>あえて村に残したまま</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>706</PointerOffsetDec>
    <PointerOffsetHex>0x2c2</PointerOffsetHex>
    <JapaneseText>こちらに来てしまったが……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>733</PointerOffsetDec>
    <PointerOffsetHex>0x2dd</PointerOffsetHex>
    <JapaneseText>ここでは同年代の子どもも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>759</PointerOffsetDec>
    <PointerOffsetHex>0x2f7</PointerOffsetHex>
    <JapaneseText>当たり前に仕事を請け負っている。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>793</PointerOffsetDec>
    <PointerOffsetHex>0x319</PointerOffsetHex>
    <JapaneseText>それを見て、少し考えが変わってな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>835</PointerOffsetDec>
    <PointerOffsetHex>0x343</PointerOffsetHex>
    <JapaneseText>もちろん、都合の付く時で構わん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>866</PointerOffsetDec>
    <PointerOffsetHex>0x362</PointerOffsetHex>
    <JapaneseText>迎えに行くまでの道中、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>890</PointerOffsetDec>
    <PointerOffsetHex>0x37a</PointerOffsetHex>
    <JapaneseText>手を貸してはもらえないか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>917</PointerOffsetDec>
    <PointerOffsetHex>0x395</PointerOffsetHex>
    <JapaneseText>クエスト「ちちんぷいぷいの魔法」が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>953</PointerOffsetDec>
    <PointerOffsetHex>0x3b9</PointerOffsetHex>
    <JapaneseText>エントリーされました。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>977</PointerOffsetDec>
    <PointerOffsetHex>0x3d1</PointerOffsetHex>
    <JapaneseText>ユージーンから受ける事が出来ます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
</Entries>