<Entries>
  <Entry>
    <PointerOffsetDec>382</PointerOffsetDec>
    <PointerOffsetHex>0x17e</PointerOffsetHex>
    <JapaneseText>メルディ、アンジュに聞いたよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>414</PointerOffsetDec>
    <PointerOffsetHex>0x19e</PointerOffsetHex>
    <JapaneseText>ティトレイが村、悪い奴に占領された……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>30</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>454</PointerOffsetDec>
    <PointerOffsetHex>0x1c6</PointerOffsetHex>
    <JapaneseText>本当なのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>31</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>467</PointerOffsetDec>
    <PointerOffsetHex>0x1d3</PointerOffsetHex>
    <JapaneseText>ああ、まあな……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>32</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>487</PointerOffsetDec>
    <PointerOffsetHex>0x1e7</PointerOffsetHex>
    <JapaneseText>あいつらだけは絶対許せねえ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>33</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>518</PointerOffsetDec>
    <PointerOffsetHex>0x206</PointerOffsetHex>
    <JapaneseText>けど、今は他に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>34</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>534</PointerOffsetDec>
    <PointerOffsetHex>0x216</PointerOffsetHex>
    <JapaneseText>もっとやるべき事があるからよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>35</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>566</PointerOffsetDec>
    <PointerOffsetHex>0x236</PointerOffsetHex>
    <JapaneseText>サレの奴をぶっ倒すのはもう少し先だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>601</PointerOffsetDec>
    <PointerOffsetHex>0x259</PointerOffsetHex>
    <JapaneseText>やるべき、こと？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>37</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>618</PointerOffsetDec>
    <PointerOffsetHex>0x26a</PointerOffsetHex>
    <JapaneseText>村に物資の仕送りをする事と</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>38</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>646</PointerOffsetDec>
    <PointerOffsetHex>0x286</PointerOffsetHex>
    <JapaneseText>ギルドできっちり働く事だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>39</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>671</PointerOffsetDec>
    <PointerOffsetHex>0x29f</PointerOffsetHex>
    <JapaneseText>このご時勢だ、仲間だったら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>699</PointerOffsetDec>
    <PointerOffsetHex>0x2bb</PointerOffsetHex>
    <JapaneseText>辛い時に助け合うのは当然だろ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>730</PointerOffsetDec>
    <PointerOffsetHex>0x2da</PointerOffsetHex>
    <JapaneseText>大変な状況なのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>748</PointerOffsetDec>
    <PointerOffsetHex>0x2ec</PointerOffsetHex>
    <JapaneseText>おれだけじゃねえんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>769</PointerOffsetDec>
    <PointerOffsetHex>0x301</PointerOffsetHex>
    <JapaneseText>ティトレイ、いい奴だな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>792</PointerOffsetDec>
    <PointerOffsetHex>0x318</PointerOffsetHex>
    <JapaneseText>おう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>800</PointerOffsetDec>
    <PointerOffsetHex>0x320</PointerOffsetHex>
    <JapaneseText>あったりまえだろ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>819</PointerOffsetDec>
    <PointerOffsetHex>0x333</PointerOffsetHex>
    <JapaneseText>おれはメルディの仲間で、家族で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>851</PointerOffsetDec>
    <PointerOffsetHex>0x353</PointerOffsetHex>
    <JapaneseText>兄ちゃんなんだからな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>872</PointerOffsetDec>
    <PointerOffsetHex>0x368</PointerOffsetHex>
    <JapaneseText>えへへ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>882</PointerOffsetDec>
    <PointerOffsetHex>0x372</PointerOffsetHex>
    <JapaneseText>ニーチャン</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
