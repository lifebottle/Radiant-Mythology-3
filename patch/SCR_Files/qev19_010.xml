<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>1029</PointerOffsetDec>
    <PointerOffsetHex>0x405</PointerOffsetHex>
    <JapaneseText>シェリア、シェリア！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1050</PointerOffsetDec>
    <PointerOffsetHex>0x41a</PointerOffsetHex>
    <JapaneseText>どうしたの、ソフィ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1072</PointerOffsetDec>
    <PointerOffsetHex>0x430</PointerOffsetHex>
    <JapaneseText>そんなに慌てて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1087</PointerOffsetDec>
    <PointerOffsetHex>0x43f</PointerOffsetHex>
    <JapaneseText>前にね、アスベルが……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1111</PointerOffsetDec>
    <PointerOffsetHex>0x457</PointerOffsetHex>
    <JapaneseText>花の種をくれたの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1128</PointerOffsetDec>
    <PointerOffsetHex>0x468</PointerOffsetHex>
    <JapaneseText>それでね……この植木鉢で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1154</PointerOffsetDec>
    <PointerOffsetHex>0x482</PointerOffsetHex>
    <JapaneseText>ずっと育ててたらね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1176</PointerOffsetDec>
    <PointerOffsetHex>0x498</PointerOffsetHex>
    <JapaneseText>今日……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1197</PointerOffsetDec>
    <PointerOffsetHex>0x4ad</PointerOffsetHex>
    <JapaneseText>あっ、つぼみが付いたのね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1222</PointerOffsetDec>
    <PointerOffsetHex>0x4c6</PointerOffsetHex>
    <JapaneseText>この様子だと、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1238</PointerOffsetDec>
    <PointerOffsetHex>0x4d6</PointerOffsetHex>
    <JapaneseText>もうすぐ咲くんじゃないかしら。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1270</PointerOffsetDec>
    <PointerOffsetHex>0x4f6</PointerOffsetHex>
    <JapaneseText>良かったわね、ソフィ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1291</PointerOffsetDec>
    <PointerOffsetHex>0x50b</PointerOffsetHex>
    <JapaneseText>うん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1296</PointerOffsetDec>
    <PointerOffsetHex>0x510</PointerOffsetHex>
    <JapaneseText>二人とも、随分楽しそうだな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1323</PointerOffsetDec>
    <PointerOffsetHex>0x52b</PointerOffsetHex>
    <JapaneseText>あれ？　それって……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1344</PointerOffsetDec>
    <PointerOffsetHex>0x540</PointerOffsetHex>
    <JapaneseText>見て、アスベル。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1362</PointerOffsetDec>
    <PointerOffsetHex>0x552</PointerOffsetHex>
    <JapaneseText>つぼみ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1372</PointerOffsetDec>
    <PointerOffsetHex>0x55c</PointerOffsetHex>
    <JapaneseText>とってもかわいい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1389</PointerOffsetDec>
    <PointerOffsetHex>0x56d</PointerOffsetHex>
    <JapaneseText>ああ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1397</PointerOffsetDec>
    <PointerOffsetHex>0x575</PointerOffsetHex>
    <JapaneseText>そうだな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1406</PointerOffsetDec>
    <PointerOffsetHex>0x57e</PointerOffsetHex>
    <JapaneseText>もしかしたら、この花はもう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1434</PointerOffsetDec>
    <PointerOffsetHex>0x59a</PointerOffsetHex>
    <JapaneseText>手に入らなくなるかもしれない……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1470</PointerOffsetDec>
    <PointerOffsetHex>0x5be</PointerOffsetHex>
    <JapaneseText>大切に育てなくちゃな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1491</PointerOffsetDec>
    <PointerOffsetHex>0x5d3</PointerOffsetHex>
    <JapaneseText>手に入らなくなる……？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1515</PointerOffsetDec>
    <PointerOffsetHex>0x5eb</PointerOffsetHex>
    <JapaneseText>どういう事なの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1532</PointerOffsetDec>
    <PointerOffsetHex>0x5fc</PointerOffsetHex>
    <JapaneseText>実は、その花の群生地である</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1560</PointerOffsetDec>
    <PointerOffsetHex>0x618</PointerOffsetHex>
    <JapaneseText>ルバーブ連山で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1576</PointerOffsetDec>
    <PointerOffsetHex>0x628</PointerOffsetHex>
    <JapaneseText>魔物が暴れているらしいんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1603</PointerOffsetDec>
    <PointerOffsetHex>0x643</PointerOffsetHex>
    <JapaneseText>魔物が？　どうして？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1624</PointerOffsetDec>
    <PointerOffsetHex>0x658</PointerOffsetHex>
    <JapaneseText>俺も詳しく知っているわけじゃないけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1664</PointerOffsetDec>
    <PointerOffsetHex>0x680</PointerOffsetHex>
    <JapaneseText>最近の魔物の異常増殖が原因らしい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1697</PointerOffsetDec>
    <PointerOffsetHex>0x6a1</PointerOffsetHex>
    <JapaneseText>その話……、聞いた事があるわ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1729</PointerOffsetDec>
    <PointerOffsetHex>0x6c1</PointerOffsetHex>
    <JapaneseText>互いに食べるものを奪い合って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1759</PointerOffsetDec>
    <PointerOffsetHex>0x6df</PointerOffsetHex>
    <JapaneseText>どんどん魔物が凶暴化してるって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1790</PointerOffsetDec>
    <PointerOffsetHex>0x6fe</PointerOffsetHex>
    <JapaneseText>ああ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1798</PointerOffsetDec>
    <PointerOffsetHex>0x706</PointerOffsetHex>
    <JapaneseText>今は被害が動物だけでなく、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1826</PointerOffsetDec>
    <PointerOffsetHex>0x722</PointerOffsetHex>
    <JapaneseText>植物にも出始めている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1847</PointerOffsetDec>
    <PointerOffsetHex>0x737</PointerOffsetHex>
    <JapaneseText>花を保護しようにも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1867</PointerOffsetDec>
    <PointerOffsetHex>0x74b</PointerOffsetHex>
    <JapaneseText>適応環境が特殊な種だから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1893</PointerOffsetDec>
    <PointerOffsetHex>0x765</PointerOffsetHex>
    <JapaneseText>このままだと、絶滅の可能性もあるって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1930</PointerOffsetDec>
    <PointerOffsetHex>0x78a</PointerOffsetHex>
    <JapaneseText>そんな……ひどい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1947</PointerOffsetDec>
    <PointerOffsetHex>0x79b</PointerOffsetHex>
    <JapaneseText>そんなの、やだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1965</PointerOffsetDec>
    <PointerOffsetHex>0x7ad</PointerOffsetHex>
    <JapaneseText>アスベル、わたし、この子達を助けたい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2002</PointerOffsetDec>
    <PointerOffsetHex>0x7d2</PointerOffsetHex>
    <JapaneseText>そう言うと思ったよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2024</PointerOffsetDec>
    <PointerOffsetHex>0x7e8</PointerOffsetHex>
    <JapaneseText>シェリアも手伝ってくれるだろう？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2057</PointerOffsetDec>
    <PointerOffsetHex>0x809</PointerOffsetHex>
    <JapaneseText>ええ、もちろん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2075</PointerOffsetDec>
    <PointerOffsetHex>0x81b</PointerOffsetHex>
    <JapaneseText>……でも、私達だけで大丈夫かしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2110</PointerOffsetDec>
    <PointerOffsetHex>0x83e</PointerOffsetHex>
    <JapaneseText>そうだな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2122</PointerOffsetDec>
    <PointerOffsetHex>0x84a</PointerOffsetHex>
    <JapaneseText>魔物がどれだけ増えているか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2150</PointerOffsetDec>
    <PointerOffsetHex>0x866</PointerOffsetHex>
    <JapaneseText>見当もつかない……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2169</PointerOffsetDec>
    <PointerOffsetHex>0x879</PointerOffsetHex>
    <JapaneseText>誰かの手を借りたいところだな……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2202</PointerOffsetDec>
    <PointerOffsetHex>0x89a</PointerOffsetHex>
    <JapaneseText>クエスト「思い出の花を守れ！」が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2236</PointerOffsetDec>
    <PointerOffsetHex>0x8bc</PointerOffsetHex>
    <JapaneseText>エントリーされました。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2260</PointerOffsetDec>
    <PointerOffsetHex>0x8d4</PointerOffsetHex>
    <JapaneseText>アスベルから受ける事が出来ます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>123</Id>
    <Status />
  </Entry>
</Entries>