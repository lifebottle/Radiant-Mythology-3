<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>812</PointerOffsetDec>
    <PointerOffsetHex>0x32c</PointerOffsetHex>
    <JapaneseText>ヘーゼル村のみんなは、無事砂漠を越えられた。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>858</PointerOffsetDec>
    <PointerOffsetHex>0x35a</PointerOffsetHex>
    <JapaneseText>協力に深く感謝する</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>877</PointerOffsetDec>
    <PointerOffsetHex>0x36d</PointerOffsetHex>
    <JapaneseText>みんなの無事を喜ぶ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>896</PointerOffsetDec>
    <PointerOffsetHex>0x380</PointerOffsetHex>
    <JapaneseText>謙遜する</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>905</PointerOffsetDec>
    <PointerOffsetHex>0x389</PointerOffsetHex>
    <JapaneseText>うむ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>913</PointerOffsetDec>
    <PointerOffsetHex>0x391</PointerOffsetHex>
    <JapaneseText>村のみんなは、お前の活躍で</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>941</PointerOffsetDec>
    <PointerOffsetHex>0x3ad</PointerOffsetHex>
    <JapaneseText>新しい生活を手に入れられた</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>968</PointerOffsetDec>
    <PointerOffsetHex>0x3c8</PointerOffsetHex>
    <JapaneseText>いつか、お返しをせねばと</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>994</PointerOffsetDec>
    <PointerOffsetHex>0x3e2</PointerOffsetHex>
    <JapaneseText>みんなが感謝していたぞ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1017</PointerOffsetDec>
    <PointerOffsetHex>0x3f9</PointerOffsetHex>
    <JapaneseText>そう謙遜する事は無い。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1041</PointerOffsetDec>
    <PointerOffsetHex>0x411</PointerOffsetHex>
    <JapaneseText>自分の力を知る事は、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1063</PointerOffsetDec>
    <PointerOffsetHex>0x427</PointerOffsetHex>
    <JapaneseText>強くなる為に必要な事だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1086</PointerOffsetDec>
    <PointerOffsetHex>0x43e</PointerOffsetHex>
    <JapaneseText>今回のお前の活躍は、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1108</PointerOffsetDec>
    <PointerOffsetHex>0x454</PointerOffsetHex>
    <JapaneseText>それだけの強さを持っていると言う証</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1143</PointerOffsetDec>
    <PointerOffsetHex>0x477</PointerOffsetHex>
    <JapaneseText>さらなる強さを求めるならば、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1173</PointerOffsetDec>
    <PointerOffsetHex>0x495</PointerOffsetHex>
    <JapaneseText>今の実力を受け入れておくのだな</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1204</PointerOffsetDec>
    <PointerOffsetHex>0x4b4</PointerOffsetHex>
    <JapaneseText>ユージーンさん達も無事で良かった</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1237</PointerOffsetDec>
    <PointerOffsetHex>0x4d5</PointerOffsetHex>
    <JapaneseText>ああ、現場にサレがいなかったのが</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1271</PointerOffsetDec>
    <PointerOffsetHex>0x4f7</PointerOffsetHex>
    <JapaneseText>幸いだった</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1282</PointerOffsetDec>
    <PointerOffsetHex>0x502</PointerOffsetHex>
    <JapaneseText>オルタ・ビレッジもうまく運営して</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1316</PointerOffsetDec>
    <PointerOffsetHex>0x524</PointerOffsetHex>
    <JapaneseText>いければいいな</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1331</PointerOffsetDec>
    <PointerOffsetHex>0x533</PointerOffsetHex>
    <JapaneseText>暗雲とした世の中、多くの者が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1361</PointerOffsetDec>
    <PointerOffsetHex>0x551</PointerOffsetHex>
    <JapaneseText>未来への展望を見失っている</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1388</PointerOffsetDec>
    <PointerOffsetHex>0x56c</PointerOffsetHex>
    <JapaneseText>みんな、不安と困惑の中にあり、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1420</PointerOffsetDec>
    <PointerOffsetHex>0x58c</PointerOffsetHex>
    <JapaneseText>どう生きていけばいいかがわからず</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1454</PointerOffsetDec>
    <PointerOffsetHex>0x5ae</PointerOffsetHex>
    <JapaneseText>世の中に失望している</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1475</PointerOffsetDec>
    <PointerOffsetHex>0x5c3</PointerOffsetHex>
    <JapaneseText>国も助けてくれないからな…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1502</PointerOffsetDec>
    <PointerOffsetHex>0x5de</PointerOffsetHex>
    <JapaneseText>だが、それでも希望を見つけ、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1532</PointerOffsetDec>
    <PointerOffsetHex>0x5fc</PointerOffsetHex>
    <JapaneseText>自分達で未来を作らなければならない</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1567</PointerOffsetDec>
    <PointerOffsetHex>0x61f</PointerOffsetHex>
    <JapaneseText>頼るばかりでなく、一人ひとりが</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1599</PointerOffsetDec>
    <PointerOffsetHex>0x63f</PointerOffsetHex>
    <JapaneseText>希望を生み出せるきっかけに</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1627</PointerOffsetDec>
    <PointerOffsetHex>0x65b</PointerOffsetHex>
    <JapaneseText>我々アドリビトムがならなければな</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1660</PointerOffsetDec>
    <PointerOffsetHex>0x67c</PointerOffsetHex>
    <JapaneseText>それじゃあ、今回もお疲れ様でした</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1693</PointerOffsetDec>
    <PointerOffsetHex>0x69d</PointerOffsetHex>
    <JapaneseText>それから、ちょっと研究室を訪ねてくれる？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1735</PointerOffsetDec>
    <PointerOffsetHex>0x6c7</PointerOffsetHex>
    <JapaneseText>ハロルドさん達の研究、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1759</PointerOffsetDec>
    <PointerOffsetHex>0x6df</PointerOffsetHex>
    <JapaneseText>難航しているみたいだし</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1782</PointerOffsetDec>
    <PointerOffsetHex>0x6f6</PointerOffsetHex>
    <JapaneseText>ちょっと経過を聞いてきて欲しいの</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1815</PointerOffsetDec>
    <PointerOffsetHex>0x717</PointerOffsetHex>
    <JapaneseText>じゃ、お願いね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1830</PointerOffsetDec>
    <PointerOffsetHex>0x726</PointerOffsetHex>
    <JapaneseText>ヴェイグとユージーン、ティトレイとアニーが</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1874</PointerOffsetDec>
    <PointerOffsetHex>0x752</PointerOffsetHex>
    <JapaneseText>別任務から帰還しました</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
</Entries>