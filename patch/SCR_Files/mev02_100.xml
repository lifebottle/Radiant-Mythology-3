<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>934</PointerOffsetDec>
    <PointerOffsetHex>0x3a6</PointerOffsetHex>
    <JapaneseText>ああ、やっと荷解きが済んだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>964</PointerOffsetDec>
    <PointerOffsetHex>0x3c4</PointerOffsetHex>
    <JapaneseText>本が多くて、大変だったよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>989</PointerOffsetDec>
    <PointerOffsetHex>0x3dd</PointerOffsetHex>
    <JapaneseText>○○、キール君とメルディはね、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1021</PointerOffsetDec>
    <PointerOffsetHex>0x3fd</PointerOffsetHex>
    <JapaneseText>「持続可能な社会」を研究しに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1051</PointerOffsetDec>
    <PointerOffsetHex>0x41b</PointerOffsetHex>
    <JapaneseText>大学へ行っていたの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1070</PointerOffsetDec>
    <PointerOffsetHex>0x42e</PointerOffsetHex>
    <JapaneseText>ぼく達の世代が、将来の世代の為に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1106</PointerOffsetDec>
    <PointerOffsetHex>0x452</PointerOffsetHex>
    <JapaneseText>環境をありのまま利用して、充足した社会を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1148</PointerOffsetDec>
    <PointerOffsetHex>0x47c</PointerOffsetHex>
    <JapaneseText>作るっていうものさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1167</PointerOffsetDec>
    <PointerOffsetHex>0x48f</PointerOffsetHex>
    <JapaneseText>よって、星晶の採掘も行わない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1196</PointerOffsetDec>
    <PointerOffsetHex>0x4ac</PointerOffsetHex>
    <JapaneseText>人間が自然を壊さず、調和して、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1228</PointerOffsetDec>
    <PointerOffsetHex>0x4cc</PointerOffsetHex>
    <JapaneseText>人々が生活する為の衣食住が十分に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1262</PointerOffsetDec>
    <PointerOffsetHex>0x4ee</PointerOffsetHex>
    <JapaneseText>行き渡る社会</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1275</PointerOffsetDec>
    <PointerOffsetHex>0x4fb</PointerOffsetHex>
    <JapaneseText>その理念で運営する村、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1299</PointerOffsetDec>
    <PointerOffsetHex>0x513</PointerOffsetHex>
    <JapaneseText>「オルタ・ビレッジ」を作るのが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1331</PointerOffsetDec>
    <PointerOffsetHex>0x533</PointerOffsetHex>
    <JapaneseText>アドリビトムの目的なの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1354</PointerOffsetDec>
    <PointerOffsetHex>0x54a</PointerOffsetHex>
    <JapaneseText>みんなでお金を貯めて、オルタ・ビレッジを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1396</PointerOffsetDec>
    <PointerOffsetHex>0x574</PointerOffsetHex>
    <JapaneseText>各地に作って、大国に消費された村の人達を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1438</PointerOffsetDec>
    <PointerOffsetHex>0x59e</PointerOffsetHex>
    <JapaneseText>移住させるのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1453</PointerOffsetDec>
    <PointerOffsetHex>0x5ad</PointerOffsetHex>
    <JapaneseText>素敵な村だ、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1472</PointerOffsetDec>
    <PointerOffsetHex>0x5c0</PointerOffsetHex>
    <JapaneseText>実現出来ればいいな、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1499</PointerOffsetDec>
    <PointerOffsetHex>0x5db</PointerOffsetHex>
    <JapaneseText>はいな！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1509</PointerOffsetDec>
    <PointerOffsetHex>0x5e5</PointerOffsetHex>
    <JapaneseText>みんな幸せ、みんな仲間、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1535</PointerOffsetDec>
    <PointerOffsetHex>0x5ff</PointerOffsetHex>
    <JapaneseText>みんな友達がなれる村！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1560</PointerOffsetDec>
    <PointerOffsetHex>0x618</PointerOffsetHex>
    <JapaneseText>きっとする！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1574</PointerOffsetDec>
    <PointerOffsetHex>0x626</PointerOffsetHex>
    <JapaneseText>キールが研究、リッパ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1598</PointerOffsetDec>
    <PointerOffsetHex>0x63e</PointerOffsetHex>
    <JapaneseText>絶対、実現させるよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1617</PointerOffsetDec>
    <PointerOffsetHex>0x651</PointerOffsetHex>
    <JapaneseText>そしたら、たくさんの人幸せになる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1650</PointerOffsetDec>
    <PointerOffsetHex>0x672</PointerOffsetHex>
    <JapaneseText>しかし、大学が休校になった今は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1684</PointerOffsetDec>
    <PointerOffsetHex>0x694</PointerOffsetHex>
    <JapaneseText>ここで研究を続けていくしかないな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1717</PointerOffsetDec>
    <PointerOffsetHex>0x6b5</PointerOffsetHex>
    <JapaneseText>そうそう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1729</PointerOffsetDec>
    <PointerOffsetHex>0x6c1</PointerOffsetHex>
    <JapaneseText>あの現象については？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1750</PointerOffsetDec>
    <PointerOffsetHex>0x6d6</PointerOffsetHex>
    <JapaneseText>星晶の採掘跡地で起こる、生物変化現象か。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1792</PointerOffsetDec>
    <PointerOffsetHex>0x700</PointerOffsetHex>
    <JapaneseText>ぼくが街で聞いたのは…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1815</PointerOffsetDec>
    <PointerOffsetHex>0x717</PointerOffsetHex>
    <JapaneseText>生物に変化が現れた場所には、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1845</PointerOffsetDec>
    <PointerOffsetHex>0x735</PointerOffsetHex>
    <JapaneseText>赤い煙の様なものが現れていたらしい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1880</PointerOffsetDec>
    <PointerOffsetHex>0x758</PointerOffsetHex>
    <JapaneseText>その後、生物への変化が見られたと言うが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1919</PointerOffsetDec>
    <PointerOffsetHex>0x77f</PointerOffsetHex>
    <JapaneseText>赤い煙？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1928</PointerOffsetDec>
    <PointerOffsetHex>0x788</PointerOffsetHex>
    <JapaneseText>はいな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1938</PointerOffsetDec>
    <PointerOffsetHex>0x792</PointerOffsetHex>
    <JapaneseText>その赤い煙も、ほんの数日現れただけな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1978</PointerOffsetDec>
    <PointerOffsetHex>0x7ba</PointerOffsetHex>
    <JapaneseText>今は消えてしまってるそうだよう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2009</PointerOffsetDec>
    <PointerOffsetHex>0x7d9</PointerOffsetHex>
    <JapaneseText>気になる話ね、その赤い煙って…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2040</PointerOffsetDec>
    <PointerOffsetHex>0x7f8</PointerOffsetHex>
    <JapaneseText>街では失職者もいるらしく、遠征採掘の</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2078</PointerOffsetDec>
    <PointerOffsetHex>0x81e</PointerOffsetHex>
    <JapaneseText>登録所に並んでいる人を見かけたよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2111</PointerOffsetDec>
    <PointerOffsetHex>0x83f</PointerOffsetHex>
    <JapaneseText>そこで聞いた話では、オルタータ火山が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2149</PointerOffsetDec>
    <PointerOffsetHex>0x865</PointerOffsetHex>
    <JapaneseText>数日前に採掘を終えたらしい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2179</PointerOffsetDec>
    <PointerOffsetHex>0x883</PointerOffsetHex>
    <JapaneseText>行ってみてはどうだ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2200</PointerOffsetDec>
    <PointerOffsetHex>0x898</PointerOffsetHex>
    <JapaneseText>そうね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2210</PointerOffsetDec>
    <PointerOffsetHex>0x8a2</PointerOffsetHex>
    <JapaneseText>では、依頼の登録をしておきましょう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2245</PointerOffsetDec>
    <PointerOffsetHex>0x8c5</PointerOffsetHex>
    <JapaneseText>キールがアドリビトムに加入しました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2280</PointerOffsetDec>
    <PointerOffsetHex>0x8e8</PointerOffsetHex>
    <JapaneseText>メルディがアドリビトムに加入しました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
</Entries>