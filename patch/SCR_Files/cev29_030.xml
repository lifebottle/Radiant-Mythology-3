    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>997</PointerOffsetDec>
    <PointerOffsetHex>0x3e5</PointerOffsetHex>
