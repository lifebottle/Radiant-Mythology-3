    <EnglishText />
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>580</PointerOffsetDec>
    <PointerOffsetHex>0x244</PointerOffsetHex>
    <JapaneseText>殊勝な心がけね。</JapaneseText>
