  <Entry>
    <PointerOffsetDec>620</PointerOffsetDec>
    <PointerOffsetHex>0x26c</PointerOffsetHex>
    <JapaneseText>聞かせてあげたいくらいだわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>39</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>647</PointerOffsetDec>
    <PointerOffsetHex>0x287</PointerOffsetHex>
