<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>1108</PointerOffsetDec>
    <PointerOffsetHex>0x454</PointerOffsetHex>
    <JapaneseText>あら、リオンさん？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1127</PointerOffsetDec>
    <PointerOffsetHex>0x467</PointerOffsetHex>
    <JapaneseText>何をしているの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1144</PointerOffsetDec>
    <PointerOffsetHex>0x478</PointerOffsetHex>
    <JapaneseText>その紅茶の缶は……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1163</PointerOffsetDec>
    <PointerOffsetHex>0x48b</PointerOffsetHex>
    <JapaneseText>何でもいいだろう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1183</PointerOffsetDec>
    <PointerOffsetHex>0x49f</PointerOffsetHex>
    <JapaneseText>僕に構うな！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1196</PointerOffsetDec>
    <PointerOffsetHex>0x4ac</PointerOffsetHex>
    <JapaneseText>お茶を淹れようとしていたのね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1228</PointerOffsetDec>
    <PointerOffsetHex>0x4cc</PointerOffsetHex>
    <JapaneseText>邪魔をしてしまって、ごめんなさい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1270</PointerOffsetDec>
    <PointerOffsetHex>0x4f6</PointerOffsetHex>
    <JapaneseText>リオンさん、もしかして</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1294</PointerOffsetDec>
    <PointerOffsetHex>0x50e</PointerOffsetHex>
    <JapaneseText>最近よく眠れていないの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1319</PointerOffsetDec>
    <PointerOffsetHex>0x527</PointerOffsetHex>
    <JapaneseText>なぜそんな事を聞く</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1338</PointerOffsetDec>
    <PointerOffsetHex>0x53a</PointerOffsetHex>
    <JapaneseText>その茶葉はね、リラックス効果が高いの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1378</PointerOffsetDec>
    <PointerOffsetHex>0x562</PointerOffsetHex>
    <JapaneseText>だから、寝る前に飲むと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1402</PointerOffsetDec>
    <PointerOffsetHex>0x57a</PointerOffsetHex>
    <JapaneseText>疲れがほぐれて、とってもよく眠れるのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1441</PointerOffsetDec>
    <PointerOffsetHex>0x5a1</PointerOffsetHex>
    <JapaneseText>もしかして、贈り物だったのかしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1477</PointerOffsetDec>
    <PointerOffsetHex>0x5c5</PointerOffsetHex>
    <JapaneseText>ご家族から？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1490</PointerOffsetDec>
    <PointerOffsetHex>0x5d2</PointerOffsetHex>
    <JapaneseText>……フン。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1502</PointerOffsetDec>
    <PointerOffsetHex>0x5de</PointerOffsetHex>
    <JapaneseText>どうだかな……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1517</PointerOffsetDec>
    <PointerOffsetHex>0x5ed</PointerOffsetHex>
    <JapaneseText>いいご家族がいるのね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1541</PointerOffsetDec>
    <PointerOffsetHex>0x605</PointerOffsetHex>
    <JapaneseText>素敵だわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1550</PointerOffsetDec>
    <PointerOffsetHex>0x60e</PointerOffsetHex>
    <JapaneseText>……おい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1562</PointerOffsetDec>
    <PointerOffsetHex>0x61a</PointerOffsetHex>
    <JapaneseText>この紅茶は、どう淹れれば</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1588</PointerOffsetDec>
    <PointerOffsetHex>0x634</PointerOffsetHex>
    <JapaneseText>一番美味くなる？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1605</PointerOffsetDec>
    <PointerOffsetHex>0x645</PointerOffsetHex>
    <JapaneseText>まずは茶器を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1619</PointerOffsetDec>
    <PointerOffsetHex>0x653</PointerOffsetHex>
    <JapaneseText>あたためる事から始めるの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1647</PointerOffsetDec>
    <PointerOffsetHex>0x66f</PointerOffsetHex>
    <JapaneseText>こうして、熱湯で中を満たして……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1680</PointerOffsetDec>
    <PointerOffsetHex>0x690</PointerOffsetHex>
    <JapaneseText>……ッ、熱い！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1695</PointerOffsetDec>
    <PointerOffsetHex>0x69f</PointerOffsetHex>
    <JapaneseText>ほら、気を付けて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1712</PointerOffsetDec>
    <PointerOffsetHex>0x6b0</PointerOffsetHex>
    <JapaneseText>こ……子ども扱いするな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1735</PointerOffsetDec>
    <PointerOffsetHex>0x6c7</PointerOffsetHex>
    <JapaneseText>……そうね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1749</PointerOffsetDec>
    <PointerOffsetHex>0x6d5</PointerOffsetHex>
    <JapaneseText>ごめんなさい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1762</PointerOffsetDec>
    <PointerOffsetHex>0x6e2</PointerOffsetHex>
    <JapaneseText>私も、次から気を付けるわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
</Entries>