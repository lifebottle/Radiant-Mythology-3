<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>406</PointerOffsetDec>
    <PointerOffsetHex>0x196</PointerOffsetHex>
    <JapaneseText>感謝の気持ちを忘れるのって</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>32</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>434</PointerOffsetDec>
    <PointerOffsetHex>0x1b2</PointerOffsetHex>
    <JapaneseText>とっても悲しい事じゃない？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>33</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>461</PointerOffsetDec>
    <PointerOffsetHex>0x1cd</PointerOffsetHex>
    <JapaneseText>いきなりどうした。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>34</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>481</PointerOffsetDec>
    <PointerOffsetHex>0x1e1</PointerOffsetHex>
    <JapaneseText>随分実感のこもった言い方だな</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>35</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>510</PointerOffsetDec>
    <PointerOffsetHex>0x1fe</PointerOffsetHex>
    <JapaneseText>それはそうよ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>526</PointerOffsetDec>
    <PointerOffsetHex>0x20e</PointerOffsetHex>
    <JapaneseText>あたしだって、神官見習いなんですもの</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>37</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>563</PointerOffsetDec>
    <PointerOffsetHex>0x233</PointerOffsetHex>
    <JapaneseText>これでも色々、人の気持ちに対して</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>38</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>597</PointerOffsetDec>
    <PointerOffsetHex>0x255</PointerOffsetHex>
    <JapaneseText>思うところがあったりするの</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>39</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>624</PointerOffsetDec>
    <PointerOffsetHex>0x270</PointerOffsetHex>
    <JapaneseText>そうだな……この時勢だ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>647</PointerOffsetDec>
    <PointerOffsetHex>0x287</PointerOffsetHex>
    <JapaneseText>端的な救いを求める者ばかりが増え</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>681</PointerOffsetDec>
    <PointerOffsetHex>0x2a9</PointerOffsetHex>
    <JapaneseText>人は、他者への気遣いや感謝の思いを</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>717</PointerOffsetDec>
    <PointerOffsetHex>0x2cd</PointerOffsetHex>
    <JapaneseText>日ごとに忘れていっている</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>742</PointerOffsetDec>
    <PointerOffsetHex>0x2e6</PointerOffsetHex>
    <JapaneseText>慈愛の心を説く教会関係者として</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>774</PointerOffsetDec>
    <PointerOffsetHex>0x306</PointerOffsetHex>
    <JapaneseText>心穏やかではいられまい</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>797</PointerOffsetDec>
    <PointerOffsetHex>0x31d</PointerOffsetHex>
    <JapaneseText>それもそうだけど……。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>821</PointerOffsetDec>
    <PointerOffsetHex>0x335</PointerOffsetHex>
    <JapaneseText>あたしが腹を立てているのは</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>849</PointerOffsetDec>
    <PointerOffsetHex>0x351</PointerOffsetHex>
    <JapaneseText>カイウスのこと！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>866</PointerOffsetDec>
    <PointerOffsetHex>0x362</PointerOffsetHex>
    <JapaneseText>カイウスったら最近、あたしが洗濯物を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>904</PointerOffsetDec>
    <PointerOffsetHex>0x388</PointerOffsetHex>
    <JapaneseText>片付けてあげたりしても、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>930</PointerOffsetDec>
    <PointerOffsetHex>0x3a2</PointerOffsetHex>
    <JapaneseText>お礼の言葉一つ言わないの</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>955</PointerOffsetDec>
    <PointerOffsetHex>0x3bb</PointerOffsetHex>
    <JapaneseText>昔はそんなんじゃなかったのに</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>985</PointerOffsetDec>
    <PointerOffsetHex>0x3d9</PointerOffsetHex>
    <JapaneseText>いつから感謝の気持ちを</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1009</PointerOffsetDec>
    <PointerOffsetHex>0x3f1</PointerOffsetHex>
    <JapaneseText>忘れてしまったのかしら</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1032</PointerOffsetDec>
    <PointerOffsetHex>0x408</PointerOffsetHex>
    <JapaneseText>ああ、思い出したら</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1052</PointerOffsetDec>
    <PointerOffsetHex>0x41c</PointerOffsetHex>
    <JapaneseText>余計に腹が立ってきちゃった！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1081</PointerOffsetDec>
    <PointerOffsetHex>0x439</PointerOffsetHex>
    <JapaneseText>それは、感謝の気持ちとは</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1107</PointerOffsetDec>
    <PointerOffsetHex>0x453</PointerOffsetHex>
    <JapaneseText>また別の問題があるんじゃないか……？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
</Entries>