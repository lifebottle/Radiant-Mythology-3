<Entries>
  <Entry>
    <PointerOffsetDec>564</PointerOffsetDec>
    <PointerOffsetHex>0x234</PointerOffsetHex>
    <JapaneseText>ナタリア、知ってます？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>38</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>587</PointerOffsetDec>
    <PointerOffsetHex>0x24b</PointerOffsetHex>
    <JapaneseText>冬になると、外の国の皆さんは</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>39</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>617</PointerOffsetDec>
    <PointerOffsetHex>0x269</PointerOffsetHex>
    <JapaneseText>「聖なる夜」にお祝いを</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>641</PointerOffsetDec>
    <PointerOffsetHex>0x281</PointerOffsetHex>
    <JapaneseText>一斉にするんだそうですよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>666</PointerOffsetDec>
    <PointerOffsetHex>0x29a</PointerOffsetHex>
    <JapaneseText>せいなるよる？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>681</PointerOffsetDec>
    <PointerOffsetHex>0x2a9</PointerOffsetHex>
    <JapaneseText>うんとね、赤い帽子と</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>703</PointerOffsetDec>
    <PointerOffsetHex>0x2bf</PointerOffsetHex>
    <JapaneseText>白いひげのおじいさんが</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>727</PointerOffsetDec>
    <PointerOffsetHex>0x2d7</PointerOffsetHex>
    <JapaneseText>煙突からみんなの家に入って来るんだって</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>766</PointerOffsetDec>
    <PointerOffsetHex>0x2fe</PointerOffsetHex>
    <JapaneseText>まぁ！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>774</PointerOffsetDec>
    <PointerOffsetHex>0x306</PointerOffsetHex>
    <JapaneseText>不法侵入ではありませんか！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>801</PointerOffsetDec>
    <PointerOffsetHex>0x321</PointerOffsetHex>
    <JapaneseText>でも、いいおじいさんなんですよ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>835</PointerOffsetDec>
    <PointerOffsetHex>0x343</PointerOffsetHex>
    <JapaneseText>子ども達の為に、プレゼントを</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>865</PointerOffsetDec>
    <PointerOffsetHex>0x361</PointerOffsetHex>
    <JapaneseText>置いていってくれるんです</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>890</PointerOffsetDec>
    <PointerOffsetHex>0x37a</PointerOffsetHex>
    <JapaneseText>うん。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>898</PointerOffsetDec>
    <PointerOffsetHex>0x382</PointerOffsetHex>
    <JapaneseText>靴下の中に入れてくれるんだよね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>929</PointerOffsetDec>
    <PointerOffsetHex>0x3a1</PointerOffsetHex>
    <JapaneseText>靴下の中……？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>945</PointerOffsetDec>
    <PointerOffsetHex>0x3b1</PointerOffsetHex>
    <JapaneseText>いささか不衛生な気もしますが……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>978</PointerOffsetDec>
    <PointerOffsetHex>0x3d2</PointerOffsetHex>
    <JapaneseText>あっ、靴下は</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>992</PointerOffsetDec>
    <PointerOffsetHex>0x3e0</PointerOffsetHex>
    <JapaneseText>新しい物を用意するんですよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1019</PointerOffsetDec>
    <PointerOffsetHex>0x3fb</PointerOffsetHex>
    <JapaneseText>そ……それくらい。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1039</PointerOffsetDec>
    <PointerOffsetHex>0x40f</PointerOffsetHex>
    <JapaneseText>もちろん、私もわかっていますわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1070</PointerOffsetDec>
    <PointerOffsetHex>0x42e</PointerOffsetHex>
    <JapaneseText>いいなぁ、「聖なる夜」……。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1100</PointerOffsetDec>
    <PointerOffsetHex>0x44c</PointerOffsetHex>
    <JapaneseText>私の所にも、おじいさん来ないかなぁ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1135</PointerOffsetDec>
    <PointerOffsetHex>0x46f</PointerOffsetHex>
    <JapaneseText>きっと来てくれますよ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1159</PointerOffsetDec>
    <PointerOffsetHex>0x487</PointerOffsetHex>
    <JapaneseText>いい子にしていれば、大丈夫です</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
