<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>464</PointerOffsetDec>
    <PointerOffsetHex>0x1d0</PointerOffsetHex>
    <JapaneseText>ぼくが聞き及んだ限りでも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>490</PointerOffsetDec>
    <PointerOffsetHex>0x1ea</PointerOffsetHex>
    <JapaneseText>すずは色々な知識に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>37</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>510</PointerOffsetDec>
    <PointerOffsetHex>0x1fe</PointerOffsetHex>
    <JapaneseText>精通している様だな……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>38</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>533</PointerOffsetDec>
    <PointerOffsetHex>0x215</PointerOffsetHex>
    <JapaneseText>どこの大学で学んだんだ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>39</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>558</PointerOffsetDec>
    <PointerOffsetHex>0x22e</PointerOffsetHex>
    <JapaneseText>大学……？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>570</PointerOffsetDec>
    <PointerOffsetHex>0x23a</PointerOffsetHex>
    <JapaneseText>いえ、私は教育施設には通っていません</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>607</PointerOffsetDec>
    <PointerOffsetHex>0x25f</PointerOffsetHex>
    <JapaneseText>何だって？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>618</PointerOffsetDec>
    <PointerOffsetHex>0x26a</PointerOffsetHex>
    <JapaneseText>このぼくですら習得に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>640</PointerOffsetDec>
    <PointerOffsetHex>0x280</PointerOffsetHex>
    <JapaneseText>時間がかかる様な難解な知識を、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>672</PointerOffsetDec>
    <PointerOffsetHex>0x2a0</PointerOffsetHex>
    <JapaneseText>独学で得たっていうのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>697</PointerOffsetDec>
    <PointerOffsetHex>0x2b9</PointerOffsetHex>
    <JapaneseText>独学というより、里の頭領に教わりました。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>739</PointerOffsetDec>
    <PointerOffsetHex>0x2e3</PointerOffsetHex>
    <JapaneseText>いつか役立つ事があるかもしれぬ、と</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>774</PointerOffsetDec>
    <PointerOffsetHex>0x306</PointerOffsetHex>
    <JapaneseText>天候変化の科学的根拠や仕組みなんて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>812</PointerOffsetDec>
    <PointerOffsetHex>0x32c</PointerOffsetHex>
    <JapaneseText>一般人にはどう役に立つって言うんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>847</PointerOffsetDec>
    <PointerOffsetHex>0x34f</PointerOffsetHex>
    <JapaneseText>一般人ではありません。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>871</PointerOffsetDec>
    <PointerOffsetHex>0x367</PointerOffsetHex>
    <JapaneseText>私は忍者ですから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>888</PointerOffsetDec>
    <PointerOffsetHex>0x378</PointerOffsetHex>
    <JapaneseText>ああ、いや、そういう事が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>914</PointerOffsetDec>
    <PointerOffsetHex>0x392</PointerOffsetHex>
    <JapaneseText>言いたかったわけじゃ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>939</PointerOffsetDec>
    <PointerOffsetHex>0x3ab</PointerOffsetHex>
    <JapaneseText>忍者とはそういうものです。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>967</PointerOffsetDec>
    <PointerOffsetHex>0x3c7</PointerOffsetHex>
    <JapaneseText>厳しい道を乗り越えた者のみが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>997</PointerOffsetDec>
    <PointerOffsetHex>0x3e5</PointerOffsetHex>
    <JapaneseText>本当の忍びになれるのです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1022</PointerOffsetDec>
    <PointerOffsetHex>0x3fe</PointerOffsetHex>
    <JapaneseText>ええと……だから……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1043</PointerOffsetDec>
    <PointerOffsetHex>0x413</PointerOffsetHex>
    <JapaneseText>何か疑問点があれば、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1065</PointerOffsetDec>
    <PointerOffsetHex>0x429</PointerOffsetHex>
    <JapaneseText>私のわかる範囲でお答えしますが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1096</PointerOffsetDec>
    <PointerOffsetHex>0x448</PointerOffsetHex>
    <JapaneseText>……いえ……もういいです……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
</Entries>