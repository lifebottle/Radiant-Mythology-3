<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>816</PointerOffsetDec>
    <PointerOffsetHex>0x330</PointerOffsetHex>
    <JapaneseText>なあ、ロイド。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>832</PointerOffsetDec>
    <PointerOffsetHex>0x340</PointerOffsetHex>
    <JapaneseText>話があるんだけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>849</PointerOffsetDec>
    <PointerOffsetHex>0x351</PointerOffsetHex>
    <JapaneseText>どうしたんだ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>865</PointerOffsetDec>
    <PointerOffsetHex>0x361</PointerOffsetHex>
    <JapaneseText>そんな改まってさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>882</PointerOffsetDec>
    <PointerOffsetHex>0x372</PointerOffsetHex>
    <JapaneseText>ロイドの武器についてなんだけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>914</PointerOffsetDec>
    <PointerOffsetHex>0x392</PointerOffsetHex>
    <JapaneseText>もっと色んな二刀流を試してみても</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>948</PointerOffsetDec>
    <PointerOffsetHex>0x3b4</PointerOffsetHex>
    <JapaneseText>いいんじゃないかと思うんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>975</PointerOffsetDec>
    <PointerOffsetHex>0x3cf</PointerOffsetHex>
    <JapaneseText>色んな二刀流？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>990</PointerOffsetDec>
    <PointerOffsetHex>0x3de</PointerOffsetHex>
    <JapaneseText>そう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>998</PointerOffsetDec>
    <PointerOffsetHex>0x3e6</PointerOffsetHex>
    <JapaneseText>何というか、「新・二刀流！」</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1028</PointerOffsetDec>
    <PointerOffsetHex>0x404</PointerOffsetHex>
    <JapaneseText>みたいな感じで！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1045</PointerOffsetDec>
    <PointerOffsetHex>0x415</PointerOffsetHex>
    <JapaneseText>新・二刀流かぁ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1064</PointerOffsetDec>
    <PointerOffsetHex>0x428</PointerOffsetHex>
    <JapaneseText>面白そうだな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1080</PointerOffsetDec>
    <PointerOffsetHex>0x438</PointerOffsetHex>
    <JapaneseText>やってみようぜ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1097</PointerOffsetDec>
    <PointerOffsetHex>0x449</PointerOffsetHex>
    <JapaneseText>よし、そう来なくっちゃ！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1124</PointerOffsetDec>
    <PointerOffsetHex>0x464</PointerOffsetHex>
    <JapaneseText>「新」って事は、何か新しい事を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1156</PointerOffsetDec>
    <PointerOffsetHex>0x484</PointerOffsetHex>
    <JapaneseText>二刀流に盛り込まなくちゃダメだろうな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1193</PointerOffsetDec>
    <PointerOffsetHex>0x4a9</PointerOffsetHex>
    <JapaneseText>俺、思ったんだけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1215</PointerOffsetDec>
    <PointerOffsetHex>0x4bf</PointerOffsetHex>
    <JapaneseText>違う種類の武器を使うのはどうかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1250</PointerOffsetDec>
    <PointerOffsetHex>0x4e2</PointerOffsetHex>
    <JapaneseText>斧と剣とか、そんな感じの……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1279</PointerOffsetDec>
    <PointerOffsetHex>0x4ff</PointerOffsetHex>
    <JapaneseText>なるほどな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1293</PointerOffsetDec>
    <PointerOffsetHex>0x50d</PointerOffsetHex>
    <JapaneseText>確かにそんな二刀流、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1315</PointerOffsetDec>
    <PointerOffsetHex>0x523</PointerOffsetHex>
    <JapaneseText>これまで試した事なかったぜ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1344</PointerOffsetDec>
    <PointerOffsetHex>0x540</PointerOffsetHex>
    <JapaneseText>何がいいんだろうな……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1370</PointerOffsetDec>
    <PointerOffsetHex>0x55a</PointerOffsetHex>
    <JapaneseText>いきなり斧は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1386</PointerOffsetDec>
    <PointerOffsetHex>0x56a</PointerOffsetHex>
    <JapaneseText>片手で扱えそうにないし……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1413</PointerOffsetDec>
    <PointerOffsetHex>0x585</PointerOffsetHex>
    <JapaneseText>……特大ドリルとかか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1436</PointerOffsetDec>
    <PointerOffsetHex>0x59c</PointerOffsetHex>
    <JapaneseText>それ、いいかもしれないぞ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1464</PointerOffsetDec>
    <PointerOffsetHex>0x5b8</PointerOffsetHex>
    <JapaneseText>なんだか強そうだ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1483</PointerOffsetDec>
    <PointerOffsetHex>0x5cb</PointerOffsetHex>
    <JapaneseText>だよな！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1493</PointerOffsetDec>
    <PointerOffsetHex>0x5d5</PointerOffsetHex>
    <JapaneseText>となると、もう一つは……何がいいかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1532</PointerOffsetDec>
    <PointerOffsetHex>0x5fc</PointerOffsetHex>
    <JapaneseText>特大ドリルは重そうだから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1560</PointerOffsetDec>
    <PointerOffsetHex>0x618</PointerOffsetHex>
    <JapaneseText>なるべく軽い武器の方がいいんじゃないか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1601</PointerOffsetDec>
    <PointerOffsetHex>0x641</PointerOffsetHex>
    <JapaneseText>ストローなんかどうかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1627</PointerOffsetDec>
    <PointerOffsetHex>0x65b</PointerOffsetHex>
    <JapaneseText>ほら、ノーマみたいな感じでさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1656</PointerOffsetDec>
    <PointerOffsetHex>0x678</PointerOffsetHex>
    <JapaneseText>なるほどな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1670</PointerOffsetDec>
    <PointerOffsetHex>0x686</PointerOffsetHex>
    <JapaneseText>それなら、ストローを口にくわえている間</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1710</PointerOffsetDec>
    <PointerOffsetHex>0x6ae</PointerOffsetHex>
    <JapaneseText>特大ドリルを両手でも使えるぞ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1741</PointerOffsetDec>
    <PointerOffsetHex>0x6cd</PointerOffsetHex>
    <JapaneseText>もしかしてこれ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1759</PointerOffsetDec>
    <PointerOffsetHex>0x6df</PointerOffsetHex>
    <JapaneseText>結構いいアイデアなんじゃないか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1792</PointerOffsetDec>
    <PointerOffsetHex>0x700</PointerOffsetHex>
    <JapaneseText>ああ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1800</PointerOffsetDec>
    <PointerOffsetHex>0x708</PointerOffsetHex>
    <JapaneseText>試してみる価値はありそうだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1827</PointerOffsetDec>
    <PointerOffsetHex>0x723</PointerOffsetHex>
    <JapaneseText>まずは特大ドリルを調達しようぜ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1861</PointerOffsetDec>
    <PointerOffsetHex>0x745</PointerOffsetHex>
    <JapaneseText>ハロルドに言えば、作ってくれるかもしれない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
</Entries>