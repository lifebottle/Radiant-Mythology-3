<Entries>
  <Entry>
    <PointerOffsetDec>54</PointerOffsetDec>
    <PointerOffsetHex>0x36</PointerOffsetHex>
    <JapaneseText>奩</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>2</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>908</PointerOffsetDec>
    <PointerOffsetHex>0x38c</PointerOffsetHex>
    <JapaneseText>アンジュ様、お手紙が届いてますよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>941</PointerOffsetDec>
    <PointerOffsetHex>0x3ad</PointerOffsetHex>
    <JapaneseText>ありがとう、ロックス</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>962</PointerOffsetDec>
    <PointerOffsetHex>0x3c2</PointerOffsetHex>
    <JapaneseText>このハンコは、オルタ・ビレッジからね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>999</PointerOffsetDec>
    <PointerOffsetHex>0x3e7</PointerOffsetHex>
    <JapaneseText>ヘーゼル村の皆さんや、ここにいる皆さんの</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1041</PointerOffsetDec>
    <PointerOffsetHex>0x411</PointerOffsetHex>
    <JapaneseText>故郷の方々も集まって、もうそろそろ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1077</PointerOffsetDec>
    <PointerOffsetHex>0x435</PointerOffsetHex>
    <JapaneseText>一つの村らしくなっているんじゃないですか？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1120</PointerOffsetDec>
    <PointerOffsetHex>0x460</PointerOffsetHex>
    <JapaneseText>まぁ……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1129</PointerOffsetDec>
    <PointerOffsetHex>0x469</PointerOffsetHex>
    <JapaneseText>どうかしました？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1147</PointerOffsetDec>
    <PointerOffsetHex>0x47b</PointerOffsetHex>
    <JapaneseText>何か、良くない事でも…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1170</PointerOffsetDec>
    <PointerOffsetHex>0x492</PointerOffsetHex>
    <JapaneseText>色んな所から人々が集まっているせいか、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1210</PointerOffsetDec>
    <PointerOffsetHex>0x4ba</PointerOffsetHex>
    <JapaneseText>文化や価値観の違いで</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1232</PointerOffsetDec>
    <PointerOffsetHex>0x4d0</PointerOffsetHex>
    <JapaneseText>衝突が起きているみたい</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1255</PointerOffsetDec>
    <PointerOffsetHex>0x4e7</PointerOffsetHex>
    <JapaneseText>それぞれのやり方を、みんな</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1283</PointerOffsetDec>
    <PointerOffsetHex>0x503</PointerOffsetHex>
    <JapaneseText>譲ろうとしないらしくて…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1308</PointerOffsetDec>
    <PointerOffsetHex>0x51c</PointerOffsetHex>
    <JapaneseText>やっぱり…、難しいものね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1333</PointerOffsetDec>
    <PointerOffsetHex>0x535</PointerOffsetHex>
    <JapaneseText>けれども…、皆さん一人ひとりは</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1365</PointerOffsetDec>
    <PointerOffsetHex>0x555</PointerOffsetHex>
    <JapaneseText>いい人なんですから</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1384</PointerOffsetDec>
    <PointerOffsetHex>0x568</PointerOffsetHex>
    <JapaneseText>それぞれ、よかれと思って</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1410</PointerOffsetDec>
    <PointerOffsetHex>0x582</PointerOffsetHex>
    <JapaneseText>自分達のやり方を提案して</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1436</PointerOffsetDec>
    <PointerOffsetHex>0x59c</PointerOffsetHex>
    <JapaneseText>くれてるんですよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1453</PointerOffsetDec>
    <PointerOffsetHex>0x5ad</PointerOffsetHex>
    <JapaneseText>でも、どうすればいいの。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1479</PointerOffsetDec>
    <PointerOffsetHex>0x5c7</PointerOffsetHex>
    <JapaneseText>みんなが一つにまとまる様にするには…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1516</PointerOffsetDec>
    <PointerOffsetHex>0x5ec</PointerOffsetHex>
    <JapaneseText>そうですね…。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1532</PointerOffsetDec>
    <PointerOffsetHex>0x5fc</PointerOffsetHex>
    <JapaneseText>種族や文化の違いは、押し付けるのではなく</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1574</PointerOffsetDec>
    <PointerOffsetHex>0x626</PointerOffsetHex>
    <JapaneseText>分かち合うべきだと思うんです</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1603</PointerOffsetDec>
    <PointerOffsetHex>0x643</PointerOffsetHex>
    <JapaneseText>そうする事で、「お互いが自らの価値観を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1643</PointerOffsetDec>
    <PointerOffsetHex>0x66b</PointerOffsetHex>
    <JapaneseText>変える事が出来ると認識」出来るんじゃ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1681</PointerOffsetDec>
    <PointerOffsetHex>0x691</PointerOffsetHex>
    <JapaneseText>ないでしょうか</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1696</PointerOffsetDec>
    <PointerOffsetHex>0x6a0</PointerOffsetHex>
    <JapaneseText>時間が必要かもしれませんが、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1726</PointerOffsetDec>
    <PointerOffsetHex>0x6be</PointerOffsetHex>
    <JapaneseText>この、分かち合ってみると言う事を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1760</PointerOffsetDec>
    <PointerOffsetHex>0x6e0</PointerOffsetHex>
    <JapaneseText>「提案」してみてはどうでしょう</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1791</PointerOffsetDec>
    <PointerOffsetHex>0x6ff</PointerOffsetHex>
    <JapaneseText>ええ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1799</PointerOffsetDec>
    <PointerOffsetHex>0x707</PointerOffsetHex>
    <JapaneseText>やってみるわ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1812</PointerOffsetDec>
    <PointerOffsetHex>0x714</PointerOffsetHex>
    <JapaneseText>ただ、ノーと言うんじゃなくて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1844</PointerOffsetDec>
    <PointerOffsetHex>0x734</PointerOffsetHex>
    <JapaneseText>こっちの方もどう、こんなのもあると</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1880</PointerOffsetDec>
    <PointerOffsetHex>0x758</PointerOffsetHex>
    <JapaneseText>選択肢を増やして、肯定の力で導くというか…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1923</PointerOffsetDec>
    <PointerOffsetHex>0x783</PointerOffsetHex>
    <JapaneseText>そうね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1930</PointerOffsetDec>
    <PointerOffsetHex>0x78a</PointerOffsetHex>
    <JapaneseText>自分自身の事をダメだと思ってても、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1966</PointerOffsetDec>
    <PointerOffsetHex>0x7ae</PointerOffsetHex>
    <JapaneseText>人って色々やってるうちに「自分にこんな</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2006</PointerOffsetDec>
    <PointerOffsetHex>0x7d6</PointerOffsetHex>
    <JapaneseText>力があったんだ」と気付けるものね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2039</PointerOffsetDec>
    <PointerOffsetHex>0x7f7</PointerOffsetHex>
    <JapaneseText>その…、アンジュ様。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2061</PointerOffsetDec>
    <PointerOffsetHex>0x80d</PointerOffsetHex>
    <JapaneseText>一人で抱え込まないで下さい。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2091</PointerOffsetDec>
    <PointerOffsetHex>0x82b</PointerOffsetHex>
    <JapaneseText>僕に出来る事なんて、たかが知れてますけど…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2134</PointerOffsetDec>
    <PointerOffsetHex>0x856</PointerOffsetHex>
    <JapaneseText>あなたも、もっと自信を持ちなさい。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2170</PointerOffsetDec>
    <PointerOffsetHex>0x87a</PointerOffsetHex>
    <JapaneseText>あなたがわたしの教会に来てくれなかったら、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2214</PointerOffsetDec>
    <PointerOffsetHex>0x8a6</PointerOffsetHex>
    <JapaneseText>きっとアドリビトムは無かったよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2245</PointerOffsetDec>
    <PointerOffsetHex>0x8c5</PointerOffsetHex>
    <JapaneseText>あなたが提案してくれて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2271</PointerOffsetDec>
    <PointerOffsetHex>0x8df</PointerOffsetHex>
    <JapaneseText>色々奔走してくれたから</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2295</PointerOffsetDec>
    <PointerOffsetHex>0x8f7</PointerOffsetHex>
    <JapaneseText>アドリビトムが誕生したんだし</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2324</PointerOffsetDec>
    <PointerOffsetHex>0x914</PointerOffsetHex>
    <JapaneseText>あ、いや、それは…。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2346</PointerOffsetDec>
    <PointerOffsetHex>0x92a</PointerOffsetHex>
    <JapaneseText>提案した以上は、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2364</PointerOffsetDec>
    <PointerOffsetHex>0x93c</PointerOffsetHex>
    <JapaneseText>僕がやらなくてはダメかなと思って…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2399</PointerOffsetDec>
    <PointerOffsetHex>0x95f</PointerOffsetHex>
    <JapaneseText>もう、あなたに全部任せて</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2425</PointerOffsetDec>
    <PointerOffsetHex>0x979</PointerOffsetHex>
    <JapaneseText>わたしは隠居してもいいかなー</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2454</PointerOffsetDec>
    <PointerOffsetHex>0x996</PointerOffsetHex>
    <JapaneseText>そ、それは困りますってば！！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2483</PointerOffsetDec>
    <PointerOffsetHex>0x9b3</PointerOffsetHex>
    <JapaneseText>おだてには乗らないかぁ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2509</PointerOffsetDec>
    <PointerOffsetHex>0x9cd</PointerOffsetHex>
    <JapaneseText>残念だなあ…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2522</PointerOffsetDec>
    <PointerOffsetHex>0x9da</PointerOffsetHex>
    <JapaneseText>（うう、また丸め込まれる所だった…）</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>123</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2559</PointerOffsetDec>
    <PointerOffsetHex>0x9ff</PointerOffsetHex>
    <JapaneseText>（家事だけって約束のはずが、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>124</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2589</PointerOffsetDec>
    <PointerOffsetHex>0xa1d</PointerOffsetHex>
    <JapaneseText>　ほとんど任されてきたもんなぁ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>125</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2623</PointerOffsetDec>
    <PointerOffsetHex>0xa3f</PointerOffsetHex>
    <JapaneseText>　トホホ…）</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>126</Id>
    <Status />
  </Entry>
