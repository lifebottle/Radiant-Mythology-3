    <EnglishText />
    <Notes />
    <Id>5</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>199</PointerOffsetDec>
    <PointerOffsetHex>0xc7</PointerOffsetHex>
