<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>224</PointerOffsetDec>
    <PointerOffsetHex>0xe0</PointerOffsetHex>
    <JapaneseText>私から伺おうと思っていましたのに、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>17</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>260</PointerOffsetDec>
    <PointerOffsetHex>0x104</PointerOffsetHex>
    <JapaneseText>ご丁寧にありがとうございます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>18</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>289</PointerOffsetDec>
    <PointerOffsetHex>0x121</PointerOffsetHex>
    <JapaneseText>私は、フィリア・フィリスと申します。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>19</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>327</PointerOffsetDec>
    <PointerOffsetHex>0x147</PointerOffsetHex>
    <JapaneseText>よろしくお願いします</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>20</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>348</PointerOffsetDec>
    <PointerOffsetHex>0x15c</PointerOffsetHex>
    <JapaneseText>私とアンジュさんは、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>21</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>370</PointerOffsetDec>
    <PointerOffsetHex>0x172</PointerOffsetHex>
    <JapaneseText>教会に勤めていた時からの知り合いなのです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>22</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>411</PointerOffsetDec>
    <PointerOffsetHex>0x19b</PointerOffsetHex>
    <JapaneseText>出身国は違いますけれど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>23</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>437</PointerOffsetDec>
    <PointerOffsetHex>0x1b5</PointerOffsetHex>
    <JapaneseText>私達は教会出身者なんですよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>24</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>464</PointerOffsetDec>
    <PointerOffsetHex>0x1d0</PointerOffsetHex>
    <JapaneseText>それで、アンジュさんが多くの人々の</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>25</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>500</PointerOffsetDec>
    <PointerOffsetHex>0x1f4</PointerOffsetHex>
    <JapaneseText>力になれるギルドを立ち上げたと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>26</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>532</PointerOffsetDec>
    <PointerOffsetHex>0x214</PointerOffsetHex>
    <JapaneseText>便りをくれて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>27</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>545</PointerOffsetDec>
    <PointerOffsetHex>0x221</PointerOffsetHex>
    <JapaneseText>私も、ぜひ協力したくて来たんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>28</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>578</PointerOffsetDec>
    <PointerOffsetHex>0x242</PointerOffsetHex>
    <JapaneseText>では、今後ともよろしくお願い致します</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
</Entries>