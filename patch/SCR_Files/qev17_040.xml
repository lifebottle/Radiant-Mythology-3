<Entries>
  <Entry>
    <PointerOffsetDec>416</PointerOffsetDec>
    <PointerOffsetHex>0x1a0</PointerOffsetHex>
    <JapaneseText>「試練にいどむ者、五つの問いに答えよ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>456</PointerOffsetDec>
    <PointerOffsetHex>0x1c8</PointerOffsetHex>
    <JapaneseText>　問いに惑い、答え乱れるが最も悪し」</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>30</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>493</PointerOffsetDec>
    <PointerOffsetHex>0x1ed</PointerOffsetHex>
    <JapaneseText>「人として生きるか、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>31</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>515</PointerOffsetDec>
    <PointerOffsetHex>0x203</PointerOffsetHex>
    <JapaneseText>　忍びとして生きるか、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>32</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>539</PointerOffsetDec>
    <PointerOffsetHex>0x21b</PointerOffsetHex>
    <JapaneseText>　心して進むがよい」</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>33</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>571</PointerOffsetDec>
    <PointerOffsetHex>0x23b</PointerOffsetHex>
    <JapaneseText>……これですか？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>35</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>589</PointerOffsetDec>
    <PointerOffsetHex>0x24d</PointerOffsetHex>
    <JapaneseText>父上が、私に残してくれた言葉です</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>622</PointerOffsetDec>
    <PointerOffsetHex>0x26e</PointerOffsetHex>
    <JapaneseText>忍者の何たるかを、まだ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>37</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>646</PointerOffsetDec>
    <PointerOffsetHex>0x286</PointerOffsetHex>
    <JapaneseText>心得る事の出来ていない私に</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>38</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>674</PointerOffsetDec>
    <PointerOffsetHex>0x2a2</PointerOffsetHex>
    <JapaneseText>訓示として残してくれたものです</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>39</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>705</PointerOffsetDec>
    <PointerOffsetHex>0x2c1</PointerOffsetHex>
    <JapaneseText>「怒り……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>717</PointerOffsetDec>
    <PointerOffsetHex>0x2cd</PointerOffsetHex>
    <JapaneseText>　それは、冷静な判断を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>741</PointerOffsetDec>
    <PointerOffsetHex>0x2e5</PointerOffsetHex>
    <JapaneseText>　失わせるものと知れ」</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>764</PointerOffsetDec>
    <PointerOffsetHex>0x2fc</PointerOffsetHex>
    <JapaneseText>「悲しみ……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>778</PointerOffsetDec>
    <PointerOffsetHex>0x30a</PointerOffsetHex>
    <JapaneseText>　それは、力を失う元と知れ」</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>807</PointerOffsetDec>
    <PointerOffsetHex>0x327</PointerOffsetHex>
    <JapaneseText>「喜び……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>819</PointerOffsetDec>
    <PointerOffsetHex>0x333</PointerOffsetHex>
    <JapaneseText>　それは、他人にねたまれる元と知れ」</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>865</PointerOffsetDec>
    <PointerOffsetHex>0x361</PointerOffsetHex>
    <JapaneseText>あなたには、もう会得出来てしまって</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>901</PointerOffsetDec>
    <PointerOffsetHex>0x385</PointerOffsetHex>
    <JapaneseText>いる事かもしれませんね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>924</PointerOffsetDec>
    <PointerOffsetHex>0x39c</PointerOffsetHex>
    <JapaneseText>でも、私は……</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>939</PointerOffsetDec>
    <PointerOffsetHex>0x3ab</PointerOffsetHex>
    <JapaneseText>……いいえ、弱気になっている場合では</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>977</PointerOffsetDec>
    <PointerOffsetHex>0x3d1</PointerOffsetHex>
    <JapaneseText>ありませんでしたね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>996</PointerOffsetDec>
    <PointerOffsetHex>0x3e4</PointerOffsetHex>
