<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>578</PointerOffsetDec>
    <PointerOffsetHex>0x242</PointerOffsetHex>
    <JapaneseText>カイル。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>588</PointerOffsetDec>
    <PointerOffsetHex>0x24c</PointerOffsetHex>
    <JapaneseText>あんた、あたし達の世界には慣れたかい？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>627</PointerOffsetDec>
    <PointerOffsetHex>0x273</PointerOffsetHex>
    <JapaneseText>うん！</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>635</PointerOffsetDec>
    <PointerOffsetHex>0x27b</PointerOffsetHex>
    <JapaneseText>見た事無い術とかあるし、面白いよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>668</PointerOffsetDec>
    <PointerOffsetHex>0x29c</PointerOffsetHex>
    <JapaneseText>全く、本当に面白いよ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>692</PointerOffsetDec>
    <PointerOffsetHex>0x2b4</PointerOffsetHex>
    <JapaneseText>ヴェラトローパの話は聞いたかい？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>725</PointerOffsetDec>
    <PointerOffsetHex>0x2d5</PointerOffsetHex>
    <JapaneseText>あたしの故郷に伝わる光気丹術は、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>759</PointerOffsetDec>
    <PointerOffsetHex>0x2f7</PointerOffsetHex>
    <JapaneseText>ソウルアルケミーの一端だったって</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>793</PointerOffsetDec>
    <PointerOffsetHex>0x319</PointerOffsetHex>
    <JapaneseText>言ってたろ？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>806</PointerOffsetDec>
    <PointerOffsetHex>0x326</PointerOffsetHex>
    <JapaneseText>その術は、遥か昔からあった</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>834</PointerOffsetDec>
    <PointerOffsetHex>0x342</PointerOffsetHex>
    <JapaneseText>ヴェラトローパの民が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>856</PointerOffsetDec>
    <PointerOffsetHex>0x358</PointerOffsetHex>
    <JapaneseText>使用してたってんだよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>877</PointerOffsetDec>
    <PointerOffsetHex>0x36d</PointerOffsetHex>
    <JapaneseText>すごいと思わないかい？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>901</PointerOffsetDec>
    <PointerOffsetHex>0x385</PointerOffsetHex>
    <JapaneseText>そんなに長い間伝わってきたなんてさ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>936</PointerOffsetDec>
    <PointerOffsetHex>0x3a8</PointerOffsetHex>
    <JapaneseText>何でも創り出せる力だったんでしょ？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>972</PointerOffsetDec>
    <PointerOffsetHex>0x3cc</PointerOffsetHex>
    <JapaneseText>いつの頃から、ヒトはその力を失ったんだろう</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1015</PointerOffsetDec>
    <PointerOffsetHex>0x3f7</PointerOffsetHex>
    <JapaneseText>さあね。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1025</PointerOffsetDec>
    <PointerOffsetHex>0x401</PointerOffsetHex>
    <JapaneseText>だけど、ほんの一部は残ってるじゃないか。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1067</PointerOffsetDec>
    <PointerOffsetHex>0x42b</PointerOffsetHex>
    <JapaneseText>あたしらが知ってる魔術って形でさ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1100</PointerOffsetDec>
    <PointerOffsetHex>0x44c</PointerOffsetHex>
    <JapaneseText>え？　魔術もソウルアルケミー？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1131</PointerOffsetDec>
    <PointerOffsetHex>0x46b</PointerOffsetHex>
    <JapaneseText>そう。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1139</PointerOffsetDec>
    <PointerOffsetHex>0x473</PointerOffsetHex>
    <JapaneseText>ドクメントの創造なんて事は、普通の人には</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1181</PointerOffsetDec>
    <PointerOffsetHex>0x49d</PointerOffsetHex>
    <JapaneseText>おいそれと出来やしないけど</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1208</PointerOffsetDec>
    <PointerOffsetHex>0x4b8</PointerOffsetHex>
    <JapaneseText>まあ、ちょっとだけ使えてるってのが</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1244</PointerOffsetDec>
    <PointerOffsetHex>0x4dc</PointerOffsetHex>
    <JapaneseText>今あたし達の間で知られてる魔術だね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1279</PointerOffsetDec>
    <PointerOffsetHex>0x4ff</PointerOffsetHex>
    <JapaneseText>魔術が使えるってだけでも</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1305</PointerOffsetDec>
    <PointerOffsetHex>0x519</PointerOffsetHex>
    <JapaneseText>すごいと思うけどなぁ…</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1328</PointerOffsetDec>
    <PointerOffsetHex>0x530</PointerOffsetHex>
    <JapaneseText>真のソウルアルケミーの使い手が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1360</PointerOffsetDec>
    <PointerOffsetHex>0x550</PointerOffsetHex>
    <JapaneseText>いるとしたら…、魔術を使うにしても</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1396</PointerOffsetDec>
    <PointerOffsetHex>0x574</PointerOffsetHex>
    <JapaneseText>詠唱も道具も要らないだろうって言われてるよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1439</PointerOffsetDec>
    <PointerOffsetHex>0x59f</PointerOffsetHex>
    <JapaneseText>願えば何でも現実になるんだから</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1470</PointerOffsetDec>
    <PointerOffsetHex>0x5be</PointerOffsetHex>
    <JapaneseText>ふぅん、やっぱ神様みたいだなぁ。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1504</PointerOffsetDec>
    <PointerOffsetHex>0x5e0</PointerOffsetHex>
    <JapaneseText>オレ達ヒトには出来ない事だよね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1535</PointerOffsetDec>
    <PointerOffsetHex>0x5ff</PointerOffsetHex>
    <JapaneseText>何でも出来ちゃあ面白くないし、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1567</PointerOffsetDec>
    <PointerOffsetHex>0x61f</PointerOffsetHex>
    <JapaneseText>それで生きてるって実感が</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1593</PointerOffsetDec>
    <PointerOffsetHex>0x639</PointerOffsetHex>
    <JapaneseText>沸くもんかねえ？</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1610</PointerOffsetDec>
    <PointerOffsetHex>0x64a</PointerOffsetHex>
    <JapaneseText>ヴェラトローパの民…ヒトの祖は、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1644</PointerOffsetDec>
    <PointerOffsetHex>0x66c</PointerOffsetHex>
    <JapaneseText>苦難に満ちたものであっても、その力を捨てて、</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1690</PointerOffsetDec>
    <PointerOffsetHex>0x69a</PointerOffsetHex>
    <JapaneseText>生きる事を学ぶと決めたとかって話だし</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1727</PointerOffsetDec>
    <PointerOffsetHex>0x6bf</PointerOffsetHex>
    <JapaneseText>そっか。</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1737</PointerOffsetDec>
    <PointerOffsetHex>0x6c9</PointerOffsetHex>
    <JapaneseText>それで今、オレ達は生きてる事の喜びや辛さを</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1781</PointerOffsetDec>
    <PointerOffsetHex>0x6f5</PointerOffsetHex>
    <JapaneseText>知る事が出来てるんだね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1804</PointerOffsetDec>
    <PointerOffsetHex>0x70c</PointerOffsetHex>
    <JapaneseText>あたしは、その力を捨てた</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1830</PointerOffsetDec>
    <PointerOffsetHex>0x726</PointerOffsetHex>
    <JapaneseText>ヒトの祖の子孫である事を</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1856</PointerOffsetDec>
    <PointerOffsetHex>0x740</PointerOffsetHex>
    <JapaneseText>誇りに思うよ</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1869</PointerOffsetDec>
    <PointerOffsetHex>0x74d</PointerOffsetHex>
    <JapaneseText>辛かった日々も、楽しかった日々も</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1903</PointerOffsetDec>
    <PointerOffsetHex>0x76f</PointerOffsetHex>
    <JapaneseText>あたしには宝物だからね</JapaneseText>
    <EnglishText />
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
</Entries>