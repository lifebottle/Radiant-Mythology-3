<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>36</PointerOffset>
      <JapaneseText>辨錮</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>52</PointerOffset>
      <JapaneseText>応</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>774</PointerOffset>
      <JapaneseText>待ってたわよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>790</PointerOffset>
      <JapaneseText>それじゃあ、早速説明を始めるわね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>823</PointerOffset>
      <JapaneseText>コクヨウ玉虫のドクメントを調べたんだけど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>867</PointerOffset>
      <JapaneseText>やっぱりこの虫本来のドクメントが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>901</PointerOffset>
      <JapaneseText>侵食されてるみたいなの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>924</PointerOffset>
      <JapaneseText>全く違う生物にドクメントが書き換えられてる。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>970</PointerOffset>
      <JapaneseText>ううん…、生物という仕組みじゃないわね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1009</PointerOffset>
      <JapaneseText>あたし達の世界には、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1031</PointerOffset>
      <JapaneseText>存在し得ないドクメントなのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1060</PointerOffset>
      <JapaneseText>そういえば、前に助けた二人のドクメントも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1102</PointerOffset>
      <JapaneseText>確かめてきたんだけど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1126</PointerOffset>
      <JapaneseText>ちゃんとヒトのドクメントに戻ってたわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1163</PointerOffset>
      <JapaneseText>あの、赤い煙…だった存在は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1193</PointerOffset>
      <JapaneseText>…あたし達の世界にはない「異質なドクメント」</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1239</PointerOffset>
      <JapaneseText>なのかもしれない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1256</PointerOffset>
      <JapaneseText>でも、どこからどうやって現れたかは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1292</PointerOffset>
      <JapaneseText>わからないし、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1308</PointerOffset>
      <JapaneseText>目的が何かもわからないわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1333</PointerOffset>
      <JapaneseText>目的がねえのが、いちばんタチ悪いんだよ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1374</PointerOffset>
      <JapaneseText>「赤い煙」が他の生物に接触する事で、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1412</PointerOffset>
      <JapaneseText>その生物のドクメントが変化…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1444</PointerOffset>
      <JapaneseText>そして、「赤い煙」自身の変容…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1475</PointerOffset>
      <JapaneseText>例えば、だけどさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1492</PointerOffset>
      <JapaneseText>今までの出来事から「願いを叶えて欲しい」</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1534</PointerOffset>
      <JapaneseText>という強い想いに「赤い煙」は反応している</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1576</PointerOffset>
      <JapaneseText>らしいじゃないか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1593</PointerOffset>
      <JapaneseText>その「赤い煙」って奴は意志を持ってて…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1634</PointerOffset>
      <JapaneseText>相手の「想い」に接触する時に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1666</PointerOffset>
      <JapaneseText>相手の「ドクメント」を覗き込む事で学習、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1708</PointerOffset>
      <JapaneseText>進化してる…としたらどうだい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1739</PointerOffset>
      <JapaneseText>人はそうかもしれねえ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1763</PointerOffset>
      <JapaneseText>でも、虫や植物の変化も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1787</PointerOffset>
      <JapaneseText>起きてるじゃねえか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1806</PointerOffset>
      <JapaneseText>そいつらにも意志や、願いがあるってのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1847</PointerOffset>
      <JapaneseText>ああ、あるよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1863</PointerOffset>
      <JapaneseText>「生存欲」さ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1876</PointerOffset>
      <JapaneseText>生物が瀕死に陥った時、もしくは絶体絶命の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1918</PointerOffset>
      <JapaneseText>危機が迫った時の意識は絶大だからね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1953</PointerOffset>
      <JapaneseText>うーん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1963</PointerOffset>
      <JapaneseText>わかる様な、わかんねえ様な</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1990</PointerOffset>
      <JapaneseText>そうだねえ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2004</PointerOffset>
      <JapaneseText>じゃ、もっと具体的な例を見せようか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>