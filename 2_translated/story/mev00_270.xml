<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>70</PointerOffset>
    <JapaneseText>奩</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>3</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>808</PointerOffset>
    <JapaneseText>ん？　なあに？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>824</PointerOffset>
    <JapaneseText>聞きたい事があるって？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>847</PointerOffset>
    <JapaneseText>ミントさんが言ってた話？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>872</PointerOffset>
    <JapaneseText>えっと、今の世界の状況についてだね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>907</PointerOffset>
    <JapaneseText>世界の国々では、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>925</PointerOffset>
    <JapaneseText>星晶（ホスチア）を巡って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>951</PointerOffset>
    <JapaneseText>国同士の争いが起きてるの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>976</PointerOffset>
    <JapaneseText>星晶について詳しく尋ねる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1001</PointerOffset>
    <JapaneseText>首をかしげる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1014</PointerOffset>
    <JapaneseText>ひょっとして、○○は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1038</PointerOffset>
    <JapaneseText>この世界についての事まで</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1064</PointerOffset>
    <JapaneseText>記憶を失ってしまったのかな…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1093</PointerOffset>
    <JapaneseText>もしかして、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1107</PointerOffset>
    <JapaneseText>星晶の事も忘れてしまったのかな…？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1142</PointerOffset>
    <JapaneseText>えっと、星晶というのはね、世界中で色んな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1184</PointerOffset>
    <JapaneseText>ものに使われる、現代のエネルギー資源だよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1225</PointerOffset>
    <JapaneseText>マナは知ってる？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1243</PointerOffset>
    <JapaneseText>世界樹が生み出す生命の源</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1268</PointerOffset>
    <JapaneseText>マナは、私達生命の源とも言える力なんだけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1314</PointerOffset>
    <JapaneseText>捉えどころの無い非物質のものだから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1352</PointerOffset>
    <JapaneseText>工業的なエネルギー源としては使えないの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1391</PointerOffset>
    <JapaneseText>でも、星晶は違うの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1413</PointerOffset>
    <JapaneseText>物質で、地下から採掘が出来て、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1445</PointerOffset>
    <JapaneseText>エネルギーとして活用消費が出来るのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1482</PointerOffset>
    <JapaneseText>星晶も、マナと同じ性質を持っているの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1522</PointerOffset>
    <JapaneseText>世界樹が、長い年月をかけて、マナを結晶化</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1564</PointerOffset>
    <JapaneseText>させたものじゃないかって言われてる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1599</PointerOffset>
    <JapaneseText>本当の所は、まだわからないらしいけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1636</PointerOffset>
    <JapaneseText>で、さっきも言ったけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1660</PointerOffset>
    <JapaneseText>今はその星晶を巡って国同士の</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1690</PointerOffset>
    <JapaneseText>争いが起きてる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1705</PointerOffset>
    <JapaneseText>星晶があるとされる小さな国や村は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1741</PointerOffset>
    <JapaneseText>力のある大国にそれらを搾取されるがまま…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1782</PointerOffset>
    <JapaneseText>星晶には、土地に恵みを与えるマナを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1818</PointerOffset>
    <JapaneseText>出す力があるの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1833</PointerOffset>
    <JapaneseText>採り尽くされてしまったら、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1861</PointerOffset>
    <JapaneseText>土地はみるみる荒れていくわ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1891</PointerOffset>
    <JapaneseText>作物なんか、育たなくなってしまうのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1928</PointerOffset>
    <JapaneseText>星晶が世界の主要エネルギーに変わってから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1972</PointerOffset>
    <JapaneseText>文明は急成長したと言うわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1997</PointerOffset>
    <JapaneseText>でも、多くの国は星晶の採掘権を巡って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2035</PointerOffset>
    <JapaneseText>他国との関係は悪化の道をたどるばかり</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2072</PointerOffset>
    <JapaneseText>それだけじゃないの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2091</PointerOffset>
    <JapaneseText>星晶を得て大量消費国となった国は、星晶の</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2133</PointerOffset>
    <JapaneseText>ない他の国や村を植民地化したり、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2167</PointerOffset>
    <JapaneseText>労働を強いたりするんだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2192</PointerOffset>
    <JapaneseText>富める国と貧しい国の差は開く一方なの…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2231</PointerOffset>
    <JapaneseText>アドリビトムは、これらによって貧富の差が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2273</PointerOffset>
    <JapaneseText>拡大した、貧しい土地の出身者ばかりなの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2312</PointerOffset>
    <JapaneseText>アンジュさんは、みんなの故郷を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2344</PointerOffset>
    <JapaneseText>危機的状況から救う為に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2370</PointerOffset>
    <JapaneseText>アドリビトムを立ち上げたのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>108</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
