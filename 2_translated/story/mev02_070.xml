<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>952</PointerOffset>
      <JapaneseText>星晶が採掘されて、あの森周辺の村々に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>990</PointerOffset>
      <JapaneseText>マナの恵みはなくなった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1013</PointerOffset>
      <JapaneseText>あそこらは、マナを生み出す世界樹の根も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1053</PointerOffset>
      <JapaneseText>張ってねぇし、星晶がわずかに出すマナだけが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1097</PointerOffset>
      <JapaneseText>ヘーゼル村に恵みを与えていたからな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1132</PointerOffset>
      <JapaneseText>ああいった生物の変化は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1158</PointerOffset>
      <JapaneseText>星晶採掘が終盤に差し掛かった頃から</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1194</PointerOffset>
      <JapaneseText>始まった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1203</PointerOffset>
      <JapaneseText>まず、土地が痩せ、畑からは作物が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1237</PointerOffset>
      <JapaneseText>取れなくなっていった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1258</PointerOffset>
      <JapaneseText>あの様な場所は他にも？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1281</PointerOffset>
      <JapaneseText>ああ、村近辺の星晶は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1303</PointerOffset>
      <JapaneseText>ゴッソリ盗っていかれたからな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1332</PointerOffset>
      <JapaneseText>それから森の生物に奇妙な変化が現れてよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1371</PointerOffset>
      <JapaneseText>具体的には、どの様な…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1396</PointerOffset>
      <JapaneseText>ほとんどの作物は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1416</PointerOffset>
      <JapaneseText>食えないものになっちまって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1443</PointerOffset>
      <JapaneseText>狩る動物も同じだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1463</PointerOffset>
      <JapaneseText>何て言やぁいい…、肉がねえんだよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1499</PointerOffset>
      <JapaneseText>生き物って感じじゃねえ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1522</PointerOffset>
      <JapaneseText>仕留めた先から溶けていくものもあった。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1562</PointerOffset>
      <JapaneseText>ちゃんとした獲物が取れた時は奇跡さ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1597</PointerOffset>
      <JapaneseText>元々星晶もなく、マナを生み出す世界樹の根も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1641</PointerOffset>
      <JapaneseText>ない土地というのはあります</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1668</PointerOffset>
      <JapaneseText>でも、その様な土地で、マナが少ないから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1708</PointerOffset>
      <JapaneseText>生き物に変化が現れたという現象は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1742</PointerOffset>
      <JapaneseText>起こっていません</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1759</PointerOffset>
      <JapaneseText>マナが少ない以外に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1781</PointerOffset>
      <JapaneseText>原因があるのかもしれないね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1808</PointerOffset>
      <JapaneseText>原因が判明しない事にはな…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1838</PointerOffset>
      <JapaneseText>あの現象も拡大しつつある</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1863</PointerOffset>
      <JapaneseText>くそっ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1873</PointerOffset>
      <JapaneseText>…村のみんなは、どうやって生活していきゃあ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1917</PointerOffset>
      <JapaneseText>いいんだよっ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1932</PointerOffset>
      <JapaneseText>サレはいなくなったけど、もう村に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1966</PointerOffset>
      <JapaneseText>物資もねぇ、採取もままならねぇ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1997</PointerOffset>
      <JapaneseText>もっと遠くに行って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2017</PointerOffset>
      <JapaneseText>探してくるしかねえのか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2044</PointerOffset>
      <JapaneseText>じゃあ、ここで働かない？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2069</PointerOffset>
      <JapaneseText>そんな話を聞いたら、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2091</PointerOffset>
      <JapaneseText>ますます人手が必要になりそうだもの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2126</PointerOffset>
      <JapaneseText>そうか、それならばヴェイグ達が今まで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2164</PointerOffset>
      <JapaneseText>してくれていた様に、俺達もここで働き、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2204</PointerOffset>
      <JapaneseText>物資を送り届ける事で、村を助けて行けるな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2245</PointerOffset>
      <JapaneseText>よっしゃ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2259</PointerOffset>
      <JapaneseText>そうと決まれば、よろしくな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2288</PointerOffset>
      <JapaneseText>ヴェイグの倍働いてやるぜ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2317</PointerOffset>
      <JapaneseText>ユージーンがアドリビトムに加入しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2356</PointerOffset>
      <JapaneseText>ティトレイがアドリビトムに加入しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>