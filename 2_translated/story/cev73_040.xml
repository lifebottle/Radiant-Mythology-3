<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>404</PointerOffset>
      <JapaneseText>うう、まだ胃がムカムカする……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>438</PointerOffset>
      <JapaneseText>一体何が入ってたんだ、あの料理……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>473</PointerOffset>
      <JapaneseText>まぁ、あのフレンが「美味い」と</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>505</PointerOffset>
      <JapaneseText>評する程の珍味……もとい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>531</PointerOffset>
      <JapaneseText>劇物だったってのは、確かみたいだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>566</PointerOffset>
      <JapaneseText>なぁ、ユーリ……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>586</PointerOffset>
      <JapaneseText>フレン隊長って、もしかして……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>617</PointerOffset>
      <JapaneseText>ん？　</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>625</PointerOffset>
      <JapaneseText>知らなかったのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>645</PointerOffset>
      <JapaneseText>あいつの味覚は、一種の生物兵器だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>678</PointerOffset>
      <JapaneseText>レシピから外れた、オリジナルの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>710</PointerOffset>
      <JapaneseText>料理なんぞさせてみろ……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>738</PointerOffset>
      <JapaneseText>一瞬で全世界が血涙に染まるぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>767</PointerOffset>
      <JapaneseText>……そういう大事な話は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>791</PointerOffset>
      <JapaneseText>もっと早くして欲しかったよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>818</PointerOffset>
      <JapaneseText>フレンの部下なら当然、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>842</PointerOffset>
      <JapaneseText>知ってると思ったんだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>865</PointerOffset>
      <JapaneseText>ま、次から気を付けるこった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>892</PointerOffset>
      <JapaneseText>うう……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>