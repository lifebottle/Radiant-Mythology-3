<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>56</PointerOffset>
    <JapaneseText>筐</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>2</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1054</PointerOffset>
    <JapaneseText>また君か……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1067</PointerOffset>
    <JapaneseText>言ったはずだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1083</PointerOffset>
    <JapaneseText>私はまだ、国へ帰るつもりはないと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1125</PointerOffset>
    <JapaneseText>ギルド……護衛？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1142</PointerOffset>
    <JapaneseText>……どうやら、私は誤解をしていたらしい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1181</PointerOffset>
    <JapaneseText>すまないが、詳しく話を聞かせて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1213</PointerOffset>
    <JapaneseText>もらえないだろうか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1232</PointerOffset>
    <JapaneseText>なるほどな……、そういう事だったのか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1269</PointerOffset>
    <JapaneseText>これは、一時の猶予を与えられたと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1303</PointerOffset>
    <JapaneseText>受け取って構わないのかもしれないな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1345</PointerOffset>
    <JapaneseText>……失礼。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1357</PointerOffset>
    <JapaneseText>私ばかりが納得してしまったな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1395</PointerOffset>
    <JapaneseText>しかし、どうしたものか……私は一応、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1433</PointerOffset>
    <JapaneseText>身分を隠し、各地を遊説している身でな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1470</PointerOffset>
    <JapaneseText>公式に、ギルドの今回の申し出を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1502</PointerOffset>
    <JapaneseText>受ける事は出来ないのだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1527</PointerOffset>
    <JapaneseText>それでは、身分を隠している意味が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1561</PointerOffset>
    <JapaneseText>なくなってしまうからね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1593</PointerOffset>
    <JapaneseText>……一つ、私に考えがあるのだが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1624</PointerOffset>
    <JapaneseText>どうだろう？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1638</PointerOffset>
    <JapaneseText>私が、そのギルドへ身を寄せるというのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1686</PointerOffset>
    <JapaneseText>表面上はどうあれ、腕に覚えのある者達と</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1726</PointerOffset>
    <JapaneseText>寝食を共にするのだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1748</PointerOffset>
    <JapaneseText>国の者達も少しは安心出来よう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1777</PointerOffset>
    <JapaneseText>何より、ギルドには民からの生の声が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1813</PointerOffset>
    <JapaneseText>多く寄せられるはず</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1832</PointerOffset>
    <JapaneseText>私自身も、大いに学ばせてもらう事が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1868</PointerOffset>
    <JapaneseText>出来そうだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1882</PointerOffset>
    <JapaneseText>悪い案ではないと思うのだが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1918</PointerOffset>
    <JapaneseText>……そうだな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1934</PointerOffset>
    <JapaneseText>一度ギルドへ向かい、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1956</PointerOffset>
    <JapaneseText>代表者と話をするのが筋だろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>108</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1985</PointerOffset>
    <JapaneseText>私はウッドロウ・ケルヴィン。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>109</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2015</PointerOffset>
    <JapaneseText>君の名前は？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>110</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2037</PointerOffset>
    <JapaneseText>……そうか。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>112</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2051</PointerOffset>
    <JapaneseText>○○君というのだね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>113</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2070</PointerOffset>
    <JapaneseText>では、改めて案内、よろしく頼む</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>114</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
