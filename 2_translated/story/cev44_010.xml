  </Entry>
  <Entry>
    <PointerOffsetDec>330</PointerOffsetDec>
    <PointerOffsetHex>0x14a</PointerOffsetHex>
    <JapaneseText>我慢しないで言ってね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>24</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>351</PointerOffsetDec>
    <PointerOffsetHex>0x15f</PointerOffsetHex>
    <JapaneseText>カノンノさん……ありがとう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>25</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>381</PointerOffsetDec>
    <PointerOffsetHex>0x17d</PointerOffsetHex>
    <JapaneseText>でも、わたしは大丈夫</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>26</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>402</PointerOffsetDec>
    <PointerOffsetHex>0x192</PointerOffsetHex>
    <JapaneseText>決めたんです。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>27</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>418</PointerOffsetDec>
    <PointerOffsetHex>0x1a2</PointerOffsetHex>
    <JapaneseText>わたしは、わたしにやれる事は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>28</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>448</PointerOffsetDec>
    <PointerOffsetHex>0x1c0</PointerOffsetHex>
    <JapaneseText>出来るだけやっていこうって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>29</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>475</PointerOffsetDec>
    <PointerOffsetHex>0x1db</PointerOffsetHex>
    <JapaneseText>誰かが何かをしてくれるのを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>30</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>503</PointerOffsetDec>
    <PointerOffsetHex>0x1f7</PointerOffsetHex>
    <JapaneseText>待っているだけじゃダメなんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>31</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>534</PointerOffsetDec>
    <PointerOffsetHex>0x216</PointerOffsetHex>
    <JapaneseText>そうやって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>32</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>548</PointerOffsetDec>
    <PointerOffsetHex>0x224</PointerOffsetHex>
    <JapaneseText>後になって後悔するのも……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>33</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>575</PointerOffsetDec>
    <PointerOffsetHex>0x23f</PointerOffsetHex>
