    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>605</PointerOffsetDec>
    <PointerOffsetHex>0x25d</PointerOffsetHex>
    <JapaneseText>見ているだけなのも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>35</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>625</PointerOffsetDec>
    <PointerOffsetHex>0x271</PointerOffsetHex>
    <JapaneseText>わたしはもう、したくないから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>36</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>654</PointerOffsetDec>
    <PointerOffsetHex>0x28e</PointerOffsetHex>
