<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>672</PointerOffset>
    <JapaneseText>お前は、いつ王位を継ぐつもりなんだ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>709</PointerOffset>
    <JapaneseText>いつまでも、今の生活を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>733</PointerOffset>
    <JapaneseText>続けるわけじゃないんだろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>760</PointerOffset>
    <JapaneseText>そうだな……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>776</PointerOffset>
    <JapaneseText>大臣達からも、口うるさく</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>802</PointerOffset>
    <JapaneseText>言われてはいるのだが……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>827</PointerOffset>
    <JapaneseText>……まだ、気にしているのか。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>857</PointerOffset>
    <JapaneseText>父君の事を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>868</PointerOffset>
    <JapaneseText>……知っての通り、父は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>892</PointerOffset>
    <JapaneseText>名実共に「賢王」と名高い人物だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>923</PointerOffset>
    <JapaneseText>果たして今の自分が、その後を継ぐに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>959</PointerOffset>
    <JapaneseText>ふさわしいのか……日々、考えている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>994</PointerOffset>
    <JapaneseText>だから甘いと言うんだ、お前は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1023</PointerOffset>
    <JapaneseText>生まれついての名君などいるはずもない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1063</PointerOffset>
    <JapaneseText>王位に就き、周囲の助力や援助の中で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1099</PointerOffset>
    <JapaneseText>名君へと成長していくものだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1126</PointerOffset>
    <JapaneseText>違うのか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1135</PointerOffset>
    <JapaneseText>アッシュ君は、そう思うのだね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1164</PointerOffset>
    <JapaneseText>ああ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1172</PointerOffset>
    <JapaneseText>民の生活や国の安寧に心を砕くお前には、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1212</PointerOffset>
    <JapaneseText>誰より王の資質があるとも思っている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1247</PointerOffset>
    <JapaneseText>それは、光栄だ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1265</PointerOffset>
    <JapaneseText>だが……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1274</PointerOffset>
    <JapaneseText>今少し、考える時間が欲しいのだよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1310</PointerOffset>
    <JapaneseText>真実を見極め……本当に民が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1338</PointerOffset>
    <JapaneseText>必要としている事、己のなすべき事、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1373</PointerOffset>
    <JapaneseText>その全てに、誠実に向き合う時間が……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1410</PointerOffset>
    <JapaneseText>……そうか。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1424</PointerOffset>
    <JapaneseText>お前がそう思うなら、仕方がない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1455</PointerOffset>
    <JapaneseText>その代わり、思いや信念は最後まで貫けよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1497</PointerOffset>
    <JapaneseText>半端な決意はいらん、誰にとっても迷惑だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1536</PointerOffset>
    <JapaneseText>励ましてくれているんだね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1564</PointerOffset>
    <JapaneseText>すまない……アッシュ君</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1587</PointerOffset>
    <JapaneseText>別に……励ましてなどいない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1614</PointerOffset>
    <JapaneseText>フン……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
