<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>288</PointerOffset>
      <JapaneseText>いよいよね…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>304</PointerOffset>
      <JapaneseText>やっとラザリスを封印出来る</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>331</PointerOffset>
      <JapaneseText>でも、これでアドリビトムの仕事が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>365</PointerOffset>
      <JapaneseText>終わるわけではない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>384</PointerOffset>
      <JapaneseText>ラザリスの為にも、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>404</PointerOffset>
      <JapaneseText>この世界はもっと変わらなきゃいけない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>441</PointerOffset>
      <JapaneseText>誰もが手を取り合い、繋がり、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>471</PointerOffset>
      <JapaneseText>お互いを受け入れられる世界にね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>504</PointerOffset>
      <JapaneseText>さて、今回の仕事の説明をするよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>535</PointerOffset>
      <JapaneseText>聖地ラングリースのボルテックスに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>569</PointerOffset>
      <JapaneseText>向かってちょうだい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>588</PointerOffset>
      <JapaneseText>同行者は、リタとカノンノ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>613</PointerOffset>
      <JapaneseText>目的地に着けば、後はリタがドクメントを</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>653</PointerOffset>
      <JapaneseText>世界樹へ送り込んでくれるから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>682</PointerOffset>
      <JapaneseText>それじゃ、気を付けて行ってらっしゃい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>