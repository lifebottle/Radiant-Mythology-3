<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>366</PointerOffset>
    <JapaneseText>みんなへの挨拶は終わったみたいね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>29</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>402</PointerOffset>
    <JapaneseText>それじゃあ、ここでの仕事を説明するよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>439</PointerOffset>
    <JapaneseText>アドリビトムは、どの国にも属さない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>31</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>477</PointerOffset>
    <JapaneseText>依頼があれば、どこへでも向かう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>509</PointerOffset>
    <JapaneseText>自由のギルド</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>522</PointerOffset>
    <JapaneseText>そのギルドに人々から寄せられる依頼を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>34</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>560</PointerOffset>
    <JapaneseText>「クエスト」と言うんだけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>35</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>587</PointerOffset>
    <JapaneseText>そのクエストは、ここでわたしが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>36</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>619</PointerOffset>
    <JapaneseText>管理しているから、仕事がしたい時は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>655</PointerOffset>
    <JapaneseText>訪ねてね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>664</PointerOffset>
    <JapaneseText>そうそう、わたしがいるカウンター以外にも、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>708</PointerOffset>
    <JapaneseText>メンバーのみんなと話すと、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>736</PointerOffset>
    <JapaneseText>直接依頼をしてくる事もあるよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>765</PointerOffset>
    <JapaneseText>その時は聞いてあげてね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>788</PointerOffset>
    <JapaneseText>でも、そうね…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>806</PointerOffset>
    <JapaneseText>初めてだとわからない事があるだろうし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>843</PointerOffset>
    <JapaneseText>まずは簡単な仕事を用意するから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>877</PointerOffset>
    <JapaneseText>見習いから始めてもらおっかな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>906</PointerOffset>
    <JapaneseText>と言うわけで、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>922</PointerOffset>
    <JapaneseText>最初はカノンノを同行者にして</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>952</PointerOffset>
    <JapaneseText>仕事の仕方を学んでね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>973</PointerOffset>
    <JapaneseText>カノンノ、しばらくの間構わない？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1006</PointerOffset>
    <JapaneseText>はい！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1013</PointerOffset>
    <JapaneseText>さてと、早速仕事を用意したから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1047</PointerOffset>
    <JapaneseText>いつ受けてもらっても構わないよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
