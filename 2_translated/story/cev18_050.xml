<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>654</PointerOffset>
    <JapaneseText>ナナリーさんは、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>672</PointerOffset>
    <JapaneseText>小さな子ども達とずっと一緒に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>702</PointerOffset>
    <JapaneseText>暮らしていたんですよね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>725</PointerOffset>
    <JapaneseText>ああ、そうだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>740</PointerOffset>
    <JapaneseText>その……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>752</PointerOffset>
    <JapaneseText>ナナリーさんのご家族って……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>788</PointerOffset>
    <JapaneseText>あ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>795</PointerOffset>
    <JapaneseText>ごめんなさい、わたし……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>821</PointerOffset>
    <JapaneseText>失礼な事を……聞いたでしょうか……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>865</PointerOffset>
    <JapaneseText>……あたしには、弟が一人いたんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>901</PointerOffset>
    <JapaneseText>ルーっていってね……、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>925</PointerOffset>
    <JapaneseText>優しい、いい子だったよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>948</PointerOffset>
    <JapaneseText>……だった……っていうのは……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>979</PointerOffset>
    <JapaneseText>……亡くなったんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1001</PointerOffset>
    <JapaneseText>重い病気にかかっちまってね……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1041</PointerOffset>
    <JapaneseText>……そう暗い顔をしないでおくれよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1077</PointerOffset>
    <JapaneseText>あたしはこうして、ルーの分も</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1107</PointerOffset>
    <JapaneseText>精一杯生きようって思ってるんだからさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1144</PointerOffset>
    <JapaneseText>ナナリーさん……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1161</PointerOffset>
    <JapaneseText>ルーの為にも……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1179</PointerOffset>
    <JapaneseText>助けられる命は助けたいし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1205</PointerOffset>
    <JapaneseText>あたしはあたしに出来る事をしたいんだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1244</PointerOffset>
    <JapaneseText>アニーみたいに医者になろうなんて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1278</PointerOffset>
    <JapaneseText>そんなちゃんとした夢や目標はないけど……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1320</PointerOffset>
    <JapaneseText>でも……何か、したいんだ。　あたしもさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1359</PointerOffset>
    <JapaneseText>……とても、立派な事だと思います。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1395</PointerOffset>
    <JapaneseText>弟さんも、きっと……そんなナナリーさんが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1437</PointerOffset>
    <JapaneseText>大好きだったんでしょうね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1462</PointerOffset>
    <JapaneseText>ははっ、そうだといいんだけどね……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1497</PointerOffset>
    <JapaneseText>あたしはただ、さ……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1521</PointerOffset>
    <JapaneseText>あの子みたいに……あんなに小さいのに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1559</PointerOffset>
    <JapaneseText>死ななきゃいけない子を増やしたくない……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1600</PointerOffset>
    <JapaneseText>増やしちゃいけないって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1624</PointerOffset>
    <JapaneseText>思うんだよ……それだけさ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
