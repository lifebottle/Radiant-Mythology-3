<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>764</PointerOffset>
      <JapaneseText>はぁ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>773</PointerOffset>
      <JapaneseText>……どうしたの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>791</PointerOffset>
      <JapaneseText>随分深いため息だけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>812</PointerOffset>
      <JapaneseText>……何か、あったの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>833</PointerOffset>
      <JapaneseText>あ、ううん……ほら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>853</PointerOffset>
      <JapaneseText>この船って、色んな人が乗ってるでしょ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>892</PointerOffset>
      <JapaneseText>だから、周りを見てて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>914</PointerOffset>
      <JapaneseText>改めて思ったんだけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>939</PointerOffset>
      <JapaneseText>僕って、友達少ないなぁって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>970</PointerOffset>
      <JapaneseText>苦手なんだよね……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>992</PointerOffset>
      <JapaneseText>友達、作るの……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1009</PointerOffset>
      <JapaneseText>わかるよ……僕だって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1031</PointerOffset>
      <JapaneseText>初めて出来た友達がマルタで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1059</PointerOffset>
      <JapaneseText>それまでは、一人も……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1082</PointerOffset>
      <JapaneseText>魔物になら、結構友達は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1106</PointerOffset>
      <JapaneseText>いるんだけどさ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1125</PointerOffset>
      <JapaneseText>えっ……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1137</PointerOffset>
      <JapaneseText>エミルって、魔物の友達がいるの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1170</PointerOffset>
      <JapaneseText>あ、うん……友達っていうか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1198</PointerOffset>
      <JapaneseText>仲間……みたいなもの、かな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1227</PointerOffset>
      <JapaneseText>すごいや。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1239</PointerOffset>
      <JapaneseText>そんなの聞いた事ないよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1265</PointerOffset>
      <JapaneseText>でも……怖くないの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1286</PointerOffset>
      <JapaneseText>そりゃあ、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1298</PointerOffset>
      <JapaneseText>最初はちょっと怖かったけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1329</PointerOffset>
      <JapaneseText>でも、最初だけだよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1351</PointerOffset>
      <JapaneseText>みんないい奴ばっかりなんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1378</PointerOffset>
      <JapaneseText>そっか……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1392</PointerOffset>
      <JapaneseText>人間と変わらないんだね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1415</PointerOffset>
      <JapaneseText>うん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1423</PointerOffset>
      <JapaneseText>それに……少なくても、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1447</PointerOffset>
      <JapaneseText>友達は友達だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1462</PointerOffset>
      <JapaneseText>そうだね……数じゃ、ないんだよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1495</PointerOffset>
      <JapaneseText>そうだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1504</PointerOffset>
      <JapaneseText>何か……エミルと話してると</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1532</PointerOffset>
      <JapaneseText>落ち着くな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1543</PointerOffset>
      <JapaneseText>僕も……ルカと話してるの、楽しいよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1578</PointerOffset>
      <JapaneseText>エミル・ルカ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1591</PointerOffset>
      <JapaneseText>……えへへ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>