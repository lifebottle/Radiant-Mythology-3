      <JapaneseText>悃</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1880</PointerOffset>
      <JapaneseText>暼</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1900</PointerOffset>
      <JapaneseText>傲</JapaneseText>
      <EnglishText/>
