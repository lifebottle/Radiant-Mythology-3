<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>684</PointerOffset>
      <JapaneseText>傲</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1285</PointerOffset>
      <JapaneseText>さあ、おやつですよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1307</PointerOffset>
      <JapaneseText>お茶はミルクティーを用意しますね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1340</PointerOffset>
      <JapaneseText>いただきます、と言う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1361</PointerOffset>
      <JapaneseText>がっつく</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1370</PointerOffset>
      <JapaneseText>はい、どうぞ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1386</PointerOffset>
      <JapaneseText>召し上がって下さい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1405</PointerOffset>
      <JapaneseText>あっ、いけませんよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1427</PointerOffset>
      <JapaneseText>ちゃんと、いただきますを言わなくては</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1464</PointerOffset>
      <JapaneseText>いただきますは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1482</PointerOffset>
      <JapaneseText>食べ物へのご挨拶なんですから！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1513</PointerOffset>
      <JapaneseText>首をかしげる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1526</PointerOffset>
      <JapaneseText>黙って話を聞く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1541</PointerOffset>
      <JapaneseText>食べ物はですね、自分の命になってくれる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1581</PointerOffset>
      <JapaneseText>身体への大事なお客様なんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1610</PointerOffset>
      <JapaneseText>だから、いただきますと言うのは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1642</PointerOffset>
      <JapaneseText>大事な事なんですよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1661</PointerOffset>
      <JapaneseText>ごちそうさま</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1674</PointerOffset>
      <JapaneseText>そう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1682</PointerOffset>
      <JapaneseText>ごちそうさま、もですよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1705</PointerOffset>
      <JapaneseText>ねえ、○○。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1719</PointerOffset>
      <JapaneseText>何か、少し思い出せた事ってある？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1752</PointerOffset>
      <JapaneseText>首を横に振る</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1765</PointerOffset>
      <JapaneseText>黙る</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1770</PointerOffset>
      <JapaneseText>そう…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1777</PointerOffset>
      <JapaneseText>でも、いつかきっと思い出せるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>114</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1808</PointerOffset>
      <JapaneseText>そう、まだ記憶は戻らないの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1835</PointerOffset>
      <JapaneseText>私も、何か力になれたらな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1862</PointerOffset>
      <JapaneseText>そうだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1872</PointerOffset>
      <JapaneseText>また絵を描いたんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1894</PointerOffset>
      <JapaneseText>見に来てくれる？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>119</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1911</PointerOffset>
      <JapaneseText>何かきっかけになるかもしれないし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1944</PointerOffset>
      <JapaneseText>それに、私に出来る事って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1970</PointerOffset>
      <JapaneseText>こんな事しかないから…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1993</PointerOffset>
      <JapaneseText>それじゃあ、待ってるね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>123</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2016</PointerOffset>
      <JapaneseText>彼女、絵を見てくれるあなたの事を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2050</PointerOffset>
      <JapaneseText>すっかり気に入ってるみたいですね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>125</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2083</PointerOffset>
      <JapaneseText>旦那様と奥様がいたら、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2107</PointerOffset>
      <JapaneseText>あんな風に絵を見せていたのかな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>127</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2140</PointerOffset>
      <JapaneseText>カノンノの両親について聞く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>128</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2167</PointerOffset>
      <JapaneseText>親代わりをしてるのはなぜか、と聞く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>129</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2202</PointerOffset>
      <JapaneseText>彼女のご両親、…旦那様と奥様は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2234</PointerOffset>
      <JapaneseText>お亡くなりになられているんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>131</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2265</PointerOffset>
      <JapaneseText>二人とも、腕の立つ医者でしたので</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>132</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2299</PointerOffset>
      <JapaneseText>従軍医として、国から戦場へと連れて行かれ…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>133</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2345</PointerOffset>
      <JapaneseText>殉職しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>134</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2358</PointerOffset>
      <JapaneseText>まだ、お嬢様が生まれてほんのひと月の事…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>135</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2402</PointerOffset>
      <JapaneseText>召集がかかり、二人は強引に連れて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>136</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2436</PointerOffset>
      <JapaneseText>行かれました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>137</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2449</PointerOffset>
      <JapaneseText>…戦争の原因ですか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>138</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2471</PointerOffset>
      <JapaneseText>お察しの通りです。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>139</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2491</PointerOffset>
      <JapaneseText>星晶を巡っての戦争でした…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2518</PointerOffset>
      <JapaneseText>僕は当時、お嬢様のご両親に使用人として</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>141</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2558</PointerOffset>
      <JapaneseText>雇われていました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>142</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2575</PointerOffset>
      <JapaneseText>そして残されたお嬢様を預かり、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>143</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2607</PointerOffset>
      <JapaneseText>二人の無事を祈りながら育てていたんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>144</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2646</PointerOffset>
      <JapaneseText>でも、やがて二人の死を国から伝えられ…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>145</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2688</PointerOffset>
      <JapaneseText>戦火は住んでいる村にまで及び、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>146</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2720</PointerOffset>
      <JapaneseText>僕は幼いお嬢様を連れて旅立ちました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>147</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2755</PointerOffset>
      <JapaneseText>仕えていた旦那様と奥様の大事な娘。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>148</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2791</PointerOffset>
      <JapaneseText>彼女は僕にとって、とても大切な…そして、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>149</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2833</PointerOffset>
      <JapaneseText>新しい主人でもあったんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>150</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2860</PointerOffset>
      <JapaneseText>責任をもって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>151</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2876</PointerOffset>
      <JapaneseText>立派な淑女に育てるって決めたんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>152</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2911</PointerOffset>
      <JapaneseText>色々お嬢様には辛い思いをさせました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>153</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2946</PointerOffset>
      <JapaneseText>でも、行き着いたアンジュ様の教会で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>154</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2982</PointerOffset>
      <JapaneseText>お世話になる事が出来て、この船に乗る事も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>155</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3024</PointerOffset>
      <JapaneseText>出来て</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>156</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3031</PointerOffset>
      <JapaneseText>何とか健やかに育ってくれてます。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>157</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3065</PointerOffset>
      <JapaneseText>僕も、お嬢様を育てる事で救われていますし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>158</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3106</PointerOffset>
      <JapaneseText>どうか、仲良くしてあげて下さいね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>159</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3142</PointerOffset>
      <JapaneseText>カノンノお嬢様と…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>160</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>