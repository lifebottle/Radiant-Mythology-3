    <Id>38</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>665</PointerOffsetDec>
    <PointerOffsetHex>0x299</PointerOffsetHex>
    <JapaneseText>アスベルは……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
