<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>854</PointerOffset>
      <JapaneseText>あン、誰だァコイツ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>875</PointerOffset>
      <JapaneseText>ちょっ、スパーダったらもう！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>905</PointerOffset>
      <JapaneseText>このギルドの仲間だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>926</PointerOffset>
      <JapaneseText>おー、そうか。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>942</PointerOffset>
      <JapaneseText>ってか、ビビらすつもりは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>968</PointerOffset>
      <JapaneseText>さらさら無かったんだけどな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>995</PointerOffset>
      <JapaneseText>君は、誰に対してもそうだけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1025</PointerOffset>
      <JapaneseText>中には恐がる人もいるし…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1050</PointerOffset>
      <JapaneseText>ルカとかな、ヒャハハハ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1076</PointerOffset>
      <JapaneseText>初めて会った時のお前、そうだったよな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1113</PointerOffset>
      <JapaneseText>オレはスパーダ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1131</PointerOffset>
      <JapaneseText>スパーダ・ベルフォルマ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1154</PointerOffset>
      <JapaneseText>こいつらのダチだ、よろしくな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1183</PointerOffset>
      <JapaneseText>ところで、スパーダ君。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1207</PointerOffset>
      <JapaneseText>手紙にも書いておいたけど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1235</PointerOffset>
      <JapaneseText>情報は仕入れてきてくれた？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1262</PointerOffset>
      <JapaneseText>あー、赤い煙ってヤツかぁ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1290</PointerOffset>
      <JapaneseText>アレ、割と知れ渡っているみたいだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1325</PointerOffset>
      <JapaneseText>気になんのは、見る者によって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1357</PointerOffset>
      <JapaneseText>その煙の姿が違うらしいって事だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1388</PointerOffset>
      <JapaneseText>姿？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1394</PointerOffset>
      <JapaneseText>煙とかモヤとか、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1412</PointerOffset>
      <JapaneseText>そんな不定形なものでしょ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1439</PointerOffset>
      <JapaneseText>ソイツの存在がわかった頃は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1467</PointerOffset>
      <JapaneseText>煙だったらしいな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1484</PointerOffset>
      <JapaneseText>けど今は、花や、虫、魚、色んな姿で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1520</PointerOffset>
      <JapaneseText>現れるらしいぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1535</PointerOffset>
      <JapaneseText>…まるで、進化しているみたいだね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1568</PointerOffset>
      <JapaneseText>それだけじゃねェ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1588</PointerOffset>
      <JapaneseText>変わってんのは街の人もだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1613</PointerOffset>
      <JapaneseText>「病気を治す」から、いつの間にか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1647</PointerOffset>
      <JapaneseText>「願いを叶える」って話になってんだぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1684</PointerOffset>
      <JapaneseText>願いを叶える？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1699</PointerOffset>
      <JapaneseText>…それはもはや……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1721</PointerOffset>
      <JapaneseText>それは、病気を治すなんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1747</PointerOffset>
      <JapaneseText>ものではない…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1762</PointerOffset>
      <JapaneseText>もっと、「とんでもない」ものよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1793</PointerOffset>
      <JapaneseText>願いを叶える、か…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1812</PointerOffset>
      <JapaneseText>じゃあ、もっとたくさんの人が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1842</PointerOffset>
      <JapaneseText>その存在に会いたがるんだろうね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1873</PointerOffset>
      <JapaneseText>スパーダがアドリビトムに加入しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>