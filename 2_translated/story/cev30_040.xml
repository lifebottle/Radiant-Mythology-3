<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>1893</PointerOffset>
      <JapaneseText>さて、と。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1905</PointerOffset>
      <JapaneseText>そろそろ仕上げに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>127</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1923</PointerOffset>
      <JapaneseText>ミソを入れようかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>128</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1946</PointerOffset>
      <JapaneseText>お待ちになって。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>129</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1964</PointerOffset>
      <JapaneseText>その前に、隠し味も用意しませんと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1997</PointerOffset>
      <JapaneseText>メロンなどが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>131</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2011</PointerOffset>
      <JapaneseText>ふさわしいのではなくて？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>132</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2036</PointerOffset>
      <JapaneseText>そうね……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>133</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2050</PointerOffset>
      <JapaneseText>あ、それならついでに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>134</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2072</PointerOffset>
      <JapaneseText>マグロも入れてみましょう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>135</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2097</PointerOffset>
      <JapaneseText>魚に含まれている栄養素には、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>136</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2127</PointerOffset>
      <JapaneseText>脳の活性化を促す作用があるの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>137</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2156</PointerOffset>
      <JapaneseText>何で、あいつらが飯作ってんだよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>138</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2189</PointerOffset>
      <JapaneseText>俺が知るかよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>139</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2205</PointerOffset>
      <JapaneseText>気が付いたら、こうなってたんだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2238</PointerOffset>
      <JapaneseText>む……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>141</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2245</PointerOffset>
      <JapaneseText>これぐらい煮込めば</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>142</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2265</PointerOffset>
      <JapaneseText>よろしいんじゃありません？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>143</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2292</PointerOffset>
      <JapaneseText>ええ、そうね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>144</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2308</PointerOffset>
      <JapaneseText>あとは盛り付けをして……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>145</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2333</PointerOffset>
      <JapaneseText>ちょっと待ったー！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>146</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2352</PointerOffset>
      <JapaneseText>あら、リタ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>147</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2366</PointerOffset>
      <JapaneseText>突然どうなさいましたの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>148</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2391</PointerOffset>
      <JapaneseText>は〜〜〜〜。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>149</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2405</PointerOffset>
      <JapaneseText>全くあんた達ってば、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>150</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2427</PointerOffset>
      <JapaneseText>料理の事を、な〜んもわかってないわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>151</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2462</PointerOffset>
      <JapaneseText>おぉ、救世主が……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>152</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2483</PointerOffset>
      <JapaneseText>いい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>153</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2490</PointerOffset>
      <JapaneseText>人間に必要な栄養素を食材で摂るんなら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>154</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2528</PointerOffset>
      <JapaneseText>明らかにミルクが足りないでしょ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>155</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2562</PointerOffset>
      <JapaneseText>ミルクが！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>156</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2575</PointerOffset>
      <JapaneseText>あの中に、ミルクを足すって……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>157</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2609</PointerOffset>
      <JapaneseText>正気かよ！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>158</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2622</PointerOffset>
      <JapaneseText>なるほど……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>159</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2638</PointerOffset>
      <JapaneseText>確かに一理ある意見だわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>160</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2661</PointerOffset>
      <JapaneseText>よし。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>161</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2669</PointerOffset>
      <JapaneseText>それじゃミルクを足して、と</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>162</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2696</PointerOffset>
      <JapaneseText>出来ましたわ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>163</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2712</PointerOffset>
      <JapaneseText>滋養強壮に効果ありの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>164</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2734</PointerOffset>
      <JapaneseText>特製スープの完成ですわ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>165</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2759</PointerOffset>
      <JapaneseText>さ、ルーク。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>166</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2773</PointerOffset>
      <JapaneseText>お食べなさい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>167</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2786</PointerOffset>
      <JapaneseText>え、俺か！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>168</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2800</PointerOffset>
      <JapaneseText>い、いや、それはちょっと……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>169</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2829</PointerOffset>
      <JapaneseText>ほら、ロイドも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>170</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2844</PointerOffset>
      <JapaneseText>お、俺は……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>171</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2857</PointerOffset>
      <JapaneseText>滋養強壮なんだから、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>172</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2879</PointerOffset>
      <JapaneseText>やっぱり最年長のクラトスに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>173</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2907</PointerOffset>
      <JapaneseText>最初に食べてもらったら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>174</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2932</PointerOffset>
      <JapaneseText>ほら、クラトス。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>175</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2950</PointerOffset>
      <JapaneseText>遠慮しないで食べて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>176</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2969</PointerOffset>
      <JapaneseText>む……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>177</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2979</PointerOffset>
      <JapaneseText>こ、この匂いは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>178</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2998</PointerOffset>
      <JapaneseText>クラトス、止めとけ……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>179</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3024</PointerOffset>
      <JapaneseText>腹壊すぞ、マジで……！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>180</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3049</PointerOffset>
      <JapaneseText>し、しかし……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>181</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3064</PointerOffset>
      <JapaneseText>遠慮する事はないわ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>182</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3086</PointerOffset>
      <JapaneseText>さぁ、早く∀</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>183</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3099</PointerOffset>
      <JapaneseText>う……、うむ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>184</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3116</PointerOffset>
      <JapaneseText>では、頂くとするか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>185</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3139</PointerOffset>
      <JapaneseText>ロイド・ルーク</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>186</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3154</PointerOffset>
      <JapaneseText>ダメだーーーー！！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>187</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>