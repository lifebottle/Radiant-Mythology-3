<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>66</PointerOffset>
    <JapaneseText>鴨</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>4</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1128</PointerOffset>
    <JapaneseText>カノンノ、返事をしろ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1149</PointerOffset>
    <JapaneseText>戻らなきゃ…！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1166</PointerOffset>
    <JapaneseText>身体に戻らなきゃ、戻らなきゃ…！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1201</PointerOffset>
    <JapaneseText>…ぁっ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1210</PointerOffset>
    <JapaneseText>カノンノ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1221</PointerOffset>
    <JapaneseText>…元に、戻れた…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1238</PointerOffset>
    <JapaneseText>現身から意識が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1254</PointerOffset>
    <JapaneseText>分離しかけているんだな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1277</PointerOffset>
    <JapaneseText>頭より気を取り込んで、腹に溜め、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1311</PointerOffset>
    <JapaneseText>そのまま足より流せ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1330</PointerOffset>
    <JapaneseText>心身と自然を一つにする、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1356</PointerOffset>
    <JapaneseText>武術の基本だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1369</PointerOffset>
    <JapaneseText>はい…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1376</PointerOffset>
    <JapaneseText>………頭から…気を取り込んで…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1408</PointerOffset>
    <JapaneseText>お腹に……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1419</PointerOffset>
    <JapaneseText>あ…、不思議…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1437</PointerOffset>
    <JapaneseText>ちゃんと身体に心が収まっていく</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1469</PointerOffset>
    <JapaneseText>感じがする！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1482</PointerOffset>
    <JapaneseText>フ…、お前は繋がりを知っているのだな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1522</PointerOffset>
    <JapaneseText>知らない者はいくらやっても</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1550</PointerOffset>
    <JapaneseText>出来ないのだが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1565</PointerOffset>
    <JapaneseText>繋がり…？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1576</PointerOffset>
    <JapaneseText>世界との繋がりだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1593</PointerOffset>
    <JapaneseText>ヒトは地に足をつけて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1617</PointerOffset>
    <JapaneseText>世界と共に生きるものだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1643</PointerOffset>
    <JapaneseText>だが、ほとんどの者はそれを忘れている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1680</PointerOffset>
    <JapaneseText>目に見える部分ばかりに気を取られてな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1720</PointerOffset>
    <JapaneseText>そうして、自らを自らの意志で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1750</PointerOffset>
    <JapaneseText>世界から切り離して生きている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1779</PointerOffset>
    <JapaneseText>ヒトで世界は溢れているが、皆孤独なのだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1821</PointerOffset>
    <JapaneseText>繋がりを忘れてしまっている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>108</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1848</PointerOffset>
    <JapaneseText>お互いを全く意識せずに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>109</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1872</PointerOffset>
    <JapaneseText>それぞれの人生を生きていると</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>110</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1902</PointerOffset>
    <JapaneseText>思い込んでいる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>111</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1917</PointerOffset>
    <JapaneseText>そこから、心の飢餓が始まる。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>112</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1947</PointerOffset>
    <JapaneseText>争いや略奪…他にも様々な事が起こりうる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>113</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1986</PointerOffset>
    <JapaneseText>ヒトや様々な生き物、そして世界。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>114</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2020</PointerOffset>
    <JapaneseText>これらが等しく「繋がり」を知る事が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>115</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2056</PointerOffset>
    <JapaneseText>出来れば…、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>116</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2069</PointerOffset>
    <JapaneseText>自他を慈しみ、権利を奪わず、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>117</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2099</PointerOffset>
    <JapaneseText>差を作る気持ちは起こらない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>118</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2126</PointerOffset>
    <JapaneseText>うん、そうだね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>119</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2144</PointerOffset>
    <JapaneseText>風来草やユルングの樹はそれを教えてくれた</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>120</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2185</PointerOffset>
    <JapaneseText>ヒトって、どうして</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>121</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2205</PointerOffset>
    <JapaneseText>そうなっていったんだろう…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>122</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2232</PointerOffset>
    <JapaneseText>覚えておくのだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>123</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2247</PointerOffset>
    <JapaneseText>どういう事態においても、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>124</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2273</PointerOffset>
    <JapaneseText>原因を知るという事は、再び危機が訪れた時、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>125</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2317</PointerOffset>
    <JapaneseText>それを回避する為の手立てにはなる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>126</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2350</PointerOffset>
    <JapaneseText>だが、大事なのは原因を追うばかりになる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>127</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2390</PointerOffset>
    <JapaneseText>過去に留まらぬ事</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>128</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2407</PointerOffset>
    <JapaneseText>今は、変化を起こす行動を選択する事だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>129</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2444</PointerOffset>
    <JapaneseText>心を「今」から離してはならない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>130</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2478</PointerOffset>
    <JapaneseText>今からお前達がする様に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>131</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2504</PointerOffset>
    <JapaneseText>過去を知りにいくならなおさらだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>132</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2535</PointerOffset>
    <JapaneseText>言葉を心に留める</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>133</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2552</PointerOffset>
    <JapaneseText>はい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>134</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2560</PointerOffset>
    <JapaneseText>わかりました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>135</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
