<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>436</PointerOffset>
      <JapaneseText>ここのところ、雨続きでしたね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>465</PointerOffset>
      <JapaneseText>ポッポ達は喜んでいましたけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>495</PointerOffset>
      <JapaneseText>こう湿気が多くては、過ごしづらくて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>531</PointerOffset>
      <JapaneseText>かないません</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>544</PointerOffset>
      <JapaneseText>そうね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>554</PointerOffset>
      <JapaneseText>……でも、ポッポ達の気持ち</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>582</PointerOffset>
      <JapaneseText>何となくわかる気がするわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>607</PointerOffset>
      <JapaneseText>雨が降ると、少し気分が落ち着くもの。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>645</PointerOffset>
      <JapaneseText>モフモフ族じゃなくたって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>673</PointerOffset>
      <JapaneseText>雨は、天からの恵みなのよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>700</PointerOffset>
      <JapaneseText>さすがリアラちゃん、繊細だねぇ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>734</PointerOffset>
      <JapaneseText>メランコリックな気分って感じ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>765</PointerOffset>
      <JapaneseText>メランコリック……ですか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>792</PointerOffset>
      <JapaneseText>そ〜そ〜。雨とかを見てて、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>820</PointerOffset>
      <JapaneseText>気分が落ち着いたりする時に言うのよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>858</PointerOffset>
      <JapaneseText>まさに、今のリアラちゃんにピッタリ∀</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>895</PointerOffset>
      <JapaneseText>正確には「憂鬱な気分」を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>921</PointerOffset>
      <JapaneseText>表す言葉ですけどね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>940</PointerOffset>
      <JapaneseText>んな、こまけー事はいいの。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>968</PointerOffset>
      <JapaneseText>憂えてるリアラちゃんが素敵なんだから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1005</PointerOffset>
      <JapaneseText>適当過ぎる……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>