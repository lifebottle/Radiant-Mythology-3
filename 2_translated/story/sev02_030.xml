<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>244</PointerOffset>
      <JapaneseText>ねえねえ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>256</PointerOffset>
      <JapaneseText>これから行く赤のオアシスって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>288</PointerOffset>
      <JapaneseText>どんな所だろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>303</PointerOffset>
      <JapaneseText>大昔の人が不吉に思ったくらいだから、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>341</PointerOffset>
      <JapaneseText>血の色かもしれないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>362</PointerOffset>
      <JapaneseText>そっかぁ〜。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>376</PointerOffset>
      <JapaneseText>いちごのジャムみたいな水だったら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>410</PointerOffset>
      <JapaneseText>いいのにね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>421</PointerOffset>
      <JapaneseText>一杯取って、好きなだけ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>445</PointerOffset>
      <JapaneseText>アイスやヨーグルトにかけたり</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>475</PointerOffset>
      <JapaneseText>出来るから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>486</PointerOffset>
      <JapaneseText>ジャムかぁ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>500</PointerOffset>
      <JapaneseText>わたしだったら、赤ミソがいいな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>533</PointerOffset>
      <JapaneseText>赤くておいしそうなものって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>563</PointerOffset>
      <JapaneseText>他に何があるかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>582</PointerOffset>
      <JapaneseText>う〜ん…、意外と思い付かないね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>