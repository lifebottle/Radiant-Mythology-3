  <Entries>
    <Entry>
      <PointerOffset>326</PointerOffset>
      <JapaneseText>ねえ、○○。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>340</PointerOffset>
      <JapaneseText>ロックスがおやつを作ってくれてるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>375</PointerOffset>
      <JapaneseText>食堂に食べに来て欲しいんだって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>406</PointerOffset>
      <JapaneseText>すぐ行く、と言う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>423</PointerOffset>
      <JapaneseText>ありがとう、と言う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>442</PointerOffset>
      <JapaneseText>うん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>450</PointerOffset>
      <JapaneseText>じゃあ、ロックスにそう伝えるね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>481</PointerOffset>
      <JapaneseText>うん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>489</PointerOffset>
      <JapaneseText>今日のおやつは、チョコケーキだよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>525</PointerOffset>
      <JapaneseText>じゃあ、お茶を用意しておくね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>562</PointerOffset>
      <JapaneseText>行ってらっしゃい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>582</PointerOffset>
      <JapaneseText>早くしないと、誰かに取られちゃうよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
