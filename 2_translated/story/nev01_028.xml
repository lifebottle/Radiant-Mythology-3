<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>264</PointerOffset>
      <JapaneseText>もしかしてこの船、「あの」海賊船、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>300</PointerOffset>
      <JapaneseText>バンエルティア号なの？　すっごいなぁ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>340</PointerOffset>
      <JapaneseText>これって大発見だよ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>363</PointerOffset>
      <JapaneseText>キミが迎えに来てくれるまで、ボクはずっと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>405</PointerOffset>
      <JapaneseText>姉さんと各地の遺跡調査をしてたんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>443</PointerOffset>
      <JapaneseText>もちろん、元々興味があったからだけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>484</PointerOffset>
      <JapaneseText>姉さん一人じゃ、遺跡モードで暴走して</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>522</PointerOffset>
      <JapaneseText>周りに迷惑掛けちゃうからね……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>556</PointerOffset>
      <JapaneseText>ボクがついてなくちゃ、心配でさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>587</PointerOffset>
      <JapaneseText>○○は、けん玉で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>605</PointerOffset>
      <JapaneseText>遊んだ事はないの？　良かったらやってみる？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>649</PointerOffset>
      <JapaneseText>ロイドってば、すっごく下手クソなんだよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>690</PointerOffset>
      <JapaneseText>ロックスはアンジュに頭が上がらないんだね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>734</PointerOffset>
      <JapaneseText>「そう簡単におだてには乗りません」とか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>774</PointerOffset>
      <JapaneseText>さっきもブツブツぼやいてたよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>803</PointerOffset>
      <JapaneseText>世界文化遺産の遺跡は、姉さんにとっては</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>843</PointerOffset>
      <JapaneseText>聖域にも等しいんだ。何しろ、世界中の遺跡の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>887</PointerOffset>
      <JapaneseText>最高峰って事だからね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>912</PointerOffset>
      <JapaneseText>聞いてよ。姉さんてば、最近しょっちゅう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>952</PointerOffset>
      <JapaneseText>操舵室にこもってて……ニアタの話が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>988</PointerOffset>
      <JapaneseText>面白いのはわかるけど、生活が極端過ぎだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1029</PointerOffset>
      <JapaneseText>ここのところ、世紀の大発見ばかりだね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1069</PointerOffset>
      <JapaneseText>おかげで姉さんたら、毎日あちこちの文献を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1111</PointerOffset>
      <JapaneseText>ひっくり返してるよ。論文を書くんだって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1150</PointerOffset>
      <JapaneseText>食事なんて、お腹にものが入ればいいやって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1192</PointerOffset>
      <JapaneseText>思ってたけど、これからはちゃんとするよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1234</PointerOffset>
      <JapaneseText>命をもらうんだから、それが礼儀、なんだよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1277</PointerOffset>
      <JapaneseText>まだ……、まだ終わってない。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1307</PointerOffset>
      <JapaneseText>封印次元の力が及ばなくても、まだボク達には</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1351</PointerOffset>
      <JapaneseText>出来る事があるんだ。だから最後まで戦うよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1394</PointerOffset>
      <JapaneseText>お帰りなさい！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1410</PointerOffset>
      <JapaneseText>ねえ聞いてよ。キミが留守の間、色々と</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1448</PointerOffset>
      <JapaneseText>大変だったんだ。姉さんとロイドがさ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1487</PointerOffset>
      <JapaneseText>……っとと、これは内緒の話なんだった。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1527</PointerOffset>
      <JapaneseText>詳しい話はまた後でするよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1555</PointerOffset>
      <JapaneseText>とにかく、大変だったんだから！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1586</PointerOffset>
      <JapaneseText>ヒドイよ…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1600</PointerOffset>
      <JapaneseText>○○のせいだからね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>