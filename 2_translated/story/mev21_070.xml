<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>1464</PointerOffset>
    <JapaneseText>あなた達にはまた、助けられました。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>108</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1500</PointerOffset>
    <JapaneseText>本当に、ありがとうございます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>109</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1529</PointerOffset>
    <JapaneseText>ここを訪ねる為に、シフノ湧泉洞を抜ける</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>110</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1569</PointerOffset>
    <JapaneseText>ところだったんですが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>111</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1590</PointerOffset>
    <JapaneseText>途中で地震に遭いまして…、足元の地面が抜け、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>112</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1636</PointerOffset>
    <JapaneseText>そのままあの場に落ちてしまったんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>113</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1673</PointerOffset>
    <JapaneseText>二度目のジルディアのキバが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>114</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1701</PointerOffset>
    <JapaneseText>現れた時の事なのね…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>115</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1722</PointerOffset>
    <JapaneseText>ところで、アドリビトムに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>116</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1748</PointerOffset>
    <JapaneseText>接触したかったそうですね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>117</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1776</PointerOffset>
    <JapaneseText>どういった御用件かしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>118</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1801</PointerOffset>
    <JapaneseText>あの、アドリビトムが探しているという</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>119</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1839</PointerOffset>
    <JapaneseText>植物について、何かの役に立てればと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>120</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1875</PointerOffset>
    <JapaneseText>思いまして</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>121</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1886</PointerOffset>
    <JapaneseText>あ…、ええと。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>122</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1902</PointerOffset>
    <JapaneseText>ツリガネトンボ草の事でしょうか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>123</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1935</PointerOffset>
    <JapaneseText>あれについてはもう、解決して…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>124</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1966</PointerOffset>
    <JapaneseText>いえ、ウズマキフスベの方です。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>125</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1998</PointerOffset>
    <JapaneseText>確か、採取協力をあちこちに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>126</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2026</PointerOffset>
    <JapaneseText>呼びかけていましたよね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>127</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2049</PointerOffset>
    <JapaneseText>ウズマキフスベを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>128</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2067</PointerOffset>
    <JapaneseText>持っているんですか！？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>129</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2090</PointerOffset>
    <JapaneseText>あ、…そのぅ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>130</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2106</PointerOffset>
    <JapaneseText>私の実家の切り株に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>131</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2128</PointerOffset>
    <JapaneseText>５年前に生えたんですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>132</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2151</PointerOffset>
    <JapaneseText>もう、絶滅したとか言う話でしたけど…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>133</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2188</PointerOffset>
    <JapaneseText>…５年前。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>134</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2200</PointerOffset>
    <JapaneseText>…じゃあ、今はもう生えてないんですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>135</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2249</PointerOffset>
    <JapaneseText>その、菌糸くらいなら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>136</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2271</PointerOffset>
    <JapaneseText>残ってるかもと思ったので、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>137</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2299</PointerOffset>
    <JapaneseText>切り株を持って来たんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>138</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2324</PointerOffset>
    <JapaneseText>ちょっ…、それ、見せて！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>139</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2351</PointerOffset>
    <JapaneseText>それ、使えるかもよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>140</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2373</PointerOffset>
    <JapaneseText>ドクメントの一片でも残ってたらね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>141</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2406</PointerOffset>
    <JapaneseText>良かった。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>142</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2418</PointerOffset>
    <JapaneseText>私達にも出来る事があって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>143</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2443</PointerOffset>
    <JapaneseText>私達は悪くなっていく世を憂いて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>144</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2477</PointerOffset>
    <JapaneseText>救い手となってくれるディセンダーを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>145</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2513</PointerOffset>
    <JapaneseText>待ち望んでいた</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>146</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2528</PointerOffset>
    <JapaneseText>けれども、そうする事で自分達は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>147</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2560</PointerOffset>
    <JapaneseText>何も生み出そうとはしていなかった</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>148</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2593</PointerOffset>
    <JapaneseText>自分達では何も変えられないと。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>149</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2625</PointerOffset>
    <JapaneseText>世界も、自分自身も信じていなかった…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>150</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2662</PointerOffset>
    <JapaneseText>逃げていただけだったんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>151</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2690</PointerOffset>
    <JapaneseText>世の中への不満や、自分の非力さに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>152</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2724</PointerOffset>
    <JapaneseText>正面からぶつかる自信がなくて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>153</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2753</PointerOffset>
    <JapaneseText>そして、世界から目をそらして</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>154</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2783</PointerOffset>
    <JapaneseText>逃げていた結果、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>155</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2801</PointerOffset>
    <JapaneseText>ラザリスに力を与えてしまった</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>156</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2830</PointerOffset>
    <JapaneseText>私達の…弱さが…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>157</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2847</PointerOffset>
    <JapaneseText>あれからもう逃げずに、自分達で世界を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>158</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2885</PointerOffset>
    <JapaneseText>守れるのならばと生き方を変えました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>159</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2920</PointerOffset>
    <JapaneseText>今起こっている世界の脅威、皆なす術もなく</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>160</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2962</PointerOffset>
    <JapaneseText>希望を見失う中、あなた達が果敢にも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>161</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2998</PointerOffset>
    <JapaneseText>立ち向かっている事を知ったのです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>162</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3031</PointerOffset>
    <JapaneseText>そうだったんですか…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>163</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3052</PointerOffset>
    <JapaneseText>ふーん…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>164</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3064</PointerOffset>
    <JapaneseText>じゃ、あんた達にも重要な仕事があるわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>165</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3101</PointerOffset>
    <JapaneseText>あんた達のさっき話した内容を、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>166</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3133</PointerOffset>
    <JapaneseText>出来るだけ多くの人に伝えるの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>167</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3162</PointerOffset>
    <JapaneseText>…その…触れ合うっていうか…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>168</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3191</PointerOffset>
    <JapaneseText>…はい？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>169</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3201</PointerOffset>
    <JapaneseText>触れ合う…ですか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>170</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3220</PointerOffset>
    <JapaneseText>世界を守ろう、変えていこうという熱意を、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>171</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3262</PointerOffset>
    <JapaneseText>ルミナシアのみんなに伝えて欲しいのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>172</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3299</PointerOffset>
    <JapaneseText>侵食を、払拭する力をね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>173</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3322</PointerOffset>
    <JapaneseText>ディセンダーの力を、広めてもらうのね！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>174</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3363</PointerOffset>
    <JapaneseText>…そ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>175</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3371</PointerOffset>
    <JapaneseText>カノンノの思いを、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>176</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3391</PointerOffset>
    <JapaneseText>繋いだ手を通してこいつらに伝える</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>177</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3424</PointerOffset>
    <JapaneseText>あたしが編み出したこの術式さえあれば、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>178</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3464</PointerOffset>
    <JapaneseText>人と人とが触れ合うだけで、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>179</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3492</PointerOffset>
    <JapaneseText>能力の転写が出来るはず…！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>180</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3519</PointerOffset>
    <JapaneseText>ディセンダーの力…ですか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>181</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3546</PointerOffset>
    <JapaneseText>みんながディセンダーになるんです。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>182</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3582</PointerOffset>
    <JapaneseText>みんなで世界を救いに導くんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>183</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3613</PointerOffset>
    <JapaneseText>どうか、未来を共に作って下さい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>184</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3644</PointerOffset>
    <JapaneseText>出来る事であれば、何でもします。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>185</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3678</PointerOffset>
    <JapaneseText>私達を必要としてくれてありがとう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>186</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3711</PointerOffset>
    <JapaneseText>よし。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>187</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3719</PointerOffset>
    <JapaneseText>さーてと、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>188</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>3731</PointerOffset>
    <JapaneseText>あたしは切り株を分析してみるわね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>189</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
