  <Entries>
    <Entry>
      <PointerOffset>204</PointerOffset>
      <JapaneseText>待っていたぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>217</PointerOffset>
      <JapaneseText>お前の手を借りるのは不本意だが……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>255</PointerOffset>
      <JapaneseText>中立の立場の人間がいた方が、後々</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>289</PointerOffset>
      <JapaneseText>いらぬ面倒事を起こさずに済むからな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>333</PointerOffset>
      <JapaneseText>それと言っておくが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>355</PointerOffset>
      <JapaneseText>この事は他言無用だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>374</PointerOffset>
      <JapaneseText>万一、誰かに漏らす様な事があれば……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>412</PointerOffset>
      <JapaneseText>わかっているな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>438</PointerOffset>
      <JapaneseText>……話は終わりだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>458</PointerOffset>
      <JapaneseText>行くぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
