<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>616</PointerOffset>
      <JapaneseText>圏</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1264</PointerOffset>
      <JapaneseText>夾</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1987</PointerOffset>
      <JapaneseText>光の中に、世界が見える。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2013</PointerOffset>
      <JapaneseText>たくさんの世界が…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2032</PointerOffset>
      <JapaneseText>私がいる。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2044</PointerOffset>
      <JapaneseText>色んな世界に色んな私が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>123</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2067</PointerOffset>
      <JapaneseText>こんなにたくさん…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2086</PointerOffset>
      <JapaneseText>世界樹は、新しい種子を実らせる時、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>125</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2122</PointerOffset>
      <JapaneseText>自らの経験を全て種子へと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2148</PointerOffset>
      <JapaneseText>受け継がせている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>127</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2165</PointerOffset>
      <JapaneseText>無論、これらの世界の経験は全て、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>128</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2199</PointerOffset>
      <JapaneseText>ルミナシアへと受け継がれているのだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>129</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2236</PointerOffset>
      <JapaneseText>そして、「今」があるのだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2264</PointerOffset>
      <JapaneseText>カノンノも、ディセンダーも、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>131</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2294</PointerOffset>
      <JapaneseText>アドリビトムの皆も…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>132</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2315</PointerOffset>
      <JapaneseText>うん、わかるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>133</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2330</PointerOffset>
      <JapaneseText>私の中にあった記憶は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>134</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2354</PointerOffset>
      <JapaneseText>これらの世界のもの。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>135</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2376</PointerOffset>
      <JapaneseText>今、ずっと遡ってる…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>136</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2397</PointerOffset>
      <JapaneseText>こんなにずっと、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>137</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2415</PointerOffset>
      <JapaneseText>世界は続いて来たんだ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>138</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2438</PointerOffset>
      <JapaneseText>あの光は…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>139</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2451</PointerOffset>
      <JapaneseText>カノンノ…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2465</PointerOffset>
      <JapaneseText>あれが、そなたの記憶の最果てだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>141</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2496</PointerOffset>
      <JapaneseText>この光、とても懐かしい感じがする…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>142</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2538</PointerOffset>
      <JapaneseText>広い宙の中に瞬く命。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>144</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2560</PointerOffset>
      <JapaneseText>受け継がれた記憶の最も先にいる者。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>145</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2596</PointerOffset>
      <JapaneseText>ようこそ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>146</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2614</PointerOffset>
      <JapaneseText>あな…たは？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>148</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2627</PointerOffset>
      <JapaneseText>私はカノンノ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>149</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2643</PointerOffset>
      <JapaneseText>根源の世界樹…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>150</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2658</PointerOffset>
      <JapaneseText>何と…、根源の世界樹とは…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>151</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2685</PointerOffset>
      <JapaneseText>そなたから、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>152</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2699</PointerOffset>
      <JapaneseText>全てが受け継がれて来たのか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>153</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2728</PointerOffset>
      <JapaneseText>そうよ…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>154</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2740</PointerOffset>
      <JapaneseText>私の世界は地殻の変動で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>155</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2764</PointerOffset>
      <JapaneseText>滅びを避けられなかった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>156</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2787</PointerOffset>
      <JapaneseText>だから、私達の世界の住人は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>157</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2817</PointerOffset>
      <JapaneseText>全ての生命を一つにして、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>158</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2843</PointerOffset>
      <JapaneseText>新しい生命の姿をとったの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>159</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2868</PointerOffset>
      <JapaneseText>全てを一つに？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>160</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2883</PointerOffset>
      <JapaneseText>そう、全ての生命を一つにして、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>161</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2915</PointerOffset>
      <JapaneseText>また新しい世界を生み出すの。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>162</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2945</PointerOffset>
      <JapaneseText>世界樹となってね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>163</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2964</PointerOffset>
      <JapaneseText>私はその器、種子になる為に選ばれたの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>164</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3001</PointerOffset>
      <JapaneseText>命を繋げる為に、世界の全ての命を宿して、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>165</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3043</PointerOffset>
      <JapaneseText>世界樹という新しい生命の種子に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>166</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3075</PointerOffset>
      <JapaneseText>なったのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>167</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3086</PointerOffset>
      <JapaneseText>命を繋げる…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>168</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3099</PointerOffset>
      <JapaneseText>そう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>169</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3107</PointerOffset>
      <JapaneseText>それは新しい可能性</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>170</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3126</PointerOffset>
      <JapaneseText>たくさんたくさん、世界を生んだわ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>171</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3162</PointerOffset>
      <JapaneseText>色んな世界を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>172</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3175</PointerOffset>
      <JapaneseText>世界にある、様々な命と共に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>173</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3205</PointerOffset>
      <JapaneseText>学びながらね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>174</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3218</PointerOffset>
      <JapaneseText>世界が一つだと寂しいから。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>175</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3246</PointerOffset>
      <JapaneseText>一緒に助け合って行ける様に。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>176</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3276</PointerOffset>
      <JapaneseText>共に創造していく為に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>177</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3297</PointerOffset>
      <JapaneseText>多くの世界を生んだのよ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>178</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3322</PointerOffset>
      <JapaneseText>助け合っていける…様に…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>179</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3347</PointerOffset>
      <JapaneseText>世界樹から生まれた世界は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>180</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3375</PointerOffset>
      <JapaneseText>みんな私の世界から生まれた尊い家族</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>181</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3410</PointerOffset>
      <JapaneseText>じゃあ、ルミナシアとジルディアが…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>182</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3446</PointerOffset>
      <JapaneseText>相容れないのは？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>183</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3463</PointerOffset>
      <JapaneseText>理を大きく離れた生き方を選ぶのも、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>184</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3499</PointerOffset>
      <JapaneseText>また世界の選択。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>185</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3517</PointerOffset>
      <JapaneseText>だが、根本は繋がっているのだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>186</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3546</PointerOffset>
      <JapaneseText>さあ、そろそろ戻りなさい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>187</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3574</PointerOffset>
      <JapaneseText>あなた達は戻って「今」を生きなければ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>188</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3611</PointerOffset>
      <JapaneseText>あなたは私。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>189</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3625</PointerOffset>
      <JapaneseText>私の記憶、世界の記憶が受け継がれた証</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>190</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3662</PointerOffset>
      <JapaneseText>…世界もあなたも、たくさんの受け継がれた</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>191</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3704</PointerOffset>
      <JapaneseText>命の上にあるのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>192</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3721</PointerOffset>
      <JapaneseText>生きなさい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>193</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3735</PointerOffset>
      <JapaneseText>最後に、ルミナシアの創世を見せてあげるわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>194</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>