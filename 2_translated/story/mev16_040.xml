<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>332</PointerOffset>
      <JapaneseText>到</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>998</PointerOffset>
      <JapaneseText>搜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1559</PointerOffset>
      <JapaneseText>クレア！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1568</PointerOffset>
      <JapaneseText>ヴェイグ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1579</PointerOffset>
      <JapaneseText>ようこそ、アドリビトムの諸君。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1611</PointerOffset>
      <JapaneseText>そして久しぶり、ヴェイグ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1636</PointerOffset>
      <JapaneseText>サレ…！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1645</PointerOffset>
      <JapaneseText>やっと会えたね、ヴェイグ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1670</PointerOffset>
      <JapaneseText>ごめんなさい…、ヴェイグ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>114</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1698</PointerOffset>
      <JapaneseText>街に出ていた間に見つかって…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1727</PointerOffset>
      <JapaneseText>ねえ、クレアちゃん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1749</PointerOffset>
      <JapaneseText>あんな場所で会うなんて、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1775</PointerOffset>
      <JapaneseText>本当に運命を感じたよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1796</PointerOffset>
      <JapaneseText>クレアちゃんには、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>119</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1816</PointerOffset>
      <JapaneseText>キミ達をおびき寄せる餌に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1842</PointerOffset>
      <JapaneseText>なってもらおうと思ってね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1867</PointerOffset>
      <JapaneseText>でも、まさかキミ達の方…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1895</PointerOffset>
      <JapaneseText>しかもヴェイグが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>123</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1913</PointerOffset>
      <JapaneseText>やって来てくれるなんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1936</PointerOffset>
      <JapaneseText>ますます運命を感じちゃうなあ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>125</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1968</PointerOffset>
      <JapaneseText>キミ達が働き手を奪ってくれたせいで、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2006</PointerOffset>
      <JapaneseText>星晶の採掘がストップしてしまったよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>127</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2041</PointerOffset>
      <JapaneseText>さあ、言うんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>128</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2059</PointerOffset>
      <JapaneseText>大事な労働資源をどこに連れて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>129</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2089</PointerOffset>
      <JapaneseText>行ったんだい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2104</PointerOffset>
      <JapaneseText>素直に話してくれないと、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>131</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2130</PointerOffset>
      <JapaneseText>ヘーゼル村イチバンの上玉、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>132</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2158</PointerOffset>
      <JapaneseText>クレアちゃんに傷が付いちゃうよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>133</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2191</PointerOffset>
      <JapaneseText>星晶の採掘が、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>134</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2207</PointerOffset>
      <JapaneseText>世界の危機を招いているんですよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>135</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2240</PointerOffset>
      <JapaneseText>今は国も、種族も奪い合うのを止めて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>136</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2276</PointerOffset>
      <JapaneseText>世界を守る為に心を一つにすべき時です</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>137</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2313</PointerOffset>
      <JapaneseText>だったら僕は、それを踏みにじるまでさ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>138</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2353</PointerOffset>
      <JapaneseText>僕は人の心の力なんて信用しないからね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>139</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2392</PointerOffset>
      <JapaneseText>本当は、働き手なんてどうでもいい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2428</PointerOffset>
      <JapaneseText>そんなものはどこでも手に入る</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>141</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2457</PointerOffset>
      <JapaneseText>ただね、キミ達アドリビトムを</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>142</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2487</PointerOffset>
      <JapaneseText>つぶしたいってのはあるんだよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>143</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2518</PointerOffset>
      <JapaneseText>アドリビトム…自由という意味だっけかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>144</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2560</PointerOffset>
      <JapaneseText>自由ってのはどういう事か知ってるかい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>145</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2599</PointerOffset>
      <JapaneseText>自由にはね、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>146</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2613</PointerOffset>
      <JapaneseText>常に多くの危険が付きまとうものさ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>147</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2649</PointerOffset>
      <JapaneseText>気の休まる暇のない危険がね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>148</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2676</PointerOffset>
      <JapaneseText>僕がキミ達をその危険から救ってあげるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>149</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2715</PointerOffset>
      <JapaneseText>自由の意味は人それぞれが決める、と言う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>150</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2754</PointerOffset>
      <JapaneseText>自由を危険と解釈するのもお前の自由、と言う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>151</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2797</PointerOffset>
      <JapaneseText>要するに、手下になれってか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>152</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2826</PointerOffset>
      <JapaneseText>不自由を手に入れて得る、平穏な日々なんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>153</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2868</PointerOffset>
      <JapaneseText>退屈でしゃーねぇなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>154</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2889</PointerOffset>
      <JapaneseText>ああ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>155</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2897</PointerOffset>
      <JapaneseText>自由が危険なんて、オレは思わねーし、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>156</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2935</PointerOffset>
      <JapaneseText>危険だからって誰かに自由を預けられっか！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>157</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2976</PointerOffset>
      <JapaneseText>オレぁ、自由を捨ててテメェに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>158</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3006</PointerOffset>
      <JapaneseText>力を頼って生きる方が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>159</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3028</PointerOffset>
      <JapaneseText>よっぽどあぶねー気がするぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>160</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3055</PointerOffset>
      <JapaneseText>誰かに力を頼る程、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>161</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3075</PointerOffset>
      <JapaneseText>その相手の力に支配されて、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>162</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3103</PointerOffset>
      <JapaneseText>自由を奪われんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>163</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3120</PointerOffset>
      <JapaneseText>オレ達アドリビトムが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>164</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3144</PointerOffset>
      <JapaneseText>てめーなんかに屈するかよ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>165</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3173</PointerOffset>
      <JapaneseText>フフフ、そうかい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>166</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3190</PointerOffset>
      <JapaneseText>いいね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>167</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3200</PointerOffset>
      <JapaneseText>キミ達程の強い心を踏みにじるのは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>168</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3236</PointerOffset>
      <JapaneseText>とても楽しそうだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>169</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3253</PointerOffset>
      <JapaneseText>…じゃあ、本気で叩き潰す事にするよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>170</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3288</PointerOffset>
      <JapaneseText>キミ達の……ヒトの心をね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>171</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3315</PointerOffset>
      <JapaneseText>面白ぇ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>172</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3325</PointerOffset>
      <JapaneseText>ケツから蹴り上げてやるぜ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>173</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3352</PointerOffset>
      <JapaneseText>待ってろ、クレア…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>174</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>