<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>642</PointerOffsetDec>
    <PointerOffsetHex>0x282</PointerOffsetHex>
    <JapaneseText>霊峰アブソールの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>660</PointerOffsetDec>
    <PointerOffsetHex>0x294</PointerOffsetHex>
    <JapaneseText>「霊峰」って、どういう意味なんだ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>695</PointerOffsetDec>
    <PointerOffsetHex>0x2b7</PointerOffsetHex>
    <JapaneseText>信仰の対象となったり、神仏をまつった</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>733</PointerOffsetDec>
    <PointerOffsetHex>0x2dd</PointerOffsetHex>
    <JapaneseText>山の事をそう呼ぶみたいだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>760</PointerOffsetDec>
    <PointerOffsetHex>0x2f8</PointerOffsetHex>
    <JapaneseText>……そうなのか。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>778</PointerOffsetDec>
    <PointerOffsetHex>0x30a</PointerOffsetHex>
    <JapaneseText>なるほどな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>789</PointerOffsetDec>
    <PointerOffsetHex>0x315</PointerOffsetHex>
    <JapaneseText>カイウス、「霊峰」の「霊」を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>819</PointerOffsetDec>
    <PointerOffsetHex>0x333</PointerOffsetHex>
    <JapaneseText>お化けの「霊」と勘違いしたんでしょう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>856</PointerOffsetDec>
    <PointerOffsetHex>0x358</PointerOffsetHex>
    <JapaneseText>そ、そんなわけあるか！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>879</PointerOffsetDec>
    <PointerOffsetHex>0x36f</PointerOffsetHex>
    <JapaneseText>そうかしら。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>893</PointerOffsetDec>
    <PointerOffsetHex>0x37d</PointerOffsetHex>
    <JapaneseText>じゃあどうして、わざわざ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>919</PointerOffsetDec>
    <PointerOffsetHex>0x397</PointerOffsetHex>
    <JapaneseText>霊峰の意味を聞いたりしたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>948</PointerOffsetDec>
    <PointerOffsetHex>0x3b4</PointerOffsetHex>
    <JapaneseText>……普通に、疑問に思ったんだよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>982</PointerOffsetDec>
    <PointerOffsetHex>0x3d6</PointerOffsetHex>
    <JapaneseText>別におかしくないだろ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1003</PointerOffsetDec>
    <PointerOffsetHex>0x3eb</PointerOffsetHex>
    <JapaneseText>いつも、授業をサボったり</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1029</PointerOffsetDec>
    <PointerOffsetHex>0x405</PointerOffsetHex>
    <JapaneseText>居眠りしたりしてるのに？　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1057</PointerOffsetDec>
    <PointerOffsetHex>0x421</PointerOffsetHex>
    <JapaneseText>今だけ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1066</PointerOffsetDec>
    <PointerOffsetHex>0x42a</PointerOffsetHex>
    <JapaneseText>何だよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1076</PointerOffsetDec>
    <PointerOffsetHex>0x434</PointerOffsetHex>
    <JapaneseText>いちいち引っかかる言い方だな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1105</PointerOffsetDec>
    <PointerOffsetHex>0x451</PointerOffsetHex>
    <JapaneseText>引っかかってるのはカイウスでしょう？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1143</PointerOffsetDec>
    <PointerOffsetHex>0x477</PointerOffsetHex>
    <JapaneseText>ちょっとは素直に、お化けが怖いって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1179</PointerOffsetDec>
    <PointerOffsetHex>0x49b</PointerOffsetHex>
    <JapaneseText>認めたらどうなのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1198</PointerOffsetDec>
    <PointerOffsetHex>0x4ae</PointerOffsetHex>
    <JapaneseText>だから、違うって……！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1221</PointerOffsetDec>
    <PointerOffsetHex>0x4c5</PointerOffsetHex>
    <JapaneseText>まあまあ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1233</PointerOffsetDec>
    <PointerOffsetHex>0x4d1</PointerOffsetHex>
    <JapaneseText>もう、その話はいいじゃないか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1262</PointerOffsetDec>
    <PointerOffsetHex>0x4ee</PointerOffsetHex>
    <JapaneseText>喧嘩する様な話じゃないだろう？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1294</PointerOffsetDec>
    <PointerOffsetHex>0x50e</PointerOffsetHex>
    <JapaneseText>二人とも、もう少し素直に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1320</PointerOffsetDec>
    <PointerOffsetHex>0x528</PointerOffsetHex>
    <JapaneseText>相手の話を聞かないとな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
</Entries>