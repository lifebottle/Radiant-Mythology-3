<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>470</PointerOffset>
    <JapaneseText>身軽なもんだな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>488</PointerOffset>
    <JapaneseText>忍びなら、これくらいの仕掛けは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>520</PointerOffset>
    <JapaneseText>朝飯前ってわけか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>537</PointerOffset>
    <JapaneseText>当然です。　これくらいで動揺したり</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>573</PointerOffset>
    <JapaneseText>足止めを食らっている様では</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>601</PointerOffset>
    <JapaneseText>とてもみんなを束ねる忍びにはなれません</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>640</PointerOffset>
    <JapaneseText>へえ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>648</PointerOffset>
    <JapaneseText>すずは将来、忍びの頭領にでもなるのか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>685</PointerOffset>
    <JapaneseText>いえ……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>697</PointerOffset>
    <JapaneseText>まだそうと決まったわけでは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>724</PointerOffset>
    <JapaneseText>いいじゃねえか。　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>744</PointerOffset>
    <JapaneseText>胸張って「はい、そうです」って言っとけよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>785</PointerOffset>
    <JapaneseText>知ってるか？　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>801</PointerOffset>
    <JapaneseText>人間の脳ってのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>819</PointerOffset>
    <JapaneseText>発した言葉が一種の自己暗示になって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>854</PointerOffset>
    <JapaneseText>言葉どおりの未来を実現しようと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>886</PointerOffset>
    <JapaneseText>影に日向に働いているらしいぜ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>915</PointerOffset>
    <JapaneseText>本人の意志に関係なく、な</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>940</PointerOffset>
    <JapaneseText>それは……言霊ですか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>963</PointerOffset>
    <JapaneseText>つまり、口にした言葉が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>987</PointerOffset>
    <JapaneseText>現実になる、という……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1010</PointerOffset>
    <JapaneseText>ああ、そういうもんかもな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1035</PointerOffset>
    <JapaneseText>言うだけならタダなんだ、だったら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1069</PointerOffset>
    <JapaneseText>夢だのなんだの、前向きな言葉は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1101</PointerOffset>
    <JapaneseText>積極的に口にしたっていいんじゃねえか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1140</PointerOffset>
    <JapaneseText>そういう考え方もあるんですね……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1173</PointerOffset>
    <JapaneseText>言葉の力……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1189</PointerOffset>
    <JapaneseText>勉強になります</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1204</PointerOffset>
    <JapaneseText>ま、あくまで一つの例だよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1229</PointerOffset>
    <JapaneseText>病は気からとも言うし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1251</PointerOffset>
    <JapaneseText>何事も気の持ち様って事かもな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
