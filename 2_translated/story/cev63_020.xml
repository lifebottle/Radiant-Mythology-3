<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>516</PointerOffsetDec>
    <PointerOffsetHex>0x204</PointerOffsetHex>
    <JapaneseText>お前の生徒はくせ者ばかりだな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>40</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>548</PointerOffsetDec>
    <PointerOffsetHex>0x224</PointerOffsetHex>
    <JapaneseText>苦労をするだろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>41</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>565</PointerOffsetDec>
    <PointerOffsetHex>0x235</PointerOffsetHex>
    <JapaneseText>ええ、そうね……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>42</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>585</PointerOffsetDec>
    <PointerOffsetHex>0x249</PointerOffsetHex>
    <JapaneseText>だからこそ、やりがいもあるのだけれど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>622</PointerOffsetDec>
    <PointerOffsetHex>0x26e</PointerOffsetHex>
    <JapaneseText>なぜそうまで入れ込む？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>646</PointerOffsetDec>
    <PointerOffsetHex>0x286</PointerOffsetHex>
    <JapaneseText>あきらめれば全てそこで片が付くだろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>683</PointerOffsetDec>
    <PointerOffsetHex>0x2ab</PointerOffsetHex>
    <JapaneseText>教師というのは、そういうものなのよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>721</PointerOffsetDec>
    <PointerOffsetHex>0x2d1</PointerOffsetHex>
    <JapaneseText>いえ、そういう存在であるべきと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>753</PointerOffsetDec>
    <PointerOffsetHex>0x2f1</PointerOffsetHex>
    <JapaneseText>言ってもいいかもしれないわね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>782</PointerOffsetDec>
    <PointerOffsetHex>0x30e</PointerOffsetHex>
    <JapaneseText>フン、随分と損な仕事だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>805</PointerOffsetDec>
    <PointerOffsetHex>0x325</PointerOffsetHex>
    <JapaneseText>そうかしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>818</PointerOffsetDec>
    <PointerOffsetHex>0x332</PointerOffsetHex>
    <JapaneseText>そうだろう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>832</PointerOffsetDec>
    <PointerOffsetHex>0x340</PointerOffsetHex>
    <JapaneseText>少なくとも戦場では</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>852</PointerOffsetDec>
    <PointerOffsetHex>0x354</PointerOffsetHex>
    <JapaneseText>弱い者が一番初めに淘汰される</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>881</PointerOffsetDec>
    <PointerOffsetHex>0x371</PointerOffsetHex>
    <JapaneseText>確かに、弱い人間を育てずに、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>911</PointerOffsetDec>
    <PointerOffsetHex>0x38f</PointerOffsetHex>
    <JapaneseText>放っておくという事も出来るでしょうね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>948</PointerOffsetDec>
    <PointerOffsetHex>0x3b4</PointerOffsetHex>
    <JapaneseText>でも、それでは遠からず</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>972</PointerOffsetDec>
    <PointerOffsetHex>0x3cc</PointerOffsetHex>
    <JapaneseText>社会も、そして世界も死んでいくわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1005</PointerOffsetDec>
    <PointerOffsetHex>0x3ed</PointerOffsetHex>
    <JapaneseText>ほう？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1012</PointerOffsetDec>
    <PointerOffsetHex>0x3f4</PointerOffsetHex>
    <JapaneseText>ここは戦場ではないの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1036</PointerOffsetDec>
    <PointerOffsetHex>0x40c</PointerOffsetHex>
    <JapaneseText>基準から漏れた者を弾けばいいというのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1076</PointerOffsetDec>
    <PointerOffsetHex>0x434</PointerOffsetHex>
    <JapaneseText>ただ都合の悪い存在を間引いているだけ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1113</PointerOffsetDec>
    <PointerOffsetHex>0x459</PointerOffsetHex>
    <JapaneseText>教育とは全く主旨の異なる考えだわ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1149</PointerOffsetDec>
    <PointerOffsetHex>0x47d</PointerOffsetHex>
    <JapaneseText>教師の仕事は、人を育てる事</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1176</PointerOffsetDec>
    <PointerOffsetHex>0x498</PointerOffsetHex>
    <JapaneseText>だから生徒にも真正面から向き合うの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1211</PointerOffsetDec>
    <PointerOffsetHex>0x4bb</PointerOffsetHex>
    <JapaneseText>育てる事と間引く事……か</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1236</PointerOffsetDec>
    <PointerOffsetHex>0x4d4</PointerOffsetHex>
    <JapaneseText>フン……やはり教師など損な仕事だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1269</PointerOffsetDec>
    <PointerOffsetHex>0x4f5</PointerOffsetHex>
    <JapaneseText>そうかもしれないわね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1293</PointerOffsetDec>
    <PointerOffsetHex>0x50d</PointerOffsetHex>
    <JapaneseText>でも、あなたも存外</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1313</PointerOffsetDec>
    <PointerOffsetHex>0x521</PointerOffsetHex>
    <JapaneseText>同じ性分なのではなくて？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1338</PointerOffsetDec>
    <PointerOffsetHex>0x53a</PointerOffsetHex>
    <JapaneseText>さあ。　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1348</PointerOffsetDec>
    <PointerOffsetHex>0x544</PointerOffsetHex>
    <JapaneseText>知らんな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
</Entries>