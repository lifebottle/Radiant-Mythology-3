<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>34</PointerOffset>
      <JapaneseText>棄</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>82</PointerOffset>
      <JapaneseText>満</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>932</PointerOffset>
      <JapaneseText>白い三兄弟達は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>950</PointerOffset>
      <JapaneseText>みんな自分の場所へ戻れたんだね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>981</PointerOffset>
      <JapaneseText>しかし、今度はそれぞれ色の違う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1013</PointerOffset>
      <JapaneseText>三つの玉がやって来たか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1050</PointerOffset>
      <JapaneseText>この玉も、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1062</PointerOffset>
      <JapaneseText>何か言っているのかなぁ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1087</PointerOffset>
      <JapaneseText>うむ、では呼びかけてみるとしよう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1120</PointerOffset>
      <JapaneseText>………ふむ、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1134</PointerOffset>
      <JapaneseText>これらの玉は今までのものよりも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1166</PointerOffset>
      <JapaneseText>強い意思を放っている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1187</PointerOffset>
      <JapaneseText>どうやら、この三色の玉を作成した者の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1225</PointerOffset>
      <JapaneseText>意思らしい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1236</PointerOffset>
      <JapaneseText>これらの玉が収まるべき場所は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1266</PointerOffset>
      <JapaneseText>霊峰アブソール…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1283</PointerOffset>
      <JapaneseText>そして、開かれた道の奥に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1311</PointerOffset>
      <JapaneseText>何かが隠されている様だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1334</PointerOffset>
      <JapaneseText>お宝かな…、と聞く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1353</PointerOffset>
      <JapaneseText>また玉かな…、と聞く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1374</PointerOffset>
      <JapaneseText>深くは語ってくれぬが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1398</PointerOffset>
      <JapaneseText>これらの玉が訴えるその場には</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1428</PointerOffset>
      <JapaneseText>何か畏敬の念の様なものを感じるな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1461</PointerOffset>
      <JapaneseText>いや、そうではない様だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1484</PointerOffset>
      <JapaneseText>これらの玉が訴えるその場に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1514</PointerOffset>
      <JapaneseText>とてつもなく価値のあるものが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1544</PointerOffset>
      <JapaneseText>待っているかもしれん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1565</PointerOffset>
      <JapaneseText>駄目よ、そんな大事な話を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1591</PointerOffset>
      <JapaneseText>リーダーに内緒にしちゃあ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1616</PointerOffset>
      <JapaneseText>わたしがいるって事は、ここは教会も同然。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1658</PointerOffset>
      <JapaneseText>許してあげるから、今の事を全て</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1690</PointerOffset>
      <JapaneseText>洗いざらい懺悔なさい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1713</PointerOffset>
      <JapaneseText>懺悔の必要性を聞く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1732</PointerOffset>
      <JapaneseText>自分が何をしたのか、しばらく考える</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1767</PointerOffset>
      <JapaneseText>あなたは、わたし…リーダーに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1797</PointerOffset>
      <JapaneseText>隠し事をしようとしているよね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1829</PointerOffset>
      <JapaneseText>さ、白状して？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1844</PointerOffset>
      <JapaneseText>もう一度言った方がいいのかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1876</PointerOffset>
      <JapaneseText>大事な話はリーダーにちゃんと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1906</PointerOffset>
      <JapaneseText>話さなくっちゃでしょ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1927</PointerOffset>
      <JapaneseText>でも、まあ、何となく話は聞いてたんだけど。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1971</PointerOffset>
      <JapaneseText>もちろん、行くよね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1992</PointerOffset>
      <JapaneseText>どこに行けと…、と問う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2015</PointerOffset>
      <JapaneseText>結局懺悔の必要は無かった、と言う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2048</PointerOffset>
      <JapaneseText>ウフフ、霊峰アブソールよ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2075</PointerOffset>
      <JapaneseText>そんな事ないわよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2095</PointerOffset>
      <JapaneseText>わたしを差し置いて行動しようとした事は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2135</PointerOffset>
      <JapaneseText>ちゃんと天に、世界樹に懺悔してくれなくちゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2178</PointerOffset>
      <JapaneseText>ま、それはおいといて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2199</PointerOffset>
      <JapaneseText>その三つの玉を収めに行くって依頼、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2235</PointerOffset>
      <JapaneseText>わたしが出すから。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2255</PointerOffset>
      <JapaneseText>受けてちょうだいね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>