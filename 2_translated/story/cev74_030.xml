<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>458</PointerOffset>
      <JapaneseText>傲</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>586</PointerOffset>
      <JapaneseText>お空の光……綺麗</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>603</PointerOffset>
      <JapaneseText>そうね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>613</PointerOffset>
      <JapaneseText>綺麗ね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>620</PointerOffset>
      <JapaneseText>ねえ、セルシウス。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>640</PointerOffset>
      <JapaneseText>どうして、お日様の光は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>664</PointerOffset>
      <JapaneseText>あんなに綺麗なんだろう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>689</PointerOffset>
      <JapaneseText>それは、世界樹が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>707</PointerOffset>
      <JapaneseText>この世界を愛しているからよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>734</PointerOffset>
      <JapaneseText>……愛？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>743</PointerOffset>
      <JapaneseText>目の前の綺麗なものを、誰かに見せたい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>783</PointerOffset>
      <JapaneseText>ソフィは、そう思った事はない？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>814</PointerOffset>
      <JapaneseText>ある。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>822</PointerOffset>
      <JapaneseText>アスベルと、シェリアに。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>848</PointerOffset>
      <JapaneseText>喜んで欲しいから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>865</PointerOffset>
      <JapaneseText>同じ事なのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>878</PointerOffset>
      <JapaneseText>世界樹は、あなた達ヒトに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>904</PointerOffset>
      <JapaneseText>綺麗なものを見せたかったのだと思うの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>941</PointerOffset>
      <JapaneseText>世界樹も、ヒトに喜んで欲しかったの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>978</PointerOffset>
      <JapaneseText>ええ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>986</PointerOffset>
      <JapaneseText>喜びを知ったヒトは、誰かに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1014</PointerOffset>
      <JapaneseText>その喜びをわけたくなる……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1041</PointerOffset>
      <JapaneseText>その思いの連鎖が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1059</PointerOffset>
      <JapaneseText>優しさを育み、幸福を生んで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1087</PointerOffset>
      <JapaneseText>やがて、世界に平和を紡ぐの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1114</PointerOffset>
      <JapaneseText>そうして編まれた未来の一つに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1144</PointerOffset>
      <JapaneseText>わたし達がいるのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1163</PointerOffset>
      <JapaneseText>……世界樹って、すごいんだね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1192</PointerOffset>
      <JapaneseText>アスベルにも、教えてあげなきゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>