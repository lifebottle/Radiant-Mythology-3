<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>789</PointerOffset>
      <JapaneseText>お招きどうもありがとさん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>814</PointerOffset>
      <JapaneseText>気持ちは変わらないのか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>837</PointerOffset>
      <JapaneseText>何度も言わせんなって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>858</PointerOffset>
      <JapaneseText>お前には、お前の。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>878</PointerOffset>
      <JapaneseText>オレには、オレのやり方がある。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>910</PointerOffset>
      <JapaneseText>そんだけの話だろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>927</PointerOffset>
      <JapaneseText>万人が等しく扱われる法秩序を築く……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>967</PointerOffset>
      <JapaneseText>それが僕の願いであり、目的だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>996</PointerOffset>
      <JapaneseText>だけどユーリ、君だって思いは同じはず。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1036</PointerOffset>
      <JapaneseText>だからこそ、一度は騎士団の門を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1068</PointerOffset>
      <JapaneseText>叩いたんだろう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1085</PointerOffset>
      <JapaneseText>じゃあ、お前、騎士団に入って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1115</PointerOffset>
      <JapaneseText>何か変わったのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1135</PointerOffset>
      <JapaneseText>変えられたのかよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1154</PointerOffset>
      <JapaneseText>確かに、まだやりきれていない事はある。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1194</PointerOffset>
      <JapaneseText>だからと言って君の様に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1220</PointerOffset>
      <JapaneseText>人が人を裁く道を肯定する事は出来ない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1257</PointerOffset>
      <JapaneseText>認めるわけにはいかないんだ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1288</PointerOffset>
      <JapaneseText>……もう一度聞く。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1308</PointerOffset>
      <JapaneseText>それでも、気持ちは変わらないか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1341</PointerOffset>
      <JapaneseText>いい加減、この問答にも飽きてきたな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1376</PointerOffset>
      <JapaneseText>そうか……ならば</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1393</PointerOffset>
      <JapaneseText>こうするより他に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1411</PointerOffset>
      <JapaneseText>道はないという事だな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1432</PointerOffset>
      <JapaneseText>そういうこったな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1449</PointerOffset>
      <JapaneseText>んじゃ、行くぜ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>