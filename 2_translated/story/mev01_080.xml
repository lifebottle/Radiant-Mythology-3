<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>982</PointerOffset>
      <JapaneseText>さて、あなた達。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1000</PointerOffset>
      <JapaneseText>もし良かったら、話を聞いてもいい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1035</PointerOffset>
      <JapaneseText>その前に自己紹介をしなくちゃね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1066</PointerOffset>
      <JapaneseText>わたしは、アンジュ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1088</PointerOffset>
      <JapaneseText>このギルド、アドリビトムのリーダーよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1125</PointerOffset>
      <JapaneseText>申し遅れました。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1143</PointerOffset>
      <JapaneseText>わたしは、ガルバンゾ国の王女エステリーゼ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1187</PointerOffset>
      <JapaneseText>エステル、って呼んで下さい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1214</PointerOffset>
      <JapaneseText>ええと、あなたの名は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1236</PointerOffset>
      <JapaneseText>○○でしたね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1249</PointerOffset>
      <JapaneseText>助けていただいて、ありがとうございます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1288</PointerOffset>
      <JapaneseText>あたしは、リタ・モルディオ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1315</PointerOffset>
      <JapaneseText>オレはユーリ・ローウェル。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1343</PointerOffset>
      <JapaneseText>ガルバンゾ国のギルドの者だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1370</PointerOffset>
      <JapaneseText>今は、このお姫様に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1390</PointerOffset>
      <JapaneseText>旅の護衛として雇われている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1417</PointerOffset>
      <JapaneseText>あなたの様な大国の王族がなぜ、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1449</PointerOffset>
      <JapaneseText>ギルドを雇って旅なんてしているの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1484</PointerOffset>
      <JapaneseText>コンフェイト大森林には、ガルバンゾ国の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1524</PointerOffset>
      <JapaneseText>星晶採掘地があるんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1547</PointerOffset>
      <JapaneseText>現在、ウリズン帝国とその土地をめぐって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1587</PointerOffset>
      <JapaneseText>緊張状態なのですが…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1608</PointerOffset>
      <JapaneseText>その採掘地で今、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1626</PointerOffset>
      <JapaneseText>変な現象が起こっているという話を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1660</PointerOffset>
      <JapaneseText>聞いたんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1673</PointerOffset>
      <JapaneseText>変な現象？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1684</PointerOffset>
      <JapaneseText>採掘が行われた土地の生物が、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1714</PointerOffset>
      <JapaneseText>変化してるって話よ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1733</PointerOffset>
      <JapaneseText>どんな変化が起こってるかってのは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1769</PointerOffset>
      <JapaneseText>明確にわかってるわけじゃないけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1803</PointerOffset>
      <JapaneseText>採掘者の間でそんな話が上がってて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1836</PointerOffset>
      <JapaneseText>それを聞いた学者が「土地にある星晶を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1874</PointerOffset>
      <JapaneseText>取り過ぎたせいではないか」って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1906</PointerOffset>
      <JapaneseText>仮説を立てて、騒ぎ始めたのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1935</PointerOffset>
      <JapaneseText>でも、それに対して国の評議会は何も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1971</PointerOffset>
      <JapaneseText>調査をせず、ついには世間を騒がせた罪で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2011</PointerOffset>
      <JapaneseText>学者まで逮捕、ってわけ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2034</PointerOffset>
      <JapaneseText>その話を知って、わたし、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2060</PointerOffset>
      <JapaneseText>本当の事が知りたくて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2081</PointerOffset>
      <JapaneseText>国が動かないのなら、まず自分で調査を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2119</PointerOffset>
      <JapaneseText>しようと国を出たんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2142</PointerOffset>
      <JapaneseText>そこまでは良かったんだが。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2170</PointerOffset>
      <JapaneseText>森で迷うわ、あのサレって奴に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2200</PointerOffset>
      <JapaneseText>エステルがさらわれるわでな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2227</PointerOffset>
      <JapaneseText>サレの所属するウリズン帝国と</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2257</PointerOffset>
      <JapaneseText>あなたのガルバンゾ国は、今星晶を巡って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2297</PointerOffset>
      <JapaneseText>緊張状態にあるものね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2320</PointerOffset>
      <JapaneseText>コンフェイト大森林の辺りは、様々な国が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>114</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2360</PointerOffset>
      <JapaneseText>星晶採掘に関わっていますし…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2389</PointerOffset>
      <JapaneseText>早く、問題の採掘地へ急がないと…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2422</PointerOffset>
      <JapaneseText>焦っては、駄目。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2440</PointerOffset>
      <JapaneseText>まず、あなた達には休養が必要よ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2471</PointerOffset>
      <JapaneseText>わたし達も何か手伝えそうだから、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>119</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2505</PointerOffset>
      <JapaneseText>今はゆっくりくつろいでいて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2532</PointerOffset>
      <JapaneseText>はい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2540</PointerOffset>
      <JapaneseText>そうさせていただきます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>