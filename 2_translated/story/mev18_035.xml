<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>460</PointerOffset>
    <JapaneseText>スタンさん…、とルーティさんは、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>35</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>494</PointerOffset>
    <JapaneseText>なぜアドリビトムへ入ったんですか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>36</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>529</PointerOffset>
    <JapaneseText>フィリアが世界中の人を手助けする</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>563</PointerOffset>
    <JapaneseText>仕事があるって教えてくれてさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>592</PointerOffset>
    <JapaneseText>それで、俺達も手伝おうって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>620</PointerOffset>
    <JapaneseText>ルーティとアドリビトムへ入ったんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>655</PointerOffset>
    <JapaneseText>（英雄の一人である、フィリアさんからか…）</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>698</PointerOffset>
    <JapaneseText>（けど、フィリアさんもまだ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>728</PointerOffset>
    <JapaneseText>　この世界では英雄になってないな）</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>763</PointerOffset>
    <JapaneseText>戦争で親を失った子ども達の為に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>797</PointerOffset>
    <JapaneseText>俺がしてあげられる事をしたいんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>830</PointerOffset>
    <JapaneseText>その為の資金も稼がなきゃって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>862</PointerOffset>
    <JapaneseText>ルーティが言うしさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>888</PointerOffset>
    <JapaneseText>ええと、孤児院とかですよね…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>920</PointerOffset>
    <JapaneseText>…その、孤児院の…名前は？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>947</PointerOffset>
    <JapaneseText>はは、そこまではまだ決めてないよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>980</PointerOffset>
    <JapaneseText>ただ俺は、困ってる人を助けて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1012</PointerOffset>
    <JapaneseText>子ども達が、明るく元気に育って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1044</PointerOffset>
    <JapaneseText>いってくれたらなって思うんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1073</PointerOffset>
    <JapaneseText>…カイルのヤツも、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1093</PointerOffset>
    <JapaneseText>困ってる人を放って置けない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1121</PointerOffset>
    <JapaneseText>性分なんだよな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1136</PointerOffset>
    <JapaneseText>うん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1144</PointerOffset>
    <JapaneseText>カイルは、優しい子だよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1167</PointerOffset>
    <JapaneseText>あんな風に、子どもがまっすぐ育つ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1201</PointerOffset>
    <JapaneseText>場所にしたいよな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1218</PointerOffset>
    <JapaneseText>はい！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1226</PointerOffset>
    <JapaneseText>スタンさんならきっと出来ます！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
