<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>202</PointerOffset>
    <JapaneseText>あの「願いを叶える」と噂される存在が、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>15</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>242</PointerOffset>
    <JapaneseText>ルバーブ連山にいるらしいの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>16</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>269</PointerOffset>
    <JapaneseText>以前見た時は「赤い煙」だったけれど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>17</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>307</PointerOffset>
    <JapaneseText>スパーダ君から聞いた通り、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>18</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>335</PointerOffset>
    <JapaneseText>今は他の姿をしている可能性があるかもね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>19</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>374</PointerOffset>
    <JapaneseText>出来うる限りその存在に接触して、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>20</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>408</PointerOffset>
    <JapaneseText>調査をしてちょうだい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>21</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>432</PointerOffset>
    <JapaneseText>方法、手段は問わないから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>22</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>457</PointerOffset>
    <JapaneseText>もし道中、その存在を求める民間人に会ったら、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>23</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>503</PointerOffset>
    <JapaneseText>接触させない様に追い返すのよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>24</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>532</PointerOffset>
    <JapaneseText>今回は、ティトレイ君とメルディが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>25</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>566</PointerOffset>
    <JapaneseText>一緒に行ってくれるから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>26</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>589</PointerOffset>
    <JapaneseText>それじゃあ、お願いね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>27</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
