<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>887</PointerOffset>
    <JapaneseText>ねえ、○○。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>901</PointerOffset>
    <JapaneseText>このへこんだ所、ちょうどあの玉が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>935</PointerOffset>
    <JapaneseText>入るサイズみたいだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>956</PointerOffset>
    <JapaneseText>うむ、黒い玉もそこに収まる事を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>988</PointerOffset>
    <JapaneseText>強く願っている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1011</PointerOffset>
    <JapaneseText>黒い玉を置いてやってくれないか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1042</PointerOffset>
    <JapaneseText>扉が開いたよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1057</PointerOffset>
    <JapaneseText>……黒い玉の意思反応が消えた</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1086</PointerOffset>
    <JapaneseText>恐らく、役目を終えたのだろう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1118</PointerOffset>
    <JapaneseText>鍵として、そしてここへの案内人としての</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1158</PointerOffset>
    <JapaneseText>役目を持たされて、生み出されたのだな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1195</PointerOffset>
    <JapaneseText>でも誰が、何の為に？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1216</PointerOffset>
    <JapaneseText>真実はわからぬ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1231</PointerOffset>
    <JapaneseText>だが、モノや場に刻まれた思念を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1263</PointerOffset>
    <JapaneseText>読み取る能力を持った者ならば、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1295</PointerOffset>
    <JapaneseText>黒い玉の意思に感応出来ただろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1326</PointerOffset>
    <JapaneseText>もしかするならば、この先に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1354</PointerOffset>
    <JapaneseText>特別な価値のあるものが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1378</PointerOffset>
    <JapaneseText>隠されているのかもしれん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1403</PointerOffset>
    <JapaneseText>何らかの術者が、それを手にするに相応しい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1445</PointerOffset>
    <JapaneseText>能力を持った者の為に、案内人、鍵として</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1485</PointerOffset>
    <JapaneseText>作ったのだと我々は思うのだが…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1516</PointerOffset>
    <JapaneseText>へぇぇ…、何があるんだろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1543</PointerOffset>
    <JapaneseText>そういえば、白い三兄弟を探せって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1577</PointerOffset>
    <JapaneseText>言ってたね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1591</PointerOffset>
    <JapaneseText>それが「価値のあるもの」なのかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1626</PointerOffset>
    <JapaneseText>じゃあ、次はこの先を進んで</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1654</PointerOffset>
    <JapaneseText>白い三兄弟を探そう！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
