<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>282</PointerOffset>
      <JapaneseText>そうか。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>292</PointerOffset>
      <JapaneseText>突然の申し出にも関わらず</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>318</PointerOffset>
      <JapaneseText>引き受けてくれて嬉しく思う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>345</PointerOffset>
      <JapaneseText>ありがとう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>365</PointerOffset>
      <JapaneseText>彼なら、遺跡でまだ任務に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>391</PointerOffset>
      <JapaneseText>あたっているところだと思うが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>420</PointerOffset>
      <JapaneseText>君がそちらへ向かう旨、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>444</PointerOffset>
      <JapaneseText>私から連絡をしておこう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>467</PointerOffset>
      <JapaneseText>ただ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>483</PointerOffset>
      <JapaneseText>いや……何でもない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>502</PointerOffset>
      <JapaneseText>道中、気を付けて行ってきてくれたまえ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>542</PointerOffset>
      <JapaneseText>吉報を待っているよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
