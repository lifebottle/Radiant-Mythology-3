<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>286</PointerOffset>
      <JapaneseText>○○の格好、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>300</PointerOffset>
      <JapaneseText>よく似合ってるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>317</PointerOffset>
      <JapaneseText>キャラメイクはどうだった？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>344</PointerOffset>
      <JapaneseText>そうそう、今回はキャラメイクが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>376</PointerOffset>
      <JapaneseText>さらにパワーアップしてるのよね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>410</PointerOffset>
      <JapaneseText>く〜っ、ウラヤマしいな〜ったく！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>443</PointerOffset>
      <JapaneseText>キャラメイクのパーツはさらに増加してるし、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>487</PointerOffset>
      <JapaneseText>身長の変更も可能になったらしいな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>520</PointerOffset>
      <JapaneseText>よりバリエーション豊かにキャラクターが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>560</PointerOffset>
      <JapaneseText>作成出来るぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>573</PointerOffset>
      <JapaneseText>それと、装備のパーツ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>597</PointerOffset>
      <JapaneseText>ココ、重要じゃない？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>618</PointerOffset>
      <JapaneseText>シリーズを通じて使用してきた装備も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>654</PointerOffset>
      <JapaneseText>リファインされているそうだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>681</PointerOffset>
      <JapaneseText>今まで遊んでくれたユーザーにも、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>715</PointerOffset>
      <JapaneseText>新鮮な気持ちで楽しんでもらえると思うぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>