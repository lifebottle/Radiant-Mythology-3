<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>412</PointerOffset>
    <JapaneseText>キールはとても優秀ね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>433</PointerOffset>
    <JapaneseText>授業態度も真面目だし、何より</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>31</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>463</PointerOffset>
    <JapaneseText>学ぼうとする意欲がある。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>489</PointerOffset>
    <JapaneseText>とても素晴らしい事よ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>510</PointerOffset>
    <JapaneseText>ありがとう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>34</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>521</PointerOffset>
    <JapaneseText>だが、ぼくは学生として</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>35</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>545</PointerOffset>
    <JapaneseText>当然の務めを果たしているに過ぎない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>36</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>580</PointerOffset>
    <JapaneseText>殊勝な心がけね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>598</PointerOffset>
    <JapaneseText>その言葉、ロイドにも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>620</PointerOffset>
    <JapaneseText>聞かせてあげたいくらいだわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>647</PointerOffset>
    <JapaneseText>ぼくは後世に残る研究成果を残したいんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>689</PointerOffset>
    <JapaneseText>そうすれば、ずっと後の未来の人々は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>725</PointerOffset>
    <JapaneseText>今よりずっと楽な生活を送る事が出来る</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>762</PointerOffset>
    <JapaneseText>あなたの目標は、人々の生活水準を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>796</PointerOffset>
    <JapaneseText>引き上げる事なのね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>815</PointerOffset>
    <JapaneseText>ああ。　人の命も、時間も有限なんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>853</PointerOffset>
    <JapaneseText>長期で合理的な研究プランを立てなくては</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>893</PointerOffset>
    <JapaneseText>とても今の目標になど手が届かない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>926</PointerOffset>
    <JapaneseText>こういう時、本当に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>946</PointerOffset>
    <JapaneseText>時間が無限にあればいいのにと思うよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>983</PointerOffset>
    <JapaneseText>……そうね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>997</PointerOffset>
    <JapaneseText>時間は有効に利用してこそだわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1026</PointerOffset>
    <JapaneseText>ああ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1034</PointerOffset>
    <JapaneseText>全くその通りだ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1051</PointerOffset>
    <JapaneseText>（無限の時間……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1071</PointerOffset>
    <JapaneseText>　そんなものが実現しても、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1099</PointerOffset>
    <JapaneseText>　見えない永遠の牢獄だと思うけれど……）</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
