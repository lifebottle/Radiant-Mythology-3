<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>332</PointerOffset>
      <JapaneseText>ちょっといいかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>353</PointerOffset>
      <JapaneseText>ヴェラトローパの壁画の事、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>381</PointerOffset>
      <JapaneseText>覚えてる？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>392</PointerOffset>
      <JapaneseText>ほら、光まとうものに送りし輝ける光器、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>432</PointerOffset>
      <JapaneseText>「レディアント」の事よ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>455</PointerOffset>
      <JapaneseText>あれから、何度かヴェラトローパへ行って、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>497</PointerOffset>
      <JapaneseText>あの壁画の情報を読んで、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>523</PointerOffset>
      <JapaneseText>レディアントがある場所がわかったの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>558</PointerOffset>
      <JapaneseText>どう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>566</PointerOffset>
      <JapaneseText>そのレディアントを見に行きたくない？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>603</PointerOffset>
      <JapaneseText>これから、レディアントを探しに行く依頼を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>645</PointerOffset>
      <JapaneseText>アンジュへ届けるから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>666</PointerOffset>
      <JapaneseText>興味があったら、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>684</PointerOffset>
      <JapaneseText>クエストカウンタから受けてみて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>