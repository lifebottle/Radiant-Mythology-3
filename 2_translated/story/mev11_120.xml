<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>890</PointerOffset>
    <JapaneseText>あれっ、○○。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>906</PointerOffset>
    <JapaneseText>どうしたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>919</PointerOffset>
    <JapaneseText>アンジュに頼まれて様子を見に来た、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>960</PointerOffset>
    <JapaneseText>心配で見に来た、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>983</PointerOffset>
    <JapaneseText>ああ、心配かけちゃったよね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1010</PointerOffset>
    <JapaneseText>謝りに行かなきゃ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1027</PointerOffset>
    <JapaneseText>ぁ…、それで来てくれたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1054</PointerOffset>
    <JapaneseText>ありがと…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1065</PointerOffset>
    <JapaneseText>でも、もう仕事出来るから。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1093</PointerOffset>
    <JapaneseText>大丈夫</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1100</PointerOffset>
    <JapaneseText>そういえば、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1114</PointerOffset>
    <JapaneseText>ヴェラトローパに行ってきたんだよね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1152</PointerOffset>
    <JapaneseText>どうだった？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1165</PointerOffset>
    <JapaneseText>ありのままの事を話す</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1186</PointerOffset>
    <JapaneseText>戸惑いがちに話す</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1203</PointerOffset>
    <JapaneseText>そう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1211</PointerOffset>
    <JapaneseText>そんなに色んな事がわかったんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1242</PointerOffset>
    <JapaneseText>そうなの。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1254</PointerOffset>
    <JapaneseText>私も、そんなに色んな事があったら、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1290</PointerOffset>
    <JapaneseText>戸惑っちゃうだろうな…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1313</PointerOffset>
    <JapaneseText>でも、今の話の中でも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1335</PointerOffset>
    <JapaneseText>ヒトの祖の話…、すごいなあ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1362</PointerOffset>
    <JapaneseText>世界と共に、創造する為に地上に降りたなんて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1405</PointerOffset>
    <JapaneseText>私達、その「ヒトの祖」の子孫なんだなって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1447</PointerOffset>
    <JapaneseText>思ったら、すごく不思議な気分</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1476</PointerOffset>
    <JapaneseText>不思議で、素敵な事だと思うけど…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1512</PointerOffset>
    <JapaneseText>でも…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1522</PointerOffset>
    <JapaneseText>今の世界はどうなのかな…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1547</PointerOffset>
    <JapaneseText>戦争をしたり、奪い合ったり、欲しがったり。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1591</PointerOffset>
    <JapaneseText>みんながみんな、別々の方向を見てる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1626</PointerOffset>
    <JapaneseText>世界樹は…、寂しがってないかな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1657</PointerOffset>
    <JapaneseText>大丈夫、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1672</PointerOffset>
    <JapaneseText>世界樹は寂しがってるはず、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1705</PointerOffset>
    <JapaneseText>○○は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1715</PointerOffset>
    <JapaneseText>みんなの事を信じているんだね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1744</PointerOffset>
    <JapaneseText>うん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1752</PointerOffset>
    <JapaneseText>今、悲観的になったらだめだよね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1783</PointerOffset>
    <JapaneseText>世界を良くする為に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1805</PointerOffset>
    <JapaneseText>アドリビトムはあるんだもん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1832</PointerOffset>
    <JapaneseText>そっか…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1844</PointerOffset>
    <JapaneseText>あなたが言うなら、そうなんだろうな…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1881</PointerOffset>
    <JapaneseText>でも、出来るよね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1901</PointerOffset>
    <JapaneseText>アドリビトムで、世界を少しずつでも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1937</PointerOffset>
    <JapaneseText>変えていけるよね？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1956</PointerOffset>
    <JapaneseText>でも、世界が良くなったら…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1986</PointerOffset>
    <JapaneseText>あなたは、予言の通りに世界樹へ帰らないと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2028</PointerOffset>
    <JapaneseText>いけないの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2041</PointerOffset>
    <JapaneseText>苦笑する</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2050</PointerOffset>
    <JapaneseText>わからない、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2069</PointerOffset>
    <JapaneseText>私、あなたがいなくなったら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2097</PointerOffset>
    <JapaneseText>寂しいよ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2108</PointerOffset>
    <JapaneseText>カノンノの治療が終了しました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
