<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>874</PointerOffset>
    <JapaneseText>じゃあな、ジーニアス！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>897</PointerOffset>
    <JapaneseText>うん、カイウス。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>915</PointerOffset>
    <JapaneseText>また明日！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>926</PointerOffset>
    <JapaneseText>あ、姉さん……と、リタ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>949</PointerOffset>
    <JapaneseText>ちょっと何よ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>965</PointerOffset>
    <JapaneseText>そのオマケみたいな呼び方</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>990</PointerOffset>
    <JapaneseText>随分楽しそうに話していたわね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1022</PointerOffset>
    <JapaneseText>明日もまた、会う約束を？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1047</PointerOffset>
    <JapaneseText>うん！　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1057</PointerOffset>
    <JapaneseText>一緒に遊ぶんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1072</PointerOffset>
    <JapaneseText>そう。　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1082</PointerOffset>
    <JapaneseText>いい事だわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1093</PointerOffset>
    <JapaneseText>ほら、カイウスってリカンツでしょ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1129</PointerOffset>
    <JapaneseText>獣に変身出来るから、今までと違う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1163</PointerOffset>
    <JapaneseText>色んな遊びが出来るんじゃないかなって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1200</PointerOffset>
    <JapaneseText>知ってた？　獣になるとね、周りの色とか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1240</PointerOffset>
    <JapaneseText>匂いとか、全然普段と違うんだって！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1275</PointerOffset>
    <JapaneseText>すごいなぁ……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1293</PointerOffset>
    <JapaneseText>一体どんな感じなんだろ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1316</PointerOffset>
    <JapaneseText>ジーニアス……気持ちはわかるけれど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1352</PointerOffset>
    <JapaneseText>あまり大きな声で「リカンツ」という</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1388</PointerOffset>
    <JapaneseText>種族名を口にしない方がいいわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1417</PointerOffset>
    <JapaneseText>どうしてさ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1430</PointerOffset>
    <JapaneseText>元々は蔑称だったのよ、リカンツって。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1468</PointerOffset>
    <JapaneseText>すっごい大昔の話だけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1491</PointerOffset>
    <JapaneseText>えっ！？　そうだったの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1516</PointerOffset>
    <JapaneseText>もちろん、現在は他意なく</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1542</PointerOffset>
    <JapaneseText>種族名として使われてはいるけれど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1576</PointerOffset>
    <JapaneseText>元々の由来を思うと……複雑なところね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1613</PointerOffset>
    <JapaneseText>そう？　</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1623</PointerOffset>
    <JapaneseText>別にいいんじゃない？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1645</PointerOffset>
    <JapaneseText>当のカイウスは気にしてないみたいだし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1682</PointerOffset>
    <JapaneseText>気にし過ぎる方が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1700</PointerOffset>
    <JapaneseText>かえって良くないでしょ、こういうの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1735</PointerOffset>
    <JapaneseText>それはそうだけれど……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1758</PointerOffset>
    <JapaneseText>ま、根本は「何が差別か？」って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1790</PointerOffset>
    <JapaneseText>とこなんでしょうけど。　あたしにとっては</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1832</PointerOffset>
    <JapaneseText>気にする方も気にしろって方も同罪ね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1867</PointerOffset>
    <JapaneseText>……リタは手厳しいわね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1890</PointerOffset>
    <JapaneseText>率直な意見を述べたまでよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
