<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>258</PointerOffset>
      <JapaneseText>ユグドラシルとは、と尋ねる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>285</PointerOffset>
      <JapaneseText>コレットの様な、羽を持つ「天使」という</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>325</PointerOffset>
      <JapaneseText>種族を集めている者。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>347</PointerOffset>
      <JapaneseText>そして、奴もまた天使だ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>372</PointerOffset>
      <JapaneseText>彼はコレットやロイドの住む村を襲い、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>410</PointerOffset>
      <JapaneseText>天使を捕まえようとした</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>433</PointerOffset>
      <JapaneseText>私やロイドは、村の者を守る為に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>465</PointerOffset>
      <JapaneseText>ユグドラシルを倒したはずだった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>496</PointerOffset>
      <JapaneseText>だが、奴が生きているとすると</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>526</PointerOffset>
      <JapaneseText>また天使を探し出す為に村を襲うだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>563</PointerOffset>
      <JapaneseText>なぜ村ごと襲う必要があるのか、と尋ねる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>602</PointerOffset>
      <JapaneseText>天使には羽があるものだが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>630</PointerOffset>
      <JapaneseText>ほとんどそれが現れないまま</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>658</PointerOffset>
      <JapaneseText>自分が天使と気付かない者もいる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>689</PointerOffset>
      <JapaneseText>だが、あるきっかけで羽を現す事があるのだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>730</PointerOffset>
      <JapaneseText>そして、そのきっかけとは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>758</PointerOffset>
      <JapaneseText>肉体的にも精神的にも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>780</PointerOffset>
      <JapaneseText>「苦痛を与えられる」事と言われている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>817</PointerOffset>
      <JapaneseText>だから、ユグドラシルにとって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>847</PointerOffset>
      <JapaneseText>村を襲うという方法が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>869</PointerOffset>
      <JapaneseText>天使を見つけ出すのに最適だっただけだ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>