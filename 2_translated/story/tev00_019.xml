<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>220</PointerOffset>
    <JapaneseText>世界中の屈強なツワモノが集う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>17</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>250</PointerOffset>
    <JapaneseText>闘技場へようこそ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>18</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>270</PointerOffset>
    <JapaneseText>オイラはこの闘技場の支配人、モルモだよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>19</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>311</PointerOffset>
    <JapaneseText>キミは、闘技場は初めてなのかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>20</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>345</PointerOffset>
    <JapaneseText>だったらルールを説明しないとね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>21</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>378</PointerOffset>
    <JapaneseText>基本は「ミッション」と呼ばれる戦闘に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>22</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>416</PointerOffset>
    <JapaneseText>挑戦して、勝ったら次のミッションに挑戦！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>23</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>458</PointerOffset>
    <JapaneseText>要はこれを繰り返せばいいんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>24</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>487</PointerOffset>
    <JapaneseText>それに「ミッション」に勝ち続けると、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>25</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>525</PointerOffset>
    <JapaneseText>思わぬ挑戦者が現れるかもしれないよ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>26</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>562</PointerOffset>
    <JapaneseText>「ミッション」では、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>27</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>584</PointerOffset>
    <JapaneseText>経験値は入らないけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>28</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>608</PointerOffset>
    <JapaneseText>アイテムが手に入る事があるよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>29</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>637</PointerOffset>
    <JapaneseText>そのアイテムは戦利品として</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>665</PointerOffset>
    <JapaneseText>自由に持ち帰ってもオッケーなんだ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>31</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>700</PointerOffset>
    <JapaneseText>んじゃ、腕に自信があるんだったら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>734</PointerOffset>
    <JapaneseText>どんどん参加して、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>754</PointerOffset>
    <JapaneseText>お客さんを楽しませてよね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>34</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
