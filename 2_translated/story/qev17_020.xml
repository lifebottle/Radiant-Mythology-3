  <Entries>
    <Entry>
      <PointerOffset>142</PointerOffset>
      <JapaneseText>一緒に行ってくれるのですね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>172</PointerOffset>
      <JapaneseText>ありがとうございます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>193</PointerOffset>
      <JapaneseText>忍びの道は険しき道。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>215</PointerOffset>
      <JapaneseText>ですが、私は早く一人前の忍者になって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>253</PointerOffset>
      <JapaneseText>皆さんのお力になりたいのです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>282</PointerOffset>
      <JapaneseText>それが、里の皆や</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>300</PointerOffset>
      <JapaneseText>今は亡き父上、母上の望みでも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>330</PointerOffset>
      <JapaneseText>あります……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>343</PointerOffset>
      <JapaneseText>よろしくお願いいたします</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
