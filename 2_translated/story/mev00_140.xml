<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>224</PointerOffset>
      <JapaneseText>私から伺おうと思っていましたのに、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260</PointerOffset>
      <JapaneseText>ご丁寧にありがとうございます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>289</PointerOffset>
      <JapaneseText>私は、フィリア・フィリスと申します。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>327</PointerOffset>
      <JapaneseText>よろしくお願いします</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>348</PointerOffset>
      <JapaneseText>私とアンジュさんは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>370</PointerOffset>
      <JapaneseText>教会に勤めていた時からの知り合いなのです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>411</PointerOffset>
      <JapaneseText>出身国は違いますけれど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>437</PointerOffset>
      <JapaneseText>私達は教会出身者なんですよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>464</PointerOffset>
      <JapaneseText>それで、アンジュさんが多くの人々の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>500</PointerOffset>
      <JapaneseText>力になれるギルドを立ち上げたと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>532</PointerOffset>
      <JapaneseText>便りをくれて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>545</PointerOffset>
      <JapaneseText>私も、ぜひ協力したくて来たんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>578</PointerOffset>
      <JapaneseText>では、今後ともよろしくお願い致します</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>