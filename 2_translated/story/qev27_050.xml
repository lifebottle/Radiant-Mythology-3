      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1084</PointerOffset>
      <JapaneseText>無事だったか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1097</PointerOffset>
      <JapaneseText>うん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1105</PointerOffset>
      <JapaneseText>無事も無事、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1119</PointerOffset>
      <JapaneseText>この通りピンピンしてるヨ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1155</PointerOffset>
      <JapaneseText>○○、紹介しよう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1175</PointerOffset>
      <JapaneseText>マオだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1190</PointerOffset>
      <JapaneseText>って事は、キミが噂の……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1215</PointerOffset>
      <JapaneseText>ユージーンから話は聞いてるよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1247</PointerOffset>
      <JapaneseText>これから、よろしくネ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1270</PointerOffset>
      <JapaneseText>マオはまだ幼いが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1290</PointerOffset>
      <JapaneseText>術の使い手としては</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1310</PointerOffset>
      <JapaneseText>なかなかのものだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1327</PointerOffset>
      <JapaneseText>今後、ギルドが請け負う仕事で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1357</PointerOffset>
      <JapaneseText>役に立つ事もあるだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1380</PointerOffset>
      <JapaneseText>ボクは火の力を操るのが得意なんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1416</PointerOffset>
      <JapaneseText>こう、ちちんぷいぷい！　ってネ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1447</PointerOffset>
      <JapaneseText>マオ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1455</PointerOffset>
      <JapaneseText>出来るからといって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1477</PointerOffset>
      <JapaneseText>あまり気安く力を使うのは感心せんな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1512</PointerOffset>
      <JapaneseText>ぶー。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1520</PointerOffset>
      <JapaneseText>わかってるヨ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1533</PointerOffset>
      <JapaneseText>とにかく、こういう力は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1557</PointerOffset>
      <JapaneseText>ヒトの幸せの為に使わないとネ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1588</PointerOffset>
      <JapaneseText>ボクもギルドで精一杯頑張るから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1620</PointerOffset>
      <JapaneseText>色々教えてね、○○！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
