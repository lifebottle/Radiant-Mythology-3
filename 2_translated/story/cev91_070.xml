<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>334</PointerOffset>
      <JapaneseText>しいな、聞きたい事がある</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>359</PointerOffset>
      <JapaneseText>ミブナの里でのソウルアルケミー、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>393</PointerOffset>
      <JapaneseText>つまり光気丹術の使い手って、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>423</PointerOffset>
      <JapaneseText>どんな奴だったんだ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>444</PointerOffset>
      <JapaneseText>そうだね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>456</PointerOffset>
      <JapaneseText>ある程度の術なら、一昔前は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>484</PointerOffset>
      <JapaneseText>ほとんどの者が使えたって話だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>515</PointerOffset>
      <JapaneseText>でも、それは真の使い手じゃない。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>549</PointerOffset>
      <JapaneseText>まあ、里にも伝説の使い手がいたって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>585</PointerOffset>
      <JapaneseText>伝わってるけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>600</PointerOffset>
      <JapaneseText>伝説の使い手？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>615</PointerOffset>
      <JapaneseText>そういう者を仙人って言うんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>647</PointerOffset>
      <JapaneseText>異類化生記では、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>665</PointerOffset>
      <JapaneseText>英雄として描かれているよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>690</PointerOffset>
      <JapaneseText>今としては、本当かどうか知る由も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>724</PointerOffset>
      <JapaneseText>ないけどさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>735</PointerOffset>
      <JapaneseText>仙人と呼ばれる者は…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>759</PointerOffset>
      <JapaneseText>肉体のドクメントを変えたんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>791</PointerOffset>
      <JapaneseText>ヒトの身体を捨て、不老不死になったって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>830</PointerOffset>
      <JapaneseText>そうして、世俗から姿を消して</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>860</PointerOffset>
      <JapaneseText>どこかで生きているとね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>883</PointerOffset>
      <JapaneseText>まるでニアタみたいだな…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>911</PointerOffset>
      <JapaneseText>彼の世界の技術とはまた違う様だが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>