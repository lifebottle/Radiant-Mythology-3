      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>578</PointerOffset>
      <JapaneseText>そう変わりはないだろうと思うがな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>611</PointerOffset>
      <JapaneseText>ジューダス……あのさ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>635</PointerOffset>
      <JapaneseText>無理はしないでよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>654</PointerOffset>
      <JapaneseText>無理？　</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>664</PointerOffset>
      <JapaneseText>何の話だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>673</PointerOffset>
      <JapaneseText>……別に、ちょっと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>693</PointerOffset>
      <JapaneseText>言ってみたくなっただけだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>720</PointerOffset>
      <JapaneseText>フン……。</JapaneseText>
