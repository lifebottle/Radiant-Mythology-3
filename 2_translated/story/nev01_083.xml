<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>556</PointerOffset>
    <JapaneseText>○○さんは、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>27</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>570</PointerOffset>
    <JapaneseText>ホタテは好きキュ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>28</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>590</PointerOffset>
    <JapaneseText>ピッポ達は大好きなんだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>29</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>619</PointerOffset>
    <JapaneseText>エステルさん、とってもいい人だキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>657</PointerOffset>
    <JapaneseText>話していると、詩人の心が伝わってくるキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>31</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>698</PointerOffset>
    <JapaneseText>お手伝いで防具屋をしているけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>730</PointerOffset>
    <JapaneseText>ピッポは実は詩人なんだキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>757</PointerOffset>
    <JapaneseText>「一寸の虫にも五分の魂」だキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>34</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>791</PointerOffset>
    <JapaneseText>あの虫さんも、大事にしてあげて欲しいキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>35</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>832</PointerOffset>
    <JapaneseText>ウィルさんは珍しい生き物に目がないキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>36</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>874</PointerOffset>
    <JapaneseText>「イキガイ」らしいキュ。でも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>904</PointerOffset>
    <JapaneseText>イキガイって……どんな貝だキュ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>937</PointerOffset>
    <JapaneseText>赤い煙って、何だか怪談みたいだキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>975</PointerOffset>
    <JapaneseText>詩人としては、ちょっとときめくものを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1013</PointerOffset>
    <JapaneseText>感じてしまうキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1030</PointerOffset>
    <JapaneseText>しいなさんは、とってもいい人だキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1068</PointerOffset>
    <JapaneseText>でも、心の奥に悲しい気持ちを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1098</PointerOffset>
    <JapaneseText>しまい込んでいる様に見えるキュ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1133</PointerOffset>
    <JapaneseText>ハロルドさんの実験は怖いキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1165</PointerOffset>
    <JapaneseText>だけど、未知のモノが誕生する瞬間を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1201</PointerOffset>
    <JapaneseText>見たい気持ちもあるキュ……複雑だキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1238</PointerOffset>
    <JapaneseText>詩人としては、精霊の存在は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1266</PointerOffset>
    <JapaneseText>力いっぱい信じたいキュ。そしてそして</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1304</PointerOffset>
    <JapaneseText>不思議なお話をいっぱい聞いてみたいキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1345</PointerOffset>
    <JapaneseText>モフモフ族伝説の英雄・カポエラッコと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1383</PointerOffset>
    <JapaneseText>ディセンダーは同じくらい羨望の的だキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1425</PointerOffset>
    <JapaneseText>しびれるキュ！　キュキューーッ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1458</PointerOffset>
    <JapaneseText>セルシウスさんに、この世界の古い伝承を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1498</PointerOffset>
    <JapaneseText>一杯聞かせてもらえたキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1526</PointerOffset>
    <JapaneseText>詩人の血が騒いだキュ……！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1555</PointerOffset>
    <JapaneseText>ジルディアのキバを見ていると</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1585</PointerOffset>
    <JapaneseText>とっても悲しい気持ちになるキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1619</PointerOffset>
    <JapaneseText>どうしてだキュ……？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1640</PointerOffset>
    <JapaneseText>キュッポ兄さんは、興奮すると</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1670</PointerOffset>
    <JapaneseText>ホタテを割るクセがあるんだキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1704</PointerOffset>
    <JapaneseText>……知ってるキュ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1723</PointerOffset>
    <JapaneseText>ロックスさんは、実はとっても</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1753</PointerOffset>
    <JapaneseText>ミステリアスな人なんだキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1783</PointerOffset>
    <JapaneseText>自分の過去は、絶対話そうとしないんだキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1824</PointerOffset>
    <JapaneseText>オルタ・ビレッジへの物資の手配は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1858</PointerOffset>
    <JapaneseText>ピッポ達もお手伝いしてるんだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1893</PointerOffset>
    <JapaneseText>船はもう、大丈夫なのかキュ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1923</PointerOffset>
    <JapaneseText>いよいよホタテ界から</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1945</PointerOffset>
    <JapaneseText>お迎えが来たかと思ったキュ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1976</PointerOffset>
    <JapaneseText>サレはどうして酷い事をするキュ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2010</PointerOffset>
    <JapaneseText>暴力はいけないキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2031</PointerOffset>
    <JapaneseText>ニアタさんは不思議な人だキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2063</PointerOffset>
    <JapaneseText>流浪の旅人って感じがするキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2095</PointerOffset>
    <JapaneseText>一つひとつの言葉が、ずっしり重いキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2132</PointerOffset>
    <JapaneseText>ロックスさんのおやつは大好きだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2170</PointerOffset>
    <JapaneseText>今度は、ホタテ風味のケーキも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2200</PointerOffset>
    <JapaneseText>作って欲しいキュ〜</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2219</PointerOffset>
    <JapaneseText>カノンノさんは、たくさんの世界の記憶と</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2259</PointerOffset>
    <JapaneseText>そこに生きたモノの心を受け継いできたキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2303</PointerOffset>
    <JapaneseText>それだけで、一大叙事詩が書けそうだキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2342</PointerOffset>
    <JapaneseText>ここまでくれば、あと一息だキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2376</PointerOffset>
    <JapaneseText>力を合わせて、頑張るキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2403</PointerOffset>
    <JapaneseText>ピッポ達はここで</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2421</PointerOffset>
    <JapaneseText>皆さんが戻るのを待ってるキュ。だけど……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2463</PointerOffset>
    <JapaneseText>いつも心は、一緒にいるキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2492</PointerOffset>
    <JapaneseText>いつも何気なく食べてるホタテも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2524</PointerOffset>
    <JapaneseText>きっとピッポ達のドクメントに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2554</PointerOffset>
    <JapaneseText>なってくれてるキュ……ちょっと感動だキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2595</PointerOffset>
    <JapaneseText>ピッポ達はここで</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2613</PointerOffset>
    <JapaneseText>皆さんが戻るのを待ってるキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2645</PointerOffset>
    <JapaneseText>行ってらっしゃいだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2670</PointerOffset>
    <JapaneseText>キュ〜〜〜っ！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2688</PointerOffset>
    <JapaneseText>お帰りなさいだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2709</PointerOffset>
    <JapaneseText>キュッポ兄さんは、最近</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2733</PointerOffset>
    <JapaneseText>興奮してよくホタテを割ってるんだキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2773</PointerOffset>
    <JapaneseText>何か嬉しい事があったみたいだキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
