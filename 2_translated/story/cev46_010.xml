<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>1685</PointerOffset>
      <JapaneseText>（くっ、どうして私が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1707</PointerOffset>
      <JapaneseText>　よりによって甲板掃除の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1733</PointerOffset>
      <JapaneseText>　当番などに……！）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1754</PointerOffset>
      <JapaneseText>（下は海なのか…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1774</PointerOffset>
      <JapaneseText>　でも、泳げなくて海が怖いから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>119</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1806</PointerOffset>
      <JapaneseText>　当番を代わってくれなんて言えないし…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1847</PointerOffset>
      <JapaneseText>（大丈夫、怖くない。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1869</PointerOffset>
      <JapaneseText>　落ち着け。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1883</PointerOffset>
      <JapaneseText>　これさえ終われば……）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>123</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1908</PointerOffset>
      <JapaneseText>（クソッ、何でよりによって俺が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1940</PointerOffset>
      <JapaneseText>　甲板の掃除当番なんかやんなきゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>125</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1974</PointerOffset>
      <JapaneseText>　なんねーんだよ！）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1995</PointerOffset>
      <JapaneseText>（うう、この揺れ……おえっぷ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>127</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2027</PointerOffset>
      <JapaneseText>　どうにかなんねえのか……！？）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>128</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2060</PointerOffset>
      <JapaneseText>（いや、そんな事より下は海…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>129</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2092</PointerOffset>
      <JapaneseText>　落ちねぇよな、この船……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2122</PointerOffset>
      <JapaneseText>　もし落ちたりなんてしたら……）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>131</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2155</PointerOffset>
      <JapaneseText>でさ、あの時のロニの顔！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>132</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2181</PointerOffset>
      <JapaneseText>デレーって鼻の下伸ばしちゃって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>133</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2212</PointerOffset>
      <JapaneseText>あはは。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>134</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2222</PointerOffset>
      <JapaneseText>その顔、目に浮かぶみたいだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>135</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2251</PointerOffset>
      <JapaneseText>……、あの二人、何してんだ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>136</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2280</PointerOffset>
      <JapaneseText>風に当たってるんじゃないかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>137</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2312</PointerOffset>
      <JapaneseText>今日は潮風が気持ちいいし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>138</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2337</PointerOffset>
      <JapaneseText>ルカ、ちょっと耳貸せよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>139</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2360</PointerOffset>
      <JapaneseText>えー、そんなの危ないよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2385</PointerOffset>
      <JapaneseText>しっ！　</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>141</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2395</PointerOffset>
      <JapaneseText>大丈夫だって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>142</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2411</PointerOffset>
      <JapaneseText>落ちやしないって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>143</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2428</PointerOffset>
      <JapaneseText>で、でも……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>144</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2441</PointerOffset>
      <JapaneseText>いいから、行くぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>145</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2458</PointerOffset>
      <JapaneseText>カイウス・ルカ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>146</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2473</PointerOffset>
      <JapaneseText>せーのっ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>147</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2484</PointerOffset>
      <JapaneseText>わーーーーーっ！！！！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>148</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2509</PointerOffset>
      <JapaneseText>ヒスイ・クロエ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>149</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2524</PointerOffset>
      <JapaneseText>ひっ……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>150</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2535</PointerOffset>
      <JapaneseText>何だ、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>151</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2543</PointerOffset>
      <JapaneseText>あんまり面白い反応じゃなかったなあ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>152</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2581</PointerOffset>
      <JapaneseText>行こうぜ、ルカ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>153</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2596</PointerOffset>
      <JapaneseText>え、え？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>154</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2606</PointerOffset>
      <JapaneseText>あ、うん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>155</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2618</PointerOffset>
      <JapaneseText>ご、ごめんなさい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>156</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2635</PointerOffset>
      <JapaneseText>うわー、ふたりでお昼寝してるの、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>157</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2669</PointerOffset>
      <JapaneseText>いいな、いいなぁ〜。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>158</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2691</PointerOffset>
      <JapaneseText>私も一緒にお昼寝しよーっと∀</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>159</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>