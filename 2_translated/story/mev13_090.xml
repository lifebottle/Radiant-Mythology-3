<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>54</PointerOffset>
      <JapaneseText>奩</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>908</PointerOffset>
      <JapaneseText>アンジュ様、お手紙が届いてますよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>941</PointerOffset>
      <JapaneseText>ありがとう、ロックス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>962</PointerOffset>
      <JapaneseText>このハンコは、オルタ・ビレッジからね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>999</PointerOffset>
      <JapaneseText>ヘーゼル村の皆さんや、ここにいる皆さんの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1041</PointerOffset>
      <JapaneseText>故郷の方々も集まって、もうそろそろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1077</PointerOffset>
      <JapaneseText>一つの村らしくなっているんじゃないですか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1120</PointerOffset>
      <JapaneseText>まぁ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1129</PointerOffset>
      <JapaneseText>どうかしました？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1147</PointerOffset>
      <JapaneseText>何か、良くない事でも…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1170</PointerOffset>
      <JapaneseText>色んな所から人々が集まっているせいか、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1210</PointerOffset>
      <JapaneseText>文化や価値観の違いで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1232</PointerOffset>
      <JapaneseText>衝突が起きているみたい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1255</PointerOffset>
      <JapaneseText>それぞれのやり方を、みんな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1283</PointerOffset>
      <JapaneseText>譲ろうとしないらしくて…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1308</PointerOffset>
      <JapaneseText>やっぱり…、難しいものね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1333</PointerOffset>
      <JapaneseText>けれども…、皆さん一人ひとりは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1365</PointerOffset>
      <JapaneseText>いい人なんですから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1384</PointerOffset>
      <JapaneseText>それぞれ、よかれと思って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1410</PointerOffset>
      <JapaneseText>自分達のやり方を提案して</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1436</PointerOffset>
      <JapaneseText>くれてるんですよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1453</PointerOffset>
      <JapaneseText>でも、どうすればいいの。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1479</PointerOffset>
      <JapaneseText>みんなが一つにまとまる様にするには…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1516</PointerOffset>
      <JapaneseText>そうですね…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1532</PointerOffset>
      <JapaneseText>種族や文化の違いは、押し付けるのではなく</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1574</PointerOffset>
      <JapaneseText>分かち合うべきだと思うんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1603</PointerOffset>
      <JapaneseText>そうする事で、「お互いが自らの価値観を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1643</PointerOffset>
      <JapaneseText>変える事が出来ると認識」出来るんじゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1681</PointerOffset>
      <JapaneseText>ないでしょうか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1696</PointerOffset>
      <JapaneseText>時間が必要かもしれませんが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1726</PointerOffset>
      <JapaneseText>この、分かち合ってみると言う事を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1760</PointerOffset>
      <JapaneseText>「提案」してみてはどうでしょう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1791</PointerOffset>
      <JapaneseText>ええ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1799</PointerOffset>
      <JapaneseText>やってみるわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1812</PointerOffset>
      <JapaneseText>ただ、ノーと言うんじゃなくて、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1844</PointerOffset>
      <JapaneseText>こっちの方もどう、こんなのもあると</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1880</PointerOffset>
      <JapaneseText>選択肢を増やして、肯定の力で導くというか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1923</PointerOffset>
      <JapaneseText>そうね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1930</PointerOffset>
      <JapaneseText>自分自身の事をダメだと思ってても、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1966</PointerOffset>
      <JapaneseText>人って色々やってるうちに「自分にこんな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2006</PointerOffset>
      <JapaneseText>力があったんだ」と気付けるものね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2039</PointerOffset>
      <JapaneseText>その…、アンジュ様。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2061</PointerOffset>
      <JapaneseText>一人で抱え込まないで下さい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2091</PointerOffset>
      <JapaneseText>僕に出来る事なんて、たかが知れてますけど…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2134</PointerOffset>
      <JapaneseText>あなたも、もっと自信を持ちなさい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2170</PointerOffset>
      <JapaneseText>あなたがわたしの教会に来てくれなかったら、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2214</PointerOffset>
      <JapaneseText>きっとアドリビトムは無かったよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2245</PointerOffset>
      <JapaneseText>あなたが提案してくれて、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2271</PointerOffset>
      <JapaneseText>色々奔走してくれたから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2295</PointerOffset>
      <JapaneseText>アドリビトムが誕生したんだし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>114</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2324</PointerOffset>
      <JapaneseText>あ、いや、それは…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2346</PointerOffset>
      <JapaneseText>提案した以上は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2364</PointerOffset>
      <JapaneseText>僕がやらなくてはダメかなと思って…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2399</PointerOffset>
      <JapaneseText>もう、あなたに全部任せて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2425</PointerOffset>
      <JapaneseText>わたしは隠居してもいいかなー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>119</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2454</PointerOffset>
      <JapaneseText>そ、それは困りますってば！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2483</PointerOffset>
      <JapaneseText>おだてには乗らないかぁ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2509</PointerOffset>
      <JapaneseText>残念だなあ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2522</PointerOffset>
      <JapaneseText>（うう、また丸め込まれる所だった…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>123</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2559</PointerOffset>
      <JapaneseText>（家事だけって約束のはずが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2589</PointerOffset>
      <JapaneseText>　ほとんど任されてきたもんなぁ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>125</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2623</PointerOffset>
      <JapaneseText>　トホホ…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>