<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>70</PointerOffset>
      <JapaneseText>傲</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>522</PointerOffset>
      <JapaneseText>なるほど。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>534</PointerOffset>
      <JapaneseText>この船は、国籍、種族を問わず様々な立場の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>576</PointerOffset>
      <JapaneseText>人々が集い、活動しているというわけですね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>617</PointerOffset>
      <JapaneseText>ええ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>625</PointerOffset>
      <JapaneseText>現在の世界情勢を考えれば</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>651</PointerOffset>
      <JapaneseText>何とも珍しい組織形態ではありますね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>686</PointerOffset>
      <JapaneseText>非常に興味深いです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>705</PointerOffset>
      <JapaneseText>……ジェイド・カーティス大佐。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>737</PointerOffset>
      <JapaneseText>あなたと、直々にこうして</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>763</PointerOffset>
      <JapaneseText>お話する機会が巡って来た事も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>792</PointerOffset>
      <JapaneseText>ほう……私をご存知でしたか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>819</PointerOffset>
      <JapaneseText>その身のこなし、観察力……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>849</PointerOffset>
      <JapaneseText>一般人ではないとは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>869</PointerOffset>
      <JapaneseText>思っていましたが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>886</PointerOffset>
      <JapaneseText>いえいえ、一般人ですよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>909</PointerOffset>
      <JapaneseText>ジェイドさんからは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>931</PointerOffset>
      <JapaneseText>たくさんの事をぜひ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>951</PointerOffset>
      <JapaneseText>学ばせていただきたいものです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>980</PointerOffset>
      <JapaneseText>向上心がある若者は歓迎ですが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1012</PointerOffset>
      <JapaneseText>鼻っ柱が強いだけの子どもは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1040</PointerOffset>
      <JapaneseText>嫌われると思いますよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1061</PointerOffset>
      <JapaneseText>……何だか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1073</PointerOffset>
      <JapaneseText>挨拶しづらい雰囲気に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1095</PointerOffset>
      <JapaneseText>なってしまいましたわね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1118</PointerOffset>
      <JapaneseText>ジェイ……といったかしら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1143</PointerOffset>
      <JapaneseText>大佐と物怖じせずに対峙出来るなんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1179</PointerOffset>
      <JapaneseText>只者ではなさそうね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>