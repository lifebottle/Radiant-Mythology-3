<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>816</PointerOffset>
      <JapaneseText>なあ、ロイド。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>832</PointerOffset>
      <JapaneseText>話があるんだけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>849</PointerOffset>
      <JapaneseText>どうしたんだ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>865</PointerOffset>
      <JapaneseText>そんな改まってさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>882</PointerOffset>
      <JapaneseText>ロイドの武器についてなんだけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>914</PointerOffset>
      <JapaneseText>もっと色んな二刀流を試してみても</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>948</PointerOffset>
      <JapaneseText>いいんじゃないかと思うんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>975</PointerOffset>
      <JapaneseText>色んな二刀流？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>990</PointerOffset>
      <JapaneseText>そう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>998</PointerOffset>
      <JapaneseText>何というか、「新・二刀流！」</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1028</PointerOffset>
      <JapaneseText>みたいな感じで！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1045</PointerOffset>
      <JapaneseText>新・二刀流かぁ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1064</PointerOffset>
      <JapaneseText>面白そうだな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1080</PointerOffset>
      <JapaneseText>やってみようぜ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1097</PointerOffset>
      <JapaneseText>よし、そう来なくっちゃ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1124</PointerOffset>
      <JapaneseText>「新」って事は、何か新しい事を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1156</PointerOffset>
      <JapaneseText>二刀流に盛り込まなくちゃダメだろうな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1193</PointerOffset>
      <JapaneseText>俺、思ったんだけど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1215</PointerOffset>
      <JapaneseText>違う種類の武器を使うのはどうかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1250</PointerOffset>
      <JapaneseText>斧と剣とか、そんな感じの……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1279</PointerOffset>
      <JapaneseText>なるほどな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1293</PointerOffset>
      <JapaneseText>確かにそんな二刀流、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1315</PointerOffset>
      <JapaneseText>これまで試した事なかったぜ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1344</PointerOffset>
      <JapaneseText>何がいいんだろうな……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1370</PointerOffset>
      <JapaneseText>いきなり斧は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1386</PointerOffset>
      <JapaneseText>片手で扱えそうにないし……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1413</PointerOffset>
      <JapaneseText>……特大ドリルとかか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1436</PointerOffset>
      <JapaneseText>それ、いいかもしれないぞ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1464</PointerOffset>
      <JapaneseText>なんだか強そうだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1483</PointerOffset>
      <JapaneseText>だよな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1493</PointerOffset>
      <JapaneseText>となると、もう一つは……何がいいかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1532</PointerOffset>
      <JapaneseText>特大ドリルは重そうだから、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1560</PointerOffset>
      <JapaneseText>なるべく軽い武器の方がいいんじゃないか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1601</PointerOffset>
      <JapaneseText>ストローなんかどうかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1627</PointerOffset>
      <JapaneseText>ほら、ノーマみたいな感じでさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1656</PointerOffset>
      <JapaneseText>なるほどな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1670</PointerOffset>
      <JapaneseText>それなら、ストローを口にくわえている間</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1710</PointerOffset>
      <JapaneseText>特大ドリルを両手でも使えるぞ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1741</PointerOffset>
      <JapaneseText>もしかしてこれ、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1759</PointerOffset>
      <JapaneseText>結構いいアイデアなんじゃないか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1792</PointerOffset>
      <JapaneseText>ああ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1800</PointerOffset>
      <JapaneseText>試してみる価値はありそうだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1827</PointerOffset>
      <JapaneseText>まずは特大ドリルを調達しようぜ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1861</PointerOffset>
      <JapaneseText>ハロルドに言えば、作ってくれるかもしれない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>