<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>1014</PointerOffset>
      <JapaneseText>今度の絵は何だ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1031</PointerOffset>
      <JapaneseText>この絵は…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1054</PointerOffset>
      <JapaneseText>世界に大地が生まれた後の事ね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1083</PointerOffset>
      <JapaneseText>「大地が生まれた後、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1105</PointerOffset>
      <JapaneseText>　ヒトの祖と精霊が生まれた」</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1134</PointerOffset>
      <JapaneseText>非物質だけども、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1152</PointerOffset>
      <JapaneseText>知的生命の誕生という事に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1178</PointerOffset>
      <JapaneseText>なるのかしら…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1193</PointerOffset>
      <JapaneseText>ジュディス、続けてくれ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1216</PointerOffset>
      <JapaneseText>ヒトの祖は、ここヴェラトローパを創り、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1256</PointerOffset>
      <JapaneseText>ヒトとなる為に肉体や潜在能力を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1288</PointerOffset>
      <JapaneseText>設計した</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1297</PointerOffset>
      <JapaneseText>そして、設計通りの身体をまとい、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1331</PointerOffset>
      <JapaneseText>地上に降りた様よ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1348</PointerOffset>
      <JapaneseText>まさに、ソウルアルケミー…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1378</PointerOffset>
      <JapaneseText>神の成せる業だな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1395</PointerOffset>
      <JapaneseText>ソウルアルケミーは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1417</PointerOffset>
      <JapaneseText>ヴェラトローパの民が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1439</PointerOffset>
      <JapaneseText>普通に持っていた能力の様ね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1466</PointerOffset>
      <JapaneseText>私達には魔術の曙、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1486</PointerOffset>
      <JapaneseText>失われた技術とされていたけど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1518</PointerOffset>
      <JapaneseText>ここでは当たり前に使われていた…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1551</PointerOffset>
      <JapaneseText>私達クリティアという種族、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1579</PointerOffset>
      <JapaneseText>そしてこのナギーグという力は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1611</PointerOffset>
      <JapaneseText>ここで決められたのね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1632</PointerOffset>
      <JapaneseText>人間も、ユージーンの様なガジュマも、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1670</PointerOffset>
      <JapaneseText>カイウスの様なリカンツも…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1697</PointerOffset>
      <JapaneseText>コレットの様に羽がある者も…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1729</PointerOffset>
      <JapaneseText>全てのヒト、人類はここから始まったんだわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1770</PointerOffset>
      <JapaneseText>だが、「ヒトの祖」はなぜ地上へ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1804</PointerOffset>
      <JapaneseText>ここに残れば、神の様に生きられただろうに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1845</PointerOffset>
      <JapaneseText>世界と共に、創造する為、ね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1872</PointerOffset>
      <JapaneseText>世界樹の被造物であるヒトの祖もまた、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1910</PointerOffset>
      <JapaneseText>世界と共に創造の担い手として生きる事を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1950</PointerOffset>
      <JapaneseText>決めたのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1961</PointerOffset>
      <JapaneseText>それは実際に生きて、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1983</PointerOffset>
      <JapaneseText>世界と調和し、経験をする事</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2010</PointerOffset>
      <JapaneseText>世界にある生命全てが学び合い、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2042</PointerOffset>
      <JapaneseText>向上していく事</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2057</PointerOffset>
      <JapaneseText>そして、そうしなければ新しい発見も</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2093</PointerOffset>
      <JapaneseText>なかったはずよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2108</PointerOffset>
      <JapaneseText>世界をより成長させていく為に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2140</PointerOffset>
      <JapaneseText>一からの学びの道を選んだのね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2169</PointerOffset>
      <JapaneseText>それが、今の世の中の有様か？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2199</PointerOffset>
      <JapaneseText>成長どころか後退してるじゃないか！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2234</PointerOffset>
      <JapaneseText>そう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>114</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2242</PointerOffset>
      <JapaneseText>今が学びの時かもしれないわよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2271</PointerOffset>
      <JapaneseText>次に行きましょう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2291</PointerOffset>
      <JapaneseText>この壁画から読み取れる情報は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2323</PointerOffset>
      <JapaneseText>もうないわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>