<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>855</PointerOffset>
      <JapaneseText>エミルはいいなぁ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>875</PointerOffset>
      <JapaneseText>マルタと仲が良くて……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>898</PointerOffset>
      <JapaneseText>それに比べて、僕とイリアは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>929</PointerOffset>
      <JapaneseText>……また、喧嘩したの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>961</PointerOffset>
      <JapaneseText>どうしてイリアは、僕に辛く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>989</PointerOffset>
      <JapaneseText>当たるんだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1004</PointerOffset>
      <JapaneseText>僕……、本当は……、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1026</PointerOffset>
      <JapaneseText>エミルとマルタみたいに……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1053</PointerOffset>
      <JapaneseText>ルカ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1062</PointerOffset>
      <JapaneseText>えっと……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1076</PointerOffset>
      <JapaneseText>僕には、詳しい事はわからないけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1113</PointerOffset>
      <JapaneseText>きっとイリアだって、ルカと仲良く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1147</PointerOffset>
      <JapaneseText>なりたいって……思ってるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1174</PointerOffset>
      <JapaneseText>だって、本当に嫌いな相手だったら……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1211</PointerOffset>
      <JapaneseText>きっと……、そもそも構ったりなんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1247</PointerOffset>
      <JapaneseText>しないだろうから……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1268</PointerOffset>
      <JapaneseText>エミル……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1279</PointerOffset>
      <JapaneseText>ル〜〜カ〜〜〜ちゃ〜〜ま♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1306</PointerOffset>
      <JapaneseText>ちょおっと、コッチに来てくれませんこと？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1354</PointerOffset>
      <JapaneseText>な、何……イリア。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1374</PointerOffset>
      <JapaneseText>どうして口調が違うの……怖……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1405</PointerOffset>
      <JapaneseText>い・い・か・ら・来・る・の！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1434</PointerOffset>
      <JapaneseText>ルカのくせにつべこべ言わない！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1465</PointerOffset>
      <JapaneseText>これでも……本当に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1485</PointerOffset>
      <JapaneseText>嫌われてないって、思う？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1510</PointerOffset>
      <JapaneseText>だ……大丈夫だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1527</PointerOffset>
      <JapaneseText>…………多分</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>