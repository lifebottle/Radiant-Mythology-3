<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>554</PointerOffset>
      <JapaneseText>あの、ジョアンさん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>576</PointerOffset>
      <JapaneseText>その病気を治す存在って、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>602</PointerOffset>
      <JapaneseText>どうやって知ったんですか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>629</PointerOffset>
      <JapaneseText>ゴフッ…、故郷のモラード村に、私と同じ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>669</PointerOffset>
      <JapaneseText>病気に…ゴホッ…なったミゲルという男が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>709</PointerOffset>
      <JapaneseText>いてね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>718</PointerOffset>
      <JapaneseText>彼はいい医者に見てもらおうと、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>750</PointerOffset>
      <JapaneseText>この坑道を通って…ゴホゴホッ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>782</PointerOffset>
      <JapaneseText>…大きな街を目指していたんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>811</PointerOffset>
      <JapaneseText>けれども、道中で発作が始まり、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>843</PointerOffset>
      <JapaneseText>どうにも身動きが取れなくなって、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>876</PointerOffset>
      <JapaneseText>死を覚悟…ゴホゴホッ…したらしい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>909</PointerOffset>
      <JapaneseText>そして、ここで死ぬのかと目をつぶり、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>947</PointerOffset>
      <JapaneseText>もっと生きたいと思っていたら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>976</PointerOffset>
      <JapaneseText>ゴホッ、ゴホッ…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>996</PointerOffset>
      <JapaneseText>どこからともなく…、赤い煙が現れたそうだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1037</PointerOffset>
      <JapaneseText>赤い煙…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1048</PointerOffset>
      <JapaneseText>赤い煙は周囲を包み、ミゲルにはそれが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1086</PointerOffset>
      <JapaneseText>あの世からの迎えだと思ったそうだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1119</PointerOffset>
      <JapaneseText>それから彼は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1133</PointerOffset>
      <JapaneseText>「死にたくない、もっと生きたい」。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1169</PointerOffset>
      <JapaneseText>ゴホッ…そう、強く願った</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1194</PointerOffset>
      <JapaneseText>すると、次に気が付いた時は赤い煙はなく、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1236</PointerOffset>
      <JapaneseText>すっかり病気は治っていたらしい…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1269</PointerOffset>
      <JapaneseText>それが病気を治す存在なんだね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1298</PointerOffset>
      <JapaneseText>実際に体験した人がいたんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1328</PointerOffset>
      <JapaneseText>不思議な話…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1341</PointerOffset>
      <JapaneseText>何だろう、精霊みたいなものかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1374</PointerOffset>
      <JapaneseText>私にもわからないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1393</PointerOffset>
      <JapaneseText>けれども、私ももっと長生きがしたい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1431</PointerOffset>
      <JapaneseText>その存在に会いたいんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1454</PointerOffset>
      <JapaneseText>本当に、わらにもすがる思いなんだよ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1491</PointerOffset>
      <JapaneseText>ゴホッ、ゴホゴホッ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>