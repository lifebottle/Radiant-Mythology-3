<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>554</PointerOffsetDec>
    <PointerOffsetHex>0x22a</PointerOffsetHex>
    <JapaneseText>あの、ジョアンさん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>43</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>576</PointerOffsetDec>
    <PointerOffsetHex>0x240</PointerOffsetHex>
    <JapaneseText>その病気を治す存在って、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>44</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>602</PointerOffsetDec>
    <PointerOffsetHex>0x25a</PointerOffsetHex>
    <JapaneseText>どうやって知ったんですか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>629</PointerOffsetDec>
    <PointerOffsetHex>0x275</PointerOffsetHex>
    <JapaneseText>ゴフッ…、故郷のモラード村に、私と同じ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>669</PointerOffsetDec>
    <PointerOffsetHex>0x29d</PointerOffsetHex>
    <JapaneseText>病気に…ゴホッ…なったミゲルという男が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>709</PointerOffsetDec>
    <PointerOffsetHex>0x2c5</PointerOffsetHex>
    <JapaneseText>いてね…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>718</PointerOffsetDec>
    <PointerOffsetHex>0x2ce</PointerOffsetHex>
    <JapaneseText>彼はいい医者に見てもらおうと、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>750</PointerOffsetDec>
    <PointerOffsetHex>0x2ee</PointerOffsetHex>
    <JapaneseText>この坑道を通って…ゴホゴホッ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>782</PointerOffsetDec>
    <PointerOffsetHex>0x30e</PointerOffsetHex>
    <JapaneseText>…大きな街を目指していたんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>811</PointerOffsetDec>
    <PointerOffsetHex>0x32b</PointerOffsetHex>
    <JapaneseText>けれども、道中で発作が始まり、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>843</PointerOffsetDec>
    <PointerOffsetHex>0x34b</PointerOffsetHex>
    <JapaneseText>どうにも身動きが取れなくなって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>876</PointerOffsetDec>
    <PointerOffsetHex>0x36c</PointerOffsetHex>
    <JapaneseText>死を覚悟…ゴホゴホッ…したらしい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>909</PointerOffsetDec>
    <PointerOffsetHex>0x38d</PointerOffsetHex>
    <JapaneseText>そして、ここで死ぬのかと目をつぶり、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>947</PointerOffsetDec>
    <PointerOffsetHex>0x3b3</PointerOffsetHex>
    <JapaneseText>もっと生きたいと思っていたら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>976</PointerOffsetDec>
    <PointerOffsetHex>0x3d0</PointerOffsetHex>
    <JapaneseText>ゴホッ、ゴホッ…、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>996</PointerOffsetDec>
    <PointerOffsetHex>0x3e4</PointerOffsetHex>
    <JapaneseText>どこからともなく…、赤い煙が現れたそうだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1037</PointerOffsetDec>
    <PointerOffsetHex>0x40d</PointerOffsetHex>
    <JapaneseText>赤い煙…？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1048</PointerOffsetDec>
    <PointerOffsetHex>0x418</PointerOffsetHex>
    <JapaneseText>赤い煙は周囲を包み、ミゲルにはそれが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1086</PointerOffsetDec>
    <PointerOffsetHex>0x43e</PointerOffsetHex>
    <JapaneseText>あの世からの迎えだと思ったそうだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1119</PointerOffsetDec>
    <PointerOffsetHex>0x45f</PointerOffsetHex>
    <JapaneseText>それから彼は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1133</PointerOffsetDec>
    <PointerOffsetHex>0x46d</PointerOffsetHex>
    <JapaneseText>「死にたくない、もっと生きたい」。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1169</PointerOffsetDec>
    <PointerOffsetHex>0x491</PointerOffsetHex>
    <JapaneseText>ゴホッ…そう、強く願った</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1194</PointerOffsetDec>
    <PointerOffsetHex>0x4aa</PointerOffsetHex>
    <JapaneseText>すると、次に気が付いた時は赤い煙はなく、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1236</PointerOffsetDec>
    <PointerOffsetHex>0x4d4</PointerOffsetHex>
    <JapaneseText>すっかり病気は治っていたらしい…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1269</PointerOffsetDec>
    <PointerOffsetHex>0x4f5</PointerOffsetHex>
    <JapaneseText>それが病気を治す存在なんだね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1298</PointerOffsetDec>
    <PointerOffsetHex>0x512</PointerOffsetHex>
    <JapaneseText>実際に体験した人がいたんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1328</PointerOffsetDec>
    <PointerOffsetHex>0x530</PointerOffsetHex>
    <JapaneseText>不思議な話…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1341</PointerOffsetDec>
    <PointerOffsetHex>0x53d</PointerOffsetHex>
    <JapaneseText>何だろう、精霊みたいなものかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1374</PointerOffsetDec>
    <PointerOffsetHex>0x55e</PointerOffsetHex>
    <JapaneseText>私にもわからないよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1393</PointerOffsetDec>
    <PointerOffsetHex>0x571</PointerOffsetHex>
    <JapaneseText>けれども、私ももっと長生きがしたい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1431</PointerOffsetDec>
    <PointerOffsetHex>0x597</PointerOffsetHex>
    <JapaneseText>その存在に会いたいんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1454</PointerOffsetDec>
    <PointerOffsetHex>0x5ae</PointerOffsetHex>
    <JapaneseText>本当に、わらにもすがる思いなんだよ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1491</PointerOffsetDec>
    <PointerOffsetHex>0x5d3</PointerOffsetHex>
    <JapaneseText>ゴホッ、ゴホゴホッ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
</Entries>