<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>482</PointerOffset>
    <JapaneseText>モンスターを倒すと、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>504</PointerOffset>
    <JapaneseText>アイテムをドロップする事があります</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>31</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>539</PointerOffset>
    <JapaneseText>なお、ドロップするアイテムは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>569</PointerOffset>
    <JapaneseText>モンスターの種類により異なります</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>602</PointerOffset>
    <JapaneseText>大丈夫だった？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>34</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>617</PointerOffset>
    <JapaneseText>あ、そろそろ船が到着する時間だ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>35</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>651</PointerOffset>
    <JapaneseText>急いで山を降りなきゃ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>36</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>674</PointerOffset>
    <JapaneseText>少し急ぐ事になるけど、いいかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>707</PointerOffset>
    <JapaneseText>船に乗ったら、あなたの希望する場所へ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>745</PointerOffset>
    <JapaneseText>送ってもらえる様に伝えるから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>774</PointerOffset>
    <JapaneseText>希望する場所について悩む</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>799</PointerOffset>
    <JapaneseText>送ると言われて悩む</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>818</PointerOffset>
    <JapaneseText>そう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>826</PointerOffset>
    <JapaneseText>どこかへ行こうとしていたんでしょう？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>864</PointerOffset>
    <JapaneseText>それで、ここへ降りてしまったとか…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>899</PointerOffset>
    <JapaneseText>え…？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>907</PointerOffset>
    <JapaneseText>どこへ行ったらいいのか、わからない？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>944</PointerOffset>
    <JapaneseText>え………</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>953</PointerOffset>
    <JapaneseText>ええっ！？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>965</PointerOffset>
    <JapaneseText>自分の名前以外、何もわからないって…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1002</PointerOffset>
    <JapaneseText>んーんんん…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1018</PointerOffset>
    <JapaneseText>それじゃあ、どうすればいいかな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1049</PointerOffset>
    <JapaneseText>とりあえず、船までおいでよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1079</PointerOffset>
    <JapaneseText>それから一緒に考えるから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1104</PointerOffset>
    <JapaneseText>行こっ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1113</PointerOffset>
    <JapaneseText>△ボタンを押すとメニューを起動します。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1153</PointerOffset>
    <JapaneseText>メニューからアイテムの使用や、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1185</PointerOffset>
    <JapaneseText>ステータスで状態の確認などが出来ます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1222</PointerOffset>
    <JapaneseText>×ボタンを押すと、ミニマップを表示し、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1262</PointerOffset>
    <JapaneseText>周囲を確認出来ます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1281</PointerOffset>
    <JapaneseText>再び非表示にするには、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1305</PointerOffset>
    <JapaneseText>×ボタンを押して下さい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
