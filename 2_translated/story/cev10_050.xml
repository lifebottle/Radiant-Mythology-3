<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>1114</PointerOffset>
      <JapaneseText>ソフィ君は、素直ないい子だね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1146</PointerOffset>
      <JapaneseText>積極的に、色々な事に興味を持っている様だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1187</PointerOffset>
      <JapaneseText>確かに、少し目を離すと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1211</PointerOffset>
      <JapaneseText>色んなものを見ているな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1238</PointerOffset>
      <JapaneseText>若いというのは良い事だ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1264</PointerOffset>
      <JapaneseText>世界の何もかもが、真新しく</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1292</PointerOffset>
      <JapaneseText>鮮やかに見えるのだろうね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1317</PointerOffset>
      <JapaneseText>これは……ウッドロウ様！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1342</PointerOffset>
      <JapaneseText>本日も良いお天気ですね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1368</PointerOffset>
      <JapaneseText>……船内の視察中ですか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1393</PointerOffset>
      <JapaneseText>なに、そんな大それたものではないよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1431</PointerOffset>
      <JapaneseText>アスベル君と世間話をしていたのだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1464</PointerOffset>
      <JapaneseText>……ウッドロウ様。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1484</PointerOffset>
      <JapaneseText>少し部下を借り受けても</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1508</PointerOffset>
      <JapaneseText>よろしいでしょうか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1529</PointerOffset>
      <JapaneseText>ああ、構わないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1546</PointerOffset>
      <JapaneseText>（アスベル、どういう事だ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1574</PointerOffset>
      <JapaneseText>　ウッドロウ様に対してあんな気安く！）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1613</PointerOffset>
      <JapaneseText>（誤解です！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1627</PointerOffset>
      <JapaneseText>　これにはちゃんと理由があって……）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1664</PointerOffset>
      <JapaneseText>ああ、そうか。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1680</PointerOffset>
      <JapaneseText>フレン君？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1691</PointerOffset>
      <JapaneseText>はい、何でしょうか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1710</PointerOffset>
      <JapaneseText>君も私を呼び捨てにしてくれて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1740</PointerOffset>
      <JapaneseText>構わないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1751</PointerOffset>
      <JapaneseText>とんでもない！　</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1769</PointerOffset>
      <JapaneseText>その様な……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1782</PointerOffset>
      <JapaneseText>ここでは特別扱いはしない、と</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1812</PointerOffset>
      <JapaneseText>アンジュ君とも約束を交わしている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1845</PointerOffset>
      <JapaneseText>君は私に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1855</PointerOffset>
      <JapaneseText>約束を反故にしろというのかね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1886</PointerOffset>
      <JapaneseText>うう……それは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1905</PointerOffset>
      <JapaneseText>（……フレン隊長の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1925</PointerOffset>
      <JapaneseText>　弱いところを見事に突いている。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1959</PointerOffset>
      <JapaneseText>　さすがだ……）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>