  <Entries>
    <Entry>
      <PointerOffset>370</PointerOffset>
      <JapaneseText>奩</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>746</PointerOffset>
      <JapaneseText>立派なお部屋だねえ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>768</PointerOffset>
      <JapaneseText>ベッドもふかふかだし、お布団から</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>802</PointerOffset>
      <JapaneseText>お日様の匂いがするよ〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>825</PointerOffset>
      <JapaneseText>ミブナの里と私達の村は近くにあって、昔から</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>869</PointerOffset>
      <JapaneseText>少し交流があったの。だから忍者じゃなくても</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>913</PointerOffset>
      <JapaneseText>しいな達と友達になれたんだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>942</PointerOffset>
      <JapaneseText>○○はディセンダーだったんだね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>976</PointerOffset>
      <JapaneseText>だから○○の瞳は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>996</PointerOffset>
      <JapaneseText>そんなにまっすぐなんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1019</PointerOffset>
      <JapaneseText>雲っていいよね、ふわふわしてて……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1057</PointerOffset>
      <JapaneseText>でも、自分の行きたい方じゃなくて、風が吹く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1101</PointerOffset>
      <JapaneseText>方にしか行けないから、ちょっと可哀相だね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1142</PointerOffset>
      <JapaneseText>○○、大丈夫？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1158</PointerOffset>
      <JapaneseText>最近倒れる事が増えてる気がして</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1190</PointerOffset>
      <JapaneseText>ちょっと心配だよ……無理は、ダメだよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1229</PointerOffset>
      <JapaneseText>辛いって気持ちが、マナを通して</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1261</PointerOffset>
      <JapaneseText>一杯伝わってくる……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1285</PointerOffset>
      <JapaneseText>世界が……悲しんでるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1308</PointerOffset>
      <JapaneseText>信者さん達、元に戻って良かったね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1344</PointerOffset>
      <JapaneseText>間違えたなって思うものがあったなら、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1382</PointerOffset>
      <JapaneseText>それを正せばいいんだもん。きっともう大丈夫</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1425</PointerOffset>
      <JapaneseText>ラザリスのやり方は、間違っていたかも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1463</PointerOffset>
      <JapaneseText>しれない……でも、間違いは正せるんだもの。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1507</PointerOffset>
      <JapaneseText>だから、一緒にまたやり直せる。そうだよね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1550</PointerOffset>
      <JapaneseText>みんな、あなたの事をお話してるよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1586</PointerOffset>
      <JapaneseText>やっぱりここに、あなたがいてくれなくちゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1628</PointerOffset>
      <JapaneseText>寂しいよね。仲間は、みんな一緒が一番だよ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1671</PointerOffset>
      <JapaneseText>ロイドは、手先がすごく器用なんだよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1709</PointerOffset>
      <JapaneseText>その技術を買われて、最近は街おこしの為に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1751</PointerOffset>
      <JapaneseText>頑張ってるんだ。わたしもお手伝いしてるの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1792</PointerOffset>
      <JapaneseText>あなたは…無事だったんだ…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1822</PointerOffset>
      <JapaneseText>良かった…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1833</PointerOffset>
      <JapaneseText>色々な所へ連れて行ってあげるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
