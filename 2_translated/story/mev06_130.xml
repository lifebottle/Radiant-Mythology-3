<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>118</PointerOffset>
      <JapaneseText>応</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>134</PointerOffset>
      <JapaneseText>応</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>726</PointerOffset>
      <JapaneseText>メルディ、何か、クラクラするよ〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>759</PointerOffset>
      <JapaneseText>ごめん。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>769</PointerOffset>
      <JapaneseText>無理をさせてしまったわね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>794</PointerOffset>
      <JapaneseText>本来、不可視のものを、今は無理矢理</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>830</PointerOffset>
      <JapaneseText>可視状態にしてるから、被験者には</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>864</PointerOffset>
      <JapaneseText>負担がかかってしまうのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>889</PointerOffset>
      <JapaneseText>細かいドクメントの展開も危険ね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>923</PointerOffset>
      <JapaneseText>本当は細かいトコまで解析させて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>955</PointerOffset>
      <JapaneseText>もらいたいけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>970</PointerOffset>
      <JapaneseText>人工精霊は、どうやって出来ているの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1007</PointerOffset>
      <JapaneseText>人工精霊は、人工的にドクメントを作り出す</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1049</PointerOffset>
      <JapaneseText>ところから始まるわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1068</PointerOffset>
      <JapaneseText>ドクメントは、精妙な非物質エネルギー。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1108</PointerOffset>
      <JapaneseText>術者の念、自然界の気なんかを</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1138</PointerOffset>
      <JapaneseText>掛け合わせてドクメントを作るの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1169</PointerOffset>
      <JapaneseText>んで、その人工ドクメントエネルギーの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1207</PointerOffset>
      <JapaneseText>振動数を、濃密な状態へ落とすと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1239</PointerOffset>
      <JapaneseText>実体化するってワケ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1258</PointerOffset>
      <JapaneseText>あ、ほら。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1270</PointerOffset>
      <JapaneseText>聖者が何もない所から、食べ物や衣類を出して</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1314</PointerOffset>
      <JapaneseText>人々に与えたって話とかあるでしょ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1349</PointerOffset>
      <JapaneseText>あれは、この技術の為と言われてるわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1384</PointerOffset>
      <JapaneseText>マナ、自然界の気、術者の意識をもって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1422</PointerOffset>
      <JapaneseText>非物質状態でドクメントを構成して</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1455</PointerOffset>
      <JapaneseText>そのドクメントの振動数を落としてやると</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1495</PointerOffset>
      <JapaneseText>物質になっていくのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1516</PointerOffset>
      <JapaneseText>それは……神の力…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1537</PointerOffset>
      <JapaneseText>まあ、術者の精神力や技量によって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1571</PointerOffset>
      <JapaneseText>まちまちよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1582</PointerOffset>
      <JapaneseText>そこまでの力を持つ様な</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1606</PointerOffset>
      <JapaneseText>精神力の持ち主は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1624</PointerOffset>
      <JapaneseText>滅多にいないと思うわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1645</PointerOffset>
      <JapaneseText>この技術は、そうそう簡単に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1673</PointerOffset>
      <JapaneseText>使えるもんじゃないわね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>