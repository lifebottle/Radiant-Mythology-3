<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>398</PointerOffset>
      <JapaneseText>ふぅー……、クロエは強いな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>428</PointerOffset>
      <JapaneseText>女性でここまでの使い手に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>454</PointerOffset>
      <JapaneseText>出会うのは、初めてだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>477</PointerOffset>
      <JapaneseText>私は騎士だ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>491</PointerOffset>
      <JapaneseText>騎士に性別は関係ない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>512</PointerOffset>
      <JapaneseText>いざという時、務めを果たせなければ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>548</PointerOffset>
      <JapaneseText>意味がないのだからな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>569</PointerOffset>
      <JapaneseText>それはそうだけど……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>593</PointerOffset>
      <JapaneseText>でも、どうしてわざわざ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>617</PointerOffset>
      <JapaneseText>騎士になろうと思ったんだい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>646</PointerOffset>
      <JapaneseText>……私は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>658</PointerOffset>
      <JapaneseText>騎士の名門ヴァレンス家の人間だ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>692</PointerOffset>
      <JapaneseText>家名に泥を塗るわけにはいかない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>723</PointerOffset>
      <JapaneseText>へぇ……、そうなんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>744</PointerOffset>
      <JapaneseText>普通の、貴族のお嬢様とは違うんだね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>782</PointerOffset>
      <JapaneseText>さすが、騎士の名門は違うなあ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>811</PointerOffset>
      <JapaneseText>あれっ、クロエ？　</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>831</PointerOffset>
      <JapaneseText>どうしたんだい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>848</PointerOffset>
      <JapaneseText>……、……何か、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>866</PointerOffset>
      <JapaneseText>気に障る事を言ったかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>