<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>487</PointerOffset>
      <JapaneseText>何かしら、今の音…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>506</PointerOffset>
      <JapaneseText>ここは近年、安定した状態と</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>534</PointerOffset>
      <JapaneseText>言われているが、見たところ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>562</PointerOffset>
      <JapaneseText>そうでもないらしいな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>583</PointerOffset>
      <JapaneseText>ここの火山活動が、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>603</PointerOffset>
      <JapaneseText>活発になっているって事？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>628</PointerOffset>
      <JapaneseText>星晶の採掘は、自然にも影響を与えてしまう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>669</PointerOffset>
      <JapaneseText>星晶も、土地に恵みを与える</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>697</PointerOffset>
      <JapaneseText>マナそのものでもあるからなのね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>728</PointerOffset>
      <JapaneseText>この火山でも、採掘は行われた。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>760</PointerOffset>
      <JapaneseText>マナの恵みが無くなり、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>784</PointerOffset>
      <JapaneseText>不安定な状態になっているのだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>817</PointerOffset>
      <JapaneseText>星晶がエネルギーとして使用される様に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>855</PointerOffset>
      <JapaneseText>なるまで、人々は世界樹が生み出すマナだけで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>899</PointerOffset>
      <JapaneseText>生きていたのに…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>916</PointerOffset>
      <JapaneseText>昔の人は、マナを生み出してくれる世界樹に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>958</PointerOffset>
      <JapaneseText>感謝を捧げていたのに、今の人や文明は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>996</PointerOffset>
      <JapaneseText>星晶をむさぼるばかり</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1017</PointerOffset>
      <JapaneseText>感謝する事すら忘れているわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1044</PointerOffset>
      <JapaneseText>星晶を採掘し尽くしてしまうと、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1076</PointerOffset>
      <JapaneseText>もう人々は今の様な豊かな暮らしは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1110</PointerOffset>
      <JapaneseText>出来なくなるな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1125</PointerOffset>
      <JapaneseText>だが、もっと恐ろしいのは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1153</PointerOffset>
      <JapaneseText>世界樹が生み出している非物質のマナを</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1191</PointerOffset>
      <JapaneseText>エネルギー利用する技術が出来た時だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1226</PointerOffset>
      <JapaneseText>そうなれば、人々はこの世界の生命を支える</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1268</PointerOffset>
      <JapaneseText>マナまでも奪い尽くすだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1295</PointerOffset>
      <JapaneseText>一時の繁栄と引き換えに世界樹は枯れ、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1333</PointerOffset>
      <JapaneseText>世界は滅亡してしまう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1354</PointerOffset>
      <JapaneseText>星晶に頼らない世界になればいいな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1389</PointerOffset>
      <JapaneseText>早く、オルタ・ビレッジが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1415</PointerOffset>
      <JapaneseText>実現すればいいのに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>