<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>1032</PointerOffset>
    <JapaneseText>ジーニアスくん？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1050</PointerOffset>
    <JapaneseText>……どうしたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1067</PointerOffset>
    <JapaneseText>……あ……クレア……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1088</PointerOffset>
    <JapaneseText>……ちょっと本を読んでただけなんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1123</PointerOffset>
    <JapaneseText>ぐすっ……ごめんね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1145</PointerOffset>
    <JapaneseText>恥ずかしいところ見せちゃった……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1178</PointerOffset>
    <JapaneseText>悲しいお話だったのね……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1206</PointerOffset>
    <JapaneseText>ほら、涙を拭いて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1223</PointerOffset>
    <JapaneseText>どんなお話だったか、聞いてもいい？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1270</PointerOffset>
    <JapaneseText>ハーフエルフと、人間の話だよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1299</PointerOffset>
    <JapaneseText>お互いがお互いを忌み嫌って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1327</PointerOffset>
    <JapaneseText>憎み合って……ヒトだけじゃなくて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1361</PointerOffset>
    <JapaneseText>最後は、世界まで傷付けてしまう話</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1403</PointerOffset>
    <JapaneseText>頭ではわかってるんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1427</PointerOffset>
    <JapaneseText>このままじゃいけないって。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1455</PointerOffset>
    <JapaneseText>その教訓の為に書かれた本なんだって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1490</PointerOffset>
    <JapaneseText>でもボク、どうしても心から</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1518</PointerOffset>
    <JapaneseText>人間を好きになれないなって……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1552</PointerOffset>
    <JapaneseText>そういう気持ちが、なかなか消せないんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1591</PointerOffset>
    <JapaneseText>嫌な奴だよね……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1608</PointerOffset>
    <JapaneseText>……あなたもたくさん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1630</PointerOffset>
    <JapaneseText>辛い事があって、傷付いて来たのね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1672</PointerOffset>
    <JapaneseText>ジーニアスくん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1690</PointerOffset>
    <JapaneseText>ハーフエルフにも、ヒトにも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1718</PointerOffset>
    <JapaneseText>同じ様に心があるわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1737</PointerOffset>
    <JapaneseText>傷付いたら、誰だって痛いと感じる。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1773</PointerOffset>
    <JapaneseText>なぜだと思う？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1788</PointerOffset>
    <JapaneseText>……同じ、ヒトだからっていうの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1821</PointerOffset>
    <JapaneseText>半分正解で、半分はずれかしら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1850</PointerOffset>
    <JapaneseText>答えはね……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1866</PointerOffset>
    <JapaneseText>心には、種族なんてないからよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1895</PointerOffset>
    <JapaneseText>心には……種族がない……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>108</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1920</PointerOffset>
    <JapaneseText>誰かを愛しいと思ったり、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>109</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1946</PointerOffset>
    <JapaneseText>食べ物を美味しいと感じたり……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>110</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1977</PointerOffset>
    <JapaneseText>そういう時、自分がどんな種族であるかを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>111</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2017</PointerOffset>
    <JapaneseText>あなたは意識しているかしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>112</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2046</PointerOffset>
    <JapaneseText>……そんな風に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>113</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2062</PointerOffset>
    <JapaneseText>思ってみた事もなかったよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>114</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2087</PointerOffset>
    <JapaneseText>そうか……そうなんだね……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>115</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2114</PointerOffset>
    <JapaneseText>今度、ジーニアスくんの為に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>116</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2142</PointerOffset>
    <JapaneseText>美味しいピーチパイを焼くわ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>117</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2172</PointerOffset>
    <JapaneseText>だから、良かったら食べに来て</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>118</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2201</PointerOffset>
    <JapaneseText>ロイドさんやコレットさんや……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>119</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2233</PointerOffset>
    <JapaneseText>あなたの大切なお友達と一緒に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>120</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2262</PointerOffset>
    <JapaneseText>……うん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>121</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2271</PointerOffset>
    <JapaneseText>みんなと……絶対、食べに行くよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>122</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
