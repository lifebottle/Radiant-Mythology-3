<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>406</PointerOffset>
    <JapaneseText>感謝の気持ちを忘れるのって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>434</PointerOffset>
    <JapaneseText>とっても悲しい事じゃない？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>461</PointerOffset>
    <JapaneseText>いきなりどうした。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>34</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>481</PointerOffset>
    <JapaneseText>随分実感のこもった言い方だな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>35</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>510</PointerOffset>
    <JapaneseText>それはそうよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>36</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>526</PointerOffset>
    <JapaneseText>あたしだって、神官見習いなんですもの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>563</PointerOffset>
    <JapaneseText>これでも色々、人の気持ちに対して</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>597</PointerOffset>
    <JapaneseText>思うところがあったりするの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>624</PointerOffset>
    <JapaneseText>そうだな……この時勢だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>647</PointerOffset>
    <JapaneseText>端的な救いを求める者ばかりが増え</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>681</PointerOffset>
    <JapaneseText>人は、他者への気遣いや感謝の思いを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>717</PointerOffset>
    <JapaneseText>日ごとに忘れていっている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>742</PointerOffset>
    <JapaneseText>慈愛の心を説く教会関係者として</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>774</PointerOffset>
    <JapaneseText>心穏やかではいられまい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>797</PointerOffset>
    <JapaneseText>それもそうだけど……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>821</PointerOffset>
    <JapaneseText>あたしが腹を立てているのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>849</PointerOffset>
    <JapaneseText>カイウスのこと！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>866</PointerOffset>
    <JapaneseText>カイウスったら最近、あたしが洗濯物を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>904</PointerOffset>
    <JapaneseText>片付けてあげたりしても、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>930</PointerOffset>
    <JapaneseText>お礼の言葉一つ言わないの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>955</PointerOffset>
    <JapaneseText>昔はそんなんじゃなかったのに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>985</PointerOffset>
    <JapaneseText>いつから感謝の気持ちを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1009</PointerOffset>
    <JapaneseText>忘れてしまったのかしら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1032</PointerOffset>
    <JapaneseText>ああ、思い出したら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1052</PointerOffset>
    <JapaneseText>余計に腹が立ってきちゃった！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1081</PointerOffset>
    <JapaneseText>それは、感謝の気持ちとは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1107</PointerOffset>
    <JapaneseText>また別の問題があるんじゃないか……？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
