<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>606</PointerOffset>
      <JapaneseText>傲</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>736</PointerOffset>
      <JapaneseText>傲</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>844</PointerOffset>
      <JapaneseText>なあ、リオン。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>860</PointerOffset>
      <JapaneseText>俺、ずっと考えてたんだ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>887</PointerOffset>
      <JapaneseText>より有利に、相手と戦って勝つには</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>921</PointerOffset>
      <JapaneseText>どうしたらいいんだろうって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>948</PointerOffset>
      <JapaneseText>ほう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>956</PointerOffset>
      <JapaneseText>お前にしては</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>970</PointerOffset>
      <JapaneseText>随分殊勝な心がけだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>991</PointerOffset>
      <JapaneseText>ああ。　この間の話で、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1015</PointerOffset>
      <JapaneseText>武器の多さが個人の強さになるのは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1049</PointerOffset>
      <JapaneseText>何となく納得出来たけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1072</PointerOffset>
      <JapaneseText>人間の腕は二本しかないだろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1102</PointerOffset>
      <JapaneseText>って事は、二本以上武器を持てないなら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1140</PointerOffset>
      <JapaneseText>どうやって相手より強くなればいいのかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1179</PointerOffset>
      <JapaneseText>まだその馬鹿げた話を続けたいのか！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1214</PointerOffset>
      <JapaneseText>そうね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1224</PointerOffset>
      <JapaneseText>単純に武器を増やしただけじゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1254</PointerOffset>
      <JapaneseText>相手と大きな差は付けられないわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1285</PointerOffset>
      <JapaneseText>武器の数は同じ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1303</PointerOffset>
      <JapaneseText>力も互角。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1315</PointerOffset>
      <JapaneseText>それなら、どうすれば相手に勝てるのか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1356</PointerOffset>
      <JapaneseText>答えは案外、簡単なところに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1384</PointerOffset>
      <JapaneseText>あるんじゃないかしら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1405</PointerOffset>
      <JapaneseText>簡単なところ……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1425</PointerOffset>
      <JapaneseText>どういう事だ、ジュディス？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1453</PointerOffset>
      <JapaneseText>教えてくれ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1466</PointerOffset>
      <JapaneseText>おい、スタン！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1481</PointerOffset>
      <JapaneseText>武器の種類を変えるのよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1504</PointerOffset>
      <JapaneseText>同じ武器同士ではなく、あえて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1534</PointerOffset>
      <JapaneseText>違う武器同士を組み合わせてみるの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1567</PointerOffset>
      <JapaneseText>ほら、斧と剣なんて組み合わせも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1599</PointerOffset>
      <JapaneseText>案外、面白い戦い方が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1621</PointerOffset>
      <JapaneseText>出来るかもしれないわよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1646</PointerOffset>
      <JapaneseText>武器の種類か……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1666</PointerOffset>
      <JapaneseText>うん、確かにそうだな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1689</PointerOffset>
      <JapaneseText>リオンだって、違う種類の武器で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1721</PointerOffset>
      <JapaneseText>戦ってるもんな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1736</PointerOffset>
      <JapaneseText>だから、それとこれとは話が別だ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1769</PointerOffset>
      <JapaneseText>こうしちゃいられない！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1793</PointerOffset>
      <JapaneseText>この事、他のみんなにも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1817</PointerOffset>
      <JapaneseText>教えて来なくちゃ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1838</PointerOffset>
      <JapaneseText>何を考えているか知らんが……、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1870</PointerOffset>
      <JapaneseText>あいつをからかって面白がるのはやめろ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1910</PointerOffset>
      <JapaneseText>いい迷惑だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1921</PointerOffset>
      <JapaneseText>あら、冷たいのね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1941</PointerOffset>
      <JapaneseText>私は冗談は嫌いよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1960</PointerOffset>
      <JapaneseText>……僕は言葉遊びも大嫌いだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1987</PointerOffset>
      <JapaneseText>あらあら……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2000</PointerOffset>
      <JapaneseText>ムキになるのは、それだけ本気……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2034</PointerOffset>
      <JapaneseText>という事なのかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>