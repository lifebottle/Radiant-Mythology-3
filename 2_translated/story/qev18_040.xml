<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>666</PointerOffset>
      <JapaneseText>コリン……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>679</PointerOffset>
      <JapaneseText>コリン！　そこにいるのかい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>717</PointerOffset>
      <JapaneseText>ここには、こんなに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>737</PointerOffset>
      <JapaneseText>コリンの気配が満ちてるのに……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>768</PointerOffset>
      <JapaneseText>どうして……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>790</PointerOffset>
      <JapaneseText>ああ……、ごめんよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>812</PointerOffset>
      <JapaneseText>一人で取り乱しちまって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>835</PointerOffset>
      <JapaneseText>……コリンは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>851</PointerOffset>
      <JapaneseText>あたしの大切な友達だったんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>880</PointerOffset>
      <JapaneseText>辛い時も、楽しい時も……、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>908</PointerOffset>
      <JapaneseText>泣きたい時も、大笑いした時だって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>942</PointerOffset>
      <JapaneseText>いつだってあたしの傍にいてくれた</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>975</PointerOffset>
      <JapaneseText>あの子だけは……、あたしを</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1003</PointerOffset>
      <JapaneseText>あきらめないでいてくれたんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1032</PointerOffset>
      <JapaneseText>最期まで……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1052</PointerOffset>
      <JapaneseText>……コリンは……、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1072</PointerOffset>
      <JapaneseText>あたしを魔物から庇って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1096</PointerOffset>
      <JapaneseText>命を落としたんだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1124</PointerOffset>
      <JapaneseText>あたしは過去に、とある事件を起こしてね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1166</PointerOffset>
      <JapaneseText>里に大きな被害が出たのはもちろん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1200</PointerOffset>
      <JapaneseText>傷付けちまった人も大勢いた</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1227</PointerOffset>
      <JapaneseText>もう、自分の何もかもに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1251</PointerOffset>
      <JapaneseText>すっかり自信が持てなくなっちまってさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1288</PointerOffset>
      <JapaneseText>そんな時でもコリンは、自分を信じろって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1328</PointerOffset>
      <JapaneseText>ずっとあたしを励ましてくれたんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1361</PointerOffset>
      <JapaneseText>あたしを庇った時だって……、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1391</PointerOffset>
      <JapaneseText>体中……傷だらけだったのに……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1431</PointerOffset>
      <JapaneseText>そんなあの子に、最後にあたしは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1463</PointerOffset>
      <JapaneseText>ひどい事を言っちまったんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1490</PointerOffset>
      <JapaneseText>だから……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1504</PointerOffset>
      <JapaneseText>どうしても、もう一度会って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1532</PointerOffset>
      <JapaneseText>直接謝りたいんだよ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>