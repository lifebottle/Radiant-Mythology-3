<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>649</PointerOffset>
    <JapaneseText>大丈夫か？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>661</PointerOffset>
    <JapaneseText>あまり顔色が良くないな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>684</PointerOffset>
    <JapaneseText>ここの外気温は、他に比べ極端に高い。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>722</PointerOffset>
    <JapaneseText>その温度差だけでも、ヒトの体には</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>756</PointerOffset>
    <JapaneseText>相当な負担になる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>773</PointerOffset>
    <JapaneseText>無理をせず、適度に補給と休憩を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>805</PointerOffset>
    <JapaneseText>取りながら進んだ方がいいだろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>845</PointerOffset>
    <JapaneseText>ん？　ああ、マオ……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>869</PointerOffset>
    <JapaneseText>これから合流する相手の事なら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>899</PointerOffset>
    <JapaneseText>心配はいらん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>912</PointerOffset>
    <JapaneseText>どういうわけか、火の恩恵を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>940</PointerOffset>
    <JapaneseText>強く受けて生まれた子の様でな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>972</PointerOffset>
    <JapaneseText>身体的には、多少の暑さはものともせん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1016</PointerOffset>
    <JapaneseText>ふむ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1024</PointerOffset>
    <JapaneseText>恐らく出生に、何か秘密が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1050</PointerOffset>
    <JapaneseText>あるのだとは思うが……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1073</PointerOffset>
    <JapaneseText>何ぶん、本人が過去の記憶を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1101</PointerOffset>
    <JapaneseText>失くしていてな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1116</PointerOffset>
    <JapaneseText>俺と会う以前の事を、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1138</PointerOffset>
    <JapaneseText>全く覚えていないのだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1166</PointerOffset>
    <JapaneseText>なに、本人はあっけらかんとしたもので</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1204</PointerOffset>
    <JapaneseText>暗い部分など少しも見せん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1229</PointerOffset>
    <JapaneseText>強い子だが……それだけに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1255</PointerOffset>
    <JapaneseText>いつかその緊張の糸が切れないかと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1289</PointerOffset>
    <JapaneseText>心配に思うところもあってな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1316</PointerOffset>
    <JapaneseText>だが、ギルドで交友関係を広げれば</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1350</PointerOffset>
    <JapaneseText>あの子自身の幅も広がる。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1376</PointerOffset>
    <JapaneseText>きっと、いい経験を積めるだろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1407</PointerOffset>
    <JapaneseText>もしかしたら、何かのきっかけで</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1439</PointerOffset>
    <JapaneseText>過去の記憶も戻るかもしれん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1475</PointerOffset>
    <JapaneseText>そうか。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1485</PointerOffset>
    <JapaneseText>そういえば、○○も</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1505</PointerOffset>
    <JapaneseText>過去の記憶が無いのだったな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1532</PointerOffset>
    <JapaneseText>こういう言い方は不謹慎かもしれんが……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1574</PointerOffset>
    <JapaneseText>○○とマオは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1588</PointerOffset>
    <JapaneseText>親しくなれるかもしれんな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1622</PointerOffset>
    <JapaneseText>……すまん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1636</PointerOffset>
    <JapaneseText>少しお喋りが過ぎた様だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1659</PointerOffset>
    <JapaneseText>とにかく先へ進もう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1681</PointerOffset>
    <JapaneseText>合流地点までは、あと少しのはずだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
