<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>202</PointerOffset>
    <JapaneseText>ピッポの防具屋にようこそだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>15</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>236</PointerOffset>
    <JapaneseText>ここでは選りすぐりの防具を仕入れて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>16</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>272</PointerOffset>
    <JapaneseText>取り扱っているんだキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>17</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>297</PointerOffset>
    <JapaneseText>ここで買った装備は、メニューの「装備」で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>18</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>339</PointerOffset>
    <JapaneseText>装備出来るキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>19</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>354</PointerOffset>
    <JapaneseText>武器や防具によって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>20</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>376</PointerOffset>
    <JapaneseText>装備出来る部位が異なるキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>21</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>403</PointerOffset>
    <JapaneseText>そうそう、大事な事を言い忘れたキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>22</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>441</PointerOffset>
    <JapaneseText>武器や防具には、装備する為に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>23</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>471</PointerOffset>
    <JapaneseText>一定のレベルが要求されるんだキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>24</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>504</PointerOffset>
    <JapaneseText>要求されているレベルに達してないと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>25</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>540</PointerOffset>
    <JapaneseText>装備出来ないんだキュ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>26</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>564</PointerOffset>
    <JapaneseText>忘れないで欲しいキュ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>27</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>585</PointerOffset>
    <JapaneseText>これからも品数を増やしていくキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>28</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>621</PointerOffset>
    <JapaneseText>試着も出来るから、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>29</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>641</PointerOffset>
    <JapaneseText>ぜひ気軽に立ち寄って欲しいキュ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
