<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>800</PointerOffsetDec>
    <PointerOffsetHex>0x320</PointerOffsetHex>
    <JapaneseText>それにしても、すごい数のゴミだなぁ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>835</PointerOffsetDec>
    <PointerOffsetHex>0x343</PointerOffsetHex>
    <JapaneseText>捨てに行くのも一苦労だぜ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>860</PointerOffsetDec>
    <PointerOffsetHex>0x35c</PointerOffsetHex>
    <JapaneseText>ここで暮らしている人数が人数だからね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>900</PointerOffsetDec>
    <PointerOffsetHex>0x384</PointerOffsetHex>
    <JapaneseText>自然と、ゴミの量が多くなるのも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>932</PointerOffsetDec>
    <PointerOffsetHex>0x3a4</PointerOffsetHex>
    <JapaneseText>仕方がないさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>945</PointerOffsetDec>
    <PointerOffsetHex>0x3b1</PointerOffsetHex>
    <JapaneseText>でも、こういうのって……えーと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>977</PointerOffsetDec>
    <PointerOffsetHex>0x3d1</PointerOffsetHex>
    <JapaneseText>「資源の再利用」っていうのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1008</PointerOffsetDec>
    <PointerOffsetHex>0x3f0</PointerOffsetHex>
    <JapaneseText>そういうのが出来るゴミだって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1038</PointerOffsetDec>
    <PointerOffsetHex>0x40e</PointerOffsetHex>
    <JapaneseText>あるんだよな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1051</PointerOffsetDec>
    <PointerOffsetHex>0x41b</PointerOffsetHex>
    <JapaneseText>ああ、ビンや缶などが再利用出来て</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1085</PointerOffsetDec>
    <PointerOffsetHex>0x43d</PointerOffsetHex>
    <JapaneseText>別の素材になったりするんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1112</PointerOffsetDec>
    <PointerOffsetHex>0x458</PointerOffsetHex>
    <JapaneseText>そういうゴミの分別が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1134</PointerOffsetDec>
    <PointerOffsetHex>0x46e</PointerOffsetHex>
    <JapaneseText>徐々に広まっている様だね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1159</PointerOffsetDec>
    <PointerOffsetHex>0x487</PointerOffsetHex>
    <JapaneseText>あ、それこの間授業で習ったぞ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1188</PointerOffsetDec>
    <PointerOffsetHex>0x4a4</PointerOffsetHex>
    <JapaneseText>資源を再利用すると</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1208</PointerOffsetDec>
    <PointerOffsetHex>0x4b8</PointerOffsetHex>
    <JapaneseText>出されるゴミの量が減るんだろ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1239</PointerOffsetDec>
    <PointerOffsetHex>0x4d7</PointerOffsetHex>
    <JapaneseText>だから、自然とかにも優しいって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1270</PointerOffsetDec>
    <PointerOffsetHex>0x4f6</PointerOffsetHex>
    <JapaneseText>うん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1278</PointerOffsetDec>
    <PointerOffsetHex>0x4fe</PointerOffsetHex>
    <JapaneseText>例えば、空きビンを幾つか集めて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1310</PointerOffsetDec>
    <PointerOffsetHex>0x51e</PointerOffsetHex>
    <JapaneseText>新しいビンを作れば、その分のゴミが減る</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1349</PointerOffsetDec>
    <PointerOffsetHex>0x545</PointerOffsetHex>
    <JapaneseText>素材に使ったビンの数だけ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1375</PointerOffsetDec>
    <PointerOffsetHex>0x55f</PointerOffsetHex>
    <JapaneseText>自然を汚さなくて済むし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1399</PointerOffsetDec>
    <PointerOffsetHex>0x577</PointerOffsetHex>
    <JapaneseText>資源の無駄使いも減るって事だね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1430</PointerOffsetDec>
    <PointerOffsetHex>0x596</PointerOffsetHex>
    <JapaneseText>でもさ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1440</PointerOffsetDec>
    <PointerOffsetHex>0x5a0</PointerOffsetHex>
    <JapaneseText>それって本当にいい事なのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1469</PointerOffsetDec>
    <PointerOffsetHex>0x5bd</PointerOffsetHex>
    <JapaneseText>いい事だから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1483</PointerOffsetDec>
    <PointerOffsetHex>0x5cb</PointerOffsetHex>
    <JapaneseText>みんなやってんだろ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1504</PointerOffsetDec>
    <PointerOffsetHex>0x5e0</PointerOffsetHex>
    <JapaneseText>だってさ、その再利用ってのも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1534</PointerOffsetDec>
    <PointerOffsetHex>0x5fe</PointerOffsetHex>
    <JapaneseText>燃料とか費用がかかんだろ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1559</PointerOffsetDec>
    <PointerOffsetHex>0x617</PointerOffsetHex>
    <JapaneseText>同じ一つのビンを作るのに、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1587</PointerOffsetDec>
    <PointerOffsetHex>0x633</PointerOffsetHex>
    <JapaneseText>再利用しない方が、金や時間を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1617</PointerOffsetDec>
    <PointerOffsetHex>0x651</PointerOffsetHex>
    <JapaneseText>かけずに出来るって事はないのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1650</PointerOffsetDec>
    <PointerOffsetHex>0x672</PointerOffsetHex>
    <JapaneseText>それは……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1661</PointerOffsetDec>
    <PointerOffsetHex>0x67d</PointerOffsetHex>
    <JapaneseText>なーんかさ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1675</PointerOffsetDec>
    <PointerOffsetHex>0x68b</PointerOffsetHex>
    <JapaneseText>再利用っていうと良く聞こえるけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1708</PointerOffsetDec>
    <PointerOffsetHex>0x6ac</PointerOffsetHex>
    <JapaneseText>ちゃんと仕組みがわからないと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1738</PointerOffsetDec>
    <PointerOffsetHex>0x6ca</PointerOffsetHex>
    <JapaneseText>何かの無駄遣いをしてるって事は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1770</PointerOffsetDec>
    <PointerOffsetHex>0x6ea</PointerOffsetHex>
    <JapaneseText>変わらないんじゃないのかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1799</PointerOffsetDec>
    <PointerOffsetHex>0x707</PointerOffsetHex>
    <JapaneseText>ロイドって……時々</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1819</PointerOffsetDec>
    <PointerOffsetHex>0x71b</PointerOffsetHex>
    <JapaneseText>鋭い事を言うよなぁ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1838</PointerOffsetDec>
    <PointerOffsetHex>0x72e</PointerOffsetHex>
    <JapaneseText>お〜〜〜。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1850</PointerOffsetDec>
    <PointerOffsetHex>0x73a</PointerOffsetHex>
    <JapaneseText>俺、そんな難しい事</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1870</PointerOffsetDec>
    <PointerOffsetHex>0x74e</PointerOffsetHex>
    <JapaneseText>考えた事もなかったよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1891</PointerOffsetDec>
    <PointerOffsetHex>0x763</PointerOffsetHex>
    <JapaneseText>すごいな、ロイドって！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1914</PointerOffsetDec>
    <PointerOffsetHex>0x77a</PointerOffsetHex>
    <JapaneseText>そ、そんな事ないって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1935</PointerOffsetDec>
    <PointerOffsetHex>0x78f</PointerOffsetHex>
    <JapaneseText>あー、それより今の話、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1959</PointerOffsetDec>
    <PointerOffsetHex>0x7a7</PointerOffsetHex>
    <JapaneseText>何か本気で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1971</PointerOffsetDec>
    <PointerOffsetHex>0x7b3</PointerOffsetHex>
    <JapaneseText>気になってきたぞ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1992</PointerOffsetDec>
    <PointerOffsetHex>0x7c8</PointerOffsetHex>
    <JapaneseText>あとで、先生に聞いてみようかな……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
</Entries>