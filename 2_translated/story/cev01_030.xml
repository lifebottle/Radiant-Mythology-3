<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>800</PointerOffset>
    <JapaneseText>それにしても、すごい数のゴミだなぁ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>835</PointerOffset>
    <JapaneseText>捨てに行くのも一苦労だぜ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>860</PointerOffset>
    <JapaneseText>ここで暮らしている人数が人数だからね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>900</PointerOffset>
    <JapaneseText>自然と、ゴミの量が多くなるのも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>932</PointerOffset>
    <JapaneseText>仕方がないさ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>945</PointerOffset>
    <JapaneseText>でも、こういうのって……えーと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>977</PointerOffset>
    <JapaneseText>「資源の再利用」っていうのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1008</PointerOffset>
    <JapaneseText>そういうのが出来るゴミだって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1038</PointerOffset>
    <JapaneseText>あるんだよな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1051</PointerOffset>
    <JapaneseText>ああ、ビンや缶などが再利用出来て</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1085</PointerOffset>
    <JapaneseText>別の素材になったりするんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1112</PointerOffset>
    <JapaneseText>そういうゴミの分別が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1134</PointerOffset>
    <JapaneseText>徐々に広まっている様だね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1159</PointerOffset>
    <JapaneseText>あ、それこの間授業で習ったぞ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1188</PointerOffset>
    <JapaneseText>資源を再利用すると</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1208</PointerOffset>
    <JapaneseText>出されるゴミの量が減るんだろ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1239</PointerOffset>
    <JapaneseText>だから、自然とかにも優しいって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1270</PointerOffset>
    <JapaneseText>うん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1278</PointerOffset>
    <JapaneseText>例えば、空きビンを幾つか集めて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1310</PointerOffset>
    <JapaneseText>新しいビンを作れば、その分のゴミが減る</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1349</PointerOffset>
    <JapaneseText>素材に使ったビンの数だけ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1375</PointerOffset>
    <JapaneseText>自然を汚さなくて済むし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1399</PointerOffset>
    <JapaneseText>資源の無駄使いも減るって事だね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1430</PointerOffset>
    <JapaneseText>でもさ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1440</PointerOffset>
    <JapaneseText>それって本当にいい事なのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1469</PointerOffset>
    <JapaneseText>いい事だから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1483</PointerOffset>
    <JapaneseText>みんなやってんだろ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1504</PointerOffset>
    <JapaneseText>だってさ、その再利用ってのも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1534</PointerOffset>
    <JapaneseText>燃料とか費用がかかんだろ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1559</PointerOffset>
    <JapaneseText>同じ一つのビンを作るのに、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1587</PointerOffset>
    <JapaneseText>再利用しない方が、金や時間を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1617</PointerOffset>
    <JapaneseText>かけずに出来るって事はないのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1650</PointerOffset>
    <JapaneseText>それは……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1661</PointerOffset>
    <JapaneseText>なーんかさ、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1675</PointerOffset>
    <JapaneseText>再利用っていうと良く聞こえるけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1708</PointerOffset>
    <JapaneseText>ちゃんと仕組みがわからないと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1738</PointerOffset>
    <JapaneseText>何かの無駄遣いをしてるって事は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1770</PointerOffset>
    <JapaneseText>変わらないんじゃないのかな？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1799</PointerOffset>
    <JapaneseText>ロイドって……時々</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1819</PointerOffset>
    <JapaneseText>鋭い事を言うよなぁ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1838</PointerOffset>
    <JapaneseText>お〜〜〜。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1850</PointerOffset>
    <JapaneseText>俺、そんな難しい事</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1870</PointerOffset>
    <JapaneseText>考えた事もなかったよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1891</PointerOffset>
    <JapaneseText>すごいな、ロイドって！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1914</PointerOffset>
    <JapaneseText>そ、そんな事ないって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1935</PointerOffset>
    <JapaneseText>あー、それより今の話、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1959</PointerOffset>
    <JapaneseText>何か本気で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1971</PointerOffset>
    <JapaneseText>気になってきたぞ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1992</PointerOffset>
    <JapaneseText>あとで、先生に聞いてみようかな……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
