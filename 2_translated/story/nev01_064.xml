<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>390</PointerOffset>
    <JapaneseText>この船を見るのは初めてじゃあない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>17</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>426</PointerOffset>
    <JapaneseText>各地のギルドの間でも、この船や</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>18</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>458</PointerOffset>
    <JapaneseText>アドリビトムってのは結構有名だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>19</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>489</PointerOffset>
    <JapaneseText>ユージーンか……でかいな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>20</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>517</PointerOffset>
    <JapaneseText>初めて見るが、ガジュマって種族は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>21</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>551</PointerOffset>
    <JapaneseText>みんなああいうもんなのか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>22</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>578</PointerOffset>
    <JapaneseText>……ったく、火山ってのは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>23</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>604</PointerOffset>
    <JapaneseText>何であんなに暑いんだ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>24</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>627</PointerOffset>
    <JapaneseText>おかげで、船にいるだけでも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>25</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>655</PointerOffset>
    <JapaneseText>体が重いっつーか……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>26</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>679</PointerOffset>
    <JapaneseText>会話をするのもやっとだぜ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>27</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>708</PointerOffset>
    <JapaneseText>エステルの奴、知識はあるけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>28</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>738</PointerOffset>
    <JapaneseText>独特の解釈する事があるからな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>29</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>770</PointerOffset>
    <JapaneseText>あんま、真に受けない方がいいぜ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>801</PointerOffset>
    <JapaneseText>アルマナック遺跡にゃ、盗掘者対策の罠が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>31</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>841</PointerOffset>
    <JapaneseText>複数仕掛けられてるんだとさ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>871</PointerOffset>
    <JapaneseText>……ったく、ご苦労なこった</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>898</PointerOffset>
    <JapaneseText>ここに来てから、エステルは毎日</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>34</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>930</PointerOffset>
    <JapaneseText>充実してるみたいだな。そんだけでも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>35</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>966</PointerOffset>
    <JapaneseText>厄介になった甲斐はあったって事か</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>36</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>999</PointerOffset>
    <JapaneseText>オレも色んな人間見てきたけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>37</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1031</PointerOffset>
    <JapaneseText>忍者ってのには初めて会ったな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>38</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1063</PointerOffset>
    <JapaneseText>……感情は不要なもの、か……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>39</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1092</PointerOffset>
    <JapaneseText>ジルディアのキバ、か。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>40</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1116</PointerOffset>
    <JapaneseText>また、とんでもねえモンが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>41</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1142</PointerOffset>
    <JapaneseText>出てきやがったな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>42</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1159</PointerOffset>
    <JapaneseText>ジューダスの仮面、何かの頭蓋骨に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>43</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1193</PointerOffset>
    <JapaneseText>見えねえか？　わざわざ被るくらいだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>44</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1231</PointerOffset>
    <JapaneseText>何か、形見の骨だったりしてな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>45</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1260</PointerOffset>
    <JapaneseText>しっかし、ここのギルドは賑やかだな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>46</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1298</PointerOffset>
    <JapaneseText>……何ていうか、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>47</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1316</PointerOffset>
    <JapaneseText>下町の空気を思い出すよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>48</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1339</PointerOffset>
    <JapaneseText>この間、食事当番になった時に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>49</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1371</PointerOffset>
    <JapaneseText>間食に軽くつまめるような</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>50</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1397</PointerOffset>
    <JapaneseText>スナック菓子を作っておいたんだが……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>51</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1434</PointerOffset>
    <JapaneseText>オレのいない間に勝手に味を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>52</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1462</PointerOffset>
    <JapaneseText>変えられて、まいったぜ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>53</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1488</PointerOffset>
    <JapaneseText>あれはオレのせいじゃねぇっつうの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>54</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1521</PointerOffset>
    <JapaneseText>創世の起こりだのジルディアだの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>55</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1553</PointerOffset>
    <JapaneseText>ちょっと前からじゃ、考えられねえ話の</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>56</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1591</PointerOffset>
    <JapaneseText>オンパレードだが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>57</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1608</PointerOffset>
    <JapaneseText>今じゃ、すっかりそれが「普通」に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>58</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1642</PointerOffset>
    <JapaneseText>なっちまった。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>59</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1658</PointerOffset>
    <JapaneseText>人生ってのは不思議なもんだな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>60</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1687</PointerOffset>
    <JapaneseText>オレ達は、生きて、自分達の選んだ選択に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>61</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1727</PointerOffset>
    <JapaneseText>責任を持っていかなきゃなんねえ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>62</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1758</PointerOffset>
    <JapaneseText>この戦い、絶対負けられねえな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>63</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1790</PointerOffset>
    <JapaneseText>オレ達が、今のずっと先を生きてく為にも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1829</PointerOffset>
    <JapaneseText>よっ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1837</PointerOffset>
    <JapaneseText>お疲れさん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1851</PointerOffset>
    <JapaneseText>元気そうじゃねえか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1870</PointerOffset>
    <JapaneseText>こっちはそれなりに、楽しくやってたぜ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1910</PointerOffset>
    <JapaneseText>近いうち、そっちの近況も聞かせろよな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1947</PointerOffset>
    <JapaneseText>今回選んだ事の責任は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1971</PointerOffset>
    <JapaneseText>ちゃんと背負っていかねえとな。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2003</PointerOffset>
    <JapaneseText>それがけじめってもんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2026</PointerOffset>
    <JapaneseText>…よう、ちっと休ませてもらうぜ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
