<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>1225</PointerOffset>
    <JapaneseText>ほう、驚きましたね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1247</PointerOffset>
    <JapaneseText>この船は星晶を使用した内燃機関と</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1281</PointerOffset>
    <JapaneseText>思っていたのですが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1300</PointerOffset>
    <JapaneseText>これは半永久機関だ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1322</PointerOffset>
    <JapaneseText>別の世界で同じ仕組みの船を見た事がある</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1361</PointerOffset>
    <JapaneseText>ふむ…、異常の原因は調整に必要な計算を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1401</PointerOffset>
    <JapaneseText>間違えた為だろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1418</PointerOffset>
    <JapaneseText>うっ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1425</PointerOffset>
    <JapaneseText>では、機器類自体に異常は無いのですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1462</PointerOffset>
    <JapaneseText>そういえば、僕達この船に乗って長いけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1504</PointerOffset>
    <JapaneseText>燃料の星晶を補充したところは、一度も</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1542</PointerOffset>
    <JapaneseText>見た事なかったよね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1561</PointerOffset>
    <JapaneseText>チャットは知っていたのですか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1592</PointerOffset>
    <JapaneseText>もちろんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1605</PointerOffset>
    <JapaneseText>…本当ですか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1620</PointerOffset>
    <JapaneseText>そういう大事な話は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1642</PointerOffset>
    <JapaneseText>早くして頂かないと困ります</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>108</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1669</PointerOffset>
    <JapaneseText>いいですか。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>109</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1683</PointerOffset>
    <JapaneseText>この船は星晶で動いていない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>110</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1713</PointerOffset>
    <JapaneseText>これがいかに重要な話かわかりますか？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>111</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1750</PointerOffset>
    <JapaneseText>星晶を採り尽くし、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>112</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1770</PointerOffset>
    <JapaneseText>後退に向かうしかないこの世界を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>113</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1802</PointerOffset>
    <JapaneseText>救う光明となるかもしれないのですよ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>114</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1839</PointerOffset>
    <JapaneseText>それじゃあ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>115</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1852</PointerOffset>
    <JapaneseText>もし、この機関の仕組みを解明し、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>116</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1886</PointerOffset>
    <JapaneseText>同じ物が作れたとしたら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>117</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1909</PointerOffset>
    <JapaneseText>星晶を巡って国家同士が争う事も、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>118</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1943</PointerOffset>
    <JapaneseText>採掘の為に人々が駆りだされる事も</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>119</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1977</PointerOffset>
    <JapaneseText>無くなりますよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>120</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1992</PointerOffset>
    <JapaneseText>そ、そうですが…！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>121</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2011</PointerOffset>
    <JapaneseText>ボクはひいおじいさんとの約束で、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>122</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2045</PointerOffset>
    <JapaneseText>この船の技術を守る様に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>123</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2069</PointerOffset>
    <JapaneseText>言われていたんですよ…！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>124</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2094</PointerOffset>
    <JapaneseText>…今回故障したのは想定外でしたけど…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>125</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2131</PointerOffset>
    <JapaneseText>灯台下暗し…だね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>126</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2148</PointerOffset>
    <JapaneseText>変わった船だなぁ、とは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>127</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2172</PointerOffset>
    <JapaneseText>思っていたけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>128</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2187</PointerOffset>
    <JapaneseText>この機関はヴェラトローパの民によるものか、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>129</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2231</PointerOffset>
    <JapaneseText>それとも異世界の物なのか…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>130</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2258</PointerOffset>
    <JapaneseText>いずれにしろ、星晶を用いない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>131</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2288</PointerOffset>
    <JapaneseText>優れた技術を持った民がいたんですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>132</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2323</PointerOffset>
    <JapaneseText>半永久機関、か。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>133</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2341</PointerOffset>
    <JapaneseText>…方法が無い訳ではないが…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>134</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2368</PointerOffset>
    <JapaneseText>何か名案でも？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>135</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2383</PointerOffset>
    <JapaneseText>…いや、すまない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>136</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2403</PointerOffset>
    <JapaneseText>我々に今出来る事ではない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>137</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2428</PointerOffset>
    <JapaneseText>ああ、直りました！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>138</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2447</PointerOffset>
    <JapaneseText>ううっ…本当に良かった…！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>139</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2476</PointerOffset>
    <JapaneseText>船の高度も上がっている様ですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>140</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2507</PointerOffset>
    <JapaneseText>やれやれ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>141</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2519</PointerOffset>
    <JapaneseText>一時はどうなる事かと思いましたよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>142</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2552</PointerOffset>
    <JapaneseText>これでまた仕事に励めますね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>143</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
