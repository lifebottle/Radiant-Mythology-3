<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>490</PointerOffset>
      <JapaneseText>挨拶は大事よ。特に、顔合わせの時と</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>526</PointerOffset>
      <JapaneseText>仕事始めの時は、重要ね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>552</PointerOffset>
      <JapaneseText>何事も、最初が肝心って言うでしょ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>587</PointerOffset>
      <JapaneseText>ショップが開店したみたいね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>617</PointerOffset>
      <JapaneseText>まだ見てないなら、行ってみるといいわよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>659</PointerOffset>
      <JapaneseText>「備えあれば憂いなし」って言うでしょ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>698</PointerOffset>
      <JapaneseText>スタンってほんと、お気楽って言うか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>734</PointerOffset>
      <JapaneseText>お人好しって言うか……。誰かがしっかりして</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>778</PointerOffset>
      <JapaneseText>見ていてやんなきゃって気持ちになるのよねぇ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>821</PointerOffset>
      <JapaneseText>お姫様が来たって聞いた時は驚いたわぁ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>861</PointerOffset>
      <JapaneseText>エステルって、結構いい子そうだし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>895</PointerOffset>
      <JapaneseText>あたしの夢に資金援助してくれないかしら♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>936</PointerOffset>
      <JapaneseText>ダンジョンで、緑色の宝箱を見つけたら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>974</PointerOffset>
      <JapaneseText>絶対開けていきましょ！　貴重なお宝が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1012</PointerOffset>
      <JapaneseText>入ってる可能性が大よ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1037</PointerOffset>
      <JapaneseText>コクヨウ玉虫って、絶滅危惧種なのよね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1077</PointerOffset>
      <JapaneseText>って事は、売却価格は高騰してるのよね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1117</PointerOffset>
      <JapaneseText>……あ、そんな事しないわよ？　思っただけ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1160</PointerOffset>
      <JapaneseText>虫っぽい見た目じゃなくなったから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1194</PointerOffset>
      <JapaneseText>気持ち悪さはなくなったけど……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1228</PointerOffset>
      <JapaneseText>何とも言えない不気味さがあるわよねえ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1269</PointerOffset>
      <JapaneseText>ちょっと！　コクヨウ玉虫って、標本として</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1311</PointerOffset>
      <JapaneseText>高く売れるらしいじゃない！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1341</PointerOffset>
      <JapaneseText>ウィルと交渉出来ないかしら……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1372</PointerOffset>
      <JapaneseText>スタンが例の宗教のチラシを、珍しそうに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1412</PointerOffset>
      <JapaneseText>見てるんだけど。まさか、面白半分で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1448</PointerOffset>
      <JapaneseText>入ってみたいとか言い出さないわよね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1487</PointerOffset>
      <JapaneseText>願いを叶える煙って、ガルドをどっさり</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1525</PointerOffset>
      <JapaneseText>出す事も出来るって事なのかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1559</PointerOffset>
      <JapaneseText>……例えばの話で、試さないわよ、もちろん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1600</PointerOffset>
      <JapaneseText>ラザリスが、星晶で封印されていた</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1634</PointerOffset>
      <JapaneseText>太古の「災厄」だったなんて、ね……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1672</PointerOffset>
      <JapaneseText>ヒトが太刀打ち出来る存在なのかしら……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1713</PointerOffset>
      <JapaneseText>カノンノの事は心配だけど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1741</PointerOffset>
      <JapaneseText>ただ心配してたってなんにもならないわ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1781</PointerOffset>
      <JapaneseText>あの子の分も、あたし達が頑張りましょ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1820</PointerOffset>
      <JapaneseText>レディアント、だったわよね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1850</PointerOffset>
      <JapaneseText>ディセンダー専用の武具があるなんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1886</PointerOffset>
      <JapaneseText>超レアじゃない！？　入手しないテはないわ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1929</PointerOffset>
      <JapaneseText>ルカってホントに真面目よねえ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1961</PointerOffset>
      <JapaneseText>イリアがからかいたくなる気持ちも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1995</PointerOffset>
      <JapaneseText>何となくわかるわ〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2014</PointerOffset>
      <JapaneseText>思ってたより、お宝って程のモノは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2048</PointerOffset>
      <JapaneseText>なかったわね〜。ま、別にいいんだけど。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2088</PointerOffset>
      <JapaneseText>これくらいでめげてたら、仕事にならないし！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2131</PointerOffset>
      <JapaneseText>あのキバって、変に見た目が綺麗じゃない？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2173</PointerOffset>
      <JapaneseText>だから、キバの破片を売り物にしようと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2211</PointerOffset>
      <JapaneseText>企む奴も出て来てるみたいね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2238</PointerOffset>
      <JapaneseText>ね、オーラクリスタルって知ってる？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2274</PointerOffset>
      <JapaneseText>精霊の加護を受けた、すっごくレアで高純度の</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2318</PointerOffset>
      <JapaneseText>天然鉱物なんだけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>74</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2341</PointerOffset>
      <JapaneseText>それって、精霊の加護がなくなったら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>75</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2377</PointerOffset>
      <JapaneseText>消えちゃうって事なのよね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>76</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2405</PointerOffset>
      <JapaneseText>って事はもう、手に入らなくなっちゃうの！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>77</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2448</PointerOffset>
      <JapaneseText>誰かが生きてく為に、切り捨てられる命なんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>78</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2492</PointerOffset>
      <JapaneseText>たくさんある。ヒトも、植物も動物も……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>79</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2534</PointerOffset>
      <JapaneseText>でも、だからって世界を塗り替えるなんて！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2575</PointerOffset>
      <JapaneseText>カノンノって、ほわ〜っとした雰囲気だけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2617</PointerOffset>
      <JapaneseText>本当に芯が強い子よね。……他人の存在を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2657</PointerOffset>
      <JapaneseText>完全に受け止めるなんて、簡単には出来ないわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2700</PointerOffset>
      <JapaneseText>とうとう最後の戦いね。誰を連れて行くか、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2742</PointerOffset>
      <JapaneseText>しっかり考えて決めるのよ？　あたし達は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2782</PointerOffset>
      <JapaneseText>みんな、あんたの事を信じてるから！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2817</PointerOffset>
      <JapaneseText>みんながやけに騒いでると思ったら……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2857</PointerOffset>
      <JapaneseText>まったく、ケロッとした顔で帰ってきて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2895</PointerOffset>
      <JapaneseText>くれちゃって！　ま、元気そうで何よりだわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2936</PointerOffset>
      <JapaneseText>スタンはね、たまーにオルタ・ビレッジへ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2976</PointerOffset>
      <JapaneseText>羊牧の指導に行ってるの。びっくりでしょ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3018</PointerOffset>
      <JapaneseText>生意気に先生なんて呼ばれてて、変な感じよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3059</PointerOffset>
      <JapaneseText>なーに？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3069</PointerOffset>
      <JapaneseText>見舞金くらい、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3085</PointerOffset>
      <JapaneseText>持ってきたんでしょーね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>