<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>340</PointerOffset>
      <JapaneseText>あの煙の噂、街の中で</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>362</PointerOffset>
      <JapaneseText>ものすごい広まってるぜ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>388</PointerOffset>
      <JapaneseText>ありゃ、危ねェな。今にもっと大騒ぎになるぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>431</PointerOffset>
      <JapaneseText>ルカの奴、何か見ててイライラしねェ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>469</PointerOffset>
      <JapaneseText>一人でウジウジ悩んでてよ……何で周りに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>509</PointerOffset>
      <JapaneseText>相談しねェんだ？　ちっといじるか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>546</PointerOffset>
      <JapaneseText>ロックスの作る飯は美味いよなぁ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>580</PointerOffset>
      <JapaneseText>オレが実家で食ってた料理と、大して違いが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>622</PointerOffset>
      <JapaneseText>ないんだぜ。何モンなんだよ、アイツ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>657</PointerOffset>
      <JapaneseText>よっ、ディセンダー。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>679</PointerOffset>
      <JapaneseText>つっても、あんま実感湧かねェけどな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>717</PointerOffset>
      <JapaneseText>正体がどうあれ、お前はお前なんだしよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>754</PointerOffset>
      <JapaneseText>前から思ってたんだけどよ……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>786</PointerOffset>
      <JapaneseText>エミルって見てるとこう、いじりたくて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>824</PointerOffset>
      <JapaneseText>ムズムズするよなぁ？　そう思わねェ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>861</PointerOffset>
      <JapaneseText>面倒事が片付いたらよ、今度は帆船か何かで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>903</PointerOffset>
      <JapaneseText>のんびり世界一周ってのもいいよなぁ……。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>945</PointerOffset>
      <JapaneseText>船旅ってェのも、結構いいもんなんだぜ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>984</PointerOffset>
      <JapaneseText>サレの奴を置いて来ちまって良かったのかァ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1028</PointerOffset>
      <JapaneseText>せめてこう、どっかに閉じ込めちまうとか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1072</PointerOffset>
      <JapaneseText>色々やり方はあったんじゃねェ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1103</PointerOffset>
      <JapaneseText>ヴェイグはマジでクレアの事が大事なんだな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1147</PointerOffset>
      <JapaneseText>サレに向かっていくあいつの気合い、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1183</PointerOffset>
      <JapaneseText>ハンパなかったぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1200</PointerOffset>
      <JapaneseText>何だかんだで、ルカちゃまにも</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1230</PointerOffset>
      <JapaneseText>それなりにダチが出来たみてェだな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1266</PointerOffset>
      <JapaneseText>あいつもやりゃあ出来ンじゃねえか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1299</PointerOffset>
      <JapaneseText>お前、相変わらず無茶すンなぁ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1331</PointerOffset>
      <JapaneseText>……おい、少しは肩の力を抜けよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1365</PointerOffset>
      <JapaneseText>ちったァ、自分の体の事も考えてやんな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1404</PointerOffset>
      <JapaneseText>そういや、お前って兄弟はいねェんだよな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1446</PointerOffset>
      <JapaneseText>いなくて正解だな。ディセンダーが二人……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1488</PointerOffset>
      <JapaneseText>なんて事ンなったら、大揉め間違いなしだぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1529</PointerOffset>
      <JapaneseText>お前、最終決戦だからって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1555</PointerOffset>
      <JapaneseText>ビビってねえよなァ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1576</PointerOffset>
      <JapaneseText>……へへっ、冗談だよ。本気にすンなって。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1618</PointerOffset>
      <JapaneseText>今度もその度胸でビシッと決めな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1652</PointerOffset>
      <JapaneseText>お前の背中は、オレが守ってやっからよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1691</PointerOffset>
      <JapaneseText>よっ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1699</PointerOffset>
      <JapaneseText>随分遅いお帰りだなァ！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1724</PointerOffset>
      <JapaneseText>心配させやがって……このツケはどうやって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1766</PointerOffset>
      <JapaneseText>払うつもりだ？　あぁ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1789</PointerOffset>
      <JapaneseText>ルカちゃまもイリアも、この頃</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1819</PointerOffset>
      <JapaneseText>何かと気合い入ってんだよな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1849</PointerOffset>
      <JapaneseText>さて、俺はこれからどうすっかな……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1886</PointerOffset>
      <JapaneseText>テメェは平気だってか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1910</PointerOffset>
      <JapaneseText>ったく、悪運が強ぇ奴だな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>