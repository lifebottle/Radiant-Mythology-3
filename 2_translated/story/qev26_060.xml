<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>408</PointerOffset>
      <JapaneseText>早速ですが……、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>426</PointerOffset>
      <JapaneseText>工房をお借りしたいのですが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>453</PointerOffset>
      <JapaneseText>えっ？　こ、工房？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>472</PointerOffset>
      <JapaneseText>ああ、お守りを作る場所が</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>498</PointerOffset>
      <JapaneseText>欲しいって事だよね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>520</PointerOffset>
      <JapaneseText>待ってね、ボクが何とかするから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>551</PointerOffset>
      <JapaneseText>……はい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>568</PointerOffset>
      <JapaneseText>今日はありがとう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>585</PointerOffset>
      <JapaneseText>ボク……、一人じゃ緊張して</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>613</PointerOffset>
      <JapaneseText>何も話せなかったかも……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>638</PointerOffset>
      <JapaneseText>だから、悔しいけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>658</PointerOffset>
      <JapaneseText>……ちょっとだけ助かったよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>694</PointerOffset>
      <JapaneseText>○○が困った時は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>714</PointerOffset>
      <JapaneseText>今度はボクが力になるから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>740</PointerOffset>
      <JapaneseText>何でも言ってね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>755</PointerOffset>
      <JapaneseText>それじゃ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>766</PointerOffset>
      <JapaneseText>プレセアがアドリビトムに加入しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>