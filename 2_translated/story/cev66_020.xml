<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>236</PointerOffset>
    <JapaneseText>あなた、随分勉強熱心なのね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>19</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>263</PointerOffset>
    <JapaneseText>当然だ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>20</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>273</PointerOffset>
    <JapaneseText>知識は有しているだけでは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>21</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>299</PointerOffset>
    <JapaneseText>ただの情報に過ぎないが</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>22</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>322</PointerOffset>
    <JapaneseText>利用の仕方によっては、多くを生み出し</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>23</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>360</PointerOffset>
    <JapaneseText>多くを助ける術にも成り得る</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>24</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>387</PointerOffset>
    <JapaneseText>だからぼくは、ぼくの知らない事を知りたい。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>25</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>431</PointerOffset>
    <JapaneseText>そして、得た知識から、この世界の未来を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>26</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>471</PointerOffset>
    <JapaneseText>切り開く手段を絶対に見つけてみせる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>27</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>506</PointerOffset>
    <JapaneseText>「オルタ・ビレッジ」を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>28</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>530</PointerOffset>
    <JapaneseText>ただの理想で終わらせない為にも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>29</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>562</PointerOffset>
    <JapaneseText>それは必要な事なんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>30</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>583</PointerOffset>
    <JapaneseText>志が高いのね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>31</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>596</PointerOffset>
    <JapaneseText>あなたの知らない世界の情報なんて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>32</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>632</PointerOffset>
    <JapaneseText>それこそ限りがないと思うけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>33</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
