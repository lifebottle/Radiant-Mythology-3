<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>1128</PointerOffset>
      <JapaneseText>ようこそ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>80</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1140</PointerOffset>
      <JapaneseText>自由のギルド、アドリビトムへ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>81</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1169</PointerOffset>
      <JapaneseText>わたしはリーダーを務めるアンジュ・セレーナ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>82</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1215</PointerOffset>
      <JapaneseText>ライマ国の方々、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>83</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1233</PointerOffset>
      <JapaneseText>まずは、ごゆっくりおくつろぎ下さい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>84</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1268</PointerOffset>
      <JapaneseText>心遣い、痛み入る</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>85</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1285</PointerOffset>
      <JapaneseText>私は、ヴァン・グランツ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>86</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1311</PointerOffset>
      <JapaneseText>ライマ国に属し、この二人に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>87</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1339</PointerOffset>
      <JapaneseText>剣を教えている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>88</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1354</PointerOffset>
      <JapaneseText>アッシュだ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>89</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1367</PointerOffset>
      <JapaneseText>俺はルーク。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1381</PointerOffset>
      <JapaneseText>ルーク・フォン・ファブレ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1406</PointerOffset>
      <JapaneseText>やれやれ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1418</PointerOffset>
      <JapaneseText>二人とも、剣の腕はともかく、礼儀の方は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1458</PointerOffset>
      <JapaneseText>別の教師を付ける必要がありますね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1491</PointerOffset>
      <JapaneseText>ここで働いて下さるなら、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1517</PointerOffset>
      <JapaneseText>わたしが教えて差し上げますけど？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1550</PointerOffset>
      <JapaneseText>はあ？　何言ってんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1574</PointerOffset>
      <JapaneseText>俺は、ここで働かせてくれなんて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1606</PointerOffset>
      <JapaneseText>頼んでねーぞ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1619</PointerOffset>
      <JapaneseText>だが、今ライマ国は動乱の最中だ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1653</PointerOffset>
      <JapaneseText>王位継承者であるお前達を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1679</PointerOffset>
      <JapaneseText>連れて戻るわけにはいかん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1704</PointerOffset>
      <JapaneseText>まあ、王位継承者なのですか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1733</PointerOffset>
      <JapaneseText>そう言えば、お顔がそっくりだけれど、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1771</PointerOffset>
      <JapaneseText>双子…なのかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1790</PointerOffset>
      <JapaneseText>ええ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1798</PointerOffset>
      <JapaneseText>二人は、ファブレ公爵家の子息</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1827</PointerOffset>
      <JapaneseText>ルークが兄で第一継承者。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1853</PointerOffset>
      <JapaneseText>アッシュが弟で第二継承者になります</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1888</PointerOffset>
      <JapaneseText>彼らはヴァンの指導の下にて、王族にのみ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1928</PointerOffset>
      <JapaneseText>伝えられるアルバート流の剣術を授かる為に、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1972</PointerOffset>
      <JapaneseText>修行の旅に出ていたのです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1997</PointerOffset>
      <JapaneseText>アンジュには既に話しましたが、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2029</PointerOffset>
      <JapaneseText>しばらくライマには戻れませんし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>114</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2060</PointerOffset>
      <JapaneseText>ここで修行の延長として働くのも、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2094</PointerOffset>
      <JapaneseText>悪くないかと思いましてね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2119</PointerOffset>
      <JapaneseText>そういうわけで、あなた方をこの船に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2155</PointerOffset>
      <JapaneseText>呼んだのですよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2170</PointerOffset>
      <JapaneseText>それじゃあ、事情がわかったところで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>119</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2206</PointerOffset>
      <JapaneseText>意思確認をさせてもらおうかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2235</PointerOffset>
      <JapaneseText>あなた方、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2247</PointerOffset>
      <JapaneseText>アドリビトムで働く気はあるかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2282</PointerOffset>
      <JapaneseText>ちっ…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>123</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2292</PointerOffset>
      <JapaneseText>他に選択肢はないんだろう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2319</PointerOffset>
      <JapaneseText>冗談じゃねえ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>125</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2335</PointerOffset>
      <JapaneseText>何で俺がそんな下々がやる様な仕事</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2369</PointerOffset>
      <JapaneseText>やんなきゃなんねーんだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>127</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2394</PointerOffset>
      <JapaneseText>あっ、そう。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>128</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2408</PointerOffset>
      <JapaneseText>じゃあ、お引取り下さいな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>129</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2433</PointerOffset>
      <JapaneseText>働かない人を養う程、ウチは楽じゃないの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2472</PointerOffset>
      <JapaneseText>なっ…！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>131</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2483</PointerOffset>
      <JapaneseText>ルーク、これも修行だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>132</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2504</PointerOffset>
      <JapaneseText>王となるべき者、民衆がどの様な助けを</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>133</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2542</PointerOffset>
      <JapaneseText>欲しているのかを知るいい機会になるだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>134</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2583</PointerOffset>
      <JapaneseText>わーかったよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>135</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2599</PointerOffset>
      <JapaneseText>師匠がそう言うんなら、修行として</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>136</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2633</PointerOffset>
      <JapaneseText>働いてやるよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>137</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2646</PointerOffset>
      <JapaneseText>ふふふ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>138</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2656</PointerOffset>
      <JapaneseText>それじゃあ、あなた達をアドリビトムの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>139</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2694</PointerOffset>
      <JapaneseText>メンバーとして迎えます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2717</PointerOffset>
      <JapaneseText>これからしっかり働いてちょうだいね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>141</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2754</PointerOffset>
      <JapaneseText>ルークがアドリビトムに加入しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>142</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2789</PointerOffset>
      <JapaneseText>アッシュがアドリビトムに加入しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>143</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2826</PointerOffset>
      <JapaneseText>ヴァンがアドリビトムに加入しました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>144</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>