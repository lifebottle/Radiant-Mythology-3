<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>1128</PointerOffsetDec>
    <PointerOffsetHex>0x468</PointerOffsetHex>
    <JapaneseText>ようこそ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>80</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1140</PointerOffsetDec>
    <PointerOffsetHex>0x474</PointerOffsetHex>
    <JapaneseText>自由のギルド、アドリビトムへ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>81</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1169</PointerOffsetDec>
    <PointerOffsetHex>0x491</PointerOffsetHex>
    <JapaneseText>わたしはリーダーを務めるアンジュ・セレーナ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>82</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1215</PointerOffsetDec>
    <PointerOffsetHex>0x4bf</PointerOffsetHex>
    <JapaneseText>ライマ国の方々、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>83</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1233</PointerOffsetDec>
    <PointerOffsetHex>0x4d1</PointerOffsetHex>
    <JapaneseText>まずは、ごゆっくりおくつろぎ下さい</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>84</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1268</PointerOffsetDec>
    <PointerOffsetHex>0x4f4</PointerOffsetHex>
    <JapaneseText>心遣い、痛み入る</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>85</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1285</PointerOffsetDec>
    <PointerOffsetHex>0x505</PointerOffsetHex>
    <JapaneseText>私は、ヴァン・グランツ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>86</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1311</PointerOffsetDec>
    <PointerOffsetHex>0x51f</PointerOffsetHex>
    <JapaneseText>ライマ国に属し、この二人に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>87</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1339</PointerOffsetDec>
    <PointerOffsetHex>0x53b</PointerOffsetHex>
    <JapaneseText>剣を教えている</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>88</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1354</PointerOffsetDec>
    <PointerOffsetHex>0x54a</PointerOffsetHex>
    <JapaneseText>アッシュだ…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>89</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1367</PointerOffsetDec>
    <PointerOffsetHex>0x557</PointerOffsetHex>
    <JapaneseText>俺はルーク。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>90</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1381</PointerOffsetDec>
    <PointerOffsetHex>0x565</PointerOffsetHex>
    <JapaneseText>ルーク・フォン・ファブレ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>91</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1406</PointerOffsetDec>
    <PointerOffsetHex>0x57e</PointerOffsetHex>
    <JapaneseText>やれやれ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>92</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1418</PointerOffsetDec>
    <PointerOffsetHex>0x58a</PointerOffsetHex>
    <JapaneseText>二人とも、剣の腕はともかく、礼儀の方は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>93</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1458</PointerOffsetDec>
    <PointerOffsetHex>0x5b2</PointerOffsetHex>
    <JapaneseText>別の教師を付ける必要がありますね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>94</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1491</PointerOffsetDec>
    <PointerOffsetHex>0x5d3</PointerOffsetHex>
    <JapaneseText>ここで働いて下さるなら、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>95</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1517</PointerOffsetDec>
    <PointerOffsetHex>0x5ed</PointerOffsetHex>
    <JapaneseText>わたしが教えて差し上げますけど？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>96</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1550</PointerOffsetDec>
    <PointerOffsetHex>0x60e</PointerOffsetHex>
    <JapaneseText>はあ？　何言ってんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>97</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1574</PointerOffsetDec>
    <PointerOffsetHex>0x626</PointerOffsetHex>
    <JapaneseText>俺は、ここで働かせてくれなんて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>98</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1606</PointerOffsetDec>
    <PointerOffsetHex>0x646</PointerOffsetHex>
    <JapaneseText>頼んでねーぞ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>99</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1619</PointerOffsetDec>
    <PointerOffsetHex>0x653</PointerOffsetHex>
    <JapaneseText>だが、今ライマ国は動乱の最中だ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>100</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1653</PointerOffsetDec>
    <PointerOffsetHex>0x675</PointerOffsetHex>
    <JapaneseText>王位継承者であるお前達を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>101</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1679</PointerOffsetDec>
    <PointerOffsetHex>0x68f</PointerOffsetHex>
    <JapaneseText>連れて戻るわけにはいかん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>102</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1704</PointerOffsetDec>
    <PointerOffsetHex>0x6a8</PointerOffsetHex>
    <JapaneseText>まあ、王位継承者なのですか…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>103</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1733</PointerOffsetDec>
    <PointerOffsetHex>0x6c5</PointerOffsetHex>
    <JapaneseText>そう言えば、お顔がそっくりだけれど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>104</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1771</PointerOffsetDec>
    <PointerOffsetHex>0x6eb</PointerOffsetHex>
    <JapaneseText>双子…なのかしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>105</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1790</PointerOffsetDec>
    <PointerOffsetHex>0x6fe</PointerOffsetHex>
    <JapaneseText>ええ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>106</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1798</PointerOffsetDec>
    <PointerOffsetHex>0x706</PointerOffsetHex>
    <JapaneseText>二人は、ファブレ公爵家の子息</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>107</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1827</PointerOffsetDec>
    <PointerOffsetHex>0x723</PointerOffsetHex>
    <JapaneseText>ルークが兄で第一継承者。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>108</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1853</PointerOffsetDec>
    <PointerOffsetHex>0x73d</PointerOffsetHex>
    <JapaneseText>アッシュが弟で第二継承者になります</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>109</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1888</PointerOffsetDec>
    <PointerOffsetHex>0x760</PointerOffsetHex>
    <JapaneseText>彼らはヴァンの指導の下にて、王族にのみ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>110</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1928</PointerOffsetDec>
    <PointerOffsetHex>0x788</PointerOffsetHex>
    <JapaneseText>伝えられるアルバート流の剣術を授かる為に、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>111</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1972</PointerOffsetDec>
    <PointerOffsetHex>0x7b4</PointerOffsetHex>
    <JapaneseText>修行の旅に出ていたのです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>112</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1997</PointerOffsetDec>
    <PointerOffsetHex>0x7cd</PointerOffsetHex>
    <JapaneseText>アンジュには既に話しましたが、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>113</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2029</PointerOffsetDec>
    <PointerOffsetHex>0x7ed</PointerOffsetHex>
    <JapaneseText>しばらくライマには戻れませんし</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>114</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2060</PointerOffsetDec>
    <PointerOffsetHex>0x80c</PointerOffsetHex>
    <JapaneseText>ここで修行の延長として働くのも、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>115</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2094</PointerOffsetDec>
    <PointerOffsetHex>0x82e</PointerOffsetHex>
    <JapaneseText>悪くないかと思いましてね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>116</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2119</PointerOffsetDec>
    <PointerOffsetHex>0x847</PointerOffsetHex>
    <JapaneseText>そういうわけで、あなた方をこの船に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>117</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2155</PointerOffsetDec>
    <PointerOffsetHex>0x86b</PointerOffsetHex>
    <JapaneseText>呼んだのですよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>118</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2170</PointerOffsetDec>
    <PointerOffsetHex>0x87a</PointerOffsetHex>
    <JapaneseText>それじゃあ、事情がわかったところで</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>119</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2206</PointerOffsetDec>
    <PointerOffsetHex>0x89e</PointerOffsetHex>
    <JapaneseText>意思確認をさせてもらおうかな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>120</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2235</PointerOffsetDec>
    <PointerOffsetHex>0x8bb</PointerOffsetHex>
    <JapaneseText>あなた方、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>121</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2247</PointerOffsetDec>
    <PointerOffsetHex>0x8c7</PointerOffsetHex>
    <JapaneseText>アドリビトムで働く気はあるかしら？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>122</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2282</PointerOffsetDec>
    <PointerOffsetHex>0x8ea</PointerOffsetHex>
    <JapaneseText>ちっ…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>123</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2292</PointerOffsetDec>
    <PointerOffsetHex>0x8f4</PointerOffsetHex>
    <JapaneseText>他に選択肢はないんだろう？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>124</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2319</PointerOffsetDec>
    <PointerOffsetHex>0x90f</PointerOffsetHex>
    <JapaneseText>冗談じゃねえ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>125</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2335</PointerOffsetDec>
    <PointerOffsetHex>0x91f</PointerOffsetHex>
    <JapaneseText>何で俺がそんな下々がやる様な仕事</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>126</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2369</PointerOffsetDec>
    <PointerOffsetHex>0x941</PointerOffsetHex>
    <JapaneseText>やんなきゃなんねーんだよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>127</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2394</PointerOffsetDec>
    <PointerOffsetHex>0x95a</PointerOffsetHex>
    <JapaneseText>あっ、そう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>128</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2408</PointerOffsetDec>
    <PointerOffsetHex>0x968</PointerOffsetHex>
    <JapaneseText>じゃあ、お引取り下さいな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>129</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2433</PointerOffsetDec>
    <PointerOffsetHex>0x981</PointerOffsetHex>
    <JapaneseText>働かない人を養う程、ウチは楽じゃないの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>130</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2472</PointerOffsetDec>
    <PointerOffsetHex>0x9a8</PointerOffsetHex>
    <JapaneseText>なっ…！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>131</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2483</PointerOffsetDec>
    <PointerOffsetHex>0x9b3</PointerOffsetHex>
    <JapaneseText>ルーク、これも修行だ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>132</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2504</PointerOffsetDec>
    <PointerOffsetHex>0x9c8</PointerOffsetHex>
    <JapaneseText>王となるべき者、民衆がどの様な助けを</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>133</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2542</PointerOffsetDec>
    <PointerOffsetHex>0x9ee</PointerOffsetHex>
    <JapaneseText>欲しているのかを知るいい機会になるだろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>134</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2583</PointerOffsetDec>
    <PointerOffsetHex>0xa17</PointerOffsetHex>
    <JapaneseText>わーかったよ！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>135</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2599</PointerOffsetDec>
    <PointerOffsetHex>0xa27</PointerOffsetHex>
    <JapaneseText>師匠がそう言うんなら、修行として</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>136</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2633</PointerOffsetDec>
    <PointerOffsetHex>0xa49</PointerOffsetHex>
    <JapaneseText>働いてやるよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>137</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2646</PointerOffsetDec>
    <PointerOffsetHex>0xa56</PointerOffsetHex>
    <JapaneseText>ふふふ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>138</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2656</PointerOffsetDec>
    <PointerOffsetHex>0xa60</PointerOffsetHex>
    <JapaneseText>それじゃあ、あなた達をアドリビトムの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>139</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2694</PointerOffsetDec>
    <PointerOffsetHex>0xa86</PointerOffsetHex>
    <JapaneseText>メンバーとして迎えます</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>140</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2717</PointerOffsetDec>
    <PointerOffsetHex>0xa9d</PointerOffsetHex>
    <JapaneseText>これからしっかり働いてちょうだいね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>141</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2754</PointerOffsetDec>
    <PointerOffsetHex>0xac2</PointerOffsetHex>
    <JapaneseText>ルークがアドリビトムに加入しました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>142</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2789</PointerOffsetDec>
    <PointerOffsetHex>0xae5</PointerOffsetHex>
    <JapaneseText>アッシュがアドリビトムに加入しました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>143</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>2826</PointerOffsetDec>
    <PointerOffsetHex>0xb0a</PointerOffsetHex>
    <JapaneseText>ヴァンがアドリビトムに加入しました</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>144</Id>
    <Status />
  </Entry>
</Entries>