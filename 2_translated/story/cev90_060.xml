<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>1144</PointerOffset>
      <JapaneseText>ニアタ様も…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>90</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1160</PointerOffset>
      <JapaneseText>元はヒトだったんですよね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>91</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1187</PointerOffset>
      <JapaneseText>そうだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>92</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1197</PointerOffset>
      <JapaneseText>我々は、こうして一つの精神集合体に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>93</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1233</PointerOffset>
      <JapaneseText>なる前は、十二人のヒトだった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>94</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1262</PointerOffset>
      <JapaneseText>それは、ニアタ様のディセンダーの為に…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>95</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1302</PointerOffset>
      <JapaneseText>…そうしたんですよね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>96</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1326</PointerOffset>
      <JapaneseText>その、ヒトの姿を捨ててまで…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>97</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1355</PointerOffset>
      <JapaneseText>僕なんかとは違う。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>98</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1375</PointerOffset>
      <JapaneseText>立派です……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1388</PointerOffset>
      <JapaneseText>そなた、何か隠している様だな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1419</PointerOffset>
      <JapaneseText>…………この事は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1439</PointerOffset>
      <JapaneseText>墓の中まで持って行こうと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>102</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1465</PointerOffset>
      <JapaneseText>思ってましたが…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>103</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1482</PointerOffset>
      <JapaneseText>案ずるな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>104</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1494</PointerOffset>
      <JapaneseText>誰にも言わぬ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>105</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1510</PointerOffset>
      <JapaneseText>心痛は解き放っておいた方が良いぞ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>106</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1545</PointerOffset>
      <JapaneseText>内密にする、と言う</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>107</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1564</PointerOffset>
      <JapaneseText>ニアタに同意する</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>108</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1581</PointerOffset>
      <JapaneseText>じゃあ、この話は…、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>109</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1603</PointerOffset>
      <JapaneseText>誰にも言わないで下さいね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>110</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1628</PointerOffset>
      <JapaneseText>僕、元は人間だったんですよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>111</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1658</PointerOffset>
      <JapaneseText>ソウルアルケミーで、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>112</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1680</PointerOffset>
      <JapaneseText>こんな姿になったんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>113</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1703</PointerOffset>
      <JapaneseText>自ら…望んで……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>114</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1720</PointerOffset>
      <JapaneseText>そなたは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>115</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1732</PointerOffset>
      <JapaneseText>ソウルアルケミーの使い手か？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>116</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1761</PointerOffset>
      <JapaneseText>いえ、僕自身は使えません。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>117</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1789</PointerOffset>
      <JapaneseText>ソウルアルケミーによる魔具を手に入れ、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1829</PointerOffset>
      <JapaneseText>その効力によって人間の姿を捨てました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>119</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1866</PointerOffset>
      <JapaneseText>それはもう、使えないのかね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>120</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1893</PointerOffset>
      <JapaneseText>ええ、効果は一度きりです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1918</PointerOffset>
      <JapaneseText>なぜ、人間の姿を捨てたのかを聞く</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1951</PointerOffset>
      <JapaneseText>話を遮らない様に黙っている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>123</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1978</PointerOffset>
      <JapaneseText>僕の国は特殊な階級社会で、僕は生まれてから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2022</PointerOffset>
      <JapaneseText>ずっと軍に所属し、国の為に戦う事を</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>125</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2058</PointerOffset>
      <JapaneseText>宿命づけられていたんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>126</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2083</PointerOffset>
      <JapaneseText>毎日毎日、教え込まれるのは</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>127</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2111</PointerOffset>
      <JapaneseText>殺しの技術ばかり</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>128</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2128</PointerOffset>
      <JapaneseText>他の生き方を選ぶ事は許されませんでした…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>129</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2169</PointerOffset>
      <JapaneseText>けど、いざ戦場に立つと…恐ろしくなって。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>130</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2211</PointerOffset>
      <JapaneseText>そのまま逃げてしまったんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>131</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2240</PointerOffset>
      <JapaneseText>僕の軍で、敵前逃亡は重罪でした。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>132</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2274</PointerOffset>
      <JapaneseText>死刑か、懲罰部隊に転属させられます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>133</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2309</PointerOffset>
      <JapaneseText>僕はもっと生きたくて、逃げたくて、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>134</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2345</PointerOffset>
      <JapaneseText>それで……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>135</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2356</PointerOffset>
      <JapaneseText>その姿に変じた、というわけか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>136</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2387</PointerOffset>
      <JapaneseText>はい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>137</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2395</PointerOffset>
      <JapaneseText>僕がこの姿になったのは、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>138</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2421</PointerOffset>
      <JapaneseText>ただ逃げたかっただけなんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>139</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2450</PointerOffset>
      <JapaneseText>あなたの様に、誰かを守りたいとか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>140</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2484</PointerOffset>
      <JapaneseText>そういうんじゃない…。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>141</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2508</PointerOffset>
      <JapaneseText>逃げたかっただけなんだ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>142</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2533</PointerOffset>
      <JapaneseText>人間に、戻りたいかね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>143</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2556</PointerOffset>
      <JapaneseText>戻りたいと思う事は何度もありました。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>144</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2594</PointerOffset>
      <JapaneseText>ですが、自ら望んで人間の姿を捨てたんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>145</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2635</PointerOffset>
      <JapaneseText>それに…、僕をヒトに戻せる程の術者は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>146</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2673</PointerOffset>
      <JapaneseText>いないでしょう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>147</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2688</PointerOffset>
      <JapaneseText>でもですね、この姿になってから、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>148</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2722</PointerOffset>
      <JapaneseText>いい事もあったんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>149</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2743</PointerOffset>
      <JapaneseText>お嬢様のご両親に出会い、料理、医療の知識、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>150</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2787</PointerOffset>
      <JapaneseText>本当に様々な事を教えてもらった事…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>151</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2822</PointerOffset>
      <JapaneseText>何より、家族の一員として受け入れて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>152</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2858</PointerOffset>
      <JapaneseText>くれた事</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>153</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2867</PointerOffset>
      <JapaneseText>戦う事しか知らないまま生きるはずだったのに、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>154</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2913</PointerOffset>
      <JapaneseText>この姿になってから、幸せな日々を過ごせ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>155</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2953</PointerOffset>
      <JapaneseText>ましたから…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>156</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2966</PointerOffset>
      <JapaneseText>でも、旦那様と奥様は、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>157</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2990</PointerOffset>
      <JapaneseText>幼いお嬢様と過ごす間もなく</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>158</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3018</PointerOffset>
      <JapaneseText>戦場で亡くなられた</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>159</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3037</PointerOffset>
      <JapaneseText>僕と違って、逃げずに、前線の負傷者を助け、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>160</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3081</PointerOffset>
      <JapaneseText>従軍医師としての役目を立派に果たして…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>161</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3120</PointerOffset>
      <JapaneseText>僕はずっと、逃げたままなのに…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>162</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3151</PointerOffset>
      <JapaneseText>そなたは、ヴェラトローパで</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>163</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3179</PointerOffset>
      <JapaneseText>カノンノの身を案じて助けに来たではないか。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>164</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3223</PointerOffset>
      <JapaneseText>逃げる事など無く…真っ先に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>165</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3261</PointerOffset>
      <JapaneseText>そなたには、カノンノという</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>167</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3289</PointerOffset>
      <JapaneseText>守るべき者がいると知っている</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>168</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3318</PointerOffset>
      <JapaneseText>もう、逃げる事はあるまいよ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>169</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>