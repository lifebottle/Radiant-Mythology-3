<?xml version='1.0' encoding='utf-8'?>
<Entries>
  <Entry>
    <PointerOffsetDec>626</PointerOffsetDec>
    <PointerOffsetHex>0x272</PointerOffsetHex>
    <JapaneseText>前から疑問に思ってたんだけどさ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>45</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>661</PointerOffsetDec>
    <PointerOffsetHex>0x295</PointerOffsetHex>
    <JapaneseText>ロックスがたまに言う、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>46</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>685</PointerOffsetDec>
    <PointerOffsetHex>0x2ad</PointerOffsetHex>
    <JapaneseText>「ほすぴたりてぃ」って何なんだ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>47</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>718</PointerOffsetDec>
    <PointerOffsetHex>0x2ce</PointerOffsetHex>
    <JapaneseText>簡単に言えば、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>48</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>734</PointerOffsetDec>
    <PointerOffsetHex>0x2de</PointerOffsetHex>
    <JapaneseText>「お互いを思いやり、手厚くもてなす」</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>49</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>772</PointerOffsetDec>
    <PointerOffsetHex>0x304</PointerOffsetHex>
    <JapaneseText>という事ですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>50</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>787</PointerOffsetDec>
    <PointerOffsetHex>0x313</PointerOffsetHex>
    <JapaneseText>お互いを思いやる……？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>51</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>811</PointerOffsetDec>
    <PointerOffsetHex>0x32b</PointerOffsetHex>
    <JapaneseText>相手を思いやるんじゃなくて？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>52</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>840</PointerOffsetDec>
    <PointerOffsetHex>0x348</PointerOffsetHex>
    <JapaneseText>いい質問です</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>53</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>853</PointerOffsetDec>
    <PointerOffsetHex>0x355</PointerOffsetHex>
    <JapaneseText>そうです。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>54</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>865</PointerOffsetDec>
    <PointerOffsetHex>0x361</PointerOffsetHex>
    <JapaneseText>コンシェルジュの奉仕とは、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>55</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>893</PointerOffsetDec>
    <PointerOffsetHex>0x37d</PointerOffsetHex>
    <JapaneseText>単にお客様にへり下る事ではないんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>56</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>930</PointerOffsetDec>
    <PointerOffsetHex>0x3a2</PointerOffsetHex>
    <JapaneseText>お仕えする者自身も仕事を通じて、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>57</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>964</PointerOffsetDec>
    <PointerOffsetHex>0x3c4</PointerOffsetHex>
    <JapaneseText>成長したり、喜びを感じる……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>58</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>996</PointerOffsetDec>
    <PointerOffsetHex>0x3e4</PointerOffsetHex>
    <JapaneseText>それがホスピタリティの精神です</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>59</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1027</PointerOffsetDec>
    <PointerOffsetHex>0x403</PointerOffsetHex>
    <JapaneseText>えっと……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>60</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1041</PointerOffsetDec>
    <PointerOffsetHex>0x411</PointerOffsetHex>
    <JapaneseText>わかる……様な、わからない様な？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>61</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1074</PointerOffsetDec>
    <PointerOffsetHex>0x432</PointerOffsetHex>
    <JapaneseText>つまり、お客様とのふれあいを通じて</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>62</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1110</PointerOffsetDec>
    <PointerOffsetHex>0x456</PointerOffsetHex>
    <JapaneseText>お互いにハッピーな気持ちになる事を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>63</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1146</PointerOffsetDec>
    <PointerOffsetHex>0x47a</PointerOffsetHex>
    <JapaneseText>仕事にさせていただいてるんです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>64</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1177</PointerOffsetDec>
    <PointerOffsetHex>0x499</PointerOffsetHex>
    <JapaneseText>ハッピーな気持ちかぁ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>65</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1201</PointerOffsetDec>
    <PointerOffsetHex>0x4b1</PointerOffsetHex>
    <JapaneseText>確かに、みんながハッピーな気持ちになるなら</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>66</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1245</PointerOffsetDec>
    <PointerOffsetHex>0x4dd</PointerOffsetHex>
    <JapaneseText>こんないい事ってないよね！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>67</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1272</PointerOffsetDec>
    <PointerOffsetHex>0x4f8</PointerOffsetHex>
    <JapaneseText>その通りです！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>68</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1288</PointerOffsetDec>
    <PointerOffsetHex>0x508</PointerOffsetHex>
    <JapaneseText>そして、それこそがコンシェルジュの信条</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>69</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1327</PointerOffsetDec>
    <PointerOffsetHex>0x52f</PointerOffsetHex>
    <JapaneseText>仕事をする上で</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>70</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1343</PointerOffsetDec>
    <PointerOffsetHex>0x53f</PointerOffsetHex>
    <JapaneseText>最も心がけている事なのです！！</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>71</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1374</PointerOffsetDec>
    <PointerOffsetHex>0x55e</PointerOffsetHex>
    <JapaneseText>へぇ〜、そうだったんだ……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>72</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1404</PointerOffsetDec>
    <PointerOffsetHex>0x57c</PointerOffsetHex>
    <JapaneseText>コンシェルジュの仕事って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>73</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1430</PointerOffsetDec>
    <PointerOffsetHex>0x596</PointerOffsetHex>
    <JapaneseText>すごいんだなぁ……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>74</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1449</PointerOffsetDec>
    <PointerOffsetHex>0x5a9</PointerOffsetHex>
    <JapaneseText>そ、そんな事は……。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>75</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1471</PointerOffsetDec>
    <PointerOffsetHex>0x5bf</PointerOffsetHex>
    <JapaneseText>それに、僕の仕事は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>76</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1491</PointerOffsetDec>
    <PointerOffsetHex>0x5d3</PointerOffsetHex>
    <JapaneseText>皆様がいて下さってのものですから</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>77</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1524</PointerOffsetDec>
    <PointerOffsetHex>0x5f4</PointerOffsetHex>
    <JapaneseText>つい熱っぽく語ってしまって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>78</Id>
    <Status />
  </Entry>
  <Entry>
    <PointerOffsetDec>1552</PointerOffsetDec>
    <PointerOffsetHex>0x610</PointerOffsetHex>
    <JapaneseText>すみません……</JapaneseText>
    <EnglishText></EnglishText>
    <Notes />
    <Id>79</Id>
    <Status />
  </Entry>
</Entries>