<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>876</PointerOffset>
    <JapaneseText>ん…、誰なの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>64</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>891</PointerOffset>
    <JapaneseText>ああ、○○だったのね。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>65</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>915</PointerOffset>
    <JapaneseText>どうしたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>66</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>928</PointerOffset>
    <JapaneseText>絵筆を持ってきてくれたの…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>67</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>958</PointerOffset>
    <JapaneseText>ロックスからだね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>68</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>975</PointerOffset>
    <JapaneseText>うん、ありがとう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>69</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>992</PointerOffset>
    <JapaneseText>描いてる絵を見せて、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>70</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1019</PointerOffset>
    <JapaneseText>絵を描くのは好きか、と聞く</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1046</PointerOffset>
    <JapaneseText>絵を？　ロックスに聞いたの？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1076</PointerOffset>
    <JapaneseText>あまり上手じゃないよ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1097</PointerOffset>
    <JapaneseText>でも、見てもらいたいな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1120</PointerOffset>
    <JapaneseText>うん、好きだよ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1138</PointerOffset>
    <JapaneseText>独学だから、上手じゃないけど</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1167</PointerOffset>
    <JapaneseText>じゃあ、あなたに</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1185</PointerOffset>
    <JapaneseText>絵を見てもらおうかなあ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1211</PointerOffset>
    <JapaneseText>いい？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1230</PointerOffset>
    <JapaneseText>こんな風景、見た事ある？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1255</PointerOffset>
    <JapaneseText>ないと答える</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1268</PointerOffset>
    <JapaneseText>どこの風景かを尋ねる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1289</PointerOffset>
    <JapaneseText>そっかぁ…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1303</PointerOffset>
    <JapaneseText>記憶の手掛かりになるかと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1329</PointerOffset>
    <JapaneseText>思ったんだけど…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1346</PointerOffset>
    <JapaneseText>私もね、この風景を</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1366</PointerOffset>
    <JapaneseText>実際に見た事無いんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1387</PointerOffset>
    <JapaneseText>私も知らない…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1402</PointerOffset>
    <JapaneseText>私はこの風景が、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1420</PointerOffset>
    <JapaneseText>どこかにあるんじゃないかなって</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1452</PointerOffset>
    <JapaneseText>思ってるんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1465</PointerOffset>
    <JapaneseText>スケッチブックの白い紙を見てるとね、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1503</PointerOffset>
    <JapaneseText>たまに見えてくるんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1527</PointerOffset>
    <JapaneseText>色んな風景が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1540</PointerOffset>
    <JapaneseText>その見えた風景を筆でなぞって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1572</PointerOffset>
    <JapaneseText>出来たのがこれらの絵なの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1597</PointerOffset>
    <JapaneseText>どうしてだかはわからない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1625</PointerOffset>
    <JapaneseText>でも、見えるんだ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1642</PointerOffset>
    <JapaneseText>他の人にも見せたけど、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1666</PointerOffset>
    <JapaneseText>誰もこの風景を知らない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1689</PointerOffset>
    <JapaneseText>それに、作り話でしょって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1717</PointerOffset>
    <JapaneseText>笑われちゃうの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1732</PointerOffset>
    <JapaneseText>カノンノの話を信じる</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1753</PointerOffset>
    <JapaneseText>事実なんだからしょうがない、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1788</PointerOffset>
    <JapaneseText>うん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1796</PointerOffset>
    <JapaneseText>…あり、がとう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1811</PointerOffset>
    <JapaneseText>うん、事実なんだもん</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1844</PointerOffset>
    <JapaneseText>○○は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>109</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1854</PointerOffset>
    <JapaneseText>信じてくれるんだね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>110</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1873</PointerOffset>
    <JapaneseText>あり、がとう…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>111</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1888</PointerOffset>
    <JapaneseText>この船でさ、色んな所を旅して、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>112</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1920</PointerOffset>
    <JapaneseText>これらの風景に出会えたら…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>113</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1947</PointerOffset>
    <JapaneseText>もし出会えたら、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>114</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1965</PointerOffset>
    <JapaneseText>どうして私に知らない風景が見えるのか、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>115</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2005</PointerOffset>
    <JapaneseText>その理由がわかるかもしれない…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>116</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2036</PointerOffset>
    <JapaneseText>それにね、「知らなきゃいけない」って</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>117</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2074</PointerOffset>
    <JapaneseText>思いもあるんだ。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>118</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2092</PointerOffset>
    <JapaneseText>…見えた風景は、綺麗なものだけじゃないの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>119</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2133</PointerOffset>
    <JapaneseText>恐い、この世の終わりみたいな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>120</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2163</PointerOffset>
    <JapaneseText>風景も見えたから…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>121</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
