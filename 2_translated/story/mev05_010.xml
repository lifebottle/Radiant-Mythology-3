<SceneText>
  <FriendlyName></FriendlyName>
  <Speakers/>
    <Section></Section>
  <Entries>
    <Entry>
      <PointerOffset>228</PointerOffset>
      <JapaneseText>今回は、ちょっと変わった依頼よ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>259</PointerOffset>
      <JapaneseText>カダイフ砂漠のオアシスまで行って、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>295</PointerOffset>
      <JapaneseText>魔物を捨ててきて欲しいの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>320</PointerOffset>
      <JapaneseText>カダイフ砂漠に着いたら、</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>346</PointerOffset>
      <JapaneseText>依頼主のトマスさんから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>370</PointerOffset>
      <JapaneseText>魔物が入ったケージを渡されるから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>403</PointerOffset>
      <JapaneseText>オアシスまで、そのケージを運んでちょうだい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>446</PointerOffset>
      <JapaneseText>今回は、クレス君とイリアが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>474</PointerOffset>
      <JapaneseText>同行する事になってるから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>499</PointerOffset>
      <JapaneseText>それと、契約で渡されたケージの中は</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>535</PointerOffset>
      <JapaneseText>絶対に見ないでくれって言われてるの。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>573</PointerOffset>
      <JapaneseText>それは守ってちょうだいね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>598</PointerOffset>
      <JapaneseText>じゃあ、頼んだわよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>620</PointerOffset>
      <JapaneseText>行ってらっしゃい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
  </Entries>
</SceneText>