<?xml version="1.0" ?>
<Entries>
  <Entry>
    <PointerOffset>1010</PointerOffset>
    <JapaneseText>ねえ、○○。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>71</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1024</PointerOffset>
    <JapaneseText>あなたって、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>72</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1038</PointerOffset>
    <JapaneseText>空からやって来たって本当？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>73</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1065</PointerOffset>
    <JapaneseText>そういう事だったらしい、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>74</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1096</PointerOffset>
    <JapaneseText>自分は知らない、と言う</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>75</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1119</PointerOffset>
    <JapaneseText>じゃあ、ほんとの事は</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>76</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1141</PointerOffset>
    <JapaneseText>わからないんじゃない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>77</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1162</PointerOffset>
    <JapaneseText>知らないって…？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>78</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1179</PointerOffset>
    <JapaneseText>それじゃあ、ウソかどうか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>79</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1205</PointerOffset>
    <JapaneseText>確かめ様が無いじゃない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>80</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1228</PointerOffset>
    <JapaneseText>でも、本当なんだもん。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>81</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1252</PointerOffset>
    <JapaneseText>光に包まれて、空から降りて来たの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>82</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1285</PointerOffset>
    <JapaneseText>そんな事、簡単には信じられないわ</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>83</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1318</PointerOffset>
    <JapaneseText>真実であるかどうかは別としても、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>84</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1352</PointerOffset>
    <JapaneseText>素敵なお話だと思いますよ？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>85</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1379</PointerOffset>
    <JapaneseText>世界樹が光を発した日に</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>86</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1403</PointerOffset>
    <JapaneseText>空から降りて来た、なんて…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>87</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1433</PointerOffset>
    <JapaneseText>まるで、ディセンダーの様ですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>88</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1464</PointerOffset>
    <JapaneseText>ディセンダー？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>89</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1479</PointerOffset>
    <JapaneseText>そう。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>90</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1487</PointerOffset>
    <JapaneseText>あたしやアンジュさんがいた教会には、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>91</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1525</PointerOffset>
    <JapaneseText>「救世主」の言い伝えがあるの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>92</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1554</PointerOffset>
    <JapaneseText>世界に危機が訪れた時、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>93</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1578</PointerOffset>
    <JapaneseText>世界樹が生み出す存在</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>94</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1599</PointerOffset>
    <JapaneseText>遠い昔から、教会に伝わる「予言」です</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>95</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1636</PointerOffset>
    <JapaneseText>世界樹から生まれたばかりのディセンダーは</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>96</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1678</PointerOffset>
    <JapaneseText>記憶が無く、世界の事も知らず、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>97</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1709</PointerOffset>
    <JapaneseText>不可能も恐れも知らない無垢な存在だと</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>98</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1747</PointerOffset>
    <JapaneseText>言います</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>99</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1756</PointerOffset>
    <JapaneseText>そして、人々の為に世界を学び、強くなり、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>100</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1798</PointerOffset>
    <JapaneseText>ゆくゆくは世界を平和に導き…、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>101</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1830</PointerOffset>
    <JapaneseText>また世界樹へと還って行くそうです</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>102</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1863</PointerOffset>
    <JapaneseText>○○さんの</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>103</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1875</PointerOffset>
    <JapaneseText>「記憶が無い」という点は、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>104</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1903</PointerOffset>
    <JapaneseText>まさにディセンダーの様ですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>105</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1932</PointerOffset>
    <JapaneseText>じゃあ、彼はディセンダー？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>106</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1959</PointerOffset>
    <JapaneseText>じゃあ、彼女はディセンダー？</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>107</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>1988</PointerOffset>
    <JapaneseText>だが、彼がディセンダーか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>108</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2014</PointerOffset>
    <JapaneseText>どうかの真偽はわからない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>109</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2042</PointerOffset>
    <JapaneseText>あくまで予言で伝えられているだけだからな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>110</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2083</PointerOffset>
    <JapaneseText>だが、彼女がディセンダーか</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>111</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2111</PointerOffset>
    <JapaneseText>どうかの真偽はわからない。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>112</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2139</PointerOffset>
    <JapaneseText>あくまで予言で伝えられているだけだからな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>113</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2180</PointerOffset>
    <JapaneseText>そもそも、その予言自体の真偽すら、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>114</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2216</PointerOffset>
    <JapaneseText>誰にもわからない</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>115</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2233</PointerOffset>
    <JapaneseText>昔の人の創作話という見方が一般的だろう</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>116</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2272</PointerOffset>
    <JapaneseText>そう、ですか…</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>117</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2287</PointerOffset>
    <JapaneseText>いや、かくいうオレも</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>118</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2309</PointerOffset>
    <JapaneseText>世界樹が光ったのを見た時、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>119</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2337</PointerOffset>
    <JapaneseText>真っ先に予言の話を思い出したからな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>120</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2372</PointerOffset>
    <JapaneseText>しかし…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>121</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2384</PointerOffset>
    <JapaneseText>世間ではディセンダーの出現を待つ宗教が</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>122</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2424</PointerOffset>
    <JapaneseText>次々に興っている様だな</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>123</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2447</PointerOffset>
    <JapaneseText>この様なご時世ですし、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>124</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2471</PointerOffset>
    <JapaneseText>救いを求める人々が多い証拠ですね</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>125</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2504</PointerOffset>
    <JapaneseText>各地で争いが絶えませんからね…。</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>126</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2538</PointerOffset>
    <JapaneseText>救いを求める人が増えても、</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>127</Id>
    <Status>To Do</Status>
  </Entry>
  <Entry>
    <PointerOffset>2566</PointerOffset>
    <JapaneseText>おかしくはありません</JapaneseText>
    <EnglishText></EnglishText>
    <Notes/>
    <Id>128</Id>
    <Status>To Do</Status>
  </Entry>
</Entries>
